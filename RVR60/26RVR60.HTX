\v 20 Por cuanto <a name="02600702000012" ll="SDBH:שׂים:000005">convirtieron</a> la <a name="02600702000004" ll="SDBH:צְבִי:000000">gloria</a> de <a name="02600702000006" ll="SDBH:עֲדִי:000000">su</a> <a name="02600702000006" ll="SDBH:עֲדִי:000000">ornamento</a> en <a name="02600702000010" ll="SDBH:גָּאֹון:001000">soberbia</a>, e <a name="02600702000022">hicieron</a> de ello las <a name="02600702000016" ll="SDBH:צֶלֶם:000000">imágenes</a> de sus abominables <a name="02600702000018" ll="SDBH:תֹּועֵבָה:000002">ídolos</a>, por <a name="02600702000026">eso</a> <a name="02600702000028">se</a> lo <a name="02600702000030">convertí</a> en cosa <a name="02600702000036" ll="SDBH:נִדָּה:000001">repugnante</a>. 
\v 21 En <a name="02600702100008">mano</a> de <a name="02600702100012" ll="SDBH:זָר:000000">extraños</a> la <a name="02600702100004">entregué</a> para ser <a name="02600702100016" ll="SDBH:בַּז:000000">saqueada</a>, y será <a name="02600702100030" ll="SDBH:שָׁלָל:000000">presa</a> de los <a name="02600702100022" ll="SDBH:רָשָׁע:000000">impíos</a> de la <a name="02600702100026" ll="SDBH:אֶרֶץ:000000">tierra</a>, y la <a name="02600702100034" ll="SDBH:חלל:000002">profanarán</a>. 
\v 22 Y <a name="02600702200004">apartaré</a> de ellos <a name="02600702200006" ll="SDBH:פָּנֶה:000000">mi</a> <a name="02600702200006" ll="SDBH:פָּנֶה:000000">rostro</a>, y será <a name="02600702200012" ll="SDBH:חלל:000002">violado</a> mi lugar <a name="02600702200016" ll="SDBH:צפן:000007">secreto</a>; pues <a name="02600702200020">entrarán</a> en él <a name="02600702200024" ll="SDBH:פָּרִיץ:000001">invasores</a> y lo <a name="02600702200028" ll="SDBH:חלל:000002">profanarán</a>.
    
\v 23 <a name="02600702300002">Haz</a> una <a name="02600702300006" ll="SDBH:רַתֹּוק:000000" tl="HOTTP3672">cadena</a>, porque la <a name="02600702300012" ll="SDBH:אֶרֶץ:000000">tierra</a> está <a name="02600702300014" ll="SDBH:מלא:000010">llena</a> de <a name="02600702300016" ll="SDBH:מִשְׁפָּט:000000" il="Tel-dan-israelite-gate-im-tor" tl="HOTTP3673">delitos</a> de <a name="02600702300018" ll="SDBH:דָּם:000003" tl="HOTTP3673">sangre</a>, y la <a name="02600702300024" ll="SDBH:עִיר:000000">ciudad</a> está <a name="02600702300026" ll="SDBH:מלא:000010">llena</a> de <a name="02600702300028" ll="SDBH:חָמָס:000000">violencia</a>. 
\v 24 <a name="02600702400004">Traeré</a>, por tanto, los más <a name="02600702400006" ll="SDBH:רַע:000001">perversos</a> de las <a name="02600702400008" ll="SDBH:גֹּוי:000000">naciones</a>, los cuales <a name="02600702400012" ll="SDBH:ירשׁ:000002">poseerán</a> las <a name="02600702400016" ll="SDBH:בַּיִת:000000">casas</a> de ellos; y haré <a name="02600702400020" ll="SDBH:שׁבת:000003">cesar</a> la <a name="02600702400022" ll="SDBH:גָּאֹון:001000">soberbia</a> de los <a name="02600702400024" ll="SDBH:עַז:000000">poderosos</a>, y sus santuarios serán <a name="02600702400028" ll="SDBH:חלל:000002">profanados</a>. 
\v 25 <a name="02600702500002" ll="SDBH:קְפָדָה:000000">Destrucción</a> <a name="02600702500004">viene</a>; y <a name="02600702500008" ll="SDBH:בקשׁ:000007">buscarán</a> la <a name="02600702500010" ll="SDBH:שָׁלֹום:000000">paz</a>, y <a name="02600702500014">no</a> la habrá. 
\v 26 <a name="02600702600002;02600702600006" ll="SDBH:הֹוָה:000000">Quebrantamiento</a> <a name="02600702600008">vendrá</a> <a name="02600702600004">sobre</a> quebrantamiento, y <a name="02600702600018">habrá</a> <a name="02600702600012" ll="SDBH:שְׁמוּעָה:000000">rumor</a> <a name="02600702600014">sobre</a> <a name="02600702600016" ll="SDBH:שְׁמוּעָה:000000">rumor</a>; y <a name="02600702600022" ll="SDBH:בקשׁ:000007">buscarán</a> <a name="02600702600024" ll="SDBH:חָזֹון:000001">respuesta</a> del <a name="02600702600028" ll="SDBH:נָבִיא:000000">profeta</a>, mas la <a name="02600702600032" ll="SDBH:תֹּורָה:000000">ley</a> <a name="02600702600034" ll="SDBH:אבד:000006">se</a> <a name="02600702600034" ll="SDBH:אבד:000006">alejará</a> del <a name="02600702600038" ll="SDBH:כֹּהֵן:000000">sacerdote</a>, y de los <a name="02600702600046" ll="SDBH:זָקֵן:000003">ancianos</a> el <a name="02600702600042" ll="SDBH:עֵצָה:000001">consejo</a>. 
\v 27 El <a name="02600702700004" ll="SDBH:מֶלֶךְ:000000">rey</a> <a name="02600702700006" ll="SDBH:אבל:000000">se</a> <a name="02600702700006" ll="SDBH:אבל:000000">enlutará</a>, y el <a name="02600702700010" ll="SDBH:נָשִׂיא:000001">príncipe</a> <a name="02600702700012" ll="SDBH:לבשׁ:000000">se</a> <a name="02600702700012" ll="SDBH:לבשׁ:000000">vestirá</a> de <a name="02600702700014" ll="SDBH:שְׁמָמָה:000001">tristeza</a>, y las <a name="02600702700018">manos</a> del <a name="02600702700020" ll="SDBH:עַם:000000">pueblo</a> de la <a name="02600702700024" ll="SDBH:אֶרֶץ:000000">tierra</a> <a name="02600702700026" ll="SDBH:בהל:001000">temblarán</a>; según <a name="02600702700030" ll="SDBH:דֶּרֶךְ:000007">su</a> <a name="02600702700030" ll="SDBH:דֶּרֶךְ:000007">camino</a> <a name="02600702700032">haré</a> con ellos, y con los <a name="02600702700040" ll="SDBH:מִשְׁפָּט:000001">juicios</a> de ellos los <a name="02600702700042" ll="SDBH:שׁפט:000004">juzgaré</a>; y <a name="02600702700046">sabrán</a> que <a name="02600702700050">yo</a> soy <a name="02600702700052" ll="SDBH:יהוה:000000">Jehová</a>.
\c 8
\v 1 En el <a name="02600800100014" ll="SDBH:שִׁשִּׁי:000000">sexto</a> <a name="02600800100010" ll="SDBH:שָׁנָה:000000">año</a>, en el mes <a name="02600800100020" ll="SDBH:שִׁשִּׁי:000000">sexto</a>, a los <a name="02600800100024">cinco</a> días del <a name="02600800100030" ll="SDBH:חֹדֶשׁ:000000">mes</a>, <a name="02600800100004">aconteció</a> que estaba <a name="02600800100032">yo</a> <a name="02600800100034" ll="SDBH:ישׁב:000000">sentado</a> en <a name="02600800100038" ll="SDBH:בַּיִת:000000">mi</a> <a name="02600800100038" ll="SDBH:בַּיִת:000000">casa</a>, y los <a name="02600800100042" ll="SDBH:זָקֵן:000003">ancianos</a> de <a name="02600800100044" ll="SDBH:יְהוּדָה:000000">Judá</a> estaban <a name="02600800100046" ll="SDBH:ישׁב:000000">sentados</a> <a name="02600800100050">delante</a> de mí, y <a name="02600800100058" ll="SDBH:שָׁם:000000">allí</a> <a name="02600800100054" ll="SDBH:נפל:000021">se</a> <a name="02600800100054" ll="SDBH:נפל:000021">posó</a> <a name="02600800100056">sobre</a> mí la <a name="02600800100060">mano</a> de <a name="02600800100064">Jehová</a> el <a name="02600800100062" ll="SDBH:אֲדֹנָי:000000">Señor</a>. 
\v 2 Y <a name="02600800200004" ll="SDBH:ראה:000000">miré</a>, y he aquí una <a name="02600800200010" ll="SDBH:דְּמוּת:000000">figura</a> que <a name="02600800200014" ll="SDBH:מַרְאֶה:000000" tl="HOTTP3674">parecía</a> de <a name="02600800200016" ll="SDBH:אֵשׁ:000000" il="Fire02" tl="HOTTP3674">hombre</a>; desde <a name="02600800200022" ll="SDBH:מָתְנַיִם:000000">sus</a> <a name="02600800200022" ll="SDBH:מָתְנַיִם:000000">lomos</a> para <a name="02600800200028" ll="SDBH:מַטָּה:000000">abajo</a>, <a name="02600800200030" ll="SDBH:אֵשׁ:000000">fuego</a>; y desde <a name="02600800200036" ll="SDBH:מָתְנַיִם:000000">sus</a> <a name="02600800200036" ll="SDBH:מָתְנַיִם:000000">lomos</a> para arriba <a name="02600800200020" ll="SDBH:מַרְאֶה:000000">parecía</a> <a name="02600800200048" ll="SDBH:זֹהַר:000000">resplandor</a>, el <a name="02600800200052" ll="SDBH:עַיִן:000004">aspecto</a> de bronce <a name="02600800200056" ll="SDBH:חַשְׁמַל:000000;SDBH:חַשְׁמַל:000001">refulgente</a>. 
\v 3 Y aquella <a name="02600800300006" ll="SDBH:תַּבְנִית:000000">figura</a> <a name="02600800300004" ll="SDBH:שׁלח:000015">extendió</a> la <a name="02600800300008">mano</a>, y me <a name="02600800300012" ll="SDBH:לקח:000000">tomó</a> por las guedejas de <a name="02600800300018" ll="SDBH:רֹאשׁ:000000">mi</a> <a name="02600800300018" ll="SDBH:רֹאשׁ:000000">cabeza</a>; y el <a name="02600800300026" ll="SDBH:רוּחַ:000000">Espíritu</a> me <a name="02600800300022" ll="SDBH:נשׂא:000003">alzó</a> <a name="02600800300028">entre</a> el <a name="02600800300040" ll="SDBH:שָׁמַיִם:000000">cielo</a> y la <a name="02600800300032" ll="SDBH:אֶרֶץ:000000">tierra</a>, y me <a name="02600800300044">llevó</a> en <a name="02600800300052" ll="SDBH:מַרְאָה:000000" tl="HOTTP3675">visiones</a> de <a name="02600800300054" ll="SDBH:אֱלֹהִים:000002" tl="HOTTP3675">Dios</a> a <a name="02600800300048" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>, <a name="02600800300056">a</a> la <a name="02600800300058" ll="SDBH:פֶּתַח:000000">entrada</a> de la <a name="02600800300060" ll="SDBH:שַׁעַר:000000">puerta</a> de <a name="02600800300064">adentro</a> que <a name="02600800300068" ll="SDBH:פנה:000001">mira</a> hacia el <a name="02600800300070" ll="SDBH:צָפֹון:000001">norte</a>, <a name="02600800300072">donde</a> estaba la <a name="02600800300076" ll="SDBH:מֹושָׁב:000000">habitación</a> de la <a name="02600800300078" ll="SDBH:סֶמֶל:000000">imagen</a> del <a name="02600800300082" ll="SDBH:קִנְאָה:000001">celo</a>, la que provoca a celos. 
\v 4 Y he aquí, <a name="02600800400006" ll="SDBH:שָׁם:000000">allí</a> estaba la <a name="02600800400008" ll="SDBH:כָּבֹוד:000004">gloria</a> del <a name="02600800400010" ll="SDBH:אֱלֹהִים:000001">Dios</a> de <a name="02600800400012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, como la <a name="02600800400018" ll="SDBH:מַרְאֶה:000006">visión</a> <a name="02600800400020">que</a> yo <a name="02600800400022" ll="SDBH:ראה:000000">había</a> <a name="02600800400022" ll="SDBH:ראה:000000">visto</a> en el <a name="02600800400028" ll="SDBH:בִּקְעָה:000000">campo</a>.
    
\v 5 Y <a name="02600800500006">me</a> <a name="02600800500004" ll="SDBH:אמר:000000">dijo</a>: <a name="02600800500008" ll="SDBH:בֵּן:000003">Hijo</a> de <a name="02600800500010" ll="SDBH:אָדָם:000000">hombre</a>, <a name="02600800500012" ll="SDBH:נשׂא:000039">alza</a> <a name="02600800500014" ll="SDBH:נָא:001000">ahora</a> <a name="02600800500016" ll="SDBH:עַיִן:000000">tus</a> <a name="02600800500016" ll="SDBH:עַיִן:000000">ojos</a> hacia el <a name="02600800500018" ll="SDBH:דֶּרֶךְ:000006">lado</a> del <a name="02600800500020" ll="SDBH:צָפֹון:000001">norte</a>. Y <a name="02600800500024" ll="SDBH:נשׂא:000039">alcé</a> <a name="02600800500026" ll="SDBH:עַיִן:000000">mis</a> <a name="02600800500026" ll="SDBH:עַיִן:000000">ojos</a> hacia el <a name="02600800500030" ll="SDBH:צָפֹון:000001">norte</a>, y <a name="02600800500034">he</a> <a name="02600800500034">aquí</a> al <a name="02600800500038" ll="SDBH:צָפֹון:000001">norte</a>, junto a la <a name="02600800500042" ll="SDBH:שַׁעַר:000000">puerta</a> del <a name="02600800500046" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, aquella <a name="02600800500048" ll="SDBH:סֶמֶל:000000">imagen</a> del <a name="02600800500052" ll="SDBH:קִנְאָה:000001">celo</a> en la <a name="02600800500062" ll="SDBH:בִּאָה:000000">entrada</a>. 
\v 6 <a name="02600800600006">Me</a> <a name="02600800600004" ll="SDBH:אמר:000000">dijo</a> entonces: <a name="02600800600008" ll="SDBH:בֵּן:000003">Hijo</a> de <a name="02600800600010" ll="SDBH:אָדָם:000000">hombre</a>, ¿no <a name="02600800600014" ll="SDBH:ראה:000000">ves</a> lo que <a name="02600800600020">éstos</a> <a name="02600800600026">hacen</a>, las <a name="02600800600030">grandes</a> <a name="02600800600028" ll="SDBH:גָּדֹול:000003;SDBH:תֹּועֵבָה:000000">abominaciones</a> <a name="02600800600032">que</a> la <a name="02600800600034" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600800600036" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="02600800600038" ll="SDBH:פֹּה:000000">hace</a> <a name="02600800600040">aquí</a> para <a name="02600800600044" ll="SDBH:רחק:000001">alejarme</a> de <a name="02600800600050" ll="SDBH:מִקְדָּשׁ:000001">mi</a> <a name="02600800600050" ll="SDBH:מִקְדָּשׁ:000001">santuario</a>? Pero <a name="02600800600056" ll="SDBH:שׁוב:000041">vuélvete</a> <a name="02600800600054">aún</a>, y <a name="02600800600058" ll="SDBH:ראה:000000">verás</a> <a name="02600800600060" ll="SDBH:גָּדֹול:000003;SDBH:תֹּועֵבָה:000000">abominaciones</a> <a name="02600800600062">mayores</a>.
    
\v 7 Y me <a name="02600800700004">llevó</a> <a name="02600800700008">a</a> la <a name="02600800700010" ll="SDBH:פֶּתַח:000000">entrada</a> del <a name="02600800700014" ll="SDBH:חָצֵר:000001">atrio</a>, y <a name="02600800700018" ll="SDBH:ראה:000000">miré</a>, y he aquí en la <a name="02600800700032" ll="SDBH:קִיר:000000">pared</a> <a name="02600800700026" ll="SDBH:אֶחָד:000000">un</a> <a name="02600800700024" ll="SDBH:חֹר:000000">agujero</a>. 
