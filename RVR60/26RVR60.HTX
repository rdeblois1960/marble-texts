\v 8 Mas <a name="02600200800004">tú</a>, <a name="02600200800006" ll="SDBH:בֵּן:000003">hijo</a> de <a name="02600200800008" ll="SDBH:אָדָם:000000">hombre</a>, <a name="02600200800010">oye</a> lo <a name="02600200800014">que</a> <a name="02600200800016">yo</a> <a name="02600200800020">te</a> <a name="02600200800018" ll="SDBH:דבר:001000">hablo</a>; <a name="02600200800052">no</a> <a name="02600200800024">seas</a> <a name="02600200800026" ll="SDBH:מְרִי:000000">rebelde</a> como la <a name="02600200800030" ll="SDBH:בַּיִת:000003">casa</a> <a name="02600200800034" ll="SDBH:מְרִי:000001">rebelde</a>; <a name="02600200800036" ll="SDBH:פצה:000000">abre</a> <a name="02600200800038" ll="SDBH:פֶּה:000000">tu</a> <a name="02600200800038" ll="SDBH:פֶּה:000000">boca</a>, y <a name="02600200800042" ll="SDBH:אכל:000000">come</a> lo <a name="02600200800046">que</a> <a name="02600200800048">yo</a> <a name="02600200800022" ll="SDBH:אַל:000000">te</a> <a name="02600200800050">doy</a>.
    
\v 9 Y <a name="02600200900004" ll="SDBH:ראה:000000">miré</a>, y he aquí una <a name="02600200900010">mano</a> <a name="02600200900012" ll="SDBH:שׁלח:000015">extendida</a> <a name="02600200900014">hacia</a> mí, y en ella había un <a name="02600200900022" ll="SDBH:מְגִלָּה:000000">rollo</a> de <a name="02600200900024">libro</a>. 
\v 10 Y lo <a name="02600201000004" ll="SDBH:פרשׂ:000000">extendió</a> <a name="02600201000010">delante</a> de mí, y estaba <a name="02600201000016" ll="SDBH:כתב:000000">escrito</a> por <a name="02600201000018" ll="SDBH:פָּנֶה:000000">delante</a> y por <a name="02600201000022" ll="SDBH:אָחֹור:000000">detrás</a>; y <a name="02600201000026" ll="SDBH:כתב:000000">había</a> <a name="02600201000026" ll="SDBH:כתב:000000">escritas</a> <a name="02600201000028">en</a> <a name="02600201000014">él</a> <a name="02600201000030" ll="SDBH:קִינָה:000000" tl="HOTTP3645">endechas</a> y <a name="02600201000034" ll="SDBH:הֶגֶה:000000" tl="HOTTP3645">lamentaciones</a> y <a name="02600201000038" ll="SDBH:הִי:000000" tl="HOTTP3645">ayes</a>.
\c 3
\v 1 <a name="02600300100006">Me</a> <a name="02600300100004" ll="SDBH:אמר:000000">dijo</a>: <a name="02600300100008" ll="SDBH:בֵּן:000003">Hijo</a> de <a name="02600300100010" ll="SDBH:אָדָם:000000">hombre</a>, <a name="02600300100018" ll="SDBH:אכל:000000">come</a> lo <a name="02600300100014">que</a> <a name="02600300100016" ll="SDBH:מצא:000012">hallas</a>; <a name="02600300100020" ll="SDBH:אכל:000000">come</a> este <a name="02600300100026" ll="SDBH:מְגִלָּה:000000">rollo</a>, y <a name="02600300100034" ll="SDBH:הלך:000001">ve</a> y <a name="02600300100036" ll="SDBH:דבר:001000">habla</a> <a name="02600300100038">a</a> la <a name="02600300100040" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600300100042" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 2 Y <a name="02600300200004" ll="SDBH:פתח:000015">abrí</a> <a name="02600300200008" ll="SDBH:פֶּה:000000">mi</a> <a name="02600300200008" ll="SDBH:פֶּה:000000">boca</a>, y me hizo <a name="02600300200012" ll="SDBH:אכל:000001">comer</a> aquel <a name="02600300200018" ll="SDBH:מְגִלָּה:000000">rollo</a>. 
\v 3 Y <a name="02600300300006">me</a> <a name="02600300300004" ll="SDBH:אמר:000000">dijo</a>: <a name="02600300300008" ll="SDBH:בֵּן:000003">Hijo</a> de <a name="02600300300010" ll="SDBH:אָדָם:000000">hombre</a>, <a name="02600300300014" ll="SDBH:אכל:000001">alimenta</a> <a name="02600300300012" ll="SDBH:בֶּטֶן:000000">tu</a> <a name="02600300300012" ll="SDBH:בֶּטֶן:000000">vientre</a>, y <a name="02600300300020" ll="SDBH:מלא:000002">llena</a> <a name="02600300300018" ll="SDBH:מֵעֶה:000000">tus</a> <a name="02600300300018" ll="SDBH:מֵעֶה:000000">entrañas</a> de este <a name="02600300300026" ll="SDBH:מְגִלָּה:000000">rollo</a> <a name="02600300300032">que</a> <a name="02600300300034">yo</a> <a name="02600300300038">te</a> <a name="02600300300036">doy</a>. Y lo <a name="02600300300042" ll="SDBH:אכל:000000">comí</a>, y <a name="02600300300046">fue</a> en <a name="02600300300050" ll="SDBH:פֶּה:000000">mi</a> <a name="02600300300050" ll="SDBH:פֶּה:000000">boca</a> <a name="02600300300058" ll="SDBH:מָתֹוק:000000">dulce</a> como <a name="02600300300054" ll="SDBH:דְּבַשׁ:000000">miel</a>.
    
\v 4 Luego <a name="02600300400006">me</a> <a name="02600300400004" ll="SDBH:אמר:000000">dijo</a>: <a name="02600300400008" ll="SDBH:בֵּן:000003">Hijo</a> de <a name="02600300400010" ll="SDBH:אָדָם:000000">hombre</a>, <a name="02600300400012" ll="SDBH:הלך:000001">ve</a> y <a name="02600300400014">entra</a> <a name="02600300400016">a</a> la <a name="02600300400018" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600300400020" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, y <a name="02600300400024" ll="SDBH:דבר:001000">habla</a> <a name="02600300400030">a</a> <a name="02600300400030">ellos</a> con <a name="02600300400028" ll="SDBH:דָּבָר:000000">mis</a> <a name="02600300400028" ll="SDBH:דָּבָר:000000">palabras</a>. 
\v 5 Porque <a name="02600300500004" ll="SDBH:לֹא:000000">no</a> eres <a name="02600300500022" ll="SDBH:שׁלח:000000">enviado</a> <a name="02600300500006">a</a> <a name="02600300500008" ll="SDBH:עַם:000000">pueblo</a> de <a name="02600300500012" ll="SDBH:שָׂפָה:000002">habla</a> <a name="02600300500010" ll="SDBH:עָמֵק:000000">profunda</a> ni de <a name="02600300500018" ll="SDBH:לָשֹׁון:000002">lengua</a> <a name="02600300500016" ll="SDBH:כָּבֵד:000000">difícil</a>, sino <a name="02600300500024">a</a> la <a name="02600300500026" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600300500028" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 6 <a name="02600300600002;02600300600022" ll="SDBH:לֹא:000000">No</a> <a name="02600300600004">a</a> <a name="02600300600008" ll="SDBH:רַב:000001">muchos</a> <a name="02600300600006" ll="SDBH:עַם:000000">pueblos</a> de <a name="02600300600012" ll="SDBH:שָׂפָה:000002">habla</a> <a name="02600300600010" ll="SDBH:עָמֵק:000000">profunda</a> ni de <a name="02600300600018" ll="SDBH:לָשֹׁון:000002">lengua</a> <a name="02600300600016" ll="SDBH:כָּבֵד:000000">difícil</a>, <a name="02600300600036">cuyas</a> <a name="02600300600026" ll="SDBH:דָּבָר:000000">palabras</a> no <a name="02600300600024">entiendas</a>; y <a name="02600300600028">si</a> <a name="02600300600032">a</a> <a name="02600300600032">ellos</a> te <a name="02600300600034" ll="SDBH:שׁלח:000000">enviara</a>, ellos <a name="02600300600040">te</a> <a name="02600300600038">oyeran</a>. 
\v 7 Mas la <a name="02600300700004" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600300700006" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="02600300700008" ll="SDBH:לֹא:000000">no</a> <a name="02600300700016">te</a> <a name="02600300700010" ll="SDBH:אבה:000000">querrá</a> oír, porque <a name="02600300700020">no</a> me <a name="02600300700022" ll="SDBH:אבה:000000">quiere</a> oír <a name="02600300700028">a</a> mí; porque <a name="02600300700032">toda</a> la <a name="02600300700034" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600300700036" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> es <a name="02600300700038" ll="SDBH:חָזָק:000001">dura</a> de <a name="02600300700040" ll="SDBH:מֵצַח:000000">frente</a> y <a name="02600300700044" ll="SDBH:קָשֶׁה:000008">obstinada</a> de <a name="02600300700046" ll="SDBH:לֵב:000001">corazón</a>. 
\v 8 He aquí yo <a name="02600300800004">he</a> <a name="02600300800004">hecho</a> <a name="02600300800008" ll="SDBH:פָּנֶה:000000">tu</a> <a name="02600300800008" ll="SDBH:פָּנֶה:000000">rostro</a> <a name="02600300800010" ll="SDBH:חָזָק:000001">fuerte</a> contra los <a name="02600300800016" ll="SDBH:פָּנֶה:000000">rostros</a> de ellos, y <a name="02600300800022" ll="SDBH:מֵצַח:000000">tu</a> <a name="02600300800022" ll="SDBH:מֵצַח:000000">frente</a> <a name="02600300800024" ll="SDBH:חָזָק:000001">fuerte</a> <a name="02600300800014">contra</a> <a name="02600300800030" ll="SDBH:מֵצַח:000000">sus</a> <a name="02600300800030" ll="SDBH:מֵצַח:000000">frentes</a>. 
\v 9 Como <a name="02600300900004" ll="SDBH:שָׁמִיר:000000">diamante</a>, más <a name="02600300900006" ll="SDBH:חָזָק:000001">fuerte</a> que <a name="02600300900010" ll="SDBH:צֹר:000000">pedernal</a> <a name="02600300900012">he</a> <a name="02600300900012">hecho</a> <a name="02600300900014" ll="SDBH:מֵצַח:000000">tu</a> <a name="02600300900014" ll="SDBH:מֵצַח:000000">frente</a>; <a name="02600300900016" ll="SDBH:לֹא:000000">no</a> los <a name="02600300900018" ll="SDBH:ירא:000000">temas</a>, <a name="02600300900024" ll="SDBH:לֹא:000000">ni</a> tengas <a name="02600300900026" ll="SDBH:חתת:000005">miedo</a> <a name="02600300900030">delante</a> de <a name="02600300900038">ellos</a>, porque son <a name="02600300900034" ll="SDBH:בַּיִת:000003">casa</a> <a name="02600300900036" ll="SDBH:מְרִי:000001">rebelde</a>. 
\v 10 Y <a name="02600301000006">me</a> <a name="02600301000004" ll="SDBH:אמר:000000">dijo</a>: <a name="02600301000008" ll="SDBH:בֵּן:000003">Hijo</a> de <a name="02600301000010" ll="SDBH:אָדָם:000000">hombre</a>, <a name="02600301000024" ll="SDBH:לקח:000000">toma</a> en <a name="02600301000028" ll="SDBH:לֵבָב:000001">tu</a> <a name="02600301000028" ll="SDBH:לֵבָב:000001">corazón</a> <a name="02600301000014">todas</a> <a name="02600301000016" ll="SDBH:דָּבָר:000000">mis</a> <a name="02600301000016" ll="SDBH:דָּבָר:000000">palabras</a> <a name="02600301000018">que</a> yo <a name="02600301000022">te</a> <a name="02600301000020" ll="SDBH:דבר:001000">hablaré</a>, y <a name="02600301000036">oye</a> con <a name="02600301000034" ll="SDBH:אֹזֶן:000000">tus</a> <a name="02600301000034" ll="SDBH:אֹזֶן:000000">oídos</a>. 
\v 11 Y <a name="02600301100004" ll="SDBH:הלך:000001">ve</a> y <a name="02600301100006">entra</a> <a name="02600301100008">a</a> los <a name="02600301100012" ll="SDBH:גֹּולָה:000001">cautivos</a>, <a name="02600301100014">a</a> los <a name="02600301100016" ll="SDBH:בֵּן:000000">hijos</a> de <a name="02600301100018" ll="SDBH:עַם:000000">tu</a> <a name="02600301100018" ll="SDBH:עַם:000000">pueblo</a>, y <a name="02600301100022" ll="SDBH:דבר:001000">háblales</a> y <a name="02600301100028" ll="SDBH:אמר:000000">diles</a>: <a name="02600301100032">Así</a> <a name="02600301100034" ll="SDBH:אמר:000000">ha</a> <a name="02600301100034" ll="SDBH:אמר:000000">dicho</a> <a name="02600301100038" ll="SDBH:יהוה:000000">Jehová</a> el <a name="02600301100036" ll="SDBH:אֲדֹנָי:000000">Señor</a>; <a name="02600301100042">escuchen</a>, o <a name="02600301100048" ll="SDBH:חדל:000002">dejen</a> de escuchar.
    
\v 12 Y me <a name="02600301200004" ll="SDBH:נשׂא:000003">levantó</a> el <a name="02600301200006" ll="SDBH:רוּחַ:000000">Espíritu</a>, y <a name="02600301200010">oí</a> <a name="02600301200012" ll="SDBH:אַחַר:001000">detrás</a> de mí una <a name="02600301200014">voz</a> de <a name="02600301200018" ll="SDBH:גָּדֹול:000003">gran</a> <a name="02600301200016" ll="SDBH:רַעַשׁ:000000">estruendo</a>, que decía: <a name="02600301200020" ll="SDBH:ברך:001001" tl="HOTTP3646">Bendita</a> sea la <a name="02600301200022" ll="SDBH:כָּבֹוד:000004" il="NBGlo20140509a;NBGlo20140708a;Pillar_of_cloud" tl="HOTTP3646">gloria</a> de <a name="02600301200024" ll="SDBH:יהוה:000000" tl="HOTTP3646">Jehová</a> desde <a name="02600301200028" ll="SDBH:מָקֹום:000000">su</a> <a name="02600301200028" ll="SDBH:מָקֹום:000000">lugar</a>. 
\v 13 Oí también el <a name="02600301300004">sonido</a> de las <a name="02600301300006" ll="SDBH:כָּנָף:000000">alas</a> de los <a name="02600301300010" ll="SDBH:חַיָּה:000001">seres</a> <a name="02600301300010" ll="SDBH:חַיָּה:000001">vivientes</a> <a name="02600301300012" ll="SDBH:נשׁק:000001">que</a> <a name="02600301300012" ll="SDBH:נשׁק:000001">se</a> <a name="02600301300012" ll="SDBH:נשׁק:000001">juntaban</a> <a name="02600301300014;02600301300016;02600301300018" ll="SDBH:אִשָּׁה:000003;SDBH:אָחֹות:000003">la</a> <a name="02600301300014;02600301300016;02600301300018" ll="SDBH:אִשָּׁה:000003;SDBH:אָחֹות:000003">una</a> <a name="02600301300014;02600301300016;02600301300018" ll="SDBH:אִשָּׁה:000003;SDBH:אָחֹות:000003">con</a> <a name="02600301300014;02600301300016;02600301300018" ll="SDBH:אִשָּׁה:000003;SDBH:אָחֹות:000003">la</a> <a name="02600301300014;02600301300016;02600301300018" ll="SDBH:אִשָּׁה:000003;SDBH:אָחֹות:000003">otra</a>, y el <a name="02600301300022">sonido</a> de las <a name="02600301300026" ll="SDBH:אֹופַן:000000">ruedas</a> <a name="02600301300030">delante</a> <a name="02600301300030">de</a> <a name="02600301300030">ellos</a>, y <a name="02600301300034">sonido</a> de <a name="02600301300038" ll="SDBH:גָּדֹול:000003">gran</a> <a name="02600301300036" ll="SDBH:רַעַשׁ:000000">estruendo</a>. 
\v 14 Me <a name="02600301400006" ll="SDBH:נשׂא:000003">levantó</a>, pues, el <a name="02600301400004;02600301400022" ll="SDBH:רוּחַ:000002">Espíritu</a>, y me <a name="02600301400010" ll="SDBH:לקח:000000">tomó</a>; y fui en <a name="02600301400016" ll="SDBH:מַר:001002">amargura</a>, en la <a name="02600301400020" ll="SDBH:חֵמָה:001001">indignación</a> de mi espíritu, pero la <a name="02600301400026">mano</a> de <a name="02600301400028" ll="SDBH:יהוה:000000">Jehová</a> era <a name="02600301400032" ll="SDBH:חזק:000003">fuerte</a> <a name="02600301400030">sobre</a> mí. 
\v 15 Y <a name="02600301500004">vine</a> <a name="02600301500006">a</a> los <a name="02600301500010" ll="SDBH:גֹּולָה:000001">cautivos</a> en Tel-abib, que <a name="02600301500016">moraban</a> <a name="02600301500018">junto</a> al <a name="02600301500020" ll="SDBH:נָהָר:000013">río</a> <a name="02600301500022" ll="SDBH:כְּבָר:001000">Quebar</a>, y me <a name="02600301500026">senté</a> <a name="02600301500036" ll="SDBH:שָׁם:000000" tl="HOTTP3647">donde</a> <a name="02600301500032">ellos</a> <a name="02600301500034">estaban</a> <a name="02600301500034">sentados</a>, y <a name="02600301500042" ll="SDBH:שָׁם:000000">allí</a> <a name="02600301500030">permanecí</a> <a name="02600301500044" ll="SDBH:שֶׁבַע:000000">siete</a> <a name="02600301500046" ll="SDBH:יֹום:000001">días</a> <a name="02600301500048" ll="SDBH:שׁמם:000003">atónito</a> <a name="02600301500050;02600301500052" ll="SDBH:תָּוֶךְ:000000">entre</a> <a name="02600301500050;02600301500052" ll="SDBH:תָּוֶךְ:000000">ellos</a>.
    
\v 16 Y <a name="02600301600004">aconteció</a> que al <a name="02600301600008" ll="SDBH:קָצֶה:000000">cabo</a> de los <a name="02600301600010" ll="SDBH:שֶׁבַע:000000">siete</a> <a name="02600301600012" ll="SDBH:יֹום:000001">días</a> <a name="02600301600016">vino</a> <a name="02600301600022">a</a> mí <a name="02600301600018" ll="SDBH:דָּבָר:000000">palabra</a> de <a name="02600301600020" ll="SDBH:יהוה:000000">Jehová</a>, <a name="02600301600026" ll="SDBH:אמר:000000">diciendo</a>: 
\v 17 <a name="02600301700002" ll="SDBH:בֵּן:000003">Hijo</a> de <a name="02600301700004" ll="SDBH:אָדָם:000000">hombre</a>, yo te <a name="02600301700008">he</a> <a name="02600301700008">puesto</a> por <a name="02600301700006" ll="SDBH:צפה:000002">atalaya</a> a la <a name="02600301700012" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600301700014" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>; <a name="02600301700018">oirás</a>, pues, tú la <a name="02600301700024" ll="SDBH:דָּבָר:000000">palabra</a> <a name="02600301700020">de</a> <a name="02600301700022" ll="SDBH:פֶּה:000000">mi</a> <a name="02600301700022" ll="SDBH:פֶּה:000000">boca</a>, y <a name="02600301700030">los</a> <a name="02600301700028" ll="SDBH:זהר:001001">amonestarás</a> <a name="02600301700032">de</a> <a name="02600301700032">mi</a> <a name="02600301700032">parte</a>. 
\v 18 Cuando yo <a name="02600301800004" ll="SDBH:אמר:000000">dijere</a> al <a name="02600301800010" ll="SDBH:רָשָׁע:000000">impío</a>: De <a name="02600301800012" ll="SDBH:מות:000000">cierto</a> <a name="02600301800014" ll="SDBH:מות:000000">morirás</a>; y tú <a name="02600301800018" ll="SDBH:לֹא:000000">no</a> <a name="02600301800046">le</a> <a name="02600301800020" ll="SDBH:זהר:001001">amonestares</a> <a name="02600301800024" ll="SDBH:לֹא:000000">ni</a> le <a name="02600301800026" ll="SDBH:דבר:001000">hablares</a>, para que el <a name="02600301800032" ll="SDBH:רָשָׁע:000000">impío</a> sea <a name="02600301800030" ll="SDBH:זהר:001001">apercibido</a> de su <a name="02600301800040" ll="SDBH:רָשָׁע:000000">mal</a> <a name="02600301800036" ll="SDBH:דֶּרֶךְ:000007">camino</a> a fin de que viva, el <a name="02600301800048" ll="SDBH:רָשָׁע:000000">impío</a> <a name="02600301800054" ll="SDBH:מות:000000">morirá</a> por su maldad, pero <a name="02600301800058" ll="SDBH:דָּם:000003">su</a> <a name="02600301800058" ll="SDBH:דָּם:000003">sangre</a> <a name="02600301800064" ll="SDBH:בקשׁ:000004">demandaré</a> de <a name="02600301800062">tu</a> <a name="02600301800062">mano</a>. 
\v 19 Pero si <a name="02600301900004">tú</a> <a name="02600301900008" ll="SDBH:זהר:001001">amonestares</a> al <a name="02600301900010" ll="SDBH:רָשָׁע:000000">impío</a>, y él <a name="02600301900014" ll="SDBH:לֹא:000000">no</a> <a name="02600301900016" ll="SDBH:שׁוב:000013">se</a> <a name="02600301900016" ll="SDBH:שׁוב:000013">convirtiere</a> de <a name="02600301900032">su</a> <a name="02600301900030" ll="SDBH:רָשָׁע:000000">impiedad</a> y de <a name="02600301900020" ll="SDBH:רֶשַׁע:000001">su</a> <a name="02600301900020" ll="SDBH:רֶשַׁע:000001">mal</a> <a name="02600301900026" ll="SDBH:דֶּרֶךְ:000007">camino</a>, él <a name="02600301900038" ll="SDBH:מות:000000">morirá</a> por su maldad, pero <a name="02600301900042">tú</a> habrás <a name="02600301900048" ll="SDBH:נצל:000005">librado</a> <a name="02600301900046" ll="SDBH:נֶפֶשׁ:000001">tu</a> <a name="02600301900046" ll="SDBH:נֶפֶשׁ:000001">alma</a>. 
\v 20 Si el <a name="02600302000008" ll="SDBH:צַדִּיק:000000">justo</a> <a name="02600302000006" ll="SDBH:שׁוב:000013">se</a> <a name="02600302000006" ll="SDBH:שׁוב:000013">apartare</a> de <a name="02600302000030">su</a> <a name="02600302000012" ll="SDBH:צֶדֶק:000001">justicia</a> e <a name="02600302000016">hiciere</a> <a name="02600302000018" ll="SDBH:עָוֶל:000000">maldad</a>, y <a name="02600302000022">pusiere</a> yo <a name="02600302000024" ll="SDBH:מִכְשֹׁול:000001">tropiezo</a> <a name="02600302000028">delante</a> de él, él <a name="02600302000032" ll="SDBH:מות:000000">morirá</a>, porque tú <a name="02600302000036" ll="SDBH:לֹא:000000">no</a> le <a name="02600302000038" ll="SDBH:זהר:001001">amonestaste</a>; en <a name="02600302000042" ll="SDBH:חַטָּאת:000000">su</a> <a name="02600302000042" ll="SDBH:חַטָּאת:000000">pecado</a> <a name="02600302000044" ll="SDBH:מות:000000">morirá</a>, y <a name="02600302000052" ll="SDBH:צְדָקָה:000002">sus</a> <a name="02600302000052" ll="SDBH:צְדָקָה:000002">justicias</a> <a name="02600302000056">que</a> <a name="02600302000058">había</a> <a name="02600302000058">hecho</a> <a name="02600302000048" ll="SDBH:לֹא:000000">no</a> vendrán en <a name="02600302000050" ll="SDBH:זכר:000000">memoria</a>; pero <a name="02600302000062" ll="SDBH:דָּם:000003">su</a> <a name="02600302000062" ll="SDBH:דָּם:000003">sangre</a> <a name="02600302000068" ll="SDBH:בקשׁ:000004">demandaré</a> de <a name="02600302000066">tu</a> <a name="02600302000066">mano</a>. 
\v 21 Pero si al <a name="02600302100010" ll="SDBH:צַדִּיק:000000">justo</a> <a name="02600302100008" ll="SDBH:זהר:001001">amonestares</a> para que no <a name="02600302100016" ll="SDBH:חטא:001005">peque</a>, y <a name="02600302100024" ll="SDBH:לֹא:000000">no</a> <a name="02600302100026" ll="SDBH:חטא:001005">pecare</a>, de cierto vivirá, porque fue <a name="02600302100034" ll="SDBH:זהר:001000">amonestado</a>; y <a name="02600302100004">tú</a> habrás <a name="02600302100044" ll="SDBH:נצל:000005">librado</a> <a name="02600302100038">tu</a> <a name="02600302100042" ll="SDBH:נֶפֶשׁ:000001">alma</a>.
    
\v 22 <a name="02600302200004">Vino</a> <a name="02600302200008" ll="SDBH:שָׁם:000000">allí</a> la <a name="02600302200010">mano</a> de <a name="02600302200012" ll="SDBH:יהוה:000000">Jehová</a> <a name="02600302200006">sobre</a> mí, y <a name="02600302200018">me</a> <a name="02600302200016" ll="SDBH:אמר:000000">dijo</a>: <a name="02600302200020">Levántate</a>, y <a name="02600302200022" ll="SDBH:יצא:000000">sal</a> <a name="02600302200024">al</a> <a name="02600302200028" ll="SDBH:בִּקְעָה:000000">campo</a>, y <a name="02600302200032" ll="SDBH:שָׁם:000000">allí</a> <a name="02600302200034" ll="SDBH:דבר:001000">hablaré</a> contigo. 
\v 23 Y me <a name="02600302300004">levanté</a> y <a name="02600302300008" ll="SDBH:יצא:000000">salí</a> <a name="02600302300010">al</a> <a name="02600302300014" ll="SDBH:בִּקְעָה:000000">campo</a>; y he aquí que <a name="02600302300020" ll="SDBH:שָׁם:000000">allí</a> <a name="02600302300026">estaba</a> la <a name="02600302300022" ll="SDBH:כָּבֹוד:000004">gloria</a> de <a name="02600302300024" ll="SDBH:יהוה:000000">Jehová</a>, como la <a name="02600302300032" ll="SDBH:כָּבֹוד:000004">gloria</a> <a name="02600302300034">que</a> <a name="02600302300036" ll="SDBH:ראה:000000">había</a> <a name="02600302300036" ll="SDBH:ראה:000000">visto</a> <a name="02600302300038">junto</a> al <a name="02600302300040" ll="SDBH:נָהָר:000013">río</a> Quebar; y me <a name="02600302300046" ll="SDBH:נפל:000040">postré</a> <a name="02600302300048">sobre</a> <a name="02600302300050">mi</a> <a name="02600302300050">rostro</a>. 
\v 24 Entonces <a name="02600302400004">entró</a> el <a name="02600302400008" ll="SDBH:רוּחַ:000000">Espíritu</a> en mí y me <a name="02600302400012">afirmó</a> <a name="02600302400014">sobre</a> <a name="02600302400016" ll="SDBH:רֶגֶל:000000">mis</a> <a name="02600302400016" ll="SDBH:רֶגֶל:000000">pies</a>, y me <a name="02600302400020" ll="SDBH:דבר:001000">habló</a>, y <a name="02600302400028">me</a> <a name="02600302400026" ll="SDBH:אמר:000000">dijo</a>: <a name="02600302400030">Entra</a>, y <a name="02600302400032" ll="SDBH:סגר:000003">enciérrate</a> <a name="02600302400036" ll="SDBH:תָּוֶךְ:000000">dentro</a> de <a name="02600302400038" ll="SDBH:בַּיִת:000000">tu</a> <a name="02600302400038" ll="SDBH:בַּיִת:000000">casa</a>. 
\v 25 Y <a name="02600302500004">tú</a>, oh <a name="02600302500006" ll="SDBH:בֵּן:000003">hijo</a> de <a name="02600302500008" ll="SDBH:אָדָם:000000">hombre</a>, he aquí que <a name="02600302500012">pondrán</a> <a name="02600302500014">sobre</a> <a name="02600302500014">ti</a> <a name="02600302500016" ll="SDBH:עֲבֹת:000000">cuerdas</a>, y con ellas te <a name="02600302500020" ll="SDBH:אסר:000000">ligarán</a>, y <a name="02600302500026" ll="SDBH:לֹא:000000">no</a> <a name="02600302500028" ll="SDBH:יצא:000000">saldrás</a> entre ellos. 
\v 26 Y haré que <a name="02600302600006" ll="SDBH:דבק:000002">se</a> <a name="02600302600006" ll="SDBH:דבק:000002">pegue</a> <a name="02600302600004" ll="SDBH:לָשֹׁון:000000">tu</a> <a name="02600302600004" ll="SDBH:לָשֹׁון:000000">lengua</a> <a name="02600302600008">a</a> <a name="02600302600010" ll="SDBH:חֵךְ:000000">tu</a> <a name="02600302600010" ll="SDBH:חֵךְ:000000">paladar</a>, y estarás <a name="02600302600014" ll="SDBH:אלם:000000">mudo</a>, y <a name="02600302600018" ll="SDBH:לֹא:000000">no</a> <a name="02600302600020">serás</a> a ellos <a name="02600302600026">varón</a> que reprende; <a name="02600302600028" ll="SDBH:יכח:000003">porque</a> son <a name="02600302600032" ll="SDBH:בַּיִת:000003">casa</a> <a name="02600302600034" ll="SDBH:מְרִי:000001">rebelde</a>. 
\v 27 Mas cuando yo te hubiere <a name="02600302700006" ll="SDBH:דבר:001000">hablado</a>, <a name="02600302700010" ll="SDBH:פתח:000003">abriré</a> <a name="02600302700014" ll="SDBH:פֶּה:000000">tu</a> <a name="02600302700014" ll="SDBH:פֶּה:000000">boca</a>, y <a name="02600302700020">les</a> <a name="02600302700018" ll="SDBH:אמר:000000">dirás</a>: <a name="02600302700022">Así</a> <a name="02600302700024" ll="SDBH:אמר:000000">ha</a> <a name="02600302700024" ll="SDBH:אמר:000000">dicho</a> <a name="02600302700028" ll="SDBH:יהוה:000000">Jehová</a> el <a name="02600302700026" ll="SDBH:אֲדֹנָי:000000">Señor</a>: El que <a name="02600302700032">oye</a>, <a name="02600302700034">oiga</a>; y el que no <a name="02600302700040" ll="SDBH:חָדֵל:000001">quiera</a> oír, no <a name="02600302700042" ll="SDBH:חדל:000002">oiga</a>; porque <a name="02600302700046" ll="SDBH:בַּיִת:000003">casa</a> <a name="02600302700048" ll="SDBH:מְרִי:000001">rebelde</a> son.
\c 4
\v 1 <a name="02600400100004">Tú</a>, <a name="02600400100006" ll="SDBH:בֵּן:000003">hijo</a> de <a name="02600400100008" ll="SDBH:אָדָם:000000">hombre</a>, <a name="02600400100010" ll="SDBH:לקח:000000">tómate</a> un <a name="02600400100014" ll="SDBH:לְבֵנָה:000000">adobe</a>, y <a name="02600400100018">ponlo</a> <a name="02600400100022;02600400100024">delante</a> <a name="02600400100022;02600400100024">de</a> <a name="02600400100022;02600400100024">ti</a>, y <a name="02600400100028" ll="SDBH:חקק:000000">diseña</a> <a name="02600400100030">sobre</a> él la <a name="02600400100032" ll="SDBH:עִיר:000000">ciudad</a> de <a name="02600400100036" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>. 
\v 2 Y <a name="02600400200004">pondrás</a> <a name="02600400200006">contra</a> <a name="02600400200006">ella</a> <a name="02600400200008" ll="SDBH:מָצֹור:001000">sitio</a>, y <a name="02600400200012" ll="SDBH:בנה:000000">edificarás</a> <a name="02600400200014">contra</a> <a name="02600400200014">ella</a> <a name="02600400200016" ll="SDBH:דָּיֵק:000000">fortaleza</a>, y <a name="02600400200020" ll="SDBH:שׁפך:000008">sacarás</a> <a name="02600400200022">contra</a> <a name="02600400200022">ella</a> baluarte, y <a name="02600400200028">pondrás</a> delante <a name="02600400200030">de</a> <a name="02600400200030">ella</a> <a name="02600400200032" ll="SDBH:מַחֲנֶה:000000">campamento</a>, y <a name="02600400200036" ll="SDBH:שׂים:000001">colocarás</a> <a name="02600400200038">contra</a> <a name="02600400200038">ella</a> <a name="02600400200040" ll="SDBH:כַּר:000003">arietes</a> <a name="02600400200042" ll="SDBH:סָבִיב:000000">alrededor</a>. 
\v 3 <a name="02600400300006" ll="SDBH:לקח:000000">Tómate</a> también una <a name="02600400300010" ll="SDBH:מַחֲבַת:000000">plancha</a> de <a name="02600400300012" ll="SDBH:בַּרְזֶל:000000">hierro</a>, y <a name="02600400300016">ponla</a> en lugar de <a name="02600400300020" ll="SDBH:קִיר:000000">muro</a> de <a name="02600400300022" ll="SDBH:בַּרְזֶל:000000">hierro</a> <a name="02600400300024">entre</a> <a name="02600400300004">ti</a> y la <a name="02600400300032" ll="SDBH:עִיר:000000">ciudad</a>; <a name="02600400300036" ll="SDBH:כון:000026">afirmarás</a> luego <a name="02600400300040" ll="SDBH:פָּנֶה:000000">tu</a> <a name="02600400300040" ll="SDBH:פָּנֶה:000000">rostro</a> <a name="02600400300042">contra</a> <a name="02600400300062">ella</a>, y <a name="02600400300046">será</a> en lugar de <a name="02600400300052" ll="SDBH:מָצֹור:001000">cerco</a>, y la <a name="02600400300056" ll="SDBH:צור:000001">sitiarás</a>. Es <a name="02600400300060" ll="SDBH:אֹות:000001">señal</a> a la <a name="02600400300066" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600400300068" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>.
    
\v 4 Y <a name="02600400400004">tú</a> te <a name="02600400400006" ll="SDBH:שׁכב:000000">acostarás</a> <a name="02600400400008">sobre</a> <a name="02600400400010" ll="SDBH:צַד:000000">tu</a> <a name="02600400400010" ll="SDBH:צַד:000000">lado</a> <a name="02600400400014" ll="SDBH:שְׂמָאלִי:000000">izquierdo</a> y <a name="02600400400018" ll="SDBH:שׂים:000002" tl="HOTTP3648">pondrás</a> <a name="02600400400028">sobre</a> <a name="02600400400028">él</a> la maldad de la <a name="02600400400024" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600400400026" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. El <a name="02600400400030" ll="SDBH:מִסְפָּר:000002">número</a> de los <a name="02600400400034" ll="SDBH:יֹום:000002">días</a> <a name="02600400400036">que</a> <a name="02600400400038" ll="SDBH:שׁכב:000000">duermas</a> <a name="02600400400040">sobre</a> <a name="02600400400040">él</a>, <a name="02600400400042" ll="SDBH:נשׂא:000007">llevarás</a> sobre ti la maldad de ellos. 
\v 5 <a name="02600400500004">Yo</a> te <a name="02600400500006">he</a> <a name="02600400500006">dado</a> los <a name="02600400500012" ll="SDBH:שָׁנָה:000000">años</a> de <a name="02600400500014" ll="SDBH:עָוֹן:000000">su</a> <a name="02600400500014" ll="SDBH:עָוֹן:000000">maldad</a> por el <a name="02600400500018" ll="SDBH:מִסְפָּר:000002">número</a> de los <a name="02600400500020" ll="SDBH:יֹום:000002">días</a>, <a name="02600400500022;02600400500024" ll="SDBH:מֵאָה:000000;SDBH:שָׁלֹשׁ:000000" tl="HOTTP3650">trescientos</a> <a name="02600400500028" ll="SDBH:תֵּשַׁע:000001">noventa</a> <a name="02600400500030" ll="SDBH:יֹום:000001">días</a>; y así <a name="02600400500034" ll="SDBH:נשׂא:000007">llevarás</a> <a name="02600400500034" ll="SDBH:נשׂא:000007">tú</a> la <a name="02600400500036" ll="SDBH:עָוֹן:000000">maldad</a> de la <a name="02600400500038" ll="SDBH:בַּיִת:000003">casa</a> de <a name="02600400500040" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
