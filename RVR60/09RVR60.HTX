\v 6 Y <a name="00902800600004" ll="SDBH:שׁאל:000000">consultó</a> <a name="00902800600006" ll="SDBH:שָׁאוּל:000000">Saúl</a> a <a name="00902800600010;00902800600018" ll="SDBH:יהוה:000000">Jehová</a>; pero <a name="00902800600010;00902800600018" ll="SDBH:יהוה:000000">Jehová</a> <a name="00902800600014" ll="SDBH:לֹא:000000">no</a> le <a name="00902800600016" ll="SDBH:ענה:000000">respondió</a> <a name="00902800600020" ll="SDBH:גַּם:000000">ni</a> por <a name="00902800600026" ll="SDBH:חֲלֹום:000000">sueños</a>, <a name="00902800600028" ll="SDBH:גַּם:000000">ni</a> por <a name="00902800600034" ll="SDBH:אוּר:000002">Urim</a>, <a name="00902800600036" ll="SDBH:גַּם:000000">ni</a> por <a name="00902800600042" ll="SDBH:נָבִיא:000000">profetas</a>. 
\v 7 Entonces <a name="00902800700006" ll="SDBH:שָׁאוּל:000000">Saúl</a> <a name="00902800700004" ll="SDBH:אמר:000000">dijo</a> a <a name="00902800700010" ll="SDBH:עֶבֶד:000000">sus</a> <a name="00902800700010" ll="SDBH:עֶבֶד:000000">criados</a>: <a name="00902800700012" ll="SDBH:בקשׁ:000000">Buscadme</a> una <a name="00902800700016" ll="SDBH:אִשָּׁה:000000">mujer</a> <a name="00902800700018;00902800700020" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">que</a> <a name="00902800700018;00902800700020" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">tenga</a> <a name="00902800700018;00902800700020" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">espíritu</a> <a name="00902800700018;00902800700020" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">de</a> <a name="00902800700018;00902800700020" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">adivinación</a>, para que <a name="00902800700024" ll="SDBH:הלך:000001">yo</a> <a name="00902800700024" ll="SDBH:הלך:000001">vaya</a> <a name="00902800700026">a</a> <a name="00902800700026">ella</a> y <a name="00902800700032">por</a> <a name="00902800700032">medio</a> <a name="00902800700032">de</a> <a name="00902800700032">ella</a> <a name="00902800700030" ll="SDBH:דרשׁ:000001">pregunte</a>. Y <a name="00902800700038" ll="SDBH:עֶבֶד:000000">sus</a> <a name="00902800700038" ll="SDBH:עֶבֶד:000000">criados</a> <a name="00902800700040">le</a> <a name="00902800700036" ll="SDBH:אמר:000000">respondieron</a>: <a name="00902800700042">He</a> <a name="00902800700042">aquí</a> hay una <a name="00902800700044" ll="SDBH:אִשָּׁה:000000">mujer</a> en <a name="00902800700052" ll="SDBH:עֵין דֹּור:000000">Endor</a> <a name="00902800700046;00902800700048" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">que</a> <a name="00902800700046;00902800700048" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">tiene</a> <a name="00902800700046;00902800700048" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">espíritu</a> <a name="00902800700046;00902800700048" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">de</a> <a name="00902800700046;00902800700048" ll="SDBH:אֹוב:001001;SDBH:בַּעֲלָה:000001">adivinación</a>.
    
\v 8 Y <a name="00902800800004" ll="SDBH:חפשׂ:000005">se</a> <a name="00902800800004" ll="SDBH:חפשׂ:000005">disfrazó</a> <a name="00902800800006" ll="SDBH:שָׁאוּל:000000">Saúl</a>, y <a name="00902800800010" ll="SDBH:לבשׁ:000000">se</a> <a name="00902800800010" ll="SDBH:לבשׁ:000000">puso</a> <a name="00902800800014" ll="SDBH:אַחֵר:000000">otros</a> <a name="00902800800012" ll="SDBH:בֶּגֶד:000001">vestidos</a>, y <a name="00902800800018" ll="SDBH:הלך:000001">se</a> <a name="00902800800018" ll="SDBH:הלך:000001">fue</a> con <a name="00902800800024" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="00902800800026">hombres</a>, y <a name="00902800800032">vinieron</a> <a name="00902800800034">a</a> aquella <a name="00902800800038" ll="SDBH:אִשָּׁה:000000">mujer</a> <a name="00902800800040" ll="SDBH:לַיְלָה:000000">de</a> <a name="00902800800040" ll="SDBH:לַיְלָה:000000">noche</a>; y él <a name="00902800800044" ll="SDBH:אמר:000000">dijo</a>: <a name="00902800800050" ll="SDBH:נָא:001000">Yo</a> <a name="00902800800050" ll="SDBH:נָא:001000">te</a> <a name="00902800800050" ll="SDBH:נָא:001000">ruego</a> que <a name="00902800800052">me</a> <a name="00902800800048" ll="SDBH:קסם:000000">adivines</a> por el <a name="00902800800058" ll="SDBH:אֹוב:001000">espíritu</a> <a name="00902800800058" ll="SDBH:אֹוב:001000">de</a> <a name="00902800800058" ll="SDBH:אֹוב:001000">adivinación</a>, y <a name="00902800800064">me</a> <a name="00902800800062" ll="SDBH:עלה:000003">hagas</a> <a name="00902800800062" ll="SDBH:עלה:000003">subir</a> a <a name="00902800800068">quien</a> <a name="00902800800070" ll="SDBH:אמר:000000">yo</a> <a name="00902800800072">te</a> <a name="00902800800070" ll="SDBH:אמר:000000">dijere</a>. 
