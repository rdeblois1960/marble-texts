\v 11 Y <a name="00900501100004" ll="SDBH:שׁלח:000003">enviaron</a> y <a name="00900501100008" ll="SDBH:אסף:000001">reunieron</a> a <a name="00900501100012">todos</a> los <a name="00900501100014" ll="SDBH:סֶרֶן:001000">príncipes</a> de los <a name="00900501100016" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a>, <a name="00900501100020" ll="SDBH:אמר:000000">diciendo</a>: <a name="00900501100022" ll="SDBH:שׁלח:000006">Enviad</a> el <a name="00900501100026" ll="SDBH:אֲרֹון:000001">arca</a> del <a name="00900501100028" ll="SDBH:אֱלֹהִים:000001">Dios</a> de <a name="00900501100030" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, y <a name="00900501100034" ll="SDBH:שׁוב:000022">vuélvase</a> a <a name="00900501100038" ll="SDBH:מָקֹום:000000">su</a> <a name="00900501100038" ll="SDBH:מָקֹום:000000">lugar</a>, y <a name="00900501100042" ll="SDBH:לֹא:000000">no</a> <a name="00900501100046">nos</a> <a name="00900501100044" ll="SDBH:מות:000001">mate</a> <a name="00900501100046">a</a> <a name="00900501100046">nosotros</a> ni a <a name="00900501100052" ll="SDBH:עַם:000000">nuestro</a> <a name="00900501100052" ll="SDBH:עַם:000000">pueblo</a>; <a name="00900501100054">porque</a> <a name="00900501100056">había</a> <a name="00900501100058" ll="SDBH:מְהוּמָה:000000">consternación</a> de <a name="00900501100060" ll="SDBH:מָוֶת:000000">muerte</a> en <a name="00900501100064">toda</a> la <a name="00900501100068" ll="SDBH:עִיר:000000">ciudad</a>, y la <a name="00900501100074">mano</a> de <a name="00900501100078" ll="SDBH:אֱלֹהִים:000002">Dios</a> <a name="00900501100070;00900501100072" ll="SDBH:כבד:000001;SDBH:מְאֹד:000001">se</a> <a name="00900501100070;00900501100072" ll="SDBH:כבד:000001;SDBH:מְאֹד:000001">había</a> <a name="00900501100070;00900501100072" ll="SDBH:כבד:000001;SDBH:מְאֹד:000001">agravado</a> <a name="00900501100080" ll="SDBH:שָׁם:000000">allí</a>. 
\v 12 Y los <a name="00900501200008">que</a> <a name="00900501200010" ll="SDBH:לֹא:000000">no</a> <a name="00900501200012" ll="SDBH:מות:000000">morían</a>, <a name="00900501200014" ll="SDBH:נכה:000010">eran</a> <a name="00900501200014" ll="SDBH:נכה:000010">heridos</a> de <a name="00900501200026">tumores</a>; y el <a name="00900501200032">clamor</a> de la <a name="00900501200036" ll="SDBH:עִיר:000000">ciudad</a> <a name="00900501200030" ll="SDBH:עלה:000019">subía</a> al <a name="00900501200040" ll="SDBH:שָׁמַיִם:000000">cielo</a>.
\c 6
\v 1 <a name="00900600100004">Estuvo</a> el <a name="00900600100006" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900600100008" ll="SDBH:יהוה:000000">Jehová</a> en la <a name="00900600100012" ll="SDBH:שָׂדֶה:000002">tierra</a> de los <a name="00900600100014" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a> <a name="00900600100016" ll="SDBH:שֶׁבַע:000000">siete</a> <a name="00900600100018" ll="SDBH:חֹדֶשׁ:000000">meses</a>. 
\v 2 Entonces los <a name="00900600200006" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a>, <a name="00900600200004" ll="SDBH:קרא:000003">llamando</a> a los <a name="00900600200012" ll="SDBH:כֹּהֵן:000000">sacerdotes</a> y <a name="00900600200020" ll="SDBH:קסם:000000">adivinos</a>, preguntaron: ¿<a name="00900600200026;00900600200040" ll="SDBH:מָה:000000">Qué</a> <a name="00900600200028">haremos</a> del <a name="00900600200032" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900600200034" ll="SDBH:יהוה:000000">Jehová</a>? Hacednos <a name="00900600200036">saber</a> de qué manera la hemos de volver a <a name="00900600200042" ll="SDBH:שׁלח:000006">enviar</a> a <a name="00900600200046" ll="SDBH:מָקֹום:000000">su</a> <a name="00900600200046" ll="SDBH:מָקֹום:000000">lugar</a>. 
\v 3 <a name="00900600300004" ll="SDBH:אמר:000000">Ellos</a> <a name="00900600300004" ll="SDBH:אמר:000000">dijeron</a>: <a name="00900600300006">Si</a> <a name="00900600300008" ll="SDBH:שׁלח:000006">enviáis</a> el <a name="00900600300012" ll="SDBH:אֲרֹון:000001">arca</a> del <a name="00900600300014" ll="SDBH:אֱלֹהִים:000001">Dios</a> de <a name="00900600300016" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, <a name="00900600300018" ll="SDBH:אַל:000000">no</a> la <a name="00900600300020" ll="SDBH:שׁלח:000006">enviéis</a> <a name="00900600300024" ll="SDBH:רֵיקָם:000000">vacía</a>, sino <a name="00900600300028;00900600300030;00900600300032" ll="SDBH:שׁוב:000051">pagadle</a> la <a name="00900600300034" ll="SDBH:אָשָׁם:000002">expiación</a>; <a name="00900600300036" ll="SDBH:אָז:000002">entonces</a> <a name="00900600300038" ll="SDBH:רפא:000001">seréis</a> <a name="00900600300038" ll="SDBH:רפא:000001">sanos</a>, y <a name="00900600300042">conoceréis</a> <a name="00900600300046">por</a> <a name="00900600300046">qué</a> <a name="00900600300048" ll="SDBH:לֹא:000000">no</a> <a name="00900600300050" ll="SDBH:סור:000007">se</a> <a name="00900600300050" ll="SDBH:סור:000007">apartó</a> <a name="00900600300054">de</a> <a name="00900600300054">vosotros</a> <a name="00900600300052">su</a> <a name="00900600300052">mano</a>. 
\v 4 Y <a name="00900600400004" ll="SDBH:אמר:000000">ellos</a> <a name="00900600400004" ll="SDBH:אמר:000000">dijeron</a>: ¿Y <a name="00900600400006" ll="SDBH:מָה:000000">qué</a> será la <a name="00900600400010" ll="SDBH:אָשָׁם:000002">expiación</a> <a name="00900600400012">que</a> <a name="00900600400016">le</a> <a name="00900600400014" ll="SDBH:שׁוב:000051">pagaremos</a>? <a name="00900600400020" ll="SDBH:אמר:000000">Ellos</a> <a name="00900600400020" ll="SDBH:אמר:000000">respondieron</a>: Conforme al <a name="00900600400022" ll="SDBH:מִסְפָּר:000002">número</a> de los <a name="00900600400024" ll="SDBH:סֶרֶן:001000">príncipes</a> de los <a name="00900600400026" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a>, <a name="00900600400028" ll="SDBH:חָמֵשׁ:000000">cinco</a> <a name="00900600400032" ll="SDBH:טְחֹרִים:000001">tumores</a> de <a name="00900600400034" ll="SDBH:זָהָב:000000">oro</a>, y <a name="00900600400038" ll="SDBH:חָמֵשׁ:000000">cinco</a> <a name="00900600400040" ll="SDBH:עַכְבָּר:000000">ratones</a> de <a name="00900600400042" ll="SDBH:זָהָב:000000">oro</a>, porque <a name="00900600400048" ll="SDBH:אֶחָד:000000">una</a> <a name="00900600400048" ll="SDBH:אֶחָד:000000">misma</a> <a name="00900600400046" ll="SDBH:מַגֵּפָה:000000">plaga</a> ha afligido a <a name="00900600400052">todos</a> <a name="00900600400052">vosotros</a> y a <a name="00900600400058" ll="SDBH:סֶרֶן:001000">vuestros</a> <a name="00900600400058" ll="SDBH:סֶרֶן:001000">príncipes</a>. 
\v 5 <a name="00900600500004">Haréis</a>, pues, <a name="00900600500006" ll="SDBH:צֶלֶם:000000">figuras</a> de <a name="00900600500010" ll="SDBH:טְחֹרִים:000001">vuestros</a> <a name="00900600500010" ll="SDBH:טְחֹרִים:000001">tumores</a>, y de <a name="00900600500016" ll="SDBH:עַכְבָּר:000000">vuestros</a> <a name="00900600500016" ll="SDBH:עַכְבָּר:000000">ratones</a> que <a name="00900600500020" ll="SDBH:שׁחת:000001">destruyen</a> la <a name="00900600500026" ll="SDBH:אֶרֶץ:000000">tierra</a>, y <a name="00900600500030">daréis</a> <a name="00900600500038" ll="SDBH:כָּבֹוד:000000">gloria</a> al <a name="00900600500034" ll="SDBH:אֱלֹהִים:000001">Dios</a> de <a name="00900600500036" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>; <a name="00900600500040" ll="SDBH:אוּלַי:000000">quizá</a> <a name="00900600500042" ll="SDBH:קלל:000003">aliviará</a> <a name="00900600500046">su</a> <a name="00900600500046">mano</a> <a name="00900600500048">de</a> <a name="00900600500050">sobre</a> <a name="00900600500050">vosotros</a> y <a name="00900600500054">de</a> <a name="00900600500056">sobre</a> <a name="00900600500058" ll="SDBH:אֱלֹהִים:000000">vuestros</a> <a name="00900600500058" ll="SDBH:אֱלֹהִים:000000">dioses</a>, y <a name="00900600500062">de</a> <a name="00900600500064">sobre</a> <a name="00900600500066" ll="SDBH:אֶרֶץ:000000">vuestra</a> <a name="00900600500066" ll="SDBH:אֶרֶץ:000000">tierra</a>. 
\v 6 ¿Por qué <a name="00900600600006" ll="SDBH:כבד:000020">endurecéis</a> <a name="00900600600010" ll="SDBH:לֵבָב:000001">vuestro</a> <a name="00900600600010" ll="SDBH:לֵבָב:000001">corazón</a>, como los <a name="00900600600018" ll="SDBH:מִצְרַיִם:000001">egipcios</a> y <a name="00900600600022" ll="SDBH:פַּרְעֹה:000000">Faraón</a> <a name="00900600600016" ll="SDBH:כבד:000020">endurecieron</a> <a name="00900600600026" ll="SDBH:לֵב:000001">su</a> <a name="00900600600026" ll="SDBH:לֵב:000001">corazón</a>? Después que los <a name="00900600600036" ll="SDBH:עלל:000000">había</a> <a name="00900600600036" ll="SDBH:עלל:000000">tratado</a> así, ¿<a name="00900600600030" ll="SDBH:לֹא:000000">no</a> los dejaron <a name="00900600600042" ll="SDBH:שׁלח:000022">ir</a>, y se fueron? 
\v 7 <a name="00900600700010">Haced</a>, pues, <a name="00900600700004">ahora</a> <a name="00900600700016" ll="SDBH:אֶחָד:000000">un</a> <a name="00900600700012" ll="SDBH:עֲגָלָה:000000">carro</a> <a name="00900600700014" ll="SDBH:חָדָשׁ:000000">nuevo</a>, y <a name="00900600700006" ll="SDBH:לקח:000000">tomad</a> luego <a name="00900600700020" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="00900600700022" ll="SDBH:פָּרָה:000000">vacas</a> que <a name="00900600700024" ll="SDBH:עול:001000">críen</a>, a las cuales <a name="00900600700028" ll="SDBH:לֹא:000000">no</a> <a name="00900600700030" ll="SDBH:עלה:000010">haya</a> <a name="00900600700030" ll="SDBH:עלה:000010">sido</a> <a name="00900600700030" ll="SDBH:עלה:000010">puesto</a> <a name="00900600700034" ll="SDBH:עֹל:000000">yugo</a>, y <a name="00900600700038" ll="SDBH:אסר:000002">uncid</a> las <a name="00900600700044" ll="SDBH:פָּרָה:000000">vacas</a> al <a name="00900600700050" ll="SDBH:עֲגָלָה:000000">carro</a>, y <a name="00900600700054" ll="SDBH:שׁוב:000051">haced</a> <a name="00900600700054" ll="SDBH:שׁוב:000051">volver</a> <a name="00900600700056" ll="SDBH:בֵּן:000001">sus</a> <a name="00900600700056" ll="SDBH:בֵּן:000001">becerros</a> de <a name="00900600700060" ll="SDBH:אַחַר:001000">detrás</a> de ellas a <a name="00900600700064" ll="SDBH:בַּיִת:000000">casa</a>. 
\v 8 <a name="00900600800004" ll="SDBH:לקח:000000">Tomaréis</a> luego el <a name="00900600800008" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900600800010" ll="SDBH:יהוה:000000">Jehová</a>, y la <a name="00900600800014">pondréis</a> <a name="00900600800018">sobre</a> el <a name="00900600800022" ll="SDBH:עֲגָלָה:000000">carro</a>, y las <a name="00900600800028" ll="SDBH:כְּלִי:000000">joyas</a> de <a name="00900600800032" ll="SDBH:זָהָב:000000">oro</a> <a name="00900600800034">que</a> le <a name="00900600800036" ll="SDBH:שׁוב:000051">habéis</a> <a name="00900600800036" ll="SDBH:שׁוב:000051">de</a> <a name="00900600800036" ll="SDBH:שׁוב:000051">pagar</a> en <a name="00900600800040" ll="SDBH:אָשָׁם:000002">ofrenda</a> <a name="00900600800040" ll="SDBH:אָשָׁם:000002">por</a> <a name="00900600800040" ll="SDBH:אָשָׁם:000002">la</a> <a name="00900600800040" ll="SDBH:אָשָׁם:000002">culpa</a>, las <a name="00900600800042" ll="SDBH:שׂים:000001">pondréis</a> en una <a name="00900600800048" ll="SDBH:אַרְגַּז:000000">caja</a> al <a name="00900600800052" ll="SDBH:צַד:000002">lado</a> de ella; y la <a name="00900600800056" ll="SDBH:שׁלח:000006">dejaréis</a> que <a name="00900600800062" ll="SDBH:הלך:000001">se</a> <a name="00900600800062" ll="SDBH:הלך:000001">vaya</a>. 
\v 9 Y <a name="00900600900004" ll="SDBH:ראה:000000">observaréis</a>; <a name="00900600900006">si</a> <a name="00900600900012" ll="SDBH:עלה:000004">sube</a> por el <a name="00900600900008" ll="SDBH:דֶּרֶךְ:000000">camino</a> de <a name="00900600900016">su</a> <a name="00900600900010" ll="SDBH:גְּבוּל:000000">tierra</a> a Bet-semes, <a name="00900600900058">él</a> nos <a name="00900600900018">ha</a> <a name="00900600900018">hecho</a> este <a name="00900600900026" ll="SDBH:רָעָה:000002">mal</a> tan <a name="00900600900030" ll="SDBH:גָּדֹול:000003">grande</a>; y <a name="00900600900038">si</a> <a name="00900600900040" ll="SDBH:לֹא:000000">no</a>, <a name="00900600900044">sabremos</a> que <a name="00900600900048" ll="SDBH:לֹא:000000">no</a> es <a name="00900600900050">su</a> <a name="00900600900050">mano</a> la que nos ha <a name="00900600900052" ll="SDBH:נגע:000001">herido</a>, sino que esto <a name="00900600900060">ocurrió</a> por <a name="00900600900056" ll="SDBH:מִקְרֶה:000000">accidente</a>.
    
\v 10 Y aquellos <a name="00900601000008" ll="SDBH:אִישׁ:000000">hombres</a> lo <a name="00900601000004">hicieron</a> <a name="00900601000010">así</a>; <a name="00900601000014" ll="SDBH:לקח:000000">tomando</a> <a name="00900601000016" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="00900601000018" ll="SDBH:פָּרָה:000000">vacas</a> que <a name="00900601000020" ll="SDBH:עול:001000">criaban</a>, las <a name="00900601000024" ll="SDBH:אסר:000002">uncieron</a> al <a name="00900601000030" ll="SDBH:עֲגָלָה:000000">carro</a>, y <a name="00900601000038" ll="SDBH:כלא:000000">encerraron</a> en <a name="00900601000044" ll="SDBH:בַּיִת:000000">casa</a> <a name="00900601000036" ll="SDBH:בֵּן:000001">sus</a> <a name="00900601000036" ll="SDBH:בֵּן:000001">becerros</a>. 
\v 11 Luego <a name="00900601100004" ll="SDBH:שׂים:000001">pusieron</a> el <a name="00900601100008" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900601100010" ll="SDBH:יהוה:000000">Jehová</a> <a name="00900601100012">sobre</a> el <a name="00900601100016" ll="SDBH:עֲגָלָה:000000">carro</a>, y la <a name="00900601100024" ll="SDBH:אַרְגַּז:000000">caja</a> con los <a name="00900601100030" ll="SDBH:עַכְבָּר:000000">ratones</a> de <a name="00900601100034" ll="SDBH:זָהָב:000000">oro</a> y las <a name="00900601100040" ll="SDBH:צֶלֶם:000000">figuras</a> de <a name="00900601100042" ll="SDBH:טְחֹרִים:000001">sus</a> <a name="00900601100042" ll="SDBH:טְחֹרִים:000001">tumores</a>. 
\v 12 Y las <a name="00900601200008" ll="SDBH:פָּרָה:000000">vacas</a> <a name="00900601200004" ll="SDBH:ישׁר:000001">se</a> <a name="00900601200004" ll="SDBH:ישׁר:000001">encaminaron</a> por el <a name="00900601200014" ll="SDBH:דֶּרֶךְ:000000">camino</a> de Bet-semes, y <a name="00900601200028" ll="SDBH:הלך:000001">seguían</a> <a name="00900601200024" ll="SDBH:מְסִלָּה:000000">camino</a> <a name="00900601200026" ll="SDBH:אֶחָד:000000">recto</a>, <a name="00900601200030" ll="SDBH:הלך:000001">andando</a> y <a name="00900601200034" ll="SDBH:געה:000000">bramando</a>, sin <a name="00900601200040" ll="SDBH:סור:000000">apartarse</a> ni a <a name="00900601200042" ll="SDBH:יָמִין:000000">derecha</a> ni a <a name="00900601200046" ll="SDBH:שְׂמֹאל:000000">izquierda</a>; y los <a name="00900601200050" ll="SDBH:סֶרֶן:001000">príncipes</a> de los <a name="00900601200052" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a> <a name="00900601200054" ll="SDBH:הלך:000001">fueron</a> <a name="00900601200056" ll="SDBH:אַחַר:001000">tras</a> ellas hasta el <a name="00900601200060" ll="SDBH:גְּבוּל:000000">límite</a> de Bet-semes. 
\v 13 Y los de Bet-semes <a name="00900601300006" ll="SDBH:קצר:000006">segaban</a> el <a name="00900601300010" ll="SDBH:חִטָּה:000000">trigo</a> en el <a name="00900601300016" ll="SDBH:עֵמֶק:000000">valle</a>; y <a name="00900601300020" ll="SDBH:נשׂא:000039">alzando</a> los <a name="00900601300024" ll="SDBH:עַיִן:000000">ojos</a> <a name="00900601300028" ll="SDBH:ראה:000000">vieron</a> el <a name="00900601300034" ll="SDBH:אֲרֹון:000001">arca</a>, y <a name="00900601300038" ll="SDBH:שׂמח:000000">se</a> <a name="00900601300038" ll="SDBH:שׂמח:000000">regocijaron</a> cuando la <a name="00900601300042" ll="SDBH:ראה:000000">vieron</a>. 
\v 14 Y el <a name="00900601400006" ll="SDBH:עֲגָלָה:000000">carro</a> <a name="00900601400008">vino</a> <a name="00900601400010">al</a> <a name="00900601400012" ll="SDBH:שָׂדֶה:000001">campo</a> de <a name="00900601400014" ll="SDBH:יְהֹושֻׁעַ:000001">Josué</a> <a name="00900601400016;00900601400020" ll="SDBH:בֵּית־הַשִּׁמְשִׁי:000000">de</a> Bet-semes, y <a name="00900601400024">paró</a> <a name="00900601400026" ll="SDBH:שָׁם:000000">allí</a> <a name="00900601400030" ll="SDBH:שָׁם:000000">donde</a> había una <a name="00900601400034" ll="SDBH:גָּדֹול:000000">gran</a> <a name="00900601400032" ll="SDBH:אֶבֶן:000001">piedra</a>; y <a name="00900601400038" ll="SDBH:בקע:000001">ellos</a> <a name="00900601400038" ll="SDBH:בקע:000001">cortaron</a> la <a name="00900601400042" ll="SDBH:עֵץ:000001">madera</a> del <a name="00900601400046" ll="SDBH:עֲגָלָה:000000">carro</a>, y <a name="00900601400056" ll="SDBH:עלה:000026">ofrecieron</a> las <a name="00900601400054" ll="SDBH:פָּרָה:000000">vacas</a> en <a name="00900601400058" ll="SDBH:עֹלָה:000000">holocausto</a> a <a name="00900601400062" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 15 Y los <a name="00900601500006" ll="SDBH:לֵוִי:000001">levitas</a> <a name="00900601500008" ll="SDBH:ירד:000002">bajaron</a> el <a name="00900601500012" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900601500014" ll="SDBH:יהוה:000000">Jehová</a>, y la <a name="00900601500022" ll="SDBH:אַרְגַּז:000000">caja</a> <a name="00900601500024">que</a> estaba <a name="00900601500026">junto</a> <a name="00900601500026">a</a> <a name="00900601500026">ella</a>, <a name="00900601500028;00900601500030">en</a> <a name="00900601500028;00900601500030">la</a> <a name="00900601500028;00900601500030">cual</a> estaban las <a name="00900601500032" ll="SDBH:כְּלִי:000000">joyas</a> de <a name="00900601500034" ll="SDBH:זָהָב:000000">oro</a>, y las <a name="00900601500038" ll="SDBH:שׂים:000001">pusieron</a> <a name="00900601500040">sobre</a> aquella <a name="00900601500048" ll="SDBH:גָּדֹול:000000">gran</a> <a name="00900601500044" ll="SDBH:אֶבֶן:000001">piedra</a>; y los <a name="00900601500052" ll="SDBH:אִישׁ:000000">hombres</a> de Bet-semes <a name="00900601500056" ll="SDBH:עלה:000026">sacrificaron</a> <a name="00900601500058" ll="SDBH:עֹלָה:000000">holocaustos</a> y <a name="00900601500062" ll="SDBH:זבח:000001">dedicaron</a> <a name="00900601500064" ll="SDBH:זֶבַח:000000">sacrificios</a> a <a name="00900601500078" ll="SDBH:יהוה:000000">Jehová</a> en <a name="00900601500074">aquel</a> <a name="00900601500070" ll="SDBH:יֹום:000001">día</a>. 
\v 16 Cuando <a name="00900601600010" ll="SDBH:ראה:000000">vieron</a> esto los <a name="00900601600004" ll="SDBH:חָמֵשׁ:000000">cinco</a> <a name="00900601600006" ll="SDBH:סֶרֶן:001000">príncipes</a> de los <a name="00900601600008" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a>, <a name="00900601600014" ll="SDBH:שׁוב:000023">volvieron</a> a <a name="00900601600016" ll="SDBH:עֶקְרֹון:000000">Ecrón</a> el <a name="00900601600026">mismo</a> <a name="00900601600022" ll="SDBH:יֹום:000001">día</a>.
    
\v 17 <a name="00900601700004" ll="SDBH:אֵלֶּה:000000">Éstos</a> fueron los <a name="00900601700006" ll="SDBH:טְחֹרִים:000001">tumores</a> de <a name="00900601700010" ll="SDBH:זָהָב:000000">oro</a> <a name="00900601700012">que</a> <a name="00900601700014" ll="SDBH:שׁוב:000051">pagaron</a> los <a name="00900601700016" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a> en <a name="00900601700018" ll="SDBH:אָשָׁם:000002">expiación</a> a <a name="00900601700022" ll="SDBH:יהוה:000000">Jehová</a>: por <a name="00900601700026" ll="SDBH:אַשְׁדֹּוד:000000">Asdod</a> <a name="00900601700028" ll="SDBH:אֶחָד:000000">uno</a>, por <a name="00900601700032" ll="SDBH:עַזָּה:000000">Gaza</a> <a name="00900601700034" ll="SDBH:אֶחָד:000000">uno</a>, por <a name="00900601700038" ll="SDBH:אַשְׁקְלֹון:000000">Ascalón</a> <a name="00900601700040" ll="SDBH:אֶחָד:000000">uno</a>, por <a name="00900601700044" ll="SDBH:גַּת:001000">Gat</a> <a name="00900601700046" ll="SDBH:אֶחָד:000000">uno</a>, por <a name="00900601700050" ll="SDBH:עֶקְרֹון:000000">Ecrón</a> <a name="00900601700052" ll="SDBH:אֶחָד:000000">uno</a>. 
\v 18 Y los <a name="00900601800004" ll="SDBH:עַכְבָּר:000000">ratones</a> de <a name="00900601800008" ll="SDBH:זָהָב:000000">oro</a> fueron conforme al <a name="00900601800010" ll="SDBH:מִסְפָּר:000002">número</a> de <a name="00900601800012">todas</a> las <a name="00900601800014" ll="SDBH:עִיר:000000">ciudades</a> de los <a name="00900601800016" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a> pertenecientes a los <a name="00900601800020">cinco</a> <a name="00900601800024" ll="SDBH:סֶרֶן:001000">príncipes</a>, así las <a name="00900601800028" ll="SDBH:עִיר:000000">ciudades</a> <a name="00900601800030" ll="SDBH:מִבְצָר:000001">fortificadas</a> como las <a name="00900601800036" ll="SDBH:כֹּפֶר:001000">aldeas</a> <a name="00900601800040" ll="SDBH:פְּרָזִי:000000">sin</a> <a name="00900601800040" ll="SDBH:פְּרָזִי:000000">muro</a>. La <a name="00900601800050" ll="SDBH:גָּדֹול:000000">gran</a> <a name="00900601800046">piedra</a> <a name="00900601800052;00900601800056">sobre</a> <a name="00900601800052;00900601800056">la</a> <a name="00900601800052;00900601800056">cual</a> <a name="00900601800054">pusieron</a> el <a name="00900601800060" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900601800062">Jehová</a> está en el <a name="00900601800076" ll="SDBH:שָׂדֶה:000001">campo</a> de <a name="00900601800078" ll="SDBH:יְהֹושֻׁעַ:000001">Josué</a> <a name="00900601800080;00900601800082;00900601800084" ll="SDBH:בֵּית־הַשִּׁמְשִׁי:000000">de</a> Bet-semes <a name="00900601800064" ll="SDBH:עַד:000001">hasta</a> <a name="00900601800066;00900601800068;00900601800070;00900601800072" ll="SDBH:זֶה:000000;SDBH:יֹום:000005">hoy</a>.
    
\v 19 Entonces Dios <a name="00900601900004" ll="SDBH:נכה:000004" tl="HOTTP1004">hizo</a> <a name="00900601900004" ll="SDBH:נכה:000004" tl="HOTTP1004">morir</a> a los <a name="00900601900008" ll="SDBH:אִישׁ:000000">hombres</a> de Bet-semes, porque <a name="00900601900014" ll="SDBH:ראה:000000" tl="HOTTP1005">habían</a> <a name="00900601900014" ll="SDBH:ראה:000000" tl="HOTTP1005">mirado</a> dentro del <a name="00900601900018" ll="SDBH:אֲרֹון:000001" il="LB00283;NBGlo20140501;NBGlo20140501e;NBGlo20140509;Realia:4.1a" tl="HOTTP1005">arca</a> de <a name="00900601900020" ll="SDBH:יהוה:000000" tl="HOTTP1005">Jehová</a>; <a name="00900601900024" ll="SDBH:נכה:000004" tl="HOTTP1006">hizo</a> <a name="00900601900024" ll="SDBH:נכה:000004" tl="HOTTP1006">morir</a> del <a name="00900601900030" ll="SDBH:עַם:000000" tl="HOTTP1006">pueblo</a> a <a name="00900601900036" ll="SDBH:חָמֵשׁ:000001" tl="HOTTP1006">cincuenta</a> <a name="00900601900038" ll="SDBH:אֶלֶף:001000" tl="HOTTP1006">mil</a> <a name="00900601900032" ll="SDBH:שֶׁבַע:000001" tl="HOTTP1006">setenta</a> <a name="00900601900034" ll="SDBH:אִישׁ:000000" tl="HOTTP1006">hombres</a>. Y <a name="00900601900044" ll="SDBH:אבל:000000">lloró</a> el <a name="00900601900048">pueblo</a>, <a name="00900601900050">porque</a> <a name="00900601900054" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="00900601900052" ll="SDBH:נכה:000008">había</a> <a name="00900601900052" ll="SDBH:נכה:000008">herido</a> con tan <a name="00900601900064" ll="SDBH:גָּדֹול:000003">gran</a> <a name="00900601900062" ll="SDBH:מַכָּה:000000">mortandad</a>. 
\v 20 Y <a name="00900602000004" ll="SDBH:אמר:000000">dijeron</a> <a name="00900602000006" ll="SDBH:אִישׁ:000000">los</a> <a name="00900602000006" ll="SDBH:אִישׁ:000000">de</a> Bet-semes: ¿<a name="00900602000010" ll="SDBH:מִי:000000">Quién</a> <a name="00900602000012" ll="SDBH:יכל:000000">podrá</a> <a name="00900602000016">estar</a> <a name="00900602000018;00900602000020">delante</a> de <a name="00900602000022" ll="SDBH:יהוה:000000">Jehová</a> el <a name="00900602000026" ll="SDBH:אֱלֹהִים:000002">Dios</a> <a name="00900602000030" ll="SDBH:קָדֹושׁ:000000">santo</a>? ¿<a name="00900602000038">A</a> <a name="00900602000040" ll="SDBH:מִי:000000">quién</a> <a name="00900602000042" ll="SDBH:עלה:000004">subirá</a> <a name="00900602000044">desde</a> <a name="00900602000046">nosotros</a>? 
\v 21 Y <a name="00900602100004" ll="SDBH:שׁלח:000000">enviaron</a> <a name="00900602100006" ll="SDBH:מַלְאָךְ:000000">mensajeros</a> <a name="00900602100008">a</a> los <a name="00900602100010">habitantes</a> de Quiriat-jearim, <a name="00900602100014;00900602100016" ll="SDBH:אמר:000000">diciendo</a>: Los <a name="00900602100020" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a> <a name="00900602100018" ll="SDBH:שׁוב:000051">han</a> <a name="00900602100018" ll="SDBH:שׁוב:000051">devuelto</a> el <a name="00900602100024" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900602100026" ll="SDBH:יהוה:000000">Jehová</a>; <a name="00900602100028" ll="SDBH:ירד:000001">descended</a>, pues, y <a name="00900602100030;00900602100032" ll="SDBH:עלה:000005">llevadla</a> <a name="00900602100034">a</a> <a name="00900602100034">vosotros</a>.
\c 7
\v 1 <a name="00900700100004">Vinieron</a> los de Quiriat-jearim y <a name="00900700100012" ll="SDBH:עלה:000005">llevaron</a> el <a name="00900700100016" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900700100018" ll="SDBH:יהוה:000000">Jehová</a>, y la <a name="00900700100022">pusieron</a> <a name="00900700100026">en</a> <a name="00900700100028" ll="SDBH:בַּיִת:000000">casa</a> de <a name="00900700100030" ll="SDBH:אֲבִינָדָב:000000">Abinadab</a>, situada en el <a name="00900700100036" ll="SDBH:גִּבְעָה:000000">collado</a>; y <a name="00900700100046" ll="SDBH:קדשׁ:000004">santificaron</a> a <a name="00900700100042" ll="SDBH:אֶלְעָזָר:000001">Eleazar</a> <a name="00900700100044" ll="SDBH:בֵּן:000000">su</a> <a name="00900700100044" ll="SDBH:בֵּן:000000">hijo</a> para que <a name="00900700100050" ll="SDBH:שׁמר:000007">guardase</a> el <a name="00900700100054" ll="SDBH:אֲרֹון:000001">arca</a> de <a name="00900700100056" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 2 <a name="00900700200006">Desde</a> el <a name="00900700200008" ll="SDBH:יֹום:000003">día</a> que <a name="00900700200010">llegó</a> el <a name="00900700200014" ll="SDBH:אֲרֹון:000001">arca</a> a Quiriat-jearim <a name="00900700200022" ll="SDBH:רבה:000000">pasaron</a> <a name="00900700200022" ll="SDBH:רבה:000000">muchos</a> <a name="00900700200026" ll="SDBH:יֹום:000002">días</a>, <a name="00900700200032" ll="SDBH:עֶשֶׂר:000001">veinte</a> <a name="00900700200034" ll="SDBH:שָׁנָה:000000">años</a>; y <a name="00900700200040">toda</a> la <a name="00900700200042" ll="SDBH:בַּיִת:000003">casa</a> de <a name="00900700200044" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00900700200038">lamentaba</a> <a name="00900700200046" ll="SDBH:אַחַר:001001">en</a> <a name="00900700200046" ll="SDBH:אַחַר:001001">pos</a> <a name="00900700200046" ll="SDBH:אַחַר:001001">de</a> <a name="00900700200048" ll="SDBH:יהוה:000000">Jehová</a>.
    
