\v 12 el <a name="00203501200006" ll="SDBH:אֲרֹון:000001">arca</a> y <a name="00203501200012" ll="SDBH:בַּד:001003">sus</a> <a name="00203501200012" ll="SDBH:בַּד:001003">varas</a>, el <a name="00203501200018" ll="SDBH:כַּפֹּרֶת:000000">propiciatorio</a>, el <a name="00203501200024" ll="SDBH:פָּרֹכֶת:000000">velo</a> de la <a name="00203501200028" ll="SDBH:מָסָךְ:000000">tienda</a>; 
\v 13 la <a name="00203501300006" ll="SDBH:שֻׁלְחָן:000000">mesa</a> y <a name="00203501300012" ll="SDBH:בַּד:001003">sus</a> <a name="00203501300012" ll="SDBH:בַּד:001003">varas</a>, y <a name="00203501300018">todos</a> <a name="00203501300020" ll="SDBH:כְּלִי:000000">sus</a> <a name="00203501300020" ll="SDBH:כְּלִי:000000">utensilios</a>, y el <a name="00203501300026" ll="SDBH:לֶחֶם:000001">pan</a> de la <a name="00203501300030" ll="SDBH:פָּנֶה:000000">proposición</a>; 
\v 14 el <a name="00203501400006" ll="SDBH:מְנֹורָה:000000">candelero</a> del <a name="00203501400010" ll="SDBH:מָאֹור:000000">alumbrado</a> y <a name="00203501400016" ll="SDBH:כְּלִי:000000">sus</a> <a name="00203501400016" ll="SDBH:כְּלִי:000000">utensilios</a>, sus lámparas, y el <a name="00203501400028" ll="SDBH:שֶׁמֶן:000000">aceite</a> para el <a name="00203501400032" ll="SDBH:מָאֹור:000000">alumbrado</a>; 
\v 15 el <a name="00203501500006" ll="SDBH:מִזְבֵּחַ:000000">altar</a> del <a name="00203501500010" ll="SDBH:קְטֹרֶת:000001">incienso</a> y <a name="00203501500016" ll="SDBH:בַּד:001003">sus</a> <a name="00203501500016" ll="SDBH:בַּד:001003">varas</a>, el <a name="00203501500022" ll="SDBH:שֶׁמֶן:000000">aceite</a> de la <a name="00203501500026" ll="SDBH:מִשְׁחָה:000000">unción</a>, el <a name="00203501500032" ll="SDBH:קְטֹרֶת:000001">incienso</a> <a name="00203501500036" ll="SDBH:סַם:000000">aromático</a>, la <a name="00203501500042" ll="SDBH:מָסָךְ:000000">cortina</a> de la <a name="00203501500046" ll="SDBH:פֶּתַח:000000">puerta</a> para la <a name="00203501500050" ll="SDBH:פֶּתַח:000000">entrada</a> del <a name="00203501500054" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a>; 
\v 16 el <a name="00203501600004" ll="SDBH:מִזְבֵּחַ:000000">altar</a> del <a name="00203501600008" ll="SDBH:עֹלָה:000000">holocausto</a>, su <a name="00203501600014" ll="SDBH:מִכְבָּר:000000">enrejado</a> de <a name="00203501600018" ll="SDBH:נְחֹשֶׁת:000000">bronce</a> y <a name="00203501600026" ll="SDBH:בַּד:001003">sus</a> <a name="00203501600026" ll="SDBH:בַּד:001003">varas</a>, y <a name="00203501600032">todos</a> <a name="00203501600034" ll="SDBH:כְּלִי:000000">sus</a> <a name="00203501600034" ll="SDBH:כְּלִי:000000">utensilios</a>, y la <a name="00203501600040" ll="SDBH:כִּיֹּור:000001">fuente</a> con su base; 
\v 17 las <a name="00203501700004" ll="SDBH:קֶלַע:001000">cortinas</a> del <a name="00203501700008" ll="SDBH:חָצֵר:000001">atrio</a>, <a name="00203501700012" ll="SDBH:עַמּוּד:000000">sus</a> <a name="00203501700012" ll="SDBH:עַמּוּד:000000">columnas</a> y <a name="00203501700018" ll="SDBH:אֶדֶן:000000">sus</a> <a name="00203501700018" ll="SDBH:אֶדֶן:000000">basas</a>, la <a name="00203501700024" ll="SDBH:מָסָךְ:000000">cortina</a> de la <a name="00203501700026" ll="SDBH:שַׁעַר:000000">puerta</a> del <a name="00203501700030" ll="SDBH:חָצֵר:000001">atrio</a>; 
\v 18 las <a name="00203501800004" ll="SDBH:יָתֵד:000000">estacas</a> del <a name="00203501800008" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a>, y las <a name="00203501800014" ll="SDBH:יָתֵד:000000">estacas</a> del <a name="00203501800018" ll="SDBH:חָצֵר:000001">atrio</a> y <a name="00203501800024" ll="SDBH:מֵיתָר:000000">sus</a> <a name="00203501800024" ll="SDBH:מֵיתָר:000000">cuerdas</a>; 
\v 19 las <a name="00203501900004" ll="SDBH:בֶּגֶד:000001">vestiduras</a> del <a name="00203501900008" ll="SDBH:שְׂרָד:000000">servicio</a> para <a name="00203501900012" ll="SDBH:שׁרת:000000">ministrar</a> en el <a name="00203501900018" ll="SDBH:קֹדֶשׁ:000002">santuario</a>, las <a name="00203501900026" ll="SDBH:קֹדֶשׁ:000001">sagradas</a> <a name="00203501900022" ll="SDBH:בֶּגֶד:000001">vestiduras</a> de <a name="00203501900030" ll="SDBH:אַהֲרֹן:000000">Aarón</a> el <a name="00203501900034" ll="SDBH:כֹּהֵן:000000">sacerdote</a>, y las <a name="00203501900040" ll="SDBH:בֶּגֶד:000001">vestiduras</a> de <a name="00203501900042" ll="SDBH:בֵּן:000000">sus</a> <a name="00203501900042" ll="SDBH:בֵּן:000000">hijos</a> para servir en el <a name="00203501900046" ll="SDBH:כהן:000000">sacerdocio</a>.
    
\v 20 Y <a name="00203502000004" ll="SDBH:יצא:000000">salió</a> <a name="00203502000006">toda</a> la <a name="00203502000008" ll="SDBH:עֵדָה:000000">congregación</a> de los <a name="00203502000010" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00203502000012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> de <a name="00203502000018">delante</a> de <a name="00203502000020" ll="SDBH:מֹשֶׁה:000000">Moisés</a>. 
\v 21 Y <a name="00203502100004">vino</a> <a name="00203502100006">todo</a> <a name="00203502100008" ll="SDBH:אִישׁ:000000">varón</a> a <a name="00203502100010">quien</a> <a name="00203502100014" ll="SDBH:לֵב:000001">su</a> <a name="00203502100014" ll="SDBH:לֵב:000001">corazón</a> <a name="00203502100012" ll="SDBH:נשׂא:000051">estimuló</a>, y <a name="00203502100018">todo</a> aquel a <a name="00203502100020">quien</a> <a name="00203502100024" ll="SDBH:רוּחַ:000002">su</a> <a name="00203502100024" ll="SDBH:רוּחַ:000002">espíritu</a> le dio <a name="00203502100022" ll="SDBH:נדב:000000">voluntad</a>, con <a name="00203502100032" ll="SDBH:תְּרוּמָה:000000">ofrenda</a> a <a name="00203502100034" ll="SDBH:יהוה:000000">Jehová</a> para la <a name="00203502100038" ll="SDBH:מְלָאכָה:000000">obra</a> del <a name="00203502100040;00203502100042" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00203502100040;00203502100042" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00203502100040;00203502100042" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a> y para <a name="00203502100048">toda</a> <a name="00203502100050" ll="SDBH:עֲבֹודָה:000000">su</a> <a name="00203502100050" ll="SDBH:עֲבֹודָה:000000">obra</a>, y para las <a name="00203502100060" ll="SDBH:קֹדֶשׁ:000001">sagradas</a> <a name="00203502100056" ll="SDBH:בֶּגֶד:000001">vestiduras</a>. 
\v 22 <a name="00203502200004">Vinieron</a> así hombres como <a name="00203502200014" ll="SDBH:אִשָּׁה:000002">mujeres</a>, <a name="00203502200016">todos</a> los <a name="00203502200018" ll="SDBH:נָדִיב:000000">voluntarios</a> de <a name="00203502200020" ll="SDBH:לֵב:000001">corazón</a>, y <a name="00203502200022">trajeron</a> <a name="00203502200024" ll="SDBH:חָח:000001">cadenas</a> y <a name="00203502200028" ll="SDBH:נֶזֶם:000000">zarcillos</a>, <a name="00203502200032" ll="SDBH:טַבַּעַת:000000">anillos</a> y <a name="00203502200036" ll="SDBH:כּוּמָז:000000">brazaletes</a> y <a name="00203502200038">toda</a> clase de <a name="00203502200040" ll="SDBH:כְּלִי:000000">joyas</a> de <a name="00203502200042" ll="SDBH:זָהָב:000000">oro</a>; y <a name="00203502200008;00203502200046" ll="SDBH:אִישׁ:000000">todos</a> presentaban <a name="00203502200054" ll="SDBH:תְּנוּפָה:000001">ofrenda</a> de <a name="00203502200056" ll="SDBH:זָהָב:000000">oro</a> a <a name="00203502200060" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 23 <a name="00203502300004">Todo</a> <a name="00203502300006" ll="SDBH:אִישׁ:000000">hombre</a> <a name="00203502300008">que</a> tenía <a name="00203502300014" ll="SDBH:תְּכֵלֶת:000000">azul</a>, <a name="00203502300018" ll="SDBH:אַרְגָּמָן:000000">púrpura</a>, <a name="00203502300022" ll="SDBH:תֹּולֵעָה:001000">carmesí</a>, <a name="00203502300028" ll="SDBH:שֵׁשׁ:002000">lino</a> fino, pelo de <a name="00203502300032" ll="SDBH:עֵז:000001">cabras</a>, <a name="00203502300036" ll="SDBH:עֹור:000001">pieles</a> de <a name="00203502300038" ll="SDBH:אַיִל:000000">carneros</a> teñidas de <a name="00203502300040" ll="SDBH:אדם:000001">rojo</a>, o <a name="00203502300044" ll="SDBH:עֹור:000001">pieles</a> de <a name="00203502300046" ll="SDBH:תַּחַשׁ:000000">tejones</a>, lo <a name="00203502300048">traía</a>. 
\v 24 <a name="00203502400002;00203502400024">Todo</a> el que <a name="00203502400004" ll="SDBH:רום:000016">ofrecía</a> <a name="00203502400006" ll="SDBH:תְּרוּמָה:000000">ofrenda</a> de <a name="00203502400008" ll="SDBH:כֶּסֶף:000000">plata</a> o de <a name="00203502400012" ll="SDBH:נְחֹשֶׁת:000000">bronce</a> <a name="00203502400014">traía</a> a <a name="00203502400020" ll="SDBH:יהוה:000000">Jehová</a> la <a name="00203502400018" ll="SDBH:תְּרוּמָה:000000">ofrenda</a>; y todo el <a name="00203502400026">que</a> tenía <a name="00203502400032" ll="SDBH:עֵץ:000001">madera</a> de <a name="00203502400034" ll="SDBH:שִׁטָּה:000000">acacia</a> la <a name="00203502400046">traía</a> para <a name="00203502400038">toda</a> la <a name="00203502400040" ll="SDBH:מְלָאכָה:000001">obra</a> del <a name="00203502400044" ll="SDBH:עֲבֹודָה:000000">servicio</a>. 
\v 25 Además <a name="00203502500004">todas</a> las <a name="00203502500006" ll="SDBH:אִשָּׁה:000000">mujeres</a> <a name="00203502500008" ll="SDBH:חָכָם, קסם:000000">sabias</a> de <a name="00203502500010" ll="SDBH:לֵב:000001">corazón</a> <a name="00203502500016" ll="SDBH:טוה:000000">hilaban</a> con <a name="00203502500014">sus</a> <a name="00203502500014">manos</a>, y <a name="00203502500020">traían</a> lo que habían <a name="00203502500022" ll="SDBH:מַטְוֶה:000000">hilado</a>: <a name="00203502500028" ll="SDBH:תְּכֵלֶת:000000">azul</a>, <a name="00203502500036" ll="SDBH:אַרְגָּמָן:000000">púrpura</a>, <a name="00203502500040" ll="SDBH:תֹּולֵעָה:001000">carmesí</a> o <a name="00203502500052" ll="SDBH:שֵׁשׁ:002000">lino</a> fino. 
\v 26 Y <a name="00203502600004">todas</a> las <a name="00203502600008" ll="SDBH:אִשָּׁה:000000">mujeres</a> <a name="00203502600010">cuyo</a> <a name="00203502600014" ll="SDBH:לֵב:000001">corazón</a> las <a name="00203502600012" ll="SDBH:נשׂא:000051">impulsó</a> en <a name="00203502600020" ll="SDBH:חָכְמָה:000000">sabiduría</a> <a name="00203502600022" ll="SDBH:טוה:000000">hilaron</a> pelo de <a name="00203502600028" ll="SDBH:עֵז:000001">cabra</a>. 
\v 27 Los <a name="00203502700006" ll="SDBH:נָשִׂיא:000001">príncipes</a> <a name="00203502700008">trajeron</a> <a name="00203502700012" ll="SDBH:אֶבֶן:000008">piedras</a> de <a name="00203502700016" ll="SDBH:שֹׁהַם:000000">ónice</a>, y las <a name="00203502700022" ll="SDBH:אֶבֶן:000008">piedras</a> de los <a name="00203502700026" ll="SDBH:מִלֻּאִים:000000">engastes</a> para el <a name="00203502700032" ll="SDBH:אֵפֹוד:000001">efod</a> y el <a name="00203502700040" ll="SDBH:חֹשֶׁן:000000">pectoral</a>, 
\v 28 y las especias <a name="00203502800008" ll="SDBH:בֹּשֶׂם:000001">aromáticas</a>, y el <a name="00203502800016" ll="SDBH:שֶׁמֶן:000000">aceite</a> para el <a name="00203502800020" ll="SDBH:מָאֹור:000000">alumbrado</a>, y para el <a name="00203502800026" ll="SDBH:שֶׁמֶן:000000">aceite</a> de la <a name="00203502800030" ll="SDBH:מִשְׁחָה:000000">unción</a>, y para el <a name="00203502800036" ll="SDBH:קְטֹרֶת:000001">incienso</a> <a name="00203502800040" ll="SDBH:סַם:000000">aromático</a>. 
\v 29 De los <a name="00203502900048" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00203502900050" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, así <a name="00203502900004" ll="SDBH:אִישׁ:000000">hombres</a> como <a name="00203502900008" ll="SDBH:אִשָּׁה:000002">mujeres</a>, <a name="00203502900002">todos</a> los <a name="00203502900010">que</a> tuvieron <a name="00203502900014" ll="SDBH:לֵב:000001">corazón</a> <a name="00203502900012" ll="SDBH:נדב:000000">voluntario</a> para <a name="00203502900020">traer</a> para <a name="00203502900024">toda</a> la <a name="00203502900028" ll="SDBH:מְלָאכָה:000000">obra</a>, <a name="00203502900030">que</a> <a name="00203502900034;00203502900056" ll="SDBH:יהוה:000000">Jehová</a> <a name="00203502900032" ll="SDBH:צוה:000000">había</a> <a name="00203502900032" ll="SDBH:צוה:000000">mandado</a> <a name="00203502900040;00203502900042">por</a> <a name="00203502900040;00203502900042">medio</a> de <a name="00203502900044" ll="SDBH:מֹשֶׁה:000000">Moisés</a> que <a name="00203502900038">hiciesen</a>, <a name="00203502900046">trajeron</a> <a name="00203502900052" ll="SDBH:נְדָבָה:000001">ofrenda</a> <a name="00203502900052" ll="SDBH:נְדָבָה:000001">voluntaria</a> a <a name="00203502900034;00203502900056" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 30 Y <a name="00203503000004" ll="SDBH:אמר:000000">dijo</a> <a name="00203503000006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00203503000008">a</a> los <a name="00203503000010" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00203503000012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>: <a name="00203503000014" ll="SDBH:ראה:000000">Mirad</a>, <a name="00203503000018" ll="SDBH:יהוה:000000">Jehová</a> <a name="00203503000016" ll="SDBH:קרא:000003">ha</a> <a name="00203503000016" ll="SDBH:קרא:000003">nombrado</a> a <a name="00203503000024" ll="SDBH:בְּצַלְאֵל:000000">Bezaleel</a> <a name="00203503000026" ll="SDBH:בֵּן:000000">hijo</a> de <a name="00203503000028" ll="SDBH:אוּרִי:000000">Uri</a>, <a name="00203503000030" ll="SDBH:בֵּן:000000">hijo</a> de <a name="00203503000032" ll="SDBH:חוּר:002001">Hur</a>, de la <a name="00203503000036" ll="SDBH:מַטֶּה:000007">tribu</a> de <a name="00203503000038" ll="SDBH:יְהוּדָה:000000">Judá</a>; 
\v 31 y lo <a name="00203503100004" ll="SDBH:מלא:000002">ha</a> <a name="00203503100004" ll="SDBH:מלא:000002">llenado</a> del <a name="00203503100008" ll="SDBH:רוּחַ:000000">Espíritu</a> de <a name="00203503100010" ll="SDBH:אֱלֹהִים:000002">Dios</a>, en <a name="00203503100014" ll="SDBH:חָכְמָה:000000">sabiduría</a>, en <a name="00203503100018" ll="SDBH:תְּבוּנָה:000000">inteligencia</a>, en <a name="00203503100024" ll="SDBH:דַּעַת:000000">ciencia</a> y en <a name="00203503100030">todo</a> <a name="00203503100032" ll="SDBH:מְלָאכָה:000000">arte</a>, 
\v 32 para <a name="00203503200006" ll="SDBH:חשׁב:000002">proyectar</a> <a name="00203503200008" ll="SDBH:מַחֲשָׁבָה:000001">diseños</a>, para <a name="00203503200012">trabajar</a> en <a name="00203503200018" ll="SDBH:זָהָב:000000">oro</a>, en <a name="00203503200026" ll="SDBH:כֶּסֶף:000000">plata</a> y en <a name="00203503200034" ll="SDBH:נְחֹשֶׁת:000000">bronce</a>, 
\v 33 y en la <a name="00203503300006" ll="SDBH:חֲרֹשֶׁת:000000">talla</a> de <a name="00203503300008" ll="SDBH:אֶבֶן:000008">piedras</a> de <a name="00203503300012" ll="SDBH:מלא:000020">engaste</a>, y en <a name="00203503300018" ll="SDBH:חֲרֹשֶׁת:000000">obra</a> de <a name="00203503300020" ll="SDBH:עֵץ:000001">madera</a>, para <a name="00203503300024">trabajar</a> en <a name="00203503300028">toda</a> <a name="00203503300030" ll="SDBH:מְלָאכָה:000000">labor</a> <a name="00203503300032" ll="SDBH:מַחֲשָׁבָה:000001">ingeniosa</a>. 
\v 34 Y <a name="00203503400008">ha</a> <a name="00203503400008">puesto</a> en <a name="00203503400012" ll="SDBH:לֵב:000001">su</a> <a name="00203503400012" ll="SDBH:לֵב:000001">corazón</a> el que pueda <a name="00203503400006" ll="SDBH:ירה:002000">enseñar</a>, así <a name="00203503400014">él</a> como <a name="00203503400018" ll="SDBH:אָהֳלִיאָב:000000">Aholiab</a> <a name="00203503400020" ll="SDBH:בֵּן:000000">hijo</a> de <a name="00203503400022" ll="SDBH:אֲחִיסָמָךְ:000000">Ahisamac</a>, de la <a name="00203503400026" ll="SDBH:מַטֶּה:000007">tribu</a> de <a name="00203503400028" ll="SDBH:דָּן:000000">Dan</a>; 
\v 35 y los <a name="00203503500002" ll="SDBH:מלא:000009">ha</a> <a name="00203503500002" ll="SDBH:מלא:000009">llenado</a> de <a name="00203503500006" ll="SDBH:חָכְמָה:000000">sabiduría</a> de <a name="00203503500008" ll="SDBH:לֵב:000001">corazón</a>, para que <a name="00203503500012">hagan</a> <a name="00203503500014">toda</a> <a name="00203503500016" ll="SDBH:מְלָאכָה:000000">obra</a> de <a name="00203503500018" ll="SDBH:חָרָשׁ:000000">arte</a> y de <a name="00203503500022" ll="SDBH:חשׁב:000003">invención</a>, y de <a name="00203503500026" ll="SDBH:רקם:000000">bordado</a> en <a name="00203503500032" ll="SDBH:תְּכֵלֶת:000000">azul</a>, en <a name="00203503500040" ll="SDBH:אַרְגָּמָן:000000">púrpura</a>, en <a name="00203503500044;00203503500048" ll="SDBH:שָׁנִי:000000;SDBH:תֹּולֵעָה:001000">carmesí</a>, en <a name="00203503500056" ll="SDBH:שֵׁשׁ:002000">lino</a> fino y en <a name="00203503500060" ll="SDBH:ארג:000001">telar</a>, para que <a name="00203503500062">hagan</a> <a name="00203503500064">toda</a> <a name="00203503500066" ll="SDBH:מְלָאכָה:000000">labor</a>, e <a name="00203503500070" ll="SDBH:חשׁב:000003">inventen</a> todo <a name="00203503500072" ll="SDBH:מַחֲשָׁבָה:000001">diseño</a>.
\c 36
\v 1 Así, pues, <a name="00203600100006" ll="SDBH:בְּצַלְאֵל:000000">Bezaleel</a> y <a name="00203600100010" ll="SDBH:אָהֳלִיאָב:000000">Aholiab</a>, y <a name="00203600100014">todo</a> <a name="00203600100016" ll="SDBH:אִישׁ:000000">hombre</a> <a name="00203600100018" ll="SDBH:חָכָם, קסם:000000">sabio</a> de <a name="00203600100020" ll="SDBH:לֵב:000001">corazón</a> a <a name="00203600100022">quien</a> <a name="00203600100026" ll="SDBH:יהוה:000000">Jehová</a> <a name="00203600100024">dio</a> <a name="00203600100028" ll="SDBH:חָכְמָה:000000">sabiduría</a> e <a name="00203600100032" ll="SDBH:תְּבוּנָה:000000">inteligencia</a> para <a name="00203600100040">saber</a> <a name="00203600100044">hacer</a> <a name="00203600100048">toda</a> la <a name="00203600100050" ll="SDBH:מְלָאכָה:000000">obra</a> del <a name="00203600100052" ll="SDBH:עֲבֹודָה:000000">servicio</a> del <a name="00203600100056" ll="SDBH:קֹדֶשׁ:000002">santuario</a>, harán <a name="00203600100060">todas</a> las cosas <a name="00203600100062">que</a> <a name="00203600100064" ll="SDBH:צוה:000000">ha</a> <a name="00203600100064" ll="SDBH:צוה:000000">mandado</a> <a name="00203600100066" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 2 Y <a name="00203600200006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00203600200004" ll="SDBH:קרא:000000">llamó</a> <a name="00203600200008">a</a> <a name="00203600200010" ll="SDBH:בְּצַלְאֵל:000000">Bezaleel</a> y <a name="00203600200014">a</a> <a name="00203600200016" ll="SDBH:אָהֳלִיאָב:000000">Aholiab</a> y <a name="00203600200020">a</a> <a name="00203600200022">todo</a> <a name="00203600200024" ll="SDBH:אִישׁ:000000">varón</a> <a name="00203600200026" ll="SDBH:חָכָם, קסם:000000">sabio</a> de <a name="00203600200028" ll="SDBH:לֵב:000001">corazón</a>, en <a name="00203600200030">cuyo</a> <a name="00203600200040" ll="SDBH:לֵב:000001">corazón</a> <a name="00203600200032">había</a> <a name="00203600200032">puesto</a> <a name="00203600200034" ll="SDBH:יהוה:000000">Jehová</a> <a name="00203600200036" ll="SDBH:חָכְמָה:000000">sabiduría</a>, <a name="00203600200042">todo</a> hombre a <a name="00203600200044">quien</a> <a name="00203600200048" ll="SDBH:לֵב:000001">su</a> <a name="00203600200048" ll="SDBH:לֵב:000001">corazón</a> le <a name="00203600200046" ll="SDBH:נשׂא:000051">movió</a> a <a name="00203600200052" ll="SDBH:קרב:000000">venir</a> <a name="00203600200054">a</a> la <a name="00203600200058" ll="SDBH:מְלָאכָה:000000">obra</a> para <a name="00203600200062">trabajar</a> en ella. 
\v 3 Y <a name="00203600300004" ll="SDBH:לקח:000000">tomaron</a> de <a name="00203600300010">delante</a> de <a name="00203600300012" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00203600300016">toda</a> la <a name="00203600300020" ll="SDBH:תְּרוּמָה:000000">ofrenda</a> <a name="00203600300022">que</a> los <a name="00203600300026" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00203600300028" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00203600300024">habían</a> <a name="00203600300024">traído</a> para la obra del <a name="00203600300034" ll="SDBH:עֲבֹודָה:000000">servicio</a> del <a name="00203600300038" ll="SDBH:קֹדֶשׁ:000002">santuario</a>, a fin de <a name="00203600300042">hacerla</a>. Y <a name="00203600300048">ellos</a> seguían <a name="00203600300054">trayéndole</a> ofrenda <a name="00203600300056" ll="SDBH:נְדָבָה:000001">voluntaria</a> cada <a name="00203600300062" ll="SDBH:בֹּקֶר:000000">mañana</a>. 
\v 4 Tanto, que <a name="00203600400004">vinieron</a> <a name="00203600400006">todos</a> los <a name="00203600400010" ll="SDBH:חָכָם, קסם:000000">maestros</a> que <a name="00203600400014">hacían</a> <a name="00203600400018">toda</a> la <a name="00203600400020" ll="SDBH:מְלָאכָה:000000">obra</a> del <a name="00203600400024" ll="SDBH:קֹדֶשׁ:000002">santuario</a>, cada <a name="00203600400026" ll="SDBH:אִישׁ:000002">uno</a> de la <a name="00203600400032" ll="SDBH:מְלָאכָה:000000">obra</a> <a name="00203600400034">que</a> <a name="00203600400038">hacía</a>, 
\v 5 y <a name="00203600500004" ll="SDBH:אמר:000000">hablaron</a> <a name="00203600500006">a</a> <a name="00203600500008" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, <a name="00203600500012" ll="SDBH:אמר:000000">diciendo</a>: El <a name="00203600500018" ll="SDBH:עַם:000000">pueblo</a> <a name="00203600500022">trae</a> <a name="00203600500014">mucho</a> más de lo que se <a name="00203600500030" ll="SDBH:עֲבֹודָה:000000">necesita</a> para la <a name="00203600500036" ll="SDBH:מְלָאכָה:000000">obra</a> <a name="00203600500038">que</a> <a name="00203600500042" ll="SDBH:יהוה:000000">Jehová</a> <a name="00203600500040" ll="SDBH:צוה:000000">ha</a> <a name="00203600500040" ll="SDBH:צוה:000000">mandado</a> que <a name="00203600500046">se</a> <a name="00203600500046">haga</a>. 
\v 6 Entonces <a name="00203600600006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00203600600004" ll="SDBH:צוה:000000;SDBH:צוה:000001">mandó</a> <a name="00203600600010">pregonar</a> por el <a name="00203600600018" ll="SDBH:מַחֲנֶה:000000">campamento</a>, <a name="00203600600022" ll="SDBH:אמר:000000">diciendo</a>: Ningún <a name="00203600600024" ll="SDBH:אִישׁ:000000">hombre</a> ni <a name="00203600600028" ll="SDBH:אִשָּׁה:000002">mujer</a> <a name="00203600600032">haga</a> <a name="00203600600034">más</a> para la <a name="00203600600040" ll="SDBH:תְּרוּמָה:000000">ofrenda</a> del <a name="00203600600044" ll="SDBH:קֹדֶשׁ:000002">santuario</a>. Así se le <a name="00203600600048" ll="SDBH:כלא:000003">impidió</a> al <a name="00203600600052" ll="SDBH:עַם:000000">pueblo</a> <a name="00203600600056">ofrecer</a> más; 
\v 7 pues <a name="00203600700008">tenían</a> material <a name="00203600700010" ll="SDBH:דַּי:000000">abundante</a> para <a name="00203600700022">hacer</a> <a name="00203600700014">toda</a> la <a name="00203600700006" ll="SDBH:מְלָאכָה:000002">obra</a>, y <a name="00203600700028" ll="SDBH:יתר:000003">sobraba</a>.
    
\v 8 <a name="00203600800006">Todos</a> los <a name="00203600800008" ll="SDBH:חָכָם, קסם:000000">sabios</a> de <a name="00203600800010" ll="SDBH:לֵב:000001">corazón</a> de entre los que <a name="00203600800004">hacían</a> la <a name="00203600800018" ll="SDBH:מְלָאכָה:000000">obra</a>, <a name="00203600800014">hicieron</a> el <a name="00203600800024" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a> de <a name="00203600800026" ll="SDBH:עֶשֶׂר:000000">diez</a> <a name="00203600800028" ll="SDBH:יְרִיעָה:000000">cortinas</a> de <a name="00203600800030" ll="SDBH:שֵׁשׁ:002000">lino</a> <a name="00203600800032" ll="SDBH:שׁזר:000000">torcido</a>, <a name="00203600800036" ll="SDBH:תְּכֵלֶת:000000">azul</a>, <a name="00203600800040" ll="SDBH:אַרְגָּמָן:000000">púrpura</a> y <a name="00203600800044" ll="SDBH:תֹּולֵעָה:001000">carmesí</a>; las <a name="00203600800054">hicieron</a> con <a name="00203600800048" ll="SDBH:כְּרוּב:000000">querubines</a> de <a name="00203600800050" ll="SDBH:מַעֲשֶׂה:000006">obra</a> <a name="00203600800052" ll="SDBH:חשׁב:000003">primorosa</a>. 
\v 9 La <a name="00203600900002" ll="SDBH:אֹרֶךְ:000000">longitud</a> de <a name="00203600900010" ll="SDBH:אֶחָד:000001">una</a> <a name="00203600900006" ll="SDBH:יְרִיעָה:000000">cortina</a> era de <a name="00203600900012;00203600900016" ll="SDBH:עֶשֶׂר:000001;SDBH:שְׁמֹנֶה:000000">veintiocho</a> <a name="00203600900022" ll="SDBH:אַמָּה:000000">codos</a>, y la <a name="00203600900026" ll="SDBH:רֹחַב:000000">anchura</a> de <a name="00203600900028" ll="SDBH:אַרְבַּע:000000">cuatro</a> <a name="00203600900034" ll="SDBH:אַמָּה:000000">codos</a>; <a name="00203600900050">todas</a> las <a name="00203600900054" ll="SDBH:יְרִיעָה:000000">cortinas</a> eran de <a name="00203600900046" ll="SDBH:אֶחָד:000000">igual</a> <a name="00203600900044" ll="SDBH:מִדָּה:000000">medida</a>.
    
\v 10 <a name="00203601000008" ll="SDBH:חָמֵשׁ:000000">Cinco</a> de las <a name="00203601000012" ll="SDBH:יְרִיעָה:000000">cortinas</a> las <a name="00203601000004" ll="SDBH:חבר:000001">unió</a> <a name="00203601000014;00203601000016;00203601000018" ll="SDBH:אֶחָד:000000">entre</a> <a name="00203601000014;00203601000016;00203601000018" ll="SDBH:אֶחָד:000000">sí</a>, y asimismo <a name="00203601000026" ll="SDBH:חבר:000001">unió</a> las otras <a name="00203601000022" ll="SDBH:חָמֵשׁ:000000">cinco</a> <a name="00203601000024" ll="SDBH:יְרִיעָה:000000">cortinas</a> <a name="00203601000028;00203601000030;00203601000032" ll="SDBH:אֶחָד:000000">entre</a> <a name="00203601000028;00203601000030;00203601000032" ll="SDBH:אֶחָד:000000">sí</a>. 
\v 11 E <a name="00203601100004">hizo</a> <a name="00203601100006" ll="SDBH:לוּלָאָה:000000">lazadas</a> de <a name="00203601100008" ll="SDBH:תְּכֵלֶת:000000">azul</a> <a name="00203601100010">en</a> la <a name="00203601100012" ll="SDBH:שָׂפָה:000003">orilla</a> de la <a name="00203601100016" ll="SDBH:יְרִיעָה:000000">cortina</a> que estaba al <a name="00203601100024" ll="SDBH:קָצָה:000000">extremo</a> de la primera <a name="00203601100030" ll="SDBH:מַחְבֶּרֶת:000000">serie</a>; e <a name="00203601100034">hizo</a> lo <a name="00203601100032">mismo</a> en la <a name="00203601100038" ll="SDBH:שָׂפָה:000003">orilla</a> de la <a name="00203601100042" ll="SDBH:יְרִיעָה:000000">cortina</a> <a name="00203601100046" ll="SDBH:קִיצֹון:000000">final</a> de la <a name="00203601100056" ll="SDBH:שֵׁנִי:000000">segunda</a> <a name="00203601100052" ll="SDBH:מַחְבֶּרֶת:000000">serie</a>. 
\v 12 <a name="00203601200002" ll="SDBH:חָמֵשׁ:000001">Cincuenta</a> <a name="00203601200004" ll="SDBH:לוּלָאָה:000000">lazadas</a> <a name="00203601200006">hizo</a> en la <a name="00203601200016" ll="SDBH:אֶחָד:000003">primera</a> <a name="00203601200012" ll="SDBH:יְרִיעָה:000000">cortina</a>, y otras <a name="00203601200020" ll="SDBH:חָמֵשׁ:000001">cincuenta</a> en la <a name="00203601200028" ll="SDBH:קָצֶה:000002">orilla</a> de la <a name="00203601200032" ll="SDBH:יְרִיעָה:000000">cortina</a> de la <a name="00203601200044" ll="SDBH:שֵׁנִי:000000">segunda</a> <a name="00203601200040" ll="SDBH:מַחְבֶּרֶת:000000">serie</a>; las <a name="00203601200022" ll="SDBH:לוּלָאָה:000000">lazadas</a> de la <a name="00203601200052" ll="SDBH:אֶחָד:000000">una</a> <a name="00203601200046" ll="SDBH:קבל:000003">correspondían</a> a las de la <a name="00203601200056" ll="SDBH:אֶחָד:000000">otra</a>. 
\v 13 <a name="00203601300004">Hizo</a> también <a name="00203601300006" ll="SDBH:חָמֵשׁ:000001">cincuenta</a> <a name="00203601300008" ll="SDBH:קֶרֶס:000000">corchetes</a> de <a name="00203601300010" ll="SDBH:זָהָב:000000">oro</a>, con los cuales <a name="00203601300014" ll="SDBH:חבר:000001">enlazó</a> las <a name="00203601300020" ll="SDBH:יְרִיעָה:000000">cortinas</a> <a name="00203601300022" ll="SDBH:אֶחָד:000000">una</a> con <a name="00203601300026" ll="SDBH:אֶחָד:000000">otra</a>, y así quedó <a name="00203601300036">formado</a> <a name="00203601300042" ll="SDBH:אֶחָד:000000">un</a> <a name="00203601300040" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a>.
    
\v 14 <a name="00203601400004;00203601400026">Hizo</a> asimismo <a name="00203601400006" ll="SDBH:יְרִיעָה:000000">cortinas</a> de pelo de <a name="00203601400008" ll="SDBH:עֵז:000001">cabra</a> para una <a name="00203601400012" ll="SDBH:אֹהֶל:000000">tienda</a> <a name="00203601400014">sobre</a> el <a name="00203601400018" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a>; <a name="00203601400020;00203601400022" ll="SDBH:עַשְׁתֵּי:000000">once</a> <a name="00203601400024" ll="SDBH:יְרִיעָה:000000">cortinas</a> hizo. 
\v 15 La <a name="00203601500002" ll="SDBH:אֹרֶךְ:000000">longitud</a> de <a name="00203601500010" ll="SDBH:אֶחָד:000001">una</a> <a name="00203601500006" ll="SDBH:יְרִיעָה:000000">cortina</a> era de <a name="00203601500012" ll="SDBH:שָׁלֹשׁ:000001">treinta</a> <a name="00203601500018" ll="SDBH:אַמָּה:000000">codos</a>, y la <a name="00203601500026" ll="SDBH:רֹחַב:000000">anchura</a> de <a name="00203601500022" ll="SDBH:אַרְבַּע:000000">cuatro</a> <a name="00203601500024" ll="SDBH:אַמָּה:000000">codos</a>; las <a name="00203601500042;00203601500044" ll="SDBH:עַשְׁתֵּי:000000">once</a> <a name="00203601500046" ll="SDBH:יְרִיעָה:000000">cortinas</a> tenían <a name="00203601500038" ll="SDBH:אֶחָד:000000">una</a> <a name="00203601500038" ll="SDBH:אֶחָד:000000">misma</a> <a name="00203601500036" ll="SDBH:מִדָּה:000000">medida</a>. 
\v 16 Y <a name="00203601600004" ll="SDBH:חבר:000001">unió</a> <a name="00203601600008" ll="SDBH:חָמֵשׁ:000000">cinco</a> de las <a name="00203601600012" ll="SDBH:יְרִיעָה:000000">cortinas</a> aparte, y las otras <a name="00203601600022" ll="SDBH:שֵׁשׁ:000000">seis</a> <a name="00203601600026" ll="SDBH:יְרִיעָה:000000">cortinas</a> aparte. 
\v 17 <a name="00203601700004">Hizo</a> además <a name="00203601700008" ll="SDBH:חָמֵשׁ:000001">cincuenta</a> <a name="00203601700006" ll="SDBH:לוּלָאָה:000000">lazadas</a> <a name="00203601700010">en</a> la <a name="00203601700012" ll="SDBH:שָׂפָה:000003">orilla</a> de la <a name="00203601700016" ll="SDBH:יְרִיעָה:000000">cortina</a> que estaba al <a name="00203601700020" ll="SDBH:קִיצֹון:000000">extremo</a> de la primera <a name="00203601700026" ll="SDBH:מַחְבֶּרֶת:000000">serie</a>, y otras <a name="00203601700030" ll="SDBH:חָמֵשׁ:000001">cincuenta</a> <a name="00203601700032" ll="SDBH:לוּלָאָה:000000">lazadas</a> <a name="00203601700036">en</a> la <a name="00203601700038" ll="SDBH:שָׂפָה:000003">orilla</a> de la <a name="00203601700042" ll="SDBH:יְרִיעָה:000000">cortina</a> final de la <a name="00203601700050" ll="SDBH:שֵׁנִי:000000">segunda</a> <a name="00203601700046" ll="SDBH:חֹבֶרֶת:000000">serie</a>. 
