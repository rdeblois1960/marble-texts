\v 16 <a name="04800501600002" ll="SDBG:λέγω:000000">Digo</a>, <a name="04800501600004" ll="SDBG:δέ:000002">pues</a>: <a name="04800501600008" ll="SDBG:περιπατέω:000001">Andad</a> en el <a name="04800501600006" ll="SDBG:πνεῦμα:000000">Espíritu</a>, <a name="04800501600010">y</a> <a name="04800501600016;04800501600018" ll="SDBG:οὐ:000003">no</a> <a name="04800501600020" ll="SDBG:τελέω:000002">satisfagáis</a> los <a name="04800501600012" ll="SDBG:ἐπιθυμία:000001">deseos</a> de la <a name="04800501600014" ll="SDBG:σάρξ:000005">carne</a>. 
\v 17 <a name="04800501700004" ll="SDBG:γάρ:000000">Porque</a> el <a name="04800501700008" ll="SDBG:ἐπιθυμέω:000001">deseo</a> de <a name="04800501700002" ll="SDBG:ὁ:000000">la</a> <a name="04800501700006" ll="SDBG:σάρξ:000005">carne</a> es <a name="04800501700010" ll="SDBG:κατά:000009">contra</a> <a name="04800501700012" ll="SDBG:ὁ:000000">el</a> <a name="04800501700014" ll="SDBG:πνεῦμα:000000">Espíritu</a>, <a name="04800501700018" ll="SDBG:δέ:000000">y</a> el <a name="04800501700016" ll="SDBG:ὁ:000000">del</a> <a name="04800501700020" ll="SDBG:πνεῦμα:000000">Espíritu</a> es <a name="04800501700022" ll="SDBG:κατά:000009">contra</a> <a name="04800501700024" ll="SDBG:ὁ:000000">la</a> <a name="04800501700026" ll="SDBG:σάρξ:000005">carne</a>; y <a name="04800501700028" ll="SDBG:οὗτος:000000">éstos</a> se <a name="04800501700034" ll="SDBG:ἀντίκειμαι:000000">oponen</a> entre <a name="04800501700032" ll="SDBG:ἀλλήλων:000000">sí</a>, para <a name="04800501700036" ll="SDBG:ἵνα:000001">que</a> <a name="04800501700038" ll="SDBG:μή:000000">no</a> <a name="04800501700048" ll="SDBG:ποιέω:000000">hagáis</a> lo <a name="04800501700040" ll="SDBG:ὅς:000000">que</a> <a name="04800501700044" ll="SDBG:θέλω:000002">quisiereis</a>. 
\v 18 <a name="04800501800004" ll="SDBG:δέ:000002">Pero</a> <a name="04800501800002" ll="SDBG:εἰ:000000">si</a> sois <a name="04800501800008" ll="SDBG:ἄγω:000003">guiados</a> por el <a name="04800501800006" ll="SDBG:πνεῦμα:000000">Espíritu</a>, <a name="04800501800010" ll="SDBG:οὐ:000000">no</a> estáis <a name="04800501800014" ll="SDBG:ὑπό:000001">bajo</a> la <a name="04800501800016" ll="SDBG:νόμος:000001">ley</a>. 
\v 19 <a name="04800501900004">Y</a> <a name="04800501900002" ll="SDBG:φανερός:000001">manifiestas</a> son <a name="04800501900008" ll="SDBG:ὁ:000000">las</a> <a name="04800501900010" ll="SDBG:ἔργον:000000">obras</a> de <a name="04800501900012" ll="SDBG:ὁ:000000">la</a> <a name="04800501900014" ll="SDBG:σάρξ:000005">carne</a>, <a name="04800501900016">que</a> son: adulterio, <a name="04800501900020" ll="SDBG:πορνεία:000000">fornicación</a>, <a name="04800501900022" ll="SDBG:ἀκαθαρσία:000000">inmundicia</a>, <a name="04800501900024" ll="SDBG:ἀσέλγεια:000000">lascivia</a>, 
\v 20 <a name="04800502000002" ll="SDBG:εἰδωλολατρία:000000">idolatría</a>, <a name="04800502000004" ll="SDBG:φαρμακεία:000000">hechicerías</a>, <a name="04800502000006" ll="SDBG:ἔχθρα:000000">enemistades</a>, <a name="04800502000008" ll="SDBG:ἔρις:000000">pleitos</a>, <a name="04800502000010" ll="SDBG:ζῆλος:000001">celos</a>, <a name="04800502000012" ll="SDBG:θυμός:000000">iras</a>, <a name="04800502000014" ll="SDBG:ἐριθεία:000000">contiendas</a>, <a name="04800502000016" ll="SDBG:διχοστασία:000000">disensiones</a>, <a name="04800502000018" ll="SDBG:αἵρεσις:000002">herejías</a>, 
