\c 6
\v 1 <a name="04800600100002" ll="SDBG:ἀδελφός:000001">Hermanos</a>, <a name="04800600100004" ll="SDBG:ἐάν:000000">si</a> <a name="04800600100010" ll="SDBG:ἄνθρωπος:000000">alguno</a> fuere <a name="04800600100008" ll="SDBG:προλαμβάνω:000000">sorprendido</a> <a name="04800600100012" ll="SDBG:ἐν:000004">en</a> <a name="04800600100014" ll="SDBG:τὶς:000000">alguna</a> <a name="04800600100016" ll="SDBG:παράπτωμα:000000">falta</a>, vosotros <a name="04800600100020" ll="SDBG:ὁ:000000">que</a> sois <a name="04800600100022" ll="SDBG:πνευματικός:000000">espirituales</a>, <a name="04800600100024;04800600100028" ll="SDBG:καταρτίζω:000000;SDBG:τοιοῦτος:000001">restauradle</a> <a name="04800600100030" ll="SDBG:ἐν:000009">con</a> <a name="04800600100032" ll="SDBG:πνεῦμα:000005">espíritu</a> de mansedumbre, <a name="04800600100036" ll="SDBG:σκοπέω:000001">considerándote</a> a <a name="04800600100038" ll="SDBG:σεαυτοῦ:000000">ti</a> mismo, <a name="04800600100040" ll="SDBG:μή:000002">no</a> sea que <a name="04800600100018" ll="SDBG:σύ:000001">tú</a> <a name="04800600100006">también</a> seas <a name="04800600100046" ll="SDBG:πειράζω:000002">tentado</a>. 
