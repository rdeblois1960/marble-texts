\v 6 También los <a name="01301600600012" ll="SDBH:כֹּהֵן:000000">sacerdotes</a> <a name="01301600600004" ll="SDBH:בְּנָיָהוּ:000002">Benaía</a> y <a name="01301600600008" ll="SDBH:יַחֲזִיאֵל:000001">Jahaziel</a> sonaban <a name="01301600600018" ll="SDBH:תָּמִיד:000000">continuamente</a> las <a name="01301600600016" ll="SDBH:חֲצֹצְרָה:000000">trompetas</a> <a name="01301600600022">delante</a> del <a name="01301600600024" ll="SDBH:אֲרֹון:000001">arca</a> del <a name="01301600600026" ll="SDBH:בְּרִית:000000">pacto</a> de <a name="01301600600030" ll="SDBH:אֱלֹהִים:000002">Dios</a>.
    
\v 7 <a name="01301600700012" ll="SDBH:אָז:000000">Entonces</a>, en aquel <a name="01301600700006" ll="SDBH:יֹום:000001">día</a>, <a name="01301600700016" ll="SDBH:דָּוִד:000000">David</a> <a name="01301600700014">comenzó</a> a <a name="01301600700026" ll="SDBH:ידה:000002">aclamar</a> a <a name="01301600700030" ll="SDBH:יהוה:000000">Jehová</a> por <a name="01301600700034">mano</a> de <a name="01301600700036" ll="SDBH:אָסָף:000000">Asaf</a> y de <a name="01301600700010">sus</a> <a name="01301600700040" ll="SDBH:אָח:000002">hermanos</a>:
    
\v 8 <a name="01301600800002" ll="SDBH:ידה:000002">Alabad</a> a <a name="01301600800006" ll="SDBH:יהוה:000000">Jehová</a>, <a name="01301600800008" ll="SDBH:קרא:000000">invocad</a> <a name="01301600800012" ll="SDBH:שֵׁם:000000">su</a> <a name="01301600800012" ll="SDBH:שֵׁם:000000">nombre</a>,Dad a <a name="01301600800014">conocer</a> en los <a name="01301600800020" ll="SDBH:עַם:000000">pueblos</a> <a name="01301600800022" ll="SDBH:עֲלִילָה:000000">sus</a> <a name="01301600800022" ll="SDBH:עֲלִילָה:000000">obras</a>.
    
\v 9 <a name="01301600900002" ll="SDBH:שׁיר:000000">Cantad</a> a él, cantadle <a name="01301600900006" ll="SDBH:זמר:000000">salmos</a>;<a name="01301600900010" ll="SDBH:שׂיח:000002">Hablad</a> de <a name="01301600900014">todas</a> sus <a name="01301600900016" ll="SDBH:פלא:000000">maravillas</a>.
    
\v 10 <a name="01301601000002" ll="SDBH:הלל:001002">Gloriaos</a> en <a name="01301601000008" ll="SDBH:קֹדֶשׁ:000001">su</a> <a name="01301601000008" ll="SDBH:קֹדֶשׁ:000001">santo</a> <a name="01301601000006" ll="SDBH:שֵׁם:000003">nombre</a>;<a name="01301601000010" ll="SDBH:שׂמח:000000">Alégrese</a> el <a name="01301601000012" ll="SDBH:לֵב:000001">corazón</a> de los que <a name="01301601000014" ll="SDBH:בקשׁ:000003">buscan</a> a <a name="01301601000016" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 11 <a name="01301601100002" ll="SDBH:דרשׁ:000003">Buscad</a> a <a name="01301601100004" ll="SDBH:יהוה:000000">Jehová</a> y <a name="01301601100008" ll="SDBH:עֹז:000002">su</a> <a name="01301601100008" ll="SDBH:עֹז:000002">poder</a>;<a name="01301601100010" ll="SDBH:בקשׁ:000003">Buscad</a> <a name="01301601100012" ll="SDBH:פָּנֶה:000000">su</a> <a name="01301601100012" ll="SDBH:פָּנֶה:000000">rostro</a> <a name="01301601100014" ll="SDBH:תָּמִיד:000000">continuamente</a>.
    
\v 12 Haced <a name="01301601200002" ll="SDBH:זכר:000000">memoria</a> de las <a name="01301601200004" ll="SDBH:פלא:000000">maravillas</a> que <a name="01301601200008">ha</a> <a name="01301601200008">hecho</a>,De <a name="01301601200010" ll="SDBH:מֹופֵת:000001">sus</a> <a name="01301601200010" ll="SDBH:מֹופֵת:000001">prodigios</a>, y de los <a name="01301601200014" ll="SDBH:מִשְׁפָּט:000001">juicios</a> de <a name="01301601200016" ll="SDBH:פֶּה:000000">su</a> <a name="01301601200016" ll="SDBH:פֶּה:000000">boca</a>,
    
\v 13 Oh vosotros, <a name="01301601300002" ll="SDBH:זֶרַע:000004">hijos</a> de <a name="01301601300004" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="01301601300006" ll="SDBH:עֶבֶד:000000">su</a> <a name="01301601300006" ll="SDBH:עֶבֶד:000000">siervo</a>,<a name="01301601300008" ll="SDBH:בֵּן:000000">Hijos</a> de <a name="01301601300010" ll="SDBH:יַעֲקֹב:000000">Jacob</a>, <a name="01301601300012" ll="SDBH:בָּחִיר:000000">sus</a> <a name="01301601300012" ll="SDBH:בָּחִיר:000000">escogidos</a>.
    
\v 14 <a name="01301601400004" ll="SDBH:יהוה:000000">Jehová</a>, <a name="01301601400002">él</a> es <a name="01301601400006" ll="SDBH:אֱלֹהִים:000001">nuestro</a> <a name="01301601400006" ll="SDBH:אֱלֹהִים:000001">Dios</a>;<a name="01301601400016" ll="SDBH:מִשְׁפָּט:000001">Sus</a> <a name="01301601400016" ll="SDBH:מִשְׁפָּט:000001">juicios</a> están en <a name="01301601400010">toda</a> la <a name="01301601400014" ll="SDBH:אֶרֶץ:000000">tierra</a>.
    
\v 15 Él hace <a name="01301601500002" ll="SDBH:זכר:000000" tl="HOTTP1540">memoria</a> de <a name="01301601500008" ll="SDBH:בְּרִית:000000">su</a> <a name="01301601500008" ll="SDBH:בְּרִית:000000">pacto</a> <a name="01301601500006" ll="SDBH:עֹולָם:000000">perpetuamente</a>,Y de la <a name="01301601500010" ll="SDBH:דָּבָר:000000">palabra</a> que él <a name="01301601500012" ll="SDBH:צוה:000000">mandó</a> para <a name="01301601500016" ll="SDBH:אֶלֶף:001000">mil</a> <a name="01301601500018" ll="SDBH:דֹּור:001000">generaciones</a>;
    
\v 16 Del pacto que <a name="01301601600004" ll="SDBH:כרת:000001">concertó</a> con <a name="01301601600008" ll="SDBH:אַבְרָהָם:000000">Abraham</a>,Y de <a name="01301601600012" ll="SDBH:שְׁבוּעָה:000000">su</a> <a name="01301601600012" ll="SDBH:שְׁבוּעָה:000000">juramento</a> a <a name="01301601600016" ll="SDBH:יִצְחָק:000000">Isaac</a>;
    
\v 17 El cual <a name="01301601700004">confirmó</a> a <a name="01301601700008" ll="SDBH:יַעֲקֹב:000000">Jacob</a> por <a name="01301601700012" ll="SDBH:חֹק:000002">estatuto</a>,Y a <a name="01301601700016" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> por <a name="01301601700018" ll="SDBH:בְּרִית:000000">pacto</a> <a name="01301601700020" ll="SDBH:עֹולָם:000000">sempiterno</a>,
    
\v 18 <a name="01301601800004" ll="SDBH:אמר:000000">Diciendo</a>: A ti <a name="01301601800008">daré</a> la <a name="01301601800010" ll="SDBH:אֶרֶץ:000000">tierra</a> de <a name="01301601800012" ll="SDBH:כְּנַעַן:001000">Canaán</a>,<a name="01301601800014" ll="SDBH:חֶבֶל:000002">Porción</a> de tu heredad.
    
\v 19 Cuando ellos <a name="01301601900004">eran</a> <a name="01301601900012" ll="SDBH:מְעַט:000000">pocos</a> en <a name="01301601900008" ll="SDBH:מִסְפָּר:000003">número</a>,Pocos y <a name="01301601900016" ll="SDBH:גור:000000">forasteros</a> en ella,
    
\v 20 Y <a name="01301602000004" ll="SDBH:הלך:000002">andaban</a> de <a name="01301602000008" ll="SDBH:גֹּוי:000000">nación</a> <a name="01301602000010">en</a> <a name="01301602000012" ll="SDBH:גֹּוי:000000">nación</a>,Y de un <a name="01301602000018" ll="SDBH:מַמְלָכָה:000001">reino</a> <a name="01301602000020">a</a> <a name="01301602000024" ll="SDBH:אַחֵר:000000">otro</a> <a name="01301602000022" ll="SDBH:עַם:000000">pueblo</a>,
    
\v 21 <a name="01301602100002" ll="SDBH:לֹא:000000">No</a> permitió que <a name="01301602100008">nadie</a> los oprimiese;Antes <a name="01301602100018">por</a> amor de ellos <a name="01301602100016" ll="SDBH:יכח:000004">castigó</a> a los <a name="01301602100020" ll="SDBH:מֶלֶךְ:000000">reyes</a>.
    
\v 22 <a name="01301602200002" ll="SDBH:אַל:000000">No</a> <a name="01301602200004" ll="SDBH:נגע:000001">toquéis</a>, dijo, a <a name="01301602200008" ll="SDBH:מָשִׁיחַ:000000">mis</a> <a name="01301602200008" ll="SDBH:מָשִׁיחַ:000000">ungidos</a>,<a name="01301602200016" ll="SDBH:אַל:000000">Ni</a> hagáis mal a <a name="01301602200014" ll="SDBH:נָבִיא:000000">mis</a> <a name="01301602200014" ll="SDBH:נָבִיא:000000">profetas</a>.
    
\v 23 <a name="01301602300002" ll="SDBH:שׁיר:000000">Cantad</a> a <a name="01301602300006" ll="SDBH:יהוה:000000">Jehová</a> <a name="01301602300008">toda</a> la <a name="01301602300012" ll="SDBH:אֶרֶץ:000000">tierra</a>,<a name="01301602300014" ll="SDBH:בשׂר:000000">Proclamad</a> de <a name="01301602300018" ll="SDBH:יֹום:000001">día</a> <a name="01301602300020">en</a> <a name="01301602300022" ll="SDBH:יֹום:000001">día</a> <a name="01301602300024" ll="SDBH:יְשׁוּעָה:000000">su</a> <a name="01301602300024" ll="SDBH:יְשׁוּעָה:000000">salvación</a>.
    
\v 24 <a name="01301602400002" ll="SDBH:ספר:000006">Cantad</a> entre las <a name="01301602400008" ll="SDBH:גֹּוי:000000">gentes</a> <a name="01301602400012" ll="SDBH:כָּבֹוד:000000">su</a> <a name="01301602400012" ll="SDBH:כָּבֹוד:000000">gloria</a>,Y en <a name="01301602400016">todos</a> los <a name="01301602400020" ll="SDBH:עַם:000000">pueblos</a> sus <a name="01301602400022" ll="SDBH:פלא:000000">maravillas</a>.
    
\v 25 Porque <a name="01301602500004" ll="SDBH:גָּדֹול:000002">grande</a> es <a name="01301602500006" ll="SDBH:יהוה:000000">Jehová</a>, y digno de <a name="01301602500012" ll="SDBH:מְאֹד:000001">suprema</a> <a name="01301602500010" ll="SDBH:הלל:001001">alabanza</a>,Y de ser <a name="01301602500016" ll="SDBH:ירא:000001">temido</a> <a name="01301602500020">sobre</a> <a name="01301602500022">todos</a> los <a name="01301602500024" ll="SDBH:אֱלֹהִים:000000">dioses</a>.
    
\v 26 Porque <a name="01301602600004">todos</a> los <a name="01301602600006" ll="SDBH:אֱלֹהִים:000001">dioses</a> de los <a name="01301602600010" ll="SDBH:עַם:000000">pueblos</a> son <a name="01301602600012" ll="SDBH:אֱלִיל:000000">ídolos</a>;Mas <a name="01301602600016" ll="SDBH:יהוה:000000">Jehová</a> <a name="01301602600020">hizo</a> los <a name="01301602600018" ll="SDBH:שָׁמַיִם:000000">cielos</a>.
    
\v 27 <a name="01301602700002" ll="SDBH:הֹוד:000000">Alabanza</a> y magnificencia <a name="01301602700010">delante</a> de él;<a name="01301602700012" ll="SDBH:עֹז:000002">Poder</a> y <a name="01301602700016" ll="SDBH:חֶדְוָה:000000">alegría</a> en <a name="01301602700020" ll="SDBH:מָקֹום:000000">su</a> <a name="01301602700020" ll="SDBH:מָקֹום:000000">morada</a>.
    
\v 28 <a name="01301602800002">Tributad</a> a <a name="01301602800006" ll="SDBH:יהוה:000000">Jehová</a>, oh <a name="01301602800008" ll="SDBH:מִשְׁפָּחָה:000000">familias</a> de los <a name="01301602800010" ll="SDBH:עַם:000000">pueblos</a>,<a name="01301602800012">Dad</a> a <a name="01301602800016" ll="SDBH:יהוה:000000">Jehová</a> <a name="01301602800018" ll="SDBH:כָּבֹוד:000000">gloria</a> y <a name="01301602800022" ll="SDBH:עֹז:000002">poder</a>.
    
\v 29 <a name="01301602900002">Dad</a> a <a name="01301602900006" ll="SDBH:יהוה:000000">Jehová</a> la <a name="01301602900008" ll="SDBH:כָּבֹוד:000000">honra</a> debida a <a name="01301602900010" ll="SDBH:שֵׁם:000002">su</a> <a name="01301602900010" ll="SDBH:שֵׁם:000002">nombre</a>;<a name="01301602900012" ll="SDBH:נשׂא:000021">Traed</a> <a name="01301602900014" ll="SDBH:מִנְחָה:000000">ofrenda</a>, y <a name="01301602900018">venid</a> <a name="01301602900020;01301602900022">delante</a> <a name="01301602900020;01301602900022">de</a> <a name="01301602900020;01301602900022">él</a>;<a name="01301602900024" ll="SDBH:חוה:001000">Postraos</a> <a name="01301602900026">delante</a> de <a name="01301602900028" ll="SDBH:יהוה:000000">Jehová</a> en la <a name="01301602900032" ll="SDBH:הֲדָרָה:000000">hermosura</a> de la <a name="01301602900034" ll="SDBH:קֹדֶשׁ:000001">santidad</a>.
    
\v 30 <a name="01301603000002" ll="SDBH:חיל:000002">Temed</a> en <a name="01301603000008">su</a> <a name="01301603000008">presencia</a>, <a name="01301603000010">toda</a> la <a name="01301603000014" ll="SDBH:אֶרֶץ:000000">tierra</a>;El <a name="01301603000020" ll="SDBH:תֵּבֵל:000000">mundo</a> será <a name="01301603000016" ll="SDBH:אַף:001000">aún</a> <a name="01301603000018" ll="SDBH:כון:000000" tl="HOTTP1541">establecido</a>, para que no <a name="01301603000024" ll="SDBH:מוט:000000">se</a> <a name="01301603000024" ll="SDBH:מוט:000000">conmueva</a>.
    
\v 31 <a name="01301603100002" ll="SDBH:שׂמח:000000">Alégrense</a> los <a name="01301603100006" ll="SDBH:שָׁמַיִם:000001">cielos</a>, y <a name="01301603100010" ll="SDBH:גיל:000000">gócese</a> la <a name="01301603100014" ll="SDBH:אֶרֶץ:000000">tierra</a>,Y <a name="01301603100018" ll="SDBH:אמר:000000">digan</a> en las <a name="01301603100024" ll="SDBH:גֹּוי:000000">naciones</a>: <a name="01301603100026" ll="SDBH:יהוה:000000">Jehová</a> <a name="01301603100028" ll="SDBH:מלך:000000">reina</a>.
    
\v 32 <a name="01301603200002" ll="SDBH:רעם:000000">Resuene</a> el <a name="01301603200006" ll="SDBH:יָם:000000">mar</a>, y <a name="01301603200010" ll="SDBH:מְלֹא:000000">su</a> <a name="01301603200010" ll="SDBH:מְלֹא:000000">plenitud</a>;<a name="01301603200012" ll="SDBH:עלץ:000000">Alégrese</a> el <a name="01301603200016" ll="SDBH:שָׂדֶה:000000">campo</a>, y <a name="01301603200020">todo</a> lo <a name="01301603200022">que</a> contiene.
    
