\v 10 <a name="03800601000002" ll="SDBH:לקח:000000">Toma</a> de los del <a name="03800601000010" ll="SDBH:גֹּולָה:000001">cautiverio</a> a <a name="03800601000014" ll="SDBH:חֶלְדַּי:000001">Heldai</a>, a <a name="03800601000022" ll="SDBH:טֹובִיָּה:000002">Tobías</a> y a <a name="03800601000030" ll="SDBH:יְדַעְיָה:000004">Jedaías</a>, los <a name="03800601000060">cuales</a> <a name="03800601000034">volvieron</a> de <a name="03800601000066" ll="SDBH:בָּבֶל:000000">Babilonia</a>; e <a name="03800601000050">irás</a> <a name="03800601000036">tú</a> en aquel <a name="03800601000042" ll="SDBH:יֹום:000006">día</a>, y <a name="03800601000062">entrarás</a> en <a name="03800601000052" ll="SDBH:בַּיִת:000000">casa</a> de <a name="03800601000054" ll="SDBH:יֹאשִׁיָּה:000000">Josías</a> <a name="03800601000056" ll="SDBH:בֵּן:000000">hijo</a> de <a name="03800601000058" ll="SDBH:צְפַנְיָה:000003">Sofonías</a>. 
\v 11 <a name="03800601100004" ll="SDBH:לקח:000000">Tomarás</a>, pues, <a name="03800601100006" ll="SDBH:כֶּסֶף:000000">plata</a> y <a name="03800601100010" ll="SDBH:זָהָב:000000">oro</a>, y <a name="03800601100014">harás</a> <a name="03800601100016" ll="SDBH:עֲטָרָה:000000" il="BK00158;EgyptianCrown-01-083;Realia:1.10.2a" tl="HOTTP4448">coronas</a>, y las <a name="03800601100020" ll="SDBH:שׂים:000001">pondrás</a> en la <a name="03800601100024" ll="SDBH:רֹאשׁ:000000">cabeza</a> del <a name="03800601100038" ll="SDBH:גָּדֹול:000002">sumo</a> <a name="03800601100034" ll="SDBH:כֹּהֵן:000000">sacerdote</a> <a name="03800601100026" ll="SDBH:יְהֹושֻׁעַ:000003">Josué</a>, <a name="03800601100028" ll="SDBH:בֵּן:000000">hijo</a> de <a name="03800601100030" ll="SDBH:יְהֹוצָדָק:000000">Josadac</a>. 
\v 12 Y <a name="03800601200006">le</a> <a name="03800601200004" ll="SDBH:אמר:000000">hablarás</a>, <a name="03800601200010" ll="SDBH:אמר:000000">diciendo</a>: <a name="03800601200012">Así</a> <a name="03800601200014" ll="SDBH:אמר:000000">ha</a> <a name="03800601200014" ll="SDBH:אמר:000000">hablado</a> <a name="03800601200016" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800601200018" ll="SDBH:צָבָא:000006">ejércitos</a>, <a name="03800601200022" ll="SDBH:אמר:000000">diciendo</a>: <a name="03800601200024">He</a> <a name="03800601200024">aquí</a> el <a name="03800601200026" ll="SDBH:אִישׁ:000000">varón</a> cuyo <a name="03800601200030" ll="SDBH:שֵׁם:000000">nombre</a> es el <a name="03800601200028" ll="SDBH:צֶמַח:000002">Renuevo</a>, el cual <a name="03800601200038" ll="SDBH:צמח:000000">brotará</a> de <a name="03800601200036">sus</a> <a name="03800601200036">raíces</a>, y <a name="03800601200042" ll="SDBH:בנה:000000">edificará</a> el <a name="03800601200046" ll="SDBH:הֵיכָל:000000">templo</a> de <a name="03800601200048" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 13 <a name="03800601300004">Él</a> <a name="03800601300006" ll="SDBH:בנה:000000">edificará</a> el <a name="03800601300010" ll="SDBH:הֵיכָל:000000">templo</a> de <a name="03800601300012" ll="SDBH:יהוה:000000">Jehová</a>, y <a name="03800601300016">él</a> <a name="03800601300018" ll="SDBH:נשׂא:000015">llevará</a> <a name="03800601300020" ll="SDBH:הֹוד:000000">gloria</a>, y <a name="03800601300024">se</a> <a name="03800601300024">sentará</a> y <a name="03800601300028" ll="SDBH:משׁל:001000">dominará</a> <a name="03800601300030">en</a> <a name="03800601300032" ll="SDBH:כִּסֵּא:000002">su</a> <a name="03800601300032" ll="SDBH:כִּסֵּא:000002">trono</a>, y <a name="03800601300036">habrá</a> <a name="03800601300038" ll="SDBH:כֹּהֵן:000000">sacerdote</a> <a name="03800601300040">a</a> <a name="03800601300042" ll="SDBH:כִּסֵּא:000002" il="Darius_In_Parse;Roman_throne;Throne_Greek;Throne_Toutankamon;mb120901102" tl="HOTTP4449">su</a> <a name="03800601300042" ll="SDBH:כִּסֵּא:000002" il="Darius_In_Parse;Roman_throne;Throne_Greek;Throne_Toutankamon;mb120901102" tl="HOTTP4449">lado</a>; y <a name="03800601300046" ll="SDBH:עֵצָה:000001">consejo</a> de <a name="03800601300048" ll="SDBH:שָׁלֹום:000000">paz</a> <a name="03800601300050">habrá</a> <a name="03800601300052">entre</a> <a name="03800601300054" ll="SDBH:שְׁנַיִם:000000">ambos</a>. 
\v 14 Las <a name="03800601400006" ll="SDBH:עֲטָרָה:000000" il="BK00158;EgyptianCrown-01-083;Realia:1.10.2a" tl="HOTTP4450">coronas</a> <a name="03800601400008">servirán</a> a <a name="03800601400012" ll="SDBH:חֵלֶם:001000" tl="HOTTP4451">Helem</a>, a <a name="03800601400018" ll="SDBH:טֹובִיָּה:000002">Tobías</a>, a <a name="03800601400024" ll="SDBH:יְדַעְיָה:000004">Jedaías</a> y a <a name="03800601400030" ll="SDBH:חֵן:001000" tl="HOTTP4452">Hen</a> <a name="03800601400032" ll="SDBH:בֵּן:000000" tl="HOTTP4452">hijo</a> de <a name="03800601400034" ll="SDBH:צְפַנְיָה:000003" tl="HOTTP4452">Sofonías</a>, como <a name="03800601400038" ll="SDBH:זִכָּרֹון:000001" tl="HOTTP4452">memoria</a> en el <a name="03800601400042" ll="SDBH:הֵיכָל:000000">templo</a> de <a name="03800601400044" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 15 Y los <a name="03800601500004" ll="SDBH:רָחֹוק:000000">que</a> <a name="03800601500004" ll="SDBH:רָחֹוק:000000">están</a> <a name="03800601500004" ll="SDBH:רָחֹוק:000000">lejos</a> <a name="03800601500006">vendrán</a> y ayudarán a <a name="03800601500010" ll="SDBH:בנה:000000">edificar</a> el <a name="03800601500014" ll="SDBH:הֵיכָל:000000">templo</a> de <a name="03800601500016" ll="SDBH:יהוה:000000">Jehová</a>, y <a name="03800601500020">conoceréis</a> que <a name="03800601500024" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800601500026" ll="SDBH:צָבָא:000006">ejércitos</a> <a name="03800601500028" ll="SDBH:שׁלח:000000">me</a> <a name="03800601500028" ll="SDBH:שׁלח:000000">ha</a> <a name="03800601500028" ll="SDBH:שׁלח:000000">enviado</a> <a name="03800601500030">a</a> <a name="03800601500030">vosotros</a>. Y esto <a name="03800601500034">sucederá</a> <a name="03800601500036">si</a> <a name="03800601500038;03800601500040">oyereis</a> <a name="03800601500038;03800601500040">obedientes</a> la <a name="03800601500044">voz</a> de <a name="03800601500046" ll="SDBH:יהוה:000000">Jehová</a> <a name="03800601500048" ll="SDBH:אֱלֹהִים:000001">vuestro</a> <a name="03800601500048" ll="SDBH:אֱלֹהִים:000001">Dios</a>.
\c 7
\v 1 <a name="03800700100004">Aconteció</a> que en el <a name="03800700100008" ll="SDBH:שָׁנָה:000000">año</a> <a name="03800700100010" ll="SDBH:אַרְבַּע:000000">cuarto</a> del <a name="03800700100018" ll="SDBH:מֶלֶךְ:000000">rey</a> <a name="03800700100014" ll="SDBH:דָּרְיָוֶשׁ:000000">Darío</a> <a name="03800700100020">vino</a> <a name="03800700100022" ll="SDBH:דָּבָר:000000">palabra</a> de <a name="03800700100024" ll="SDBH:יהוה:000000">Jehová</a> <a name="03800700100026">a</a> <a name="03800700100028" ll="SDBH:זְכַרְיָה:000005">Zacarías</a>, a los <a name="03800700100032" ll="SDBH:אַרְבַּע:000000">cuatro</a> días del <a name="03800700100038" ll="SDBH:חֹדֶשׁ:000000">mes</a> <a name="03800700100042" ll="SDBH:תְּשִׁיעִי:000000">noveno</a>, que es <a name="03800700100046" ll="SDBH:כִּסְלֵו:000000">Quisleu</a>, 
\v 2 cuando el pueblo de Bet-el <a name="03800700200004" ll="SDBH:שׁלח:000000">había</a> <a name="03800700200004" ll="SDBH:שׁלח:000000">enviado</a> a <a name="03800700200008" ll="SDBH:שַׂרְאֶצֶר:000001">Sarezer</a>, con Regem-melec y <a name="03800700200016" ll="SDBH:אִישׁ:000000">sus</a> <a name="03800700200016" ll="SDBH:אִישׁ:000000">hombres</a>, a <a name="03800700200020;03800700200024" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">implorar</a> <a name="03800700200006" ll="SDBH:בֵּית־אֵל:000000">el</a> <a name="03800700200020;03800700200024" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">favor</a> de <a name="03800700200026" ll="SDBH:יהוה:000000">Jehová</a>, 
\v 3 y a <a name="03800700300004" ll="SDBH:אמר:000000">hablar</a> <a name="03800700300006">a</a> los <a name="03800700300010" ll="SDBH:כֹּהֵן:000000">sacerdotes</a> <a name="03800700300012">que</a> estaban en la <a name="03800700300016" ll="SDBH:בַּיִת:000000">casa</a> de <a name="03800700300018" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800700300020" ll="SDBH:צָבָא:000006">ejércitos</a>, y <a name="03800700300024">a</a> los <a name="03800700300028" ll="SDBH:נָבִיא:000000">profetas</a>, <a name="03800700300032" ll="SDBH:אמר:000000">diciendo</a>: ¿<a name="03800700300036" ll="SDBH:בכה:000000">Lloraremos</a> en el <a name="03800700300042" ll="SDBH:חֹדֶשׁ:000000">mes</a> <a name="03800700300046" ll="SDBH:חֲמִישִׁי:000000">quinto</a>? ¿Haremos <a name="03800700300048" ll="SDBH:נזר:000000">abstinencia</a> como hemos <a name="03800700300054">hecho</a> ya algunos <a name="03800700300062" ll="SDBH:שָׁנָה:000000">años</a>? 
\v 4 <a name="03800700400004">Vino</a>, pues, <a name="03800700400012">a</a> mí <a name="03800700400006" ll="SDBH:דָּבָר:000000">palabra</a> de <a name="03800700400008" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800700400010" ll="SDBH:צָבָא:000006">ejércitos</a>, <a name="03800700400016" ll="SDBH:אמר:000000">diciendo</a>: 
\v 5 <a name="03800700500002" ll="SDBH:אמר:000000">Habla</a> <a name="03800700500004">a</a> <a name="03800700500006">todo</a> el <a name="03800700500008" ll="SDBH:עַם:000000">pueblo</a> del <a name="03800700500012" ll="SDBH:אֶרֶץ:000000">país</a>, y <a name="03800700500016">a</a> los <a name="03800700500020" ll="SDBH:כֹּהֵן:000000">sacerdotes</a>, <a name="03800700500024" ll="SDBH:אמר:000000">diciendo</a>: Cuando <a name="03800700500028" ll="SDBH:צום:000000">ayunasteis</a> y <a name="03800700500032" ll="SDBH:ספד:000000">llorasteis</a> en el <a name="03800700500038">quinto</a> y en el <a name="03800700500046" ll="SDBH:שְׁבִיעִי:000000">séptimo</a> mes estos setenta <a name="03800700500054" ll="SDBH:שָׁנָה:000000">años</a>, ¿<a name="03800700500058" ll="SDBH:צום:000000">habéis</a> <a name="03800700500058" ll="SDBH:צום:000000">ayunado</a> para <a name="03800700500062">mí</a>? 
\v 6 Y <a name="03800700600004">cuando</a> <a name="03800700600006" ll="SDBH:אכל:000000">coméis</a> y <a name="03800700600012" ll="SDBH:שׁתה:000000">bebéis</a>, ¿<a name="03800700600016" ll="SDBH:לֹא:000000">no</a> <a name="03800700600018;03800700600022" ll="SDBH:אכל:000000">coméis</a> y <a name="03800700600026;03800700600030" ll="SDBH:שׁתה:000000">bebéis</a> para vosotros mismos? 
\v 7 ¿<a name="03800700700004" ll="SDBH:לֹא:000000">No</a> son éstas las <a name="03800700700010" ll="SDBH:דָּבָר:000000">palabras</a> <a name="03800700700012">que</a> <a name="03800700700014" ll="SDBH:קרא:000000">proclamó</a> <a name="03800700700016" ll="SDBH:יהוה:000000">Jehová</a> <a name="03800700700018;03800700700020">por</a> <a name="03800700700018;03800700700020">medio</a> de los <a name="03800700700024" ll="SDBH:נָבִיא:000000">profetas</a> <a name="03800700700028" ll="SDBH:רִאשֹׁון:000000">primeros</a>, cuando <a name="03800700700034" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a> <a name="03800700700032">estaba</a> <a name="03800700700036">habitada</a> y tranquila, y <a name="03800700700044" ll="SDBH:עִיר:000000">sus</a> <a name="03800700700044" ll="SDBH:עִיר:000000">ciudades</a> en <a name="03800700700046" ll="SDBH:סָבִיב:000000">sus</a> <a name="03800700700046" ll="SDBH:סָבִיב:000000">alrededores</a> y el <a name="03800700700052" ll="SDBH:נֶגֶב:000000">Neguev</a> y la <a name="03800700700058" ll="SDBH:שְׁפֵלָה:000000">Sefela</a> estaban también <a name="03800700700060">habitados</a>?
    
\v 8 Y <a name="03800700800004">vino</a> <a name="03800700800006" ll="SDBH:דָּבָר:000000">palabra</a> de <a name="03800700800008" ll="SDBH:יהוה:000000">Jehová</a> <a name="03800700800010">a</a> <a name="03800700800012" ll="SDBH:זְכַרְיָה:000005">Zacarías</a>, <a name="03800700800016" ll="SDBH:אמר:000000">diciendo</a>: 
\v 9 <a name="03800700900002">Así</a> <a name="03800700900004" ll="SDBH:אמר:000000">habló</a> <a name="03800700900006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800700900008" ll="SDBH:צָבָא:000006">ejércitos</a>, <a name="03800700900010;03800700900012" ll="SDBH:אמר:000000">diciendo</a>: <a name="03800700900018" ll="SDBH:שׁפט:000000">Juzgad</a> <a name="03800700900014" ll="SDBH:מִשְׁפָּט:000001">conforme</a> a la <a name="03800700900016" ll="SDBH:אֱמֶת:000002">verdad</a>, y <a name="03800700900028">haced</a> <a name="03800700900022" ll="SDBH:חֶסֶד:000000">misericordia</a> y <a name="03800700900026" ll="SDBH:רַחֲמִים:000000">piedad</a> <a name="03800700900030;03800700900032;03800700900034" ll="SDBH:אִישׁ:000002;SDBH:אָח:000005">cada</a> <a name="03800700900030;03800700900032;03800700900034" ll="SDBH:אִישׁ:000002;SDBH:אָח:000005">cual</a> <a name="03800700900030;03800700900032;03800700900034" ll="SDBH:אִישׁ:000002;SDBH:אָח:000005">con</a> <a name="03800700900030;03800700900032;03800700900034" ll="SDBH:אִישׁ:000002;SDBH:אָח:000005">su</a> <a name="03800700900030;03800700900032;03800700900034" ll="SDBH:אִישׁ:000002;SDBH:אָח:000005">hermano</a>; 
\v 10 <a name="03800701000016" ll="SDBH:אַל:000000">no</a> <a name="03800701000018">oprimáis</a> a la <a name="03800701000004" ll="SDBH:אַלְמָנָה:000000">viuda</a>, al <a name="03800701000008" ll="SDBH:יָתֹום:000000">huérfano</a>, al <a name="03800701000010" ll="SDBH:גֵּר:000000">extranjero</a> ni al <a name="03800701000014" ll="SDBH:עָנִי:000000">pobre</a>; ni <a name="03800701000024;03800701000026;03800701000028" ll="SDBH:אִישׁ:000002;SDBH:אַל:000000;SDBH:אָח:000005">ninguno</a> <a name="03800701000030" ll="SDBH:חשׁב:000001">piense</a> <a name="03800701000022" ll="SDBH:רָעָה:000000">mal</a> en <a name="03800701000034" ll="SDBH:לֵבָב:000001">su</a> <a name="03800701000034" ll="SDBH:לֵבָב:000001">corazón</a> <a name="03800701000024;03800701000026;03800701000028" ll="SDBH:אִישׁ:000002;SDBH:אַל:000000;SDBH:אָח:000005">contra</a> <a name="03800701000024;03800701000026;03800701000028" ll="SDBH:אִישׁ:000002;SDBH:אַל:000000;SDBH:אָח:000005">su</a> <a name="03800701000024;03800701000026;03800701000028" ll="SDBH:אִישׁ:000002;SDBH:אַל:000000;SDBH:אָח:000005">hermano</a>. 
\v 11 Pero no <a name="03800701100004" ll="SDBH:מאן:000000">quisieron</a> escuchar, antes <a name="03800701100012">volvieron</a> la <a name="03800701100014" ll="SDBH:כָּתֵף:000006">espalda</a>, y <a name="03800701100022" ll="SDBH:כבד:000017">taparon</a> <a name="03800701100020" ll="SDBH:אֹזֶן:000000">sus</a> <a name="03800701100020" ll="SDBH:אֹזֶן:000000">oídos</a> para no <a name="03800701100026">oír</a>; 
\v 12 y <a name="03800701200006" ll="SDBH:שׂים:000005">pusieron</a> <a name="03800701200004" ll="SDBH:לֵב:000001">su</a> <a name="03800701200004" ll="SDBH:לֵב:000001">corazón</a> como <a name="03800701200008" ll="SDBH:שָׁמִיר:000000">diamante</a>, para no <a name="03800701200012">oír</a> la <a name="03800701200018" ll="SDBH:תֹּורָה:000000">ley</a> ni las <a name="03800701200026" ll="SDBH:דָּבָר:000000">palabras</a> <a name="03800701200028">que</a> <a name="03800701200032" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800701200034" ll="SDBH:צָבָא:000006">ejércitos</a> <a name="03800701200030" ll="SDBH:שׁלח:000020">enviaba</a> por <a name="03800701200038" ll="SDBH:רוּחַ:000000">su</a> <a name="03800701200038" ll="SDBH:רוּחַ:000000">Espíritu</a>, <a name="03800701200040;03800701200042">por</a> <a name="03800701200040;03800701200042">medio</a> de los <a name="03800701200046" ll="SDBH:נָבִיא:000000">profetas</a> <a name="03800701200050" ll="SDBH:רִאשֹׁון:000000">primeros</a>; <a name="03800701200054">vino</a>, por tanto, <a name="03800701200058" ll="SDBH:גָּדֹול:000003">gran</a> <a name="03800701200056" ll="SDBH:קֶצֶף:000001">enojo</a> de <a name="03800701200014">parte</a> de <a name="03800701200064" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800701200066" ll="SDBH:צָבָא:000006">ejércitos</a>. 
\v 13 Y <a name="03800701300004">aconteció</a> que así como él <a name="03800701300010" ll="SDBH:קרא:000000">clamó</a>, y <a name="03800701300014" ll="SDBH:לֹא:000000">no</a> <a name="03800701300016">escucharon</a>, <a name="03800701300018">también</a> ellos <a name="03800701300020" ll="SDBH:קרא:000000">clamaron</a>, y yo <a name="03800701300024" ll="SDBH:לֹא:000000">no</a> <a name="03800701300026">escuché</a>, <a name="03800701300028" ll="SDBH:אמר:000000">dice</a> <a name="03800701300030" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800701300032" ll="SDBH:צָבָא:000006">ejércitos</a>; 
\v 14 sino que <a name="03800701400004" ll="SDBH:סער:000001" tl="HOTTP4454">los</a> <a name="03800701400004" ll="SDBH:סער:000001" tl="HOTTP4454">esparcí</a> <a name="03800701400004" ll="SDBH:סער:000001" tl="HOTTP4454">con</a> <a name="03800701400004" ll="SDBH:סער:000001" tl="HOTTP4454">torbellino</a> <a name="03800701400006">por</a> <a name="03800701400008">todas</a> las <a name="03800701400012" ll="SDBH:גֹּוי:000000">naciones</a> <a name="03800701400014">que</a> <a name="03800701400018">ellos</a> <a name="03800701400016" ll="SDBH:לֹא:000000">no</a> <a name="03800701400018">conocían</a>, y la <a name="03800701400024" ll="SDBH:אֶרֶץ:000000">tierra</a> <a name="03800701400026" ll="SDBH:שׁמם:000001">fue</a> <a name="03800701400026" ll="SDBH:שׁמם:000001">desolada</a> <a name="03800701400028" ll="SDBH:אַחַר:001000">tras</a> <a name="03800701400028" ll="SDBH:אַחַר:001000">ellos</a>, sin quedar <a name="03800701400032" ll="SDBH:עבר:000000">quien</a> <a name="03800701400032" ll="SDBH:עבר:000000">fuese</a> ni <a name="03800701400038" ll="SDBH:שׁוב:000023">viniese</a>; pues <a name="03800701400042" ll="SDBH:שׂים:000005">convirtieron</a> en <a name="03800701400050" ll="SDBH:שַׁמָּה:000000">desierto</a> la <a name="03800701400044" ll="SDBH:אֶרֶץ:000000">tierra</a> <a name="03800701400046" ll="SDBH:חֶמְדָּה:000001">deseable</a>.
\c 8
\v 1 <a name="03800800100004">Vino</a> a mí <a name="03800800100006" ll="SDBH:דָּבָר:000000">palabra</a> de <a name="03800800100008" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800100010" ll="SDBH:צָבָא:000006">ejércitos</a>, <a name="03800800100014" ll="SDBH:אמר:000000">diciendo</a>: 
\v 2 <a name="03800800200002">Así</a> <a name="03800800200004" ll="SDBH:אמר:000000">ha</a> <a name="03800800200004" ll="SDBH:אמר:000000">dicho</a> <a name="03800800200006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800200008" ll="SDBH:צָבָא:000006">ejércitos</a>: <a name="03800800200010;03800800200026" ll="SDBH:קנא:000000">Celé</a> a <a name="03800800200014" ll="SDBH:צִיֹּון:000000">Sion</a> con <a name="03800800200018" ll="SDBH:גָּדֹול:000003">gran</a> <a name="03800800200016" ll="SDBH:קִנְאָה:000000">celo</a>, y con <a name="03800800200024" ll="SDBH:גָּדֹול:000003">gran</a> <a name="03800800200022" ll="SDBH:חֵמָה:001001">ira</a> la celé. 
\v 3 <a name="03800800300002">Así</a> <a name="03800800300004" ll="SDBH:אמר:000000">dice</a> <a name="03800800300006" ll="SDBH:יהוה:000000">Jehová</a>: Yo <a name="03800800300008" ll="SDBH:שׁוב:000023">he</a> <a name="03800800300008" ll="SDBH:שׁוב:000023">restaurado</a> <a name="03800800300010">a</a> <a name="03800800300012" ll="SDBH:צִיֹּון:000000">Sion</a>, y <a name="03800800300016" ll="SDBH:שׁכן:000000">moraré</a> en <a name="03800800300020" ll="SDBH:תָּוֶךְ:000000">medio</a> de <a name="03800800300022" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>; y <a name="03800800300028" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a> <a name="03800800300026" ll="SDBH:קרא:000007">se</a> <a name="03800800300026" ll="SDBH:קרא:000007">llamará</a> <a name="03800800300030" ll="SDBH:עִיר:000000">Ciudad</a> de la <a name="03800800300034" ll="SDBH:אֱמֶת:000000">Verdad</a>, y el <a name="03800800300038;03800800300044" ll="SDBH:הַר:000000">monte</a> de <a name="03800800300040" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800300042" ll="SDBH:צָבָא:000006">ejércitos</a>, Monte de <a name="03800800300048" ll="SDBH:קֹדֶשׁ:000001">Santidad</a>. 
\v 4 <a name="03800800400002">Así</a> <a name="03800800400004" ll="SDBH:אמר:000000">ha</a> <a name="03800800400004" ll="SDBH:אמר:000000">dicho</a> <a name="03800800400006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800400008" ll="SDBH:צָבָא:000006">ejércitos</a>: <a name="03800800400010">Aún</a> han de <a name="03800800400012" ll="SDBH:ישׁב:000000">morar</a> <a name="03800800400014" ll="SDBH:זָקֵן:000001">ancianos</a> y <a name="03800800400018" ll="SDBH:זָקֵן:000001">ancianas</a> en las <a name="03800800400022" ll="SDBH:רְחֹוב:000000">calles</a> de <a name="03800800400024" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>, <a name="03800800400028" ll="SDBH:אִישׁ:000002">cada</a> cual con <a name="03800800400030" ll="SDBH:מִשְׁעֶנֶת:000000">bordón</a> en <a name="03800800400034">su</a> <a name="03800800400034">mano</a> por la <a name="03800800400038" ll="SDBH:רֹב:000000">multitud</a> de los <a name="03800800400040" ll="SDBH:יֹום:000004">días</a>. 
\v 5 Y las <a name="03800800500004" ll="SDBH:רְחֹוב:000000">calles</a> de la <a name="03800800500008" ll="SDBH:עִיר:000000">ciudad</a> estarán <a name="03800800500010" ll="SDBH:מלא:000000">llenas</a> de <a name="03800800500012" ll="SDBH:יֶלֶד:000000">muchachos</a> y <a name="03800800500016" ll="SDBH:יַלְדָּה:000000">muchachas</a> que <a name="03800800500018" ll="SDBH:שׂחק:000001">jugarán</a> en ellas. 
\v 6 <a name="03800800600002">Así</a> <a name="03800800600004" ll="SDBH:אמר:000000">dice</a> <a name="03800800600006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800600008" ll="SDBH:צָבָא:000006">ejércitos</a>: Si esto parecerá <a name="03800800600012" ll="SDBH:פלא:000000">maravilloso</a> a los <a name="03800800600016" ll="SDBH:עַיִן:000000">ojos</a> del <a name="03800800600018" ll="SDBH:שְׁאֵרִית:000000">remanente</a> de este <a name="03800800600022" ll="SDBH:עַם:000000">pueblo</a> en aquellos <a name="03800800600032" ll="SDBH:יֹום:000006">días</a>, ¿<a name="03800800600038" ll="SDBH:גַּם:000000">también</a> será <a name="03800800600044" ll="SDBH:פלא:000000">maravilloso</a> delante de <a name="03800800600042" ll="SDBH:עַיִן:000000">mis</a> <a name="03800800600042" ll="SDBH:עַיִן:000000">ojos</a>? <a name="03800800600046" ll="SDBH:נְאֻם:000000">dice</a> <a name="03800800600048" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800600050" ll="SDBH:צָבָא:000006">ejércitos</a>. 
\v 7 <a name="03800800700002">Así</a> <a name="03800800700004" ll="SDBH:אמר:000000">ha</a> <a name="03800800700004" ll="SDBH:אמר:000000">dicho</a> <a name="03800800700006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800700008" ll="SDBH:צָבָא:000006">ejércitos</a>: He aquí, yo <a name="03800800700012" ll="SDBH:ישׁע:000001">salvo</a> a <a name="03800800700016" ll="SDBH:עַם:000000">mi</a> <a name="03800800700016" ll="SDBH:עַם:000000">pueblo</a> de la <a name="03800800700020" ll="SDBH:אֶרֶץ:000000">tierra</a> del <a name="03800800700022" ll="SDBH:מִזְרָח:000000">oriente</a>, y de la <a name="03800800700028" ll="SDBH:אֶרֶץ:000000">tierra</a> donde se <a name="03800800700030" ll="SDBH:מָבֹוא:000002">pone</a> el <a name="03800800700034" ll="SDBH:שֶׁמֶשׁ:000000">sol</a>; 
\v 8 y los <a name="03800800800004">traeré</a>, y <a name="03800800800010" ll="SDBH:שׁכן:000000">habitarán</a> en <a name="03800800800014" ll="SDBH:תָּוֶךְ:000000">medio</a> de <a name="03800800800016" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>; y me <a name="03800800800020">serán</a> por <a name="03800800800026" ll="SDBH:עַם:000000">pueblo</a>, y <a name="03800800800030">yo</a> <a name="03800800800032">seré</a> a ellos por <a name="03800800800038" ll="SDBH:אֱלֹהִים:000001">Dios</a> en verdad y en <a name="03800800800048" ll="SDBH:צְדָקָה:000001">justicia</a>.
    
\v 9 <a name="03800800900002">Así</a> <a name="03800800900004" ll="SDBH:אמר:000000">ha</a> <a name="03800800900004" ll="SDBH:אמר:000000">dicho</a> <a name="03800800900006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800900008" ll="SDBH:צָבָא:000006">ejércitos</a>: <a name="03800800900010" ll="SDBH:חזק:000006">Esfuércense</a> <a name="03800800900012">vuestras</a> <a name="03800800900012">manos</a>, los que <a name="03800800900016">oís</a> en estos <a name="03800800900022" ll="SDBH:יֹום:000006">días</a> estas <a name="03800800900032" ll="SDBH:דָּבָר:000000">palabras</a> de la <a name="03800800900040" ll="SDBH:פֶּה:000000">boca</a> de los <a name="03800800900044" ll="SDBH:נָבִיא:000000">profetas</a>, <a name="03800800900046">desde</a> el <a name="03800800900050" ll="SDBH:יֹום:000003" tl="HOTTP4455">día</a> que se echó el <a name="03800800900052" ll="SDBH:יסד:000000">cimiento</a> a la <a name="03800800900054" ll="SDBH:בַּיִת:000000">casa</a> de <a name="03800800900056" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800800900058" ll="SDBH:צָבָא:000006">ejércitos</a>, para <a name="03800800900066" ll="SDBH:בנה:000000">edificar</a> el <a name="03800800900062" ll="SDBH:הֵיכָל:000000">templo</a>. 
\v 10 Porque <a name="03800801000006">antes</a> de estos <a name="03800801000010" ll="SDBH:יֹום:000006">días</a> <a name="03800801000022" ll="SDBH:לֹא:000000">no</a> <a name="03800801000024">ha</a> <a name="03800801000024">habido</a> <a name="03800801000016" ll="SDBH:שָׂכָר:000000">paga</a> de <a name="03800801000020" ll="SDBH:אָדָם:000000">hombre</a> ni <a name="03800801000028" ll="SDBH:שָׂכָר:000000">paga</a> de <a name="03800801000032" ll="SDBH:בְּהֵמָה:000001">bestia</a>, ni <a name="03800801000034">hubo</a> <a name="03800801000054" ll="SDBH:שָׁלֹום:000000">paz</a> para el que <a name="03800801000042" ll="SDBH:יצא:000000">salía</a> ni para el que <a name="03800801000050">entraba</a>, a causa <a name="03800801000056">del</a> <a name="03800801000060" ll="SDBH:צַר:000003">enemigo</a>; y yo <a name="03800801000064" ll="SDBH:שׁלח:000002" tl="HOTTP4456">dejé</a> a <a name="03800801000068">todos</a> los <a name="03800801000072" ll="SDBH:אָדָם:000000">hombres</a> cada <a name="03800801000074" ll="SDBH:אִישׁ:000002">cual</a> contra su compañero. 
\v 11 Mas <a name="03800801100004">ahora</a> <a name="03800801100006" ll="SDBH:לֹא:000000">no</a> lo haré con el <a name="03800801100022" ll="SDBH:שְׁאֵרִית:000000">remanente</a> de este <a name="03800801100026" ll="SDBH:עַם:000000">pueblo</a> como en aquellos <a name="03800801100012" ll="SDBH:יֹום:000005">días</a> pasados, <a name="03800801100032" ll="SDBH:נְאֻם:000000">dice</a> <a name="03800801100034" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800801100036" ll="SDBH:צָבָא:000006">ejércitos</a>. 
\v 12 Porque habrá <a name="03800801200004" ll="SDBH:זֶרַע:000001">simiente</a> de <a name="03800801200008" ll="SDBH:שָׁלֹום:000000">paz</a>; la <a name="03800801200012" ll="SDBH:גֶּפֶן:000001">vid</a> <a name="03800801200014">dará</a> <a name="03800801200016" ll="SDBH:פְּרִי:000000">su</a> <a name="03800801200016" ll="SDBH:פְּרִי:000000">fruto</a>, y <a name="03800801200024">dará</a> <a name="03800801200028" ll="SDBH:יְבוּל:000000">su</a> <a name="03800801200028" ll="SDBH:יְבוּל:000000">producto</a> la <a name="03800801200022" ll="SDBH:אֶרֶץ:000000">tierra</a>, y los <a name="03800801200034" ll="SDBH:שָׁמַיִם:000000">cielos</a> <a name="03800801200036">darán</a> <a name="03800801200038" ll="SDBH:טַל:000000">su</a> <a name="03800801200038" ll="SDBH:טַל:000000">rocío</a>; y haré que el <a name="03800801200046" ll="SDBH:שְׁאֵרִית:000000">remanente</a> de este <a name="03800801200050" ll="SDBH:עַם:000000">pueblo</a> <a name="03800801200042" ll="SDBH:נחל:000002">posea</a> <a name="03800801200058">todo</a> <a name="03800801200060" ll="SDBH:אֵלֶּה:000000">esto</a>. 
\v 13 Y <a name="03800801300004">sucederá</a> que como <a name="03800801300010">fuisteis</a> <a name="03800801300012" ll="SDBH:קְלָלָה:000001">maldición</a> entre las <a name="03800801300018" ll="SDBH:גֹּוי:000000">naciones</a>, oh <a name="03800801300020" ll="SDBH:בַּיִת:000003">casa</a> de <a name="03800801300022" ll="SDBH:יְהוּדָה:000000">Judá</a> y <a name="03800801300026" ll="SDBH:בַּיִת:000003">casa</a> de <a name="03800801300028" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, <a name="03800801300030">así</a> os <a name="03800801300032" ll="SDBH:ישׁע:000001">salvaré</a> y <a name="03800801300038">seréis</a> <a name="03800801300040" ll="SDBH:בְּרָכָה:000000">bendición</a>. <a name="03800801300042" ll="SDBH:אַל:000000">No</a> <a name="03800801300044" ll="SDBH:ירא:000000">temáis</a>, mas <a name="03800801300046" ll="SDBH:חזק:000006">esfuércense</a> <a name="03800801300048">vuestras</a> <a name="03800801300048">manos</a>.
    
\v 14 Porque <a name="03800801400004">así</a> <a name="03800801400006" ll="SDBH:אמר:000000">ha</a> <a name="03800801400006" ll="SDBH:אמר:000000">dicho</a> <a name="03800801400008" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800801400010" ll="SDBH:צָבָא:000006">ejércitos</a>: Como <a name="03800801400016" ll="SDBH:זמם:000000">pensé</a> haceros mal cuando <a name="03800801400028" ll="SDBH:אָב:000001">vuestros</a> <a name="03800801400028" ll="SDBH:אָב:000001">padres</a> me provocaron a <a name="03800801400026" ll="SDBH:קצף:000002">ira</a>, <a name="03800801400032" ll="SDBH:אמר:000000">dice</a> <a name="03800801400034" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800801400036" ll="SDBH:צָבָא:000006">ejércitos</a>, y <a name="03800801400040" ll="SDBH:לֹא:000000">no</a> me <a name="03800801400042" ll="SDBH:נחם:001000">arrepentí</a>, 
\v 15 <a name="03800801500002">así</a> al <a name="03800801500004" ll="SDBH:שׁוב:000041">contrario</a> <a name="03800801500006" ll="SDBH:זמם:000000">he</a> <a name="03800801500006" ll="SDBH:זמם:000000">pensado</a> hacer <a name="03800801500020" ll="SDBH:יטב:000007">bien</a> a <a name="03800801500024" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a> y a la <a name="03800801500030" ll="SDBH:בַּיִת:000003">casa</a> de <a name="03800801500032" ll="SDBH:יְהוּדָה:000000">Judá</a> en estos <a name="03800801500012" ll="SDBH:יֹום:000006">días</a>; <a name="03800801500034" ll="SDBH:אַל:000000">no</a> <a name="03800801500036" ll="SDBH:ירא:000000">temáis</a>. 
\v 16 <a name="03800801600002" ll="SDBH:אֵלֶּה:000000">Éstas</a> son las <a name="03800801600006" ll="SDBH:דָּבָר:000002">cosas</a> <a name="03800801600008">que</a> habéis de <a name="03800801600010">hacer</a>: <a name="03800801600012" ll="SDBH:דבר:001000">Hablad</a> verdad cada <a name="03800801600016" ll="SDBH:אִישׁ:000002">cual</a> con su prójimo; <a name="03800801600030" ll="SDBH:שׁפט:000000">juzgad</a> según la verdad y lo <a name="03800801600026" ll="SDBH:מִשְׁפָּט:000001">conducente</a> a la <a name="03800801600028" ll="SDBH:שָׁלֹום:000000">paz</a> en <a name="03800801600034" ll="SDBH:שַׁעַר:000000">vuestras</a> <a name="03800801600034" ll="SDBH:שַׁעַר:000000">puertas</a>. 
\v 17 Y <a name="03800801700004;03800801700012" ll="SDBH:אִישׁ:000002;SDBH:אַל:000000">ninguno</a> de vosotros <a name="03800801700014" ll="SDBH:חשׁב:000001">piense</a> <a name="03800801700008" ll="SDBH:רָעָה:000000">mal</a> en <a name="03800801700018" ll="SDBH:לֵבָב:000001">su</a> <a name="03800801700018" ll="SDBH:לֵבָב:000001">corazón</a> contra <a name="03800801700010" ll="SDBH:רֵעַ:001000">su</a> <a name="03800801700010" ll="SDBH:רֵעַ:001000">prójimo</a>, <a name="03800801700026" ll="SDBH:אַל:000000">ni</a> <a name="03800801700028" ll="SDBH:אהב:000000">améis</a> el <a name="03800801700022" ll="SDBH:שְׁבוּעָה:000000">juramento</a> <a name="03800801700024" ll="SDBH:שֶׁקֶר:000000">falso</a>; <a name="03800801700030">porque</a> <a name="03800801700034">todas</a> <a name="03800801700036" ll="SDBH:אֵלֶּה:000000">estas</a> son cosas <a name="03800801700038">que</a> <a name="03800801700040" ll="SDBH:שׂנא:000000">aborrezco</a>, <a name="03800801700042" ll="SDBH:נְאֻם:000000">dice</a> <a name="03800801700044" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 18 <a name="03800801800004">Vino</a> <a name="03800801800012">a</a> mí <a name="03800801800006" ll="SDBH:דָּבָר:000000">palabra</a> de <a name="03800801800008" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800801800010" ll="SDBH:צָבָא:000006">ejércitos</a>, <a name="03800801800016" ll="SDBH:אמר:000000">diciendo</a>: 
\v 19 <a name="03800801900002">Así</a> <a name="03800801900004" ll="SDBH:אמר:000000">ha</a> <a name="03800801900004" ll="SDBH:אמר:000000">dicho</a> <a name="03800801900006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800801900008" ll="SDBH:צָבָא:000006">ejércitos</a>: El <a name="03800801900010" ll="SDBH:צֹום:000000">ayuno</a> del <a name="03800801900014" ll="SDBH:רְבִיעִי:000000">cuarto</a> mes, el <a name="03800801900018" ll="SDBH:צֹום:000000">ayuno</a> del <a name="03800801900022" ll="SDBH:חֲמִישִׁי:000000">quinto</a>, el <a name="03800801900026" ll="SDBH:צֹום:000000">ayuno</a> del <a name="03800801900030" ll="SDBH:שְׁבִיעִי:000000">séptimo</a>, y el <a name="03800801900034" ll="SDBH:צֹום:000000">ayuno</a> del <a name="03800801900038" ll="SDBH:עֲשִׂירִי:000000">décimo</a>, <a name="03800801900040">se</a> <a name="03800801900040">convertirán</a> para la <a name="03800801900044" ll="SDBH:בַּיִת:000003">casa</a> de <a name="03800801900046" ll="SDBH:יְהוּדָה:000000">Judá</a> en <a name="03800801900050" ll="SDBH:שָׂשֹׂון:000000">gozo</a> y <a name="03800801900056" ll="SDBH:שִׂמְחָה:000000">alegría</a>, y en <a name="03800801900064" ll="SDBH:טֹוב:000002">festivas</a> <a name="03800801900062" ll="SDBH:מֹועֵד:000002">solemnidades</a>. <a name="03800801900078" ll="SDBH:אהב:000000">Amad</a>, pues, la verdad y la <a name="03800801900076" ll="SDBH:שָׁלֹום:000000">paz</a>.
    
\v 20 <a name="03800802000002">Así</a> <a name="03800802000004" ll="SDBH:אמר:000000">ha</a> <a name="03800802000004" ll="SDBH:אמר:000000">dicho</a> <a name="03800802000006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800802000008" ll="SDBH:צָבָא:000006">ejércitos</a>: <a name="03800802000010">Aún</a> <a name="03800802000014">vendrán</a> <a name="03800802000016" ll="SDBH:עַם:000000">pueblos</a>, y <a name="03800802000020">habitantes</a> de <a name="03800802000024" ll="SDBH:רַב:000001">muchas</a> <a name="03800802000022" ll="SDBH:עִיר:000000">ciudades</a>; 
\v 21 y <a name="03800802100004" ll="SDBH:הלך:000001">vendrán</a> los <a name="03800802100006">habitantes</a> de <a name="03800802100008" ll="SDBH:אֶחָד:000000">una</a> ciudad <a name="03800802100010">a</a> <a name="03800802100012" ll="SDBH:אֶחָד:000000">otra</a>, y <a name="03800802100016" ll="SDBH:אמר:000000">dirán</a>: <a name="03800802100018;03800802100020" ll="SDBH:הלך:000001">Vamos</a> a <a name="03800802100024;03800802100028" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">implorar</a> <a name="03800802100024;03800802100028" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">el</a> <a name="03800802100024;03800802100028" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">favor</a> de <a name="03800802100030" ll="SDBH:יהוה:000000">Jehová</a>, y a <a name="03800802100036" ll="SDBH:בקשׁ:000003">buscar</a> a <a name="03800802100040" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800802100042" ll="SDBH:צָבָא:000006">ejércitos</a>. <a name="03800802100048">Yo</a> <a name="03800802100046" ll="SDBH:גַּם:000000">también</a> <a name="03800802100044" ll="SDBH:הלך:000001">iré</a>. 
\v 22 Y <a name="03800802200004">vendrán</a> <a name="03800802200008" ll="SDBH:רַב:000001">muchos</a> <a name="03800802200006" ll="SDBH:עַם:000000">pueblos</a> y <a name="03800802200014" ll="SDBH:עָצוּם:000000">fuertes</a> <a name="03800802200012" ll="SDBH:גֹּוי:000000">naciones</a> a <a name="03800802200018" ll="SDBH:בקשׁ:000003">buscar</a> a <a name="03800802200022" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800802200024" ll="SDBH:צָבָא:000006">ejércitos</a> en <a name="03800802200028" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>, y a <a name="03800802200034;03800802200038" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">implorar</a> <a name="03800802200034;03800802200038" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">el</a> <a name="03800802200034;03800802200038" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">favor</a> de <a name="03800802200040" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 23 <a name="03800802300002">Así</a> <a name="03800802300004" ll="SDBH:אמר:000000">ha</a> <a name="03800802300004" ll="SDBH:אמר:000000">dicho</a> <a name="03800802300006" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800802300008" ll="SDBH:צָבָא:000006">ejércitos</a>: En aquellos <a name="03800802300014" ll="SDBH:יֹום:000006">días</a> acontecerá <a name="03800802300020">que</a> <a name="03800802300024" ll="SDBH:עֶשֶׂר:000000">diez</a> hombres de las <a name="03800802300036" ll="SDBH:גֹּוי:000000">naciones</a> de <a name="03800802300030">toda</a> <a name="03800802300032" ll="SDBH:לָשֹׁון:000002">lengua</a> <a name="03800802300022" ll="SDBH:חזק:000017">tomarán</a> del <a name="03800802300044" ll="SDBH:כָּנָף:000001">manto</a> a un <a name="03800802300026;03800802300048" ll="SDBH:אִישׁ:000000;SDBH:יְהוּדִי:000001">judío</a>, <a name="03800802300052" ll="SDBH:אמר:000000">diciendo</a>: Iremos <a name="03800802300056">con</a> <a name="03800802300056">vosotros</a>, porque hemos <a name="03800802300060">oído</a> que <a name="03800802300062" ll="SDBH:אֱלֹהִים:000002">Dios</a> está <a name="03800802300064">con</a> <a name="03800802300064">vosotros</a>.
\c 9
\v 1 La profecía de la <a name="03800900100004" ll="SDBH:דָּבָר:000000">palabra</a> de <a name="03800900100006" ll="SDBH:יהוה:000000">Jehová</a> está contra la <a name="03800900100010" ll="SDBH:אֶרֶץ:000000" tl="HOTTP4457">tierra</a> de <a name="03800900100012" ll="SDBH:חַדְרָךְ:000000">Hadrac</a> y sobre <a name="03800900100016" ll="SDBH:דַּמֶּשֶׂק:000000">Damasco</a>; porque a <a name="03800900100024" ll="SDBH:יהוה:000000" tl="HOTTP4458">Jehová</a> deben mirar los <a name="03800900100026" ll="SDBH:עַיִן:000000" tl="HOTTP4458">ojos</a> de los <a name="03800900100028" ll="SDBH:אָדָם:000000" tl="HOTTP4458">hombres</a>, y de <a name="03800900100032">todas</a> las <a name="03800900100034" ll="SDBH:שֵׁבֶט:000006">tribus</a> de <a name="03800900100036" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 2 <a name="03800900200004" ll="SDBH:גַּם:000000">También</a> <a name="03800900200006" ll="SDBH:חֲמָת:000000">Hamat</a> <a name="03800900200008" ll="SDBH:גבל:000003">será</a> <a name="03800900200008" ll="SDBH:גבל:000003">comprendida</a> en el territorio de éste; <a name="03800900200012" ll="SDBH:צֹר:001000">Tiro</a> y <a name="03800900200016" ll="SDBH:צִידֹון:000001">Sidón</a>, aunque sean <a name="03800900200022" ll="SDBH:מְאֹד:000001">muy</a> <a name="03800900200020" ll="SDBH:חכם:000000">sabias</a>. 
\v 3 Bien que <a name="03800900300006" ll="SDBH:צֹר:001000">Tiro</a> <a name="03800900300004" ll="SDBH:בנה:000000">se</a> <a name="03800900300004" ll="SDBH:בנה:000000">edificó</a> <a name="03800900300008" ll="SDBH:מָצֹור:001002">fortaleza</a>, y <a name="03800900300014" ll="SDBH:צבר:000000">amontonó</a> <a name="03800900300016" ll="SDBH:כֶּסֶף:000000">plata</a> como <a name="03800900300022" ll="SDBH:עָפָר:000000">polvo</a>, y <a name="03800900300026" ll="SDBH:חָרוּץ:000000">oro</a> como <a name="03800900300030" ll="SDBH:טִיט:000000">lodo</a> de las <a name="03800900300032" ll="SDBH:חוּץ:000004">calles</a>, 
\v 4 he aquí, el <a name="03800900400004" ll="SDBH:אֲדֹנָי:000000">Señor</a> <a name="03800900400022">la</a> <a name="03800900400006" ll="SDBH:ירשׁ:000005">empobrecerá</a>, y <a name="03800900400010">herirá</a> en el <a name="03800900400016" ll="SDBH:יָם:000000">mar</a> su poderío, y ella será <a name="03800900400030" ll="SDBH:אכל:000002">consumida</a> de <a name="03800900400028" ll="SDBH:אֵשׁ:000000">fuego</a>.
    
\v 5 <a name="03800900500002" ll="SDBH:ראה:000000">Verá</a> <a name="03800900500004" ll="SDBH:אַשְׁקְלֹון:000000">Ascalón</a>, y <a name="03800900500008" ll="SDBH:ירא:000000">temerá</a>; <a name="03800900500012" ll="SDBH:עַזָּה:000000">Gaza</a> también, y <a name="03800900500016" ll="SDBH:חיל:000002">se</a> <a name="03800900500016" ll="SDBH:חיל:000002">dolerá</a> <a name="03800900500018" ll="SDBH:מְאֹד:000001">en</a> <a name="03800900500018" ll="SDBH:מְאֹד:000001">gran</a> <a name="03800900500018" ll="SDBH:מְאֹד:000001">manera</a>; asimismo <a name="03800900500022" ll="SDBH:עֶקְרֹון:000000">Ecrón</a>, <a name="03800900500024">porque</a> <a name="03800900500028" ll="SDBH:מַבָּט:000000">su</a> <a name="03800900500028" ll="SDBH:מַבָּט:000000">esperanza</a> <a name="03800900500026" ll="SDBH:בושׁ:000003">será</a> <a name="03800900500026" ll="SDBH:בושׁ:000003">confundida</a>; y <a name="03800900500032" ll="SDBH:אבד:000004">perecerá</a> el <a name="03800900500034" ll="SDBH:מֶלֶךְ:000000">rey</a> de <a name="03800900500038" ll="SDBH:עַזָּה:000000">Gaza</a>, y <a name="03800900500042" ll="SDBH:אַשְׁקְלֹון:000000">Ascalón</a> <a name="03800900500044" ll="SDBH:לֹא:000000">no</a> <a name="03800900500046">será</a> <a name="03800900500046">habitada</a>. 
\v 6 <a name="03800900600004">Habitará</a> en <a name="03800900600010" ll="SDBH:אַשְׁדֹּוד:000000">Asdod</a> un <a name="03800900600006" ll="SDBH:מַמְזֵר:000000">extranjero</a>, y pondré <a name="03800900600014" ll="SDBH:כרת:000006">fin</a> a la <a name="03800900600016" ll="SDBH:גָּאֹון:001000" tl="HOTTP4459">soberbia</a> de los <a name="03800900600018" ll="SDBH:פְּלִשְׁתִּי:000000" il="il_qasile_houses;il_qasile_temple_d01" tl="HOTTP4459">filisteos</a>. 
\v 7 <a name="03800900700004" ll="SDBH:סור:000006">Quitaré</a> la <a name="03800900700006" ll="SDBH:דָּם:000000">sangre</a> de <a name="03800900700028">su</a> <a name="03800900700024" ll="SDBH:שׁאר:000000">boca</a>, y <a name="03800900700014" ll="SDBH:שִׁקּוּץ:000000">sus</a> <a name="03800900700014" ll="SDBH:שִׁקּוּץ:000000">abominaciones</a> de <a name="03800900700018">entre</a> <a name="03800900700020" ll="SDBH:שֵׁן:000000">sus</a> <a name="03800900700020" ll="SDBH:שֵׁן:000000">dientes</a>, y <a name="03800900700024" ll="SDBH:שׁאר:000000">quedará</a> <a name="03800900700026" ll="SDBH:גַּם:000000">también</a> <a name="03800900700024" ll="SDBH:שׁאר:000000">un</a> <a name="03800900700024" ll="SDBH:שׁאר:000000">remanente</a> para <a name="03800900700032" ll="SDBH:אֱלֹהִים:000001">nuestro</a> <a name="03800900700032" ll="SDBH:אֱלֹהִים:000001">Dios</a>, y <a name="03800900700036">serán</a> como <a name="03800900700040" ll="SDBH:אַלּוּף:000000" tl="HOTTP4460">capitanes</a> en <a name="03800900700044" ll="SDBH:יְהוּדָה:000000">Judá</a>, y <a name="03800900700048" ll="SDBH:עֶקְרֹון:000000">Ecrón</a> será como el <a name="03800900700052" ll="SDBH:יְבוּסִי:000000">jebuseo</a>. 
\v 8 Entonces <a name="03800900800004" ll="SDBH:חנה:000001" tl="HOTTP4461">acamparé</a> alrededor de <a name="03800900800008" ll="SDBH:בַּיִת:000000">mi</a> <a name="03800900800008" ll="SDBH:בַּיִת:000000">casa</a> como un <a name="03800900800012" ll="SDBH:צָבָא:000003" tl="HOTTP4462">guarda</a>, para que ninguno <a name="03800900800016" ll="SDBH:עבר:000000">vaya</a> ni <a name="03800900800022" ll="SDBH:שׁוב:000023">venga</a>, y <a name="03800900800026" ll="SDBH:לֹא:000000">no</a> <a name="03800900800028" ll="SDBH:עבר:000000">pasará</a> <a name="03800900800032">más</a> <a name="03800900800030">sobre</a> <a name="03800900800030">ellos</a> el <a name="03800900800034" ll="SDBH:נגשׂ:000001">opresor</a>; <a name="03800900800036">porque</a> <a name="03800900800038">ahora</a> <a name="03800900800040" ll="SDBH:ראה:000000">miraré</a> con <a name="03800900800044" ll="SDBH:עַיִן:000000">mis</a> <a name="03800900800044" ll="SDBH:עַיִן:000000">ojos</a>.
    
\v 9 <a name="03800900900002" ll="SDBH:גיל:000000">Alégrate</a> <a name="03800900900004" ll="SDBH:מְאֹד:000001">mucho</a>, <a name="03800900900006" ll="SDBH:בַּת:000002">hija</a> de <a name="03800900900008" ll="SDBH:צִיֹּון:000000">Sion</a>; da voces de júbilo, <a name="03800900900012" ll="SDBH:בַּת:000002">hija</a> de <a name="03800900900014" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>; <a name="03800900900016">he</a> <a name="03800900900016">aquí</a> <a name="03800900900018" ll="SDBH:מֶלֶךְ:000000">tu</a> <a name="03800900900018" ll="SDBH:מֶלֶךְ:000000">rey</a> <a name="03800900900020">vendrá</a> <a name="03800900900022">a</a> <a name="03800900900022">ti</a>, <a name="03800900900024" ll="SDBH:צַדִּיק:000000">justo</a> y <a name="03800900900028" ll="SDBH:ישׁע:000001">salvador</a>, <a name="03800900900032" ll="SDBH:עָנִי:000001">humilde</a>, y <a name="03800900900036" ll="SDBH:רכב:000000">cabalgando</a> <a name="03800900900038">sobre</a> un <a name="03800900900040" ll="SDBH:חֲמֹור:000000">asno</a>, <a name="03800900900044">sobre</a> un <a name="03800900900046" ll="SDBH:עַיִר:000000">pollino</a> <a name="03800900900048" ll="SDBH:בֵּן:000001">hijo</a> de <a name="03800900900050" ll="SDBH:אָתֹון:000000">asna</a>. 
\v 10 Y de <a name="03800901000010" ll="SDBH:אֶפְרַיִם:000001">Efraín</a> <a name="03800901000004" ll="SDBH:כרת:000004" tl="HOTTP4463">destruiré</a> los <a name="03800901000006" ll="SDBH:רֶכֶב:001001">carros</a>, y los <a name="03800901000014" ll="SDBH:סוּס:000000">caballos</a> de <a name="03800901000018" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>, y los <a name="03800901000024" ll="SDBH:קֶשֶׁת:000000">arcos</a> de <a name="03800901000026" ll="SDBH:מִלְחָמָה:000000">guerra</a> <a name="03800901000022" ll="SDBH:כרת:000004">serán</a> <a name="03800901000022" ll="SDBH:כרת:000004">quebrados</a>; y <a name="03800901000030" ll="SDBH:דבר:001000">hablará</a> <a name="03800901000032" ll="SDBH:שָׁלֹום:000000">paz</a> a las <a name="03800901000038" ll="SDBH:גֹּוי:000000">naciones</a>, y <a name="03800901000042" ll="SDBH:מֹשֶׁל:001000">su</a> <a name="03800901000042" ll="SDBH:מֹשֶׁל:001000">señorío</a> será <a name="03800901000044">de</a> <a name="03800901000046" ll="SDBH:יָם:000000">mar</a> <a name="03800901000048" ll="SDBH:עַד:000000">a</a> <a name="03800901000050" ll="SDBH:יָם:000000">mar</a>, y <a name="03800901000054">desde</a> el <a name="03800901000056" ll="SDBH:נָהָר:000004">río</a> <a name="03800901000058" ll="SDBH:עַד:000000">hasta</a> <a name="03800901000060;03800901000062" ll="SDBH:אֶפֶס:000001;SDBH:אֶרֶץ:000000">los</a> <a name="03800901000060;03800901000062" ll="SDBH:אֶפֶס:000001;SDBH:אֶרֶץ:000000">fines</a> <a name="03800901000060;03800901000062" ll="SDBH:אֶפֶס:000001;SDBH:אֶרֶץ:000000">de</a> <a name="03800901000060;03800901000062" ll="SDBH:אֶפֶס:000001;SDBH:אֶרֶץ:000000">la</a> <a name="03800901000060;03800901000062" ll="SDBH:אֶפֶס:000001;SDBH:אֶרֶץ:000000">tierra</a>.
    
\v 11 Y <a name="03800901100004">tú</a> <a name="03800901100002" ll="SDBH:גַּם:000000">también</a> por la <a name="03800901100008" ll="SDBH:דָּם:000000">sangre</a> de <a name="03800901100010" ll="SDBH:בְּרִית:000000">tu</a> <a name="03800901100010" ll="SDBH:בְּרִית:000000">pacto</a> serás salva; <a name="03800901100012" ll="SDBH:שׁלח:000009">yo</a> <a name="03800901100012" ll="SDBH:שׁלח:000009">he</a> <a name="03800901100012" ll="SDBH:שׁלח:000009">sacado</a> <a name="03800901100014" ll="SDBH:אָסִיר:000000">tus</a> <a name="03800901100014" ll="SDBH:אָסִיר:000000">presos</a> de la <a name="03800901100018" ll="SDBH:בֹּור:000000">cisterna</a> <a name="03800901100024">en</a> <a name="03800901100024">que</a> <a name="03800901100020">no</a> <a name="03800901100020">hay</a> <a name="03800901100022" ll="SDBH:מַיִם:000000">agua</a>. 
\v 12 <a name="03800901200002" ll="SDBH:שׁוב:000023">Volveos</a> a la <a name="03800901200006" ll="SDBH:בִּצָּרֹון:000000">fortaleza</a>, oh <a name="03800901200008" ll="SDBH:אָסִיר:000000">prisioneros</a> de <a name="03800901200012" ll="SDBH:תִּקְוָה:001000">esperanza</a>; <a name="03800901200018" ll="SDBH:יֹום:000005">hoy</a> <a name="03800901200014" ll="SDBH:גַּם:000000">también</a> os <a name="03800901200020" ll="SDBH:נגד:000000" tl="HOTTP4464">anuncio</a> que os restauraré el <a name="03800901200022" ll="SDBH:מִשְׁנֶה:000000">doble</a>. 
\v 13 <a name="03800901300002">Porque</a> <a name="03800901300004" ll="SDBH:דרך:001000">he</a> <a name="03800901300004" ll="SDBH:דרך:001000">entesado</a> <a name="03800901300006">para</a> <a name="03800901300006">mí</a> a <a name="03800901300008" ll="SDBH:יְהוּדָה:000000">Judá</a> como <a name="03800901300010" ll="SDBH:קֶשֶׁת:000000">arco</a>, e <a name="03800901300012" ll="SDBH:מלא:000021">hice</a> a <a name="03800901300014" ll="SDBH:אֶפְרַיִם:000001">Efraín</a> <a name="03800901300012" ll="SDBH:מלא:000021">su</a> <a name="03800901300012" ll="SDBH:מלא:000021">flecha</a>, y <a name="03800901300018" ll="SDBH:עור:000003">despertaré</a> a <a name="03800901300020" ll="SDBH:בֵּן:000000">tus</a> <a name="03800901300020" ll="SDBH:בֵּן:000000">hijos</a>, oh <a name="03800901300022" ll="SDBH:צִיֹּון:000000">Sion</a>, <a name="03800901300024">contra</a> <a name="03800901300026" ll="SDBH:בֵּן:000000" tl="HOTTP4465">tus</a> <a name="03800901300026" ll="SDBH:בֵּן:000000" tl="HOTTP4465">hijos</a>, oh <a name="03800901300028" ll="SDBH:יָוָן:000001" il="tb010717246" tl="HOTTP4465">Grecia</a>, y te <a name="03800901300032" ll="SDBH:שׂים:000005">pondré</a> como <a name="03800901300036" ll="SDBH:חֶרֶב:000001">espada</a> de <a name="03800901300038" ll="SDBH:גִּבֹּור:000000">valiente</a>.
    
\v 14 Y <a name="03800901400004" ll="SDBH:יהוה:000000">Jehová</a> será <a name="03800901400008" ll="SDBH:ראה:000001">visto</a> <a name="03800901400006">sobre</a> <a name="03800901400006">ellos</a>, y <a name="03800901400020" ll="SDBH:חֵץ:000001">su</a> <a name="03800901400020" ll="SDBH:חֵץ:000001">dardo</a> <a name="03800901400012" ll="SDBH:יצא:000000">saldrá</a> como <a name="03800901400018" ll="SDBH:בָּרָק:000000">relámpago</a>; y <a name="03800901400026" ll="SDBH:יהוה:000000">Jehová</a> el <a name="03800901400024" ll="SDBH:אֲדֹנָי:000000">Señor</a> <a name="03800901400034" ll="SDBH:תקע:000006">tocará</a> <a name="03800901400032" ll="SDBH:שֹׁופָר:000000">trompeta</a>, e <a name="03800901400038" ll="SDBH:הלך:000001">irá</a> entre <a name="03800901400042" ll="SDBH:סְעָרָה:000000">torbellinos</a> del <a name="03800901400044" ll="SDBH:תֵּימָן:000000">austro</a>. 
\v 15 <a name="03800901500002" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03800901500004" ll="SDBH:צָבָא:000006">ejércitos</a> los <a name="03800901500006" ll="SDBH:גנן:000000">amparará</a>, y ellos <a name="03800901500012" ll="SDBH:אכל:000002">devorarán</a>, y <a name="03800901500016" ll="SDBH:כבשׁ:000000">hollarán</a> las <a name="03800901500018" ll="SDBH:אֶבֶן:000001">piedras</a> de la <a name="03800901500020" ll="SDBH:קֶלַע:000000">honda</a>, y <a name="03800901500024" ll="SDBH:שׁתה:000000" tl="HOTTP4466">beberán</a>, y harán <a name="03800901500026" ll="SDBH:המה:000001" tl="HOTTP4466">estrépito</a> como tomados de <a name="03800901500030" ll="SDBH:יַיִן:000000">vino</a>; y <a name="03800901500034" ll="SDBH:מלא:000000">se</a> <a name="03800901500034" ll="SDBH:מלא:000000">llenarán</a> como <a name="03800901500040" ll="SDBH:מִזְרָק:000000">tazón</a>, o como cuernos del <a name="03800901500046" ll="SDBH:מִזְבֵּחַ:000000">altar</a>. 
\v 16 Y los <a name="03800901600004" ll="SDBH:ישׁע:000001">salvará</a> en aquel <a name="03800901600014" ll="SDBH:יֹום:000006">día</a> <a name="03800901600006" ll="SDBH:יהוה:000000">Jehová</a> <a name="03800901600008" ll="SDBH:אֱלֹהִים:000001">su</a> <a name="03800901600008" ll="SDBH:אֱלֹהִים:000001">Dios</a> como <a name="03800901600022" ll="SDBH:צֹאן:000000">rebaño</a> de <a name="03800901600018">su</a> <a name="03800901600024" ll="SDBH:עַם:000000">pueblo</a>; porque como <a name="03800901600028" ll="SDBH:אֶבֶן:000008">piedras</a> de <a name="03800901600030" ll="SDBH:נֵזֶר:000003">diadema</a> serán enaltecidos <a name="03800901600034">en</a> <a name="03800901600036" ll="SDBH:אֲדָמָה:000000">su</a> <a name="03800901600036" ll="SDBH:אֲדָמָה:000000">tierra</a>. 
\v 17 Porque ¡<a name="03800901700004" ll="SDBH:מָה:000000">cuánta</a> es <a name="03800901700006" ll="SDBH:טוּב:000000">su</a> <a name="03800901700006" ll="SDBH:טוּב:000000">bondad</a>, y <a name="03800901700010" ll="SDBH:מָה:000000">cuánta</a> <a name="03800901700012" ll="SDBH:יֳפִי:000000">su</a> <a name="03800901700012" ll="SDBH:יֳפִי:000000">hermosura</a>! El <a name="03800901700014" ll="SDBH:דָּגָן:000000">trigo</a> alegrará a los <a name="03800901700016" ll="SDBH:בָּחוּר:000000">jóvenes</a>, y el <a name="03800901700020" ll="SDBH:תִּירֹושׁ:000000">vino</a> a las <a name="03800901700024" ll="SDBH:בְּתוּלָה:000000">doncellas</a>.
\c 10
\v 1 <a name="03801000100002" ll="SDBH:שׁאל:000000">Pedid</a> a <a name="03801000100006" ll="SDBH:יהוה:000000">Jehová</a> <a name="03801000100008" ll="SDBH:מָטָר:000000">lluvia</a> en la <a name="03801000100012" ll="SDBH:עֵת:000000" tl="HOTTP4467">estación</a> <a name="03801000100014" ll="SDBH:מַלְקֹושׁ:000000" il="Rain;Rain2" tl="HOTTP4467">tardía</a>. <a name="03801000100016" ll="SDBH:יהוה:000000">Jehová</a> <a name="03801000100018">hará</a> <a name="03801000100020" ll="SDBH:חֲזִיז:000000">relámpagos</a>, y os <a name="03801000100028">dará</a> <a name="03801000100026" ll="SDBH:גֶּשֶׁם:000000">lluvia</a> <a name="03801000100024" ll="SDBH:מָטָר:000000">abundante</a>, y <a name="03801000100036" ll="SDBH:עֵשֶׂב:000000">hierba</a> verde en el <a name="03801000100042" ll="SDBH:שָׂדֶה:000000">campo</a> a cada <a name="03801000100034" ll="SDBH:אִישׁ:000002">uno</a>. 
\v 2 Porque los <a name="03801000200006" ll="SDBH:תְּרָפִים:000000">terafines</a> han <a name="03801000200008" ll="SDBH:דבר:001000">dado</a> <a name="03801000200010" ll="SDBH:אָוֶן:000002">vanos</a> oráculos, y los <a name="03801000200016" ll="SDBH:קסם:000000">adivinos</a> han <a name="03801000200018" ll="SDBH:חזה:000004">visto</a> <a name="03801000200020" ll="SDBH:שֶׁקֶר:000000">mentira</a>, han <a name="03801000200030" ll="SDBH:דבר:001000">hablado</a> <a name="03801000200024" ll="SDBH:חֲלֹום:000000">sueños</a> <a name="03801000200028" ll="SDBH:שָׁוְא:000000">vanos</a>, y <a name="03801000200032" ll="SDBH:הֶבֶל:000002">vano</a> es su <a name="03801000200034" ll="SDBH:נחם:000000">consuelo</a>; <a name="03801000200036">por</a> lo <a name="03801000200038">cual</a> el pueblo <a name="03801000200040" ll="SDBH:נסע:000003">vaga</a> como <a name="03801000200044" ll="SDBH:צֹאן:000000">ovejas</a>, y sufre porque <a name="03801000200050">no</a> tiene <a name="03801000200052" ll="SDBH:רעה:000003">pastor</a>.
    
\v 3 <a name="03801000300002">Contra</a> los <a name="03801000300006" ll="SDBH:רעה:000003">pastores</a> <a name="03801000300008" ll="SDBH:חרה:000001">se</a> <a name="03801000300008" ll="SDBH:חרה:000001">ha</a> <a name="03801000300008" ll="SDBH:חרה:000001">encendido</a> mi enojo, y <a name="03801000300020" ll="SDBH:פקד:000006">castigaré</a> <a name="03801000300014">a</a> los <a name="03801000300018" ll="SDBH:עַתּוּד:000001">jefes</a>; pero <a name="03801000300026" ll="SDBH:יהוה:000000">Jehová</a> de los <a name="03801000300028" ll="SDBH:צָבָא:000006">ejércitos</a> <a name="03801000300024" ll="SDBH:פקד:000005">visitará</a> su rebaño, la <a name="03801000300036" ll="SDBH:בַּיִת:000003">casa</a> de <a name="03801000300038" ll="SDBH:יְהוּדָה:000000">Judá</a>, y los <a name="03801000300042" ll="SDBH:שׂים:000005">pondrá</a> como su <a name="03801000300048" ll="SDBH:סוּס:000000">caballo</a> de <a name="03801000300050" ll="SDBH:הֹוד:000000">honor</a> en la <a name="03801000300056" ll="SDBH:מִלְחָמָה:000000">guerra</a>. 
\v 4 <a name="03801000400002">De</a> <a name="03801000400002">él</a> <a name="03801000400018" ll="SDBH:יצא:000005">saldrá</a> la <a name="03801000400004" ll="SDBH:פִּנָּה:000007">piedra</a> <a name="03801000400004" ll="SDBH:פִּנָּה:000007">angular</a>, <a name="03801000400006">de</a> <a name="03801000400006">él</a> la <a name="03801000400008" ll="SDBH:יָתֵד:000000">clavija</a>, <a name="03801000400010">de</a> <a name="03801000400010">él</a> el <a name="03801000400012" ll="SDBH:קֶשֶׁת:000000">arco</a> de <a name="03801000400014" ll="SDBH:מִלְחָמָה:000000">guerra</a>, <a name="03801000400016">de</a> <a name="03801000400016">él</a> también <a name="03801000400020">todo</a> <a name="03801000400022" ll="SDBH:נגשׂ:000002">apremiador</a>. 
\v 5 Y <a name="03801000500004">serán</a> como <a name="03801000500008" ll="SDBH:גִּבֹּור:000000">valientes</a> que en la <a name="03801000500022" ll="SDBH:מִלְחָמָה:000000">batalla</a> huellan al enemigo en el <a name="03801000500014" ll="SDBH:טִיט:000000">lodo</a> de las <a name="03801000500016" ll="SDBH:חוּץ:000004">calles</a>; y <a name="03801000500026" ll="SDBH:לחם:001001">pelearán</a>, porque <a name="03801000500030" ll="SDBH:יהוה:000000">Jehová</a> estará con <a name="03801000500032">ellos</a>; y los que <a name="03801000500038" ll="SDBH:רכב:000000">cabalgan</a> en <a name="03801000500040" ll="SDBH:סוּס:000000">caballos</a> serán avergonzados.
    
\v 6 Porque yo <a name="03801000600004" ll="SDBH:גבר:000002">fortaleceré</a> la <a name="03801000600008" ll="SDBH:בַּיִת:000003">casa</a> de <a name="03801000600010" ll="SDBH:יְהוּדָה:000000">Judá</a>, y <a name="03801000600020" ll="SDBH:ישׁע:000001">guardaré</a> la <a name="03801000600016" ll="SDBH:בַּיִת:000003">casa</a> de <a name="03801000600018" ll="SDBH:יֹוסֵף:000000">José</a>, y los <a name="03801000600024">haré</a> <a name="03801000600024">volver</a>; <a name="03801000600026">porque</a> <a name="03801000600028" ll="SDBH:רחם:000001">de</a> <a name="03801000600028" ll="SDBH:רחם:000001">ellos</a> <a name="03801000600028" ll="SDBH:רחם:000001">tendré</a> <a name="03801000600028" ll="SDBH:רחם:000001">piedad</a>, y <a name="03801000600032">serán</a> <a name="03801000600034;03801000600036">como</a> si <a name="03801000600038" ll="SDBH:לֹא:000000">no</a> <a name="03801000600040" ll="SDBH:זנח:000001">los</a> <a name="03801000600040" ll="SDBH:זנח:000001">hubiera</a> <a name="03801000600040" ll="SDBH:זנח:000001">desechado</a>; <a name="03801000600042">porque</a> <a name="03801000600044">yo</a> soy <a name="03801000600046" ll="SDBH:יהוה:000000">Jehová</a> <a name="03801000600048" ll="SDBH:אֱלֹהִים:000001">su</a> <a name="03801000600048" ll="SDBH:אֱלֹהִים:000001">Dios</a>, y <a name="03801000600052" ll="SDBH:ענה:000000">los</a> <a name="03801000600052" ll="SDBH:ענה:000000">oiré</a>. 
\v 7 Y <a name="03801000700004">será</a> <a name="03801000700010" ll="SDBH:אֶפְרַיִם:000001">Efraín</a> como <a name="03801000700008" ll="SDBH:גִּבֹּור:000000">valiente</a>, y <a name="03801000700014" ll="SDBH:שׂמח:000000">se</a> <a name="03801000700014" ll="SDBH:שׂמח:000000">alegrará</a> <a name="03801000700016" ll="SDBH:לֵב:000001">su</a> <a name="03801000700016" ll="SDBH:לֵב:000001">corazón</a> como a causa del <a name="03801000700020" ll="SDBH:יַיִן:000000">vino</a>; <a name="03801000700024" ll="SDBH:בֵּן:000000">sus</a> <a name="03801000700024" ll="SDBH:בֵּן:000000">hijos</a> también <a name="03801000700026" ll="SDBH:ראה:000000">verán</a>, y <a name="03801000700030" ll="SDBH:שׂמח:000000">se</a> <a name="03801000700030" ll="SDBH:שׂמח:000000">alegrarán</a>; <a name="03801000700034" ll="SDBH:לֵב:000001">su</a> <a name="03801000700034" ll="SDBH:לֵב:000001">corazón</a> <a name="03801000700032" ll="SDBH:גיל:000000">se</a> <a name="03801000700032" ll="SDBH:גיל:000000">gozará</a> en <a name="03801000700038" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 8 Yo los llamaré con un <a name="03801000800002" ll="SDBH:שׁרק:000000">silbido</a>, y los <a name="03801000800008" ll="SDBH:קבץ:000001">reuniré</a>, porque los <a name="03801000800012" ll="SDBH:פדה:000002">he</a> <a name="03801000800012" ll="SDBH:פדה:000002">redimido</a>; y serán <a name="03801000800016">multiplicados</a> tanto como fueron <a name="03801000800020">antes</a>. 
\v 9 Bien que <a name="03801000900004" ll="SDBH:זרע:000000">los</a> <a name="03801000900004" ll="SDBH:זרע:000000">esparciré</a> entre los <a name="03801000900010" ll="SDBH:עַם:000000">pueblos</a>, aun en <a name="03801000900018" ll="SDBH:מֶרְחָק:000000">lejanos</a> <a name="03801000900018" ll="SDBH:מֶרְחָק:000000">países</a> <a name="03801000900020" ll="SDBH:זכר:000000">se</a> <a name="03801000900020" ll="SDBH:זכר:000000">acordarán</a> <a name="03801000900020" ll="SDBH:זכר:000000">de</a> <a name="03801000900020" ll="SDBH:זכר:000000">mí</a>; y <a name="03801000900024" ll="SDBH:חיה:000000" tl="HOTTP4469">vivirán</a> con <a name="03801000900028" ll="SDBH:בֵּן:000000">sus</a> <a name="03801000900028" ll="SDBH:בֵּן:000000">hijos</a>, y <a name="03801000900032" ll="SDBH:שׁוב:000023">volverán</a>. 
\v 10 Porque yo los <a name="03801001000004" ll="SDBH:שׁוב:000051">traeré</a> de la <a name="03801001000008" ll="SDBH:אֶרֶץ:000000">tierra</a> de <a name="03801001000010" ll="SDBH:מִצְרַיִם:000001">Egipto</a>, y los <a name="03801001000018" ll="SDBH:קבץ:000001">recogeré</a> de <a name="03801001000016" ll="SDBH:אַשּׁוּר:000001">Asiria</a>; y los <a name="03801001000032">traeré</a> <a name="03801001000022">a</a> la <a name="03801001000024" ll="SDBH:אֶרֶץ:000000">tierra</a> de <a name="03801001000026" ll="SDBH:גִּלְעָד:000001">Galaad</a> y del <a name="03801001000030" ll="SDBH:לְבָנֹון:000000">Líbano</a>, y <a name="03801001000036" ll="SDBH:לֹא:000000">no</a> les <a name="03801001000038" ll="SDBH:מצא:000017">bastará</a>. 
\v 11 Y la <a name="03801001100012" ll="SDBH:צָרָה:000000" tl="HOTTP4471">tribulación</a> <a name="03801001100004" ll="SDBH:עבר:000000" tl="HOTTP4470">pasará</a> por el <a name="03801001100010" ll="SDBH:יָם:000000" il="Rough_seas" tl="HOTTP4471">mar</a>, y <a name="03801001100016">herirá</a> en el <a name="03801001100022" ll="SDBH:יָם:000000">mar</a> las <a name="03801001100024" ll="SDBH:גַּל:000000">ondas</a>, y se secarán <a name="03801001100030">todas</a> las <a name="03801001100032" ll="SDBH:מְצֹולָה:000000">profundidades</a> del <a name="03801001100034" ll="SDBH:יְאֹר:000000">río</a>; y la <a name="03801001100040" ll="SDBH:גָּאֹון:001000">soberbia</a> de <a name="03801001100042" ll="SDBH:אַשּׁוּר:000001">Asiria</a> será <a name="03801001100038" ll="SDBH:ירד:000015">derribada</a>, y <a name="03801001100050" ll="SDBH:סור:000007">se</a> <a name="03801001100050" ll="SDBH:סור:000007">perderá</a> el <a name="03801001100046" ll="SDBH:שֵׁבֶט:000004">cetro</a> de <a name="03801001100048" ll="SDBH:מִצְרַיִם:000001">Egipto</a>. 
\v 12 Y yo los <a name="03801001200004" ll="SDBH:גבר:000002" tl="HOTTP4472">fortaleceré</a> en <a name="03801001200008" ll="SDBH:יהוה:000000">Jehová</a>, y <a name="03801001200016" ll="SDBH:הלך:000004" tl="HOTTP4473">caminarán</a> en <a name="03801001200014" ll="SDBH:שֵׁם:000002">su</a> <a name="03801001200014" ll="SDBH:שֵׁם:000002">nombre</a>, <a name="03801001200018" ll="SDBH:נְאֻם:000000">dice</a> <a name="03801001200020" ll="SDBH:יהוה:000000">Jehová</a>.
\c 11
\v 1 Oh <a name="03801100100004" ll="SDBH:לְבָנֹון:000000">Líbano</a>, <a name="03801100100002" ll="SDBH:פתח:000001">abre</a> <a name="03801100100006" ll="SDBH:דֶּלֶת:000003">tus</a> <a name="03801100100006" ll="SDBH:דֶּלֶת:000003">puertas</a>, y <a name="03801100100010" ll="SDBH:אכל:000002">consuma</a> el <a name="03801100100012" ll="SDBH:אֵשׁ:000000">fuego</a> <a name="03801100100016" ll="SDBH:אֶרֶז:000000">tus</a> <a name="03801100100016" ll="SDBH:אֶרֶז:000000">cedros</a>. 
\v 2 <a name="03801100200002" ll="SDBH:ילל:000000">Aúlla</a>, oh <a name="03801100200004" ll="SDBH:בְּרֹושׁ:000000">ciprés</a>, porque el <a name="03801100200010" ll="SDBH:אֶרֶז:000000">cedro</a> <a name="03801100200008" ll="SDBH:נפל:000000">cayó</a>, <a name="03801100200012">porque</a> los árboles <a name="03801100200014" ll="SDBH:אַדִּיר:000000">magníficos</a> son <a name="03801100200016" ll="SDBH:שׁדד:000000">derribados</a>. <a name="03801100200018" ll="SDBH:ילל:000000">Aullad</a>, <a name="03801100200020" ll="SDBH:אַלֹּון:000000">encinas</a> de <a name="03801100200022" ll="SDBH:בָּשָׁן:000000">Basán</a>, porque el <a name="03801100200028" ll="SDBH:יַעַר:000001">bosque</a> espeso es <a name="03801100200026" ll="SDBH:ירד:000000">derribado</a>. 
\v 3 <a name="03801100300002">Voz</a> de aullido de <a name="03801100300008" ll="SDBH:רעה:000003">pastores</a>, porque <a name="03801100300014" ll="SDBH:אַדֶּרֶת:000000">su</a> <a name="03801100300014" ll="SDBH:אַדֶּרֶת:000000">magnificencia</a> es <a name="03801100300012" ll="SDBH:שׁדד:000000">asolada</a>; <a name="03801100300016">estruendo</a> de <a name="03801100300018" ll="SDBH:שְׁאָגָה:000000">rugidos</a> de <a name="03801100300020" ll="SDBH:כְּפִיר:000000">cachorros</a> de leones, porque la <a name="03801100300026" ll="SDBH:גָּאֹון:000000">gloria</a> del <a name="03801100300030" ll="SDBH:יַרְדֵּן:000000">Jordán</a> es <a name="03801100300024" ll="SDBH:שׁדד:000000">destruida</a>.
    
\v 4 <a name="03801100400002">Así</a> <a name="03801100400004" ll="SDBH:אמר:000000">ha</a> <a name="03801100400004" ll="SDBH:אמר:000000">dicho</a> <a name="03801100400006" ll="SDBH:יהוה:000000">Jehová</a> <a name="03801100400008" ll="SDBH:אֱלֹהִים:000001">mi</a> <a name="03801100400008" ll="SDBH:אֱלֹהִים:000001">Dios</a>: <a name="03801100400010" ll="SDBH:רעה:000001">Apacienta</a> las <a name="03801100400014" ll="SDBH:צֹאן:000000">ovejas</a> de la <a name="03801100400018" ll="SDBH:הֲרֵגָה:000000">matanza</a>, 
\v 5 a las <a name="03801100500002">cuales</a> <a name="03801100500006" ll="SDBH:הרג:000000">matan</a> sus <a name="03801100500004" ll="SDBH:קנה:000000">compradores</a>, y <a name="03801100500010" ll="SDBH:לֹא:000000">no</a> se tienen por <a name="03801100500012" ll="SDBH:אשׁם:000002">culpables</a>; y el que las <a name="03801100500016" ll="SDBH:מכר:000000">vende</a>, <a name="03801100500018" ll="SDBH:אמר:000000">dice</a>: <a name="03801100500020" ll="SDBH:ברך:001001">Bendito</a> sea <a name="03801100500022" ll="SDBH:יהוה:000000">Jehová</a>, porque <a name="03801100500026" ll="SDBH:עשׁר:000000">he</a> <a name="03801100500026" ll="SDBH:עשׁר:000000">enriquecido</a>; <a name="03801100500032" ll="SDBH:לֹא:000000">ni</a> sus <a name="03801100500030" ll="SDBH:רעה:000003">pastores</a> tienen <a name="03801100500034" ll="SDBH:חמל:000000">piedad</a> de ellas. 
\v 6 Por tanto, <a name="03801100600004" ll="SDBH:לֹא:000000">no</a> tendré ya <a name="03801100600008">más</a> <a name="03801100600006" ll="SDBH:חמל:000000">piedad</a> <a name="03801100600010">de</a> los <a name="03801100600012">moradores</a> de la <a name="03801100600016" ll="SDBH:אֶרֶץ:000000">tierra</a>, <a name="03801100600018" ll="SDBH:נְאֻם:000000">dice</a> <a name="03801100600020" ll="SDBH:יהוה:000000">Jehová</a>; porque he aquí, <a name="03801100600026">yo</a> <a name="03801100600028" ll="SDBH:מצא:000015">entregaré</a> los <a name="03801100600034" ll="SDBH:אָדָם:000000">hombres</a> cada <a name="03801100600036" ll="SDBH:אִישׁ:000002">cual</a> en <a name="03801100600040">mano</a> de su compañero y en <a name="03801100600048">mano</a> de <a name="03801100600050" ll="SDBH:מֶלֶךְ:000000">su</a> <a name="03801100600050" ll="SDBH:מֶלֶךְ:000000">rey</a>; y <a name="03801100600054" ll="SDBH:כתת:000001">asolarán</a> la <a name="03801100600060" ll="SDBH:אֶרֶץ:000000">tierra</a>, y yo <a name="03801100600064" ll="SDBH:לֹא:000000">no</a> los <a name="03801100600066" ll="SDBH:נצל:000005">libraré</a> de <a name="03801100600070">sus</a> <a name="03801100600070">manos</a>.
    
\v 7 <a name="03801100700004;03801100700052" ll="SDBH:רעה:000001">Apacenté</a>, pues, las <a name="03801100700008" ll="SDBH:צֹאן:000000">ovejas</a> de la <a name="03801100700012" ll="SDBH:הֲרֵגָה:000000">matanza</a>, esto es, a los <a name="03801100700016" ll="SDBH:כְּנַעֲנִי:000000" tl="HOTTP4474">pobres</a> del <a name="03801100700020">rebaño</a>. Y <a name="03801100700024">tomé</a> para mí <a name="03801100700028" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="03801100700030" ll="SDBH:מַקֵּל:000001">cayados</a>: al <a name="03801100700034" ll="SDBH:אֶחָד:000003">uno</a> <a name="03801100700036" ll="SDBH:קרא:000007">puse</a> por nombre <a name="03801100700038" ll="SDBH:נֹעַם:000001">Gracia</a>, y al <a name="03801100700044" ll="SDBH:אֶחָד:000003">otro</a> Ataduras; y apacenté las <a name="03801100700058">ovejas</a>. 
\v 8 Y <a name="03801100800004" ll="SDBH:כחד:000002">destruí</a> a <a name="03801100800008" ll="SDBH:שָׁלֹשׁ:000000">tres</a> <a name="03801100800012" ll="SDBH:רעה:000003">pastores</a> en <a name="03801100800018" ll="SDBH:אֶחָד:000000">un</a> <a name="03801100800016" ll="SDBH:יֶרַח:000000">mes</a>; pues <a name="03801100800024" ll="SDBH:נֶפֶשׁ:000001">mi</a> <a name="03801100800024" ll="SDBH:נֶפֶשׁ:000001">alma</a> <a name="03801100800022" ll="SDBH:קצר:000004">se</a> <a name="03801100800022" ll="SDBH:קצר:000004">impacientó</a> contra ellos, y <a name="03801100800030" ll="SDBH:גַּם:000000">también</a> el <a name="03801100800032" ll="SDBH:נֶפֶשׁ:000001">alma</a> de ellos me <a name="03801100800034" ll="SDBH:בחל:000000">aborreció</a> a mí. 
\v 9 Y <a name="03801100900004" ll="SDBH:אמר:000000">dije</a>: <a name="03801100900006" ll="SDBH:לֹא:000000">No</a> os <a name="03801100900008" ll="SDBH:רעה:000001">apacentaré</a>; la que <a name="03801100900014" ll="SDBH:מות:000000">muriere</a>, que <a name="03801100900016" ll="SDBH:מות:000000">muera</a>; y la que <a name="03801100900022" ll="SDBH:כחד:000002">se</a> <a name="03801100900022" ll="SDBH:כחד:000002">perdiere</a>, que <a name="03801100900024" ll="SDBH:כחד:000002">se</a> <a name="03801100900024" ll="SDBH:כחד:000002">pierda</a>; y las que <a name="03801100900030" ll="SDBH:שׁאר:000000">quedaren</a>, que cada <a name="03801100900034" ll="SDBH:אִשָּׁה:000003">una</a> <a name="03801100900032" ll="SDBH:אכל:000000">coma</a> la <a name="03801100900038" ll="SDBH:בָּשָׂר:000000">carne</a> de <a name="03801100900040" ll="SDBH:רְעוּת:000000">su</a> <a name="03801100900040" ll="SDBH:רְעוּת:000000">compañera</a>. 
\v 10 <a name="03801101000004" ll="SDBH:לקח:000000">Tomé</a> luego <a name="03801101000008" ll="SDBH:מַקֵּל:000001">mi</a> <a name="03801101000008" ll="SDBH:מַקֵּל:000001">cayado</a> <a name="03801101000012" ll="SDBH:נֹעַם:000001">Gracia</a>, y lo <a name="03801101000016" ll="SDBH:גדע:000002">quebré</a>, para <a name="03801101000022" ll="SDBH:פרר:000002">romper</a> <a name="03801101000026" ll="SDBH:בְּרִית:000000">mi</a> <a name="03801101000026" ll="SDBH:בְּרִית:000000">pacto</a> <a name="03801101000028">que</a> <a name="03801101000030" ll="SDBH:כרת:000001">concerté</a> <a name="03801101000006">con</a> <a name="03801101000034">todos</a> los <a name="03801101000038" ll="SDBH:עַם:000000">pueblos</a>. 
\v 11 Y fue <a name="03801101100004" ll="SDBH:פרר:000002">deshecho</a> en ese <a name="03801101100010" ll="SDBH:יֹום:000006">día</a>, y <a name="03801101100020">así</a> <a name="03801101100018">conocieron</a> los <a name="03801101100022" ll="SDBH:כְּנַעֲנִי:000000" tl="HOTTP4475">pobres</a> del <a name="03801101100026" ll="SDBH:צֹאן:000000" il="LB00012;LB00015;LB00017;NBGbs20120208-08;NBGbs20120710-139;NBGpb20140401b;Realia:1.2.1;tb111706002" tl="HOTTP4475">rebaño</a> que <a name="03801101100030" ll="SDBH:שׁמר:000000">miraban</a> a mí, que era <a name="03801101100036" ll="SDBH:דָּבָר:000000">palabra</a> de <a name="03801101100038" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 12 Y <a name="03801101200006">les</a> <a name="03801101200004" ll="SDBH:אמר:000000">dije</a>: <a name="03801101200008">Si</a> os <a name="03801101200014" ll="SDBH:עַיִן:000000">parece</a> <a name="03801101200010" ll="SDBH:טוב:000000">bien</a>, <a name="03801101200016">dadme</a> <a name="03801101200018" ll="SDBH:שָׂכָר:000000">mi</a> <a name="03801101200018" ll="SDBH:שָׂכָר:000000">salario</a>; y <a name="03801101200022">si</a> <a name="03801101200024" ll="SDBH:לֹא:000000">no</a>, <a name="03801101200026" ll="SDBH:חדל:000002">dejadlo</a>. Y <a name="03801101200030" ll="SDBH:שׁקל:000000">pesaron</a> por <a name="03801101200034" ll="SDBH:שָׂכָר:000000">mi</a> <a name="03801101200034" ll="SDBH:שָׂכָר:000000">salario</a> <a name="03801101200036" ll="SDBH:שָׁלֹשׁ:000001">treinta</a> <a name="03801101200038" ll="SDBH:כֶּסֶף:000000">piezas</a> <a name="03801101200038" ll="SDBH:כֶּסֶף:000000">de</a> <a name="03801101200038" ll="SDBH:כֶּסֶף:000000">plata</a>. 
