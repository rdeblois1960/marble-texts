\v 18 Y <a name="02900201800006" ll="SDBH:יהוה:000000">Jehová</a>, <a name="02900201800004" ll="SDBH:קנא:000000">solícito</a> por <a name="02900201800010" ll="SDBH:אֶרֶץ:000000">su</a> <a name="02900201800010" ll="SDBH:אֶרֶץ:000000">tierra</a>, <a name="02900201800014" ll="SDBH:חמל:000000">perdonará</a> <a name="02900201800016">a</a> <a name="02900201800018" ll="SDBH:עַם:000000">su</a> <a name="02900201800018" ll="SDBH:עַם:000000">pueblo</a>. 
\v 19 <a name="02900201900004" ll="SDBH:ענה:000000">Responderá</a> <a name="02900201900006" ll="SDBH:יהוה:000000">Jehová</a>, y <a name="02900201900010" ll="SDBH:אמר:000000">dirá</a> a <a name="02900201900014" ll="SDBH:עַם:000000">su</a> <a name="02900201900014" ll="SDBH:עַם:000000">pueblo</a>: He aquí yo os <a name="02900201900018" ll="SDBH:שׁלח:000006">envío</a> <a name="02900201900026" ll="SDBH:דָּגָן:000000">pan</a>, <a name="02900201900032" ll="SDBH:תִּירֹושׁ:000000">mosto</a> y <a name="02900201900038" ll="SDBH:יִצְהָר:000000">aceite</a>, y seréis <a name="02900201900042" ll="SDBH:שׂבע:000000">saciados</a> de ellos; y <a name="02900201900048" ll="SDBH:לֹא:000000">nunca</a> <a name="02900201900054">más</a> os <a name="02900201900050">pondré</a> en <a name="02900201900056" ll="SDBH:חֶרְפָּה:000003">oprobio</a> entre las <a name="02900201900062" ll="SDBH:גֹּוי:000000">naciones</a>. 
\v 20 Y <a name="02900202000010" ll="SDBH:רחק:000001">haré</a> <a name="02900202000010" ll="SDBH:רחק:000001">alejar</a> <a name="02900202000012;02900202000014">de</a> <a name="02900202000012;02900202000014">vosotros</a> al del <a name="02900202000008" ll="SDBH:צְפֹונִי:000000">norte</a>, y <a name="02900202000018" ll="SDBH:נדח:001000">lo</a> <a name="02900202000018" ll="SDBH:נדח:001000">echaré</a> <a name="02900202000020">en</a> <a name="02900202000022;02900202000024" ll="SDBH:אֶרֶץ:000000;SDBH:צִיָּה:000001">tierra</a> <a name="02900202000022;02900202000024" ll="SDBH:אֶרֶץ:000000;SDBH:צִיָּה:000001">seca</a> y <a name="02900202000028" ll="SDBH:שְׁמָמָה:000000">desierta</a>; <a name="02900202000032" ll="SDBH:פָּנֶה:000000">su</a> <a name="02900202000032" ll="SDBH:פָּנֶה:000000">faz</a> será <a name="02900202000034">hacia</a> el <a name="02900202000038;02900202000042" ll="SDBH:יָם:000002;SDBH:קַדְמֹנִי:000001">mar</a> <a name="02900202000038;02900202000042" ll="SDBH:יָם:000002;SDBH:קַדְמֹנִי:000001">oriental</a>, y <a name="02900202000046" ll="SDBH:סֹוף:000000">su</a> <a name="02900202000046" ll="SDBH:סֹוף:000000">fin</a> <a name="02900202000048">al</a> <a name="02900202000052;02900202000056" ll="SDBH:אַחֲרֹון:000002;SDBH:יָם:000001">mar</a> <a name="02900202000052;02900202000056" ll="SDBH:אַחֲרֹון:000002;SDBH:יָם:000001">occidental</a>; y <a name="02900202000060" ll="SDBH:עלה:000022">exhalará</a> <a name="02900202000062" ll="SDBH:בְּאֹשׁ:000000">su</a> <a name="02900202000062" ll="SDBH:בְּאֹשׁ:000000">hedor</a>, y <a name="02900202000066" ll="SDBH:עלה:000022">subirá</a> <a name="02900202000068" ll="SDBH:צַחֲנָה:000000">su</a> <a name="02900202000068" ll="SDBH:צַחֲנָה:000000">pudrición</a>, <a name="02900202000070">porque</a> <a name="02900202000076">hizo</a> <a name="02900202000072" ll="SDBH:גדל:000010">grandes</a> cosas.
    
\v 21 <a name="02900202100006" ll="SDBH:אֲדָמָה:000000">Tierra</a>, <a name="02900202100002" ll="SDBH:אַל:000000">no</a> <a name="02900202100004" ll="SDBH:ירא:000000">temas</a>; <a name="02900202100008" ll="SDBH:גיל:000000">alégrate</a> y <a name="02900202100012" ll="SDBH:שׂמח:000000">gózate</a>, porque <a name="02900202100018" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900202100022">hará</a> <a name="02900202100016" ll="SDBH:גדל:000010">grandes</a> cosas. 
\v 22 <a name="02900202200006" ll="SDBH:בְּהֵמָה:000000">Animales</a> del <a name="02900202200008">campo</a>, <a name="02900202200002" ll="SDBH:אַל:000000">no</a> <a name="02900202200004" ll="SDBH:ירא:000000">temáis</a>; porque los pastos del <a name="02900202200016" ll="SDBH:מִדְבָּר:000000">desierto</a> <a name="02900202200012" ll="SDBH:דשׁא:000000">reverdecerán</a>, porque los <a name="02900202200020" ll="SDBH:עֵץ:000000">árboles</a> <a name="02900202200022" ll="SDBH:נשׂא:000023">llevarán</a> <a name="02900202200024" ll="SDBH:פְּרִי:000000">su</a> <a name="02900202200024" ll="SDBH:פְּרִי:000000">fruto</a>, la <a name="02900202200026" ll="SDBH:תְּאֵנָה:000000">higuera</a> y la <a name="02900202200030" ll="SDBH:גֶּפֶן:000001">vid</a> <a name="02900202200032">darán</a> sus frutos. 
\v 23 Vosotros también, <a name="02900202300004" ll="SDBH:בֵּן:000004">hijos</a> de <a name="02900202300006" ll="SDBH:צִיֹּון:000000">Sion</a>, <a name="02900202300008" ll="SDBH:גיל:000000">alegraos</a> y <a name="02900202300012" ll="SDBH:שׂמח:000000">gozaos</a> en <a name="02900202300016" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900202300018" ll="SDBH:אֱלֹהִים:000001">vuestro</a> <a name="02900202300018" ll="SDBH:אֱלֹהִים:000001">Dios</a>; <a name="02900202300020">porque</a> <a name="02900202300024">os</a> <a name="02900202300022">ha</a> <a name="02900202300022">dado</a> la <a name="02900202300030" ll="SDBH:מֹורֶה:000000">primera</a> <a name="02900202300030" ll="SDBH:מֹורֶה:000000">lluvia</a> a su <a name="02900202300034" ll="SDBH:צְדָקָה:000001">tiempo</a>, y <a name="02900202300038" ll="SDBH:ירד:000002">hará</a> <a name="02900202300038" ll="SDBH:ירד:000002">descender</a> <a name="02900202300040">sobre</a> <a name="02900202300040">vosotros</a> <a name="02900202300042;02900202300044" ll="SDBH:גֶּשֶׁם:000000;SDBH:מֹורֶה:000000" il="Rain;Rain2" tl="HOTTP4200">lluvia</a> <a name="02900202300042;02900202300044" ll="SDBH:גֶּשֶׁם:000000;SDBH:מֹורֶה:000000" il="Rain;Rain2" tl="HOTTP4200">temprana</a> y <a name="02900202300048" ll="SDBH:מַלְקֹושׁ:000000" il="Rain;Rain2" tl="HOTTP4200">tardía</a> como al <a name="02900202300054" ll="SDBH:רִאשֹׁון:000000" tl="HOTTP4201">principio</a>. 
\v 24 Las <a name="02900202400008" ll="SDBH:גֹּרֶן:000000">eras</a> <a name="02900202400004" ll="SDBH:מלא:000000">se</a> <a name="02900202400004" ll="SDBH:מלא:000000">llenarán</a> de <a name="02900202400010" ll="SDBH:בַּר:002000">trigo</a>, y los <a name="02900202400018" ll="SDBH:יֶקֶב:000000">lagares</a> rebosarán de <a name="02900202400020" ll="SDBH:תִּירֹושׁ:000000">vino</a> y <a name="02900202400024" ll="SDBH:יִצְהָר:000000">aceite</a>. 
\v 25 Y <a name="02900202500006">os</a> <a name="02900202500004" ll="SDBH:שׁלם:000007">restituiré</a> los <a name="02900202500012" ll="SDBH:שָׁנָה:000000">años</a> <a name="02900202500014">que</a> <a name="02900202500016" ll="SDBH:אכל:000000">comió</a> la <a name="02900202500020" ll="SDBH:אַרְבֶּה:000000">oruga</a>, el <a name="02900202500024" ll="SDBH:יֶלֶק:000000">saltón</a>, el <a name="02900202500030" ll="SDBH:חָסִיל:000000">revoltón</a> y la <a name="02900202500036" ll="SDBH:גָּזָם:000000">langosta</a>, <a name="02900202500038" ll="SDBH:חַיִל:000002">mi</a> <a name="02900202500042" ll="SDBH:גָּדֹול:000000">gran</a> <a name="02900202500038" ll="SDBH:חַיִל:000002">ejército</a> <a name="02900202500044">que</a> <a name="02900202500046" ll="SDBH:שׁלח:000002">envié</a> <a name="02900202500048">contra</a> <a name="02900202500048">vosotros</a>.
    
\v 26 <a name="02900202600004" ll="SDBH:אכל:000000">Comeréis</a> hasta <a name="02900202600006" ll="SDBH:אכל:000000">saciaros</a>, y <a name="02900202600014" ll="SDBH:הלל:001000">alabaréis</a> el <a name="02900202600018" ll="SDBH:שֵׁם:000003">nombre</a> de <a name="02900202600020" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900202600022" ll="SDBH:אֱלֹהִים:000001">vuestro</a> <a name="02900202600022" ll="SDBH:אֱלֹהִים:000001">Dios</a>, el cual <a name="02900202600024">hizo</a> <a name="02900202600026">maravillas</a> con <a name="02900202600028">vosotros</a>; y <a name="02900202600036" ll="SDBH:לֹא:000000">nunca</a> <a name="02900202600044" ll="SDBH:עֹולָם:000000">jamás</a> será <a name="02900202600040" ll="SDBH:עַם:000000">mi</a> <a name="02900202600040" ll="SDBH:עַם:000000">pueblo</a> <a name="02900202600038" ll="SDBH:בושׁ:000000">avergonzado</a>. 
\v 27 Y <a name="02900202700004">conoceréis</a> que en <a name="02900202700010">medio</a> de <a name="02900202700012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> estoy <a name="02900202700014">yo</a>, y que <a name="02900202700018">yo</a> soy <a name="02900202700020" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900202700022" ll="SDBH:אֱלֹהִים:000001">vuestro</a> <a name="02900202700022" ll="SDBH:אֱלֹהִים:000001">Dios</a>, y <a name="02900202700026">no</a> hay <a name="02900202700028">otro</a>; y <a name="02900202700036" ll="SDBH:עַם:000000">mi</a> <a name="02900202700036" ll="SDBH:עַם:000000">pueblo</a> <a name="02900202700032" ll="SDBH:לֹא:000000">nunca</a> <a name="02900202700040" ll="SDBH:עֹולָם:000000">jamás</a> será <a name="02900202700034" ll="SDBH:בושׁ:000000">avergonzado</a>.
    
\c 3
\v 1 (28) Y <a name="02900300100006" ll="SDBH:אַחַר:002001">después</a> de <a name="02900300100008">esto</a> <a name="02900300100010" ll="SDBH:שׁפך:000001">derramaré</a> <a name="02900300100014" ll="SDBH:רוּחַ:000000">mi</a> <a name="02900300100014" ll="SDBH:רוּחַ:000000">Espíritu</a> <a name="02900300100016">sobre</a> <a name="02900300100018">toda</a> <a name="02900300100020" ll="SDBH:בָּשָׂר:000004">carne</a>, y <a name="02900300100024" ll="SDBH:נבא:000000">profetizarán</a> <a name="02900300100026" ll="SDBH:בֵּן:000000">vuestros</a> <a name="02900300100026" ll="SDBH:בֵּן:000000">hijos</a> y <a name="02900300100030" ll="SDBH:בַּת:000000">vuestras</a> <a name="02900300100030" ll="SDBH:בַּת:000000">hijas</a>; <a name="02900300100032" ll="SDBH:זָקֵן:000001">vuestros</a> <a name="02900300100032" ll="SDBH:זָקֵן:000001">ancianos</a> <a name="02900300100036" ll="SDBH:חלם:001000">soñarán</a> <a name="02900300100034" ll="SDBH:חֲלֹום:000000">sueños</a>, y <a name="02900300100038" ll="SDBH:בָּחוּר:000000">vuestros</a> <a name="02900300100038" ll="SDBH:בָּחוּר:000000">jóvenes</a> <a name="02900300100042" ll="SDBH:ראה:000000">verán</a> <a name="02900300100040" ll="SDBH:חִזָּיֹון:000000">visiones</a>. 
\v 2 (29) Y <a name="02900300200004" ll="SDBH:גַּם:000000">también</a> <a name="02900300200006">sobre</a> los <a name="02900300200010" ll="SDBH:עֶבֶד:000000">siervos</a> y <a name="02900300200014">sobre</a> las <a name="02900300200018" ll="SDBH:שִׁפְחָה:000000">siervas</a> <a name="02900300200030" ll="SDBH:שׁפך:000001">derramaré</a> <a name="02900300200034" ll="SDBH:רוּחַ:000000">mi</a> <a name="02900300200034" ll="SDBH:רוּחַ:000000">Espíritu</a> en aquellos <a name="02900300200024" ll="SDBH:יֹום:000006">días</a>.
    
\v 3 (30) Y <a name="02900300300004">daré</a> <a name="02900300300006" ll="SDBH:מֹופֵת:000001">prodigios</a> en el <a name="02900300300012" ll="SDBH:שָׁמַיִם:000001">cielo</a> y en la <a name="02900300300020" ll="SDBH:אֶרֶץ:000000">tierra</a>, <a name="02900300300022" ll="SDBH:דָּם:000000">sangre</a>, y <a name="02900300300026" ll="SDBH:אֵשׁ:000000">fuego</a>, y columnas de <a name="02900300300032" ll="SDBH:עָשָׁן:000000">humo</a>. 
\v 4 (31) El <a name="02900300400004" ll="SDBH:שֶׁמֶשׁ:000000">sol</a> <a name="02900300400006" ll="SDBH:הפך:000008">se</a> <a name="02900300400006" ll="SDBH:הפך:000008">convertirá</a> en <a name="02900300400010" ll="SDBH:חֹשֶׁךְ:000000">tinieblas</a>, y la <a name="02900300400016" ll="SDBH:יָרֵחַ:000000">luna</a> en <a name="02900300400020" ll="SDBH:דָּם:000000">sangre</a>, <a name="02900300400024">antes</a> que <a name="02900300400026">venga</a> el <a name="02900300400028" ll="SDBH:יֹום:000006">día</a> <a name="02900300400034" ll="SDBH:גָּדֹול:000003">grande</a> y <a name="02900300400040" ll="SDBH:ירא:000001">espantoso</a> de <a name="02900300400030" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 5 (32) Y <a name="02900300500006">todo</a> aquel <a name="02900300500008">que</a> <a name="02900300500010" ll="SDBH:קרא:000000">invocare</a> el <a name="02900300500014" ll="SDBH:שֵׁם:000000">nombre</a> de <a name="02900300500016" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900300500018" ll="SDBH:מלט:000000">será</a> <a name="02900300500018" ll="SDBH:מלט:000000">salvo</a>;, <a name="02900300500020">porque</a> en el <a name="02900300500024" ll="SDBH:הַר:000000">monte</a> de <a name="02900300500026" ll="SDBH:צִיֹּון:000000">Sion</a> y en <a name="02900300500032" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a> habrá <a name="02900300500036" ll="SDBH:פְּלֵיטָה:000000">salvación</a>, como <a name="02900300500042" ll="SDBH:אמר:000000">ha</a> <a name="02900300500042" ll="SDBH:אמר:000000">dicho</a> <a name="02900300500044" ll="SDBH:יהוה:000000">Jehová</a>, y entre el <a name="02900300500052" ll="SDBH:שָׂרִיד:000000" tl="HOTTP4202">remanente</a> al cual él <a name="02900300500058" ll="SDBH:קרא:000003">habrá</a> <a name="02900300500058" ll="SDBH:קרא:000003">llamado</a>.
\c 4
\v 1 Porque he aquí que en aquellos <a name="02900400100010" ll="SDBH:יֹום:000006">días</a>, y en aquel <a name="02900400100022" ll="SDBH:עֵת:000000">tiempo</a> en <a name="02900400100028">que</a> haré <a name="02900400100030" ll="SDBH:שׁוב:000038">volver</a> la <a name="02900400100036" ll="SDBH:שְׁבוּת:000000">cautividad</a> de <a name="02900400100038" ll="SDBH:יְהוּדָה:000000">Judá</a> y de <a name="02900400100042" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>, 
\v 2 <a name="02900400200004" ll="SDBH:קבץ:000001">reuniré</a> a <a name="02900400200008">todas</a> las <a name="02900400200012" ll="SDBH:גֹּוי:000000">naciones</a>, y las haré <a name="02900400200016" ll="SDBH:ירד:000002">descender</a> <a name="02900400200018">al</a> <a name="02900400200020" ll="SDBH:עֵמֶק:000000">valle</a> de <a name="02900400200022" ll="SDBH:יְהֹושָׁפָט:000006">Josafat</a>, y <a name="02900400200030" ll="SDBH:שָׁם:000000">allí</a> entraré en <a name="02900400200026" ll="SDBH:שׁפט:000007">juicio</a> <a name="02900400200028">con</a> ellas a causa <a name="02900400200032">de</a> <a name="02900400200034" ll="SDBH:עַם:000000">mi</a> <a name="02900400200034" ll="SDBH:עַם:000000">pueblo</a>, y de <a name="02900400200040" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> mi heredad, a <a name="02900400200042">quien</a> ellas <a name="02900400200044" ll="SDBH:פזר:000000">esparcieron</a> entre las <a name="02900400200050" ll="SDBH:גֹּוי:000000">naciones</a>, y <a name="02900400200058" ll="SDBH:חלק:000006">repartieron</a> <a name="02900400200056" ll="SDBH:אֶרֶץ:000000">mi</a> <a name="02900400200056" ll="SDBH:אֶרֶץ:000000">tierra</a>; 
\v 3 y <a name="02900400300008" ll="SDBH:ידד:000000">echaron</a> <a name="02900400300010" ll="SDBH:גֹּורָל:000001">suertes</a> <a name="02900400300004">sobre</a> <a name="02900400300006" ll="SDBH:עַם:000000">mi</a> <a name="02900400300006" ll="SDBH:עַם:000000">pueblo</a>, y <a name="02900400300014">dieron</a> los <a name="02900400300018" ll="SDBH:יֶלֶד:000000">niños</a> por una <a name="02900400300024" ll="SDBH:זֹנָה:000000">ramera</a>, y <a name="02900400300032" ll="SDBH:מכר:000000">vendieron</a> las <a name="02900400300030" ll="SDBH:יַלְדָּה:000000">niñas</a> por <a name="02900400300038" ll="SDBH:יַיִן:000000">vino</a> para <a name="02900400300042" ll="SDBH:שׁתה:000000">beber</a>.
    
\v 4 Y <a name="02900400400004" ll="SDBH:גַּם:000000">también</a>, ¿<a name="02900400400006" ll="SDBH:מָה:000000">qué</a> tengo yo con <a name="02900400400008">vosotras</a>, <a name="02900400400012" ll="SDBH:צֹר:001000">Tiro</a> y <a name="02900400400016" ll="SDBH:צִידֹון:000001">Sidón</a>, y <a name="02900400400020">todo</a> el <a name="02900400400022" ll="SDBH:גְּלִילָה:000000">territorio</a> de <a name="02900400400024" ll="SDBH:פְּלֶשֶׁת:000000">Filistea</a>? ¿<a name="02900400400028;02900400400032" ll="SDBH:גְּמוּל:000001;SDBH:שׁלם:000007">Queréis</a> <a name="02900400400028;02900400400032" ll="SDBH:גְּמוּל:000001;SDBH:שׁלם:000007">vengaros</a> <a name="02900400400034">de</a> <a name="02900400400034">mí</a>? Y <a name="02900400400038">si</a> <a name="02900400400044">de</a> <a name="02900400400044">mí</a> os <a name="02900400400040" ll="SDBH:גמל:001000">vengáis</a>, <a name="02900400400046" ll="SDBH:קַל:000001">bien</a> <a name="02900400400048" ll="SDBH:מְהֵרָה:000000">pronto</a> <a name="02900400400050;02900400400052" ll="SDBH:גְּמוּל:000000;SDBH:שׁוב:000032">haré</a> <a name="02900400400050;02900400400052" ll="SDBH:גְּמוּל:000000;SDBH:שׁוב:000032">yo</a> <a name="02900400400050;02900400400052" ll="SDBH:גְּמוּל:000000;SDBH:שׁוב:000032">recaer</a> <a name="02900400400050;02900400400052" ll="SDBH:גְּמוּל:000000;SDBH:שׁוב:000032">la</a> <a name="02900400400050;02900400400052" ll="SDBH:גְּמוּל:000000;SDBH:שׁוב:000032">paga</a> sobre <a name="02900400400056" ll="SDBH:רֹאשׁ:000000">vuestra</a> <a name="02900400400056" ll="SDBH:רֹאשׁ:000000">cabeza</a>. 
\v 5 <a name="02900400500002">Porque</a> <a name="02900400500010" ll="SDBH:לקח:000000">habéis</a> <a name="02900400500010" ll="SDBH:לקח:000000">llevado</a> <a name="02900400500004" ll="SDBH:כֶּסֶף:000000">mi</a> <a name="02900400500004" ll="SDBH:כֶּסֶף:000000">plata</a> y <a name="02900400500008" ll="SDBH:זָהָב:000000">mi</a> <a name="02900400500008" ll="SDBH:זָהָב:000000">oro</a>, y <a name="02900400500014" ll="SDBH:מַחְמָד:000000">mis</a> cosas <a name="02900400500014" ll="SDBH:מַחְמָד:000000">preciosas</a> y <a name="02900400500018" ll="SDBH:טֹוב:000000">hermosas</a> <a name="02900400500020">metisteis</a> en <a name="02900400500024" ll="SDBH:הֵיכָל:000000">vuestros</a> <a name="02900400500024" ll="SDBH:הֵיכָל:000000">templos</a>; 
\v 6 y <a name="02900400600014" ll="SDBH:מכר:000000">vendisteis</a> los <a name="02900400600004" ll="SDBH:בֵּן:000000">hijos</a> de <a name="02900400600006" ll="SDBH:יְהוּדָה:000000">Judá</a> y los <a name="02900400600010" ll="SDBH:בֵּן:000004">hijos</a> de <a name="02900400600012" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a> a los <a name="02900400600018" ll="SDBH:בֵּן:000000">hijos</a> de los <a name="02900400600022" ll="SDBH:יְוָנִי:000000">griegos</a>, para <a name="02900400600026" ll="SDBH:רחק:000001">alejarlos</a> de <a name="02900400600032" ll="SDBH:גְּבוּל:000000">su</a> <a name="02900400600032" ll="SDBH:גְּבוּל:000000">tierra</a>. 
\v 7 <a name="02900400700002">He</a> <a name="02900400700002">aquí</a> <a name="02900400700002">yo</a> <a name="02900400700004" ll="SDBH:עור:000003">los</a> <a name="02900400700004" ll="SDBH:עור:000003">levantaré</a> <a name="02900400700006">del</a> <a name="02900400700010" ll="SDBH:מָקֹום:000000">lugar</a> <a name="02900400700012;02900400700018" ll="SDBH:שָׁם:000000">donde</a> <a name="02900400700016">los</a> <a name="02900400700014" ll="SDBH:מכר:000000">vendisteis</a>, y <a name="02900400700022" ll="SDBH:שׁוב:000032">volveré</a> <a name="02900400700024" ll="SDBH:גְּמוּל:000000">vuestra</a> <a name="02900400700024" ll="SDBH:גְּמוּל:000000">paga</a> sobre <a name="02900400700028" ll="SDBH:רֹאשׁ:000000">vuestra</a> <a name="02900400700028" ll="SDBH:רֹאשׁ:000000">cabeza</a>; 
\v 8 y <a name="02900400800004" ll="SDBH:מכר:000000">venderé</a> <a name="02900400800008" ll="SDBH:בֵּן:000000">vuestros</a> <a name="02900400800008" ll="SDBH:בֵּן:000000">hijos</a> y <a name="02900400800014" ll="SDBH:בַּת:000000">vuestras</a> <a name="02900400800014" ll="SDBH:בַּת:000000">hijas</a> <a name="02900400800016;02900400800018">a</a> los <a name="02900400800020" ll="SDBH:בֵּן:000000">hijos</a> de <a name="02900400800022" ll="SDBH:יְהוּדָה:000000">Judá</a>, y <a name="02900400800026" ll="SDBH:מכר:000000">ellos</a> <a name="02900400800026" ll="SDBH:מכר:000000">los</a> <a name="02900400800026" ll="SDBH:מכר:000000">venderán</a> a los <a name="02900400800030" ll="SDBH:שְׁבָאִים:000000">sabeos</a>, <a name="02900400800034" ll="SDBH:גֹּוי:000000">nación</a> <a name="02900400800036" ll="SDBH:רָחֹוק:000000">lejana</a>; <a name="02900400800038">porque</a> <a name="02900400800040" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900400800042" ll="SDBH:דבר:001000">ha</a> <a name="02900400800042" ll="SDBH:דבר:001000">hablado</a>.
    
\v 9 <a name="02900400900002;02900400900012" ll="SDBH:קדשׁ:000011;SDBH:קרא:000006">Proclamad</a> <a name="02900400900004" ll="SDBH:זֶה:000000;SDBH:זֹה:000000">esto</a> entre las <a name="02900400900010" ll="SDBH:גֹּוי:000000">naciones</a>, proclamad <a name="02900400900014" ll="SDBH:מִלְחָמָה:000000">guerra</a>, <a name="02900400900016" ll="SDBH:עור:000003">despertad</a> a los <a name="02900400900020" ll="SDBH:גִּבֹּור:000000">valientes</a>, <a name="02900400900022" ll="SDBH:נגשׁ:000002">acérquense</a>, <a name="02900400900024" ll="SDBH:עלה:000004">vengan</a> <a name="02900400900026">todos</a> los hombres de <a name="02900400900032" ll="SDBH:מִלְחָמָה:000002">guerra</a>. 
\v 10 <a name="02900401000002" ll="SDBH:כתת:000002">Forjad</a> <a name="02900401000008" ll="SDBH:חֶרֶב:000001">espadas</a> de <a name="02900401000004" ll="SDBH:אֵת:000000">vuestros</a> <a name="02900401000004" ll="SDBH:אֵת:000000">azadones</a>, <a name="02900401000016" ll="SDBH:רֹמַח:000000">lanzas</a> de <a name="02900401000012" ll="SDBH:מַזְמֵרָה:000000">vuestras</a> <a name="02900401000012" ll="SDBH:מַזְמֵרָה:000000">hoces</a>; <a name="02900401000022" ll="SDBH:אמר:000000">diga</a> el <a name="02900401000020" ll="SDBH:חַלָּשׁ:000000">débil</a>: <a name="02900401000024" ll="SDBH:גִּבֹּור:000000">Fuerte</a> <a name="02900401000026">soy</a>. 
\v 11 <a name="02900401100002" ll="SDBH:עושׁ:000000">Juntaos</a> y <a name="02900401100006">venid</a>, <a name="02900401100012" ll="SDBH:גֹּוי:000000">naciones</a> <a name="02900401100008">todas</a> de <a name="02900401100016" ll="SDBH:סָבִיב:000000">alrededor</a>, y <a name="02900401100020" ll="SDBH:קבץ:000000">congregaos</a>; haz <a name="02900401100024" ll="SDBH:נחת:000001" tl="HOTTP4203">venir</a> <a name="02900401100022" ll="SDBH:שָׁם:000000">allí</a>, oh <a name="02900401100026" ll="SDBH:יהוה:000000" tl="HOTTP4203">Jehová</a>, a <a name="02900401100028" ll="SDBH:גִּבֹּור:000000" tl="HOTTP4203">tus</a> <a name="02900401100028" ll="SDBH:גִּבֹּור:000000" tl="HOTTP4203">fuertes</a>. 
\v 12 <a name="02900401200002" ll="SDBH:עור:000002">Despiértense</a> las <a name="02900401200010" ll="SDBH:גֹּוי:000000">naciones</a>, y <a name="02900401200006" ll="SDBH:עלה:000004">suban</a> <a name="02900401200012">al</a> <a name="02900401200014" ll="SDBH:עֵמֶק:000000">valle</a> de <a name="02900401200016" ll="SDBH:יְהֹושָׁפָט:000006">Josafat</a>; porque <a name="02900401200020" ll="SDBH:שָׁם:000000">allí</a> me <a name="02900401200022" ll="SDBH:ישׁב:000000">sentaré</a> para <a name="02900401200026" ll="SDBH:שׁפט:000001">juzgar</a> a <a name="02900401200030">todas</a> las <a name="02900401200034" ll="SDBH:גֹּוי:000000">naciones</a> de <a name="02900401200038" ll="SDBH:סָבִיב:000000">alrededor</a>. 
\v 13 <a name="02900401300002" ll="SDBH:שׁלח:000016">Echad</a> la <a name="02900401300004" ll="SDBH:מַגָּל:000000">hoz</a>, porque la <a name="02900401300010" ll="SDBH:קָצִיר:000002">mies</a> está ya <a name="02900401300008" ll="SDBH:בשׁל:000003">madura</a>. <a name="02900401300012">Venid</a>, <a name="02900401300014">descended</a>, porque el <a name="02900401300020" ll="SDBH:גַּת:000000">lagar</a> está <a name="02900401300018" ll="SDBH:מלא:000000">lleno</a>, rebosan las <a name="02900401300026" ll="SDBH:יֶקֶב:000000">cubas</a>; porque <a name="02900401300030" ll="SDBH:רַב:000002">mucha</a> es la <a name="02900401300032" ll="SDBH:רָעָה:000000">maldad</a> de ellos. 
\v 14 Muchos <a name="02900401400002" ll="SDBH:הָמֹון:000002">pueblos</a> en el <a name="02900401400008" ll="SDBH:עֵמֶק:000000">valle</a> de la decisión; porque <a name="02900401400016">cercano</a> está el <a name="02900401400018" ll="SDBH:יֹום:000006">día</a> de <a name="02900401400020" ll="SDBH:יהוה:000000">Jehová</a> en el <a name="02900401400024" ll="SDBH:עֵמֶק:000000">valle</a> de la decisión. 
\v 15 El <a name="02900401500002" ll="SDBH:שֶׁמֶשׁ:000000">sol</a> y la <a name="02900401500006" ll="SDBH:יָרֵחַ:000000">luna</a> <a name="02900401500008" ll="SDBH:קדר:000001">se</a> <a name="02900401500008" ll="SDBH:קדר:000001">oscurecerán</a>, y las <a name="02900401500012" ll="SDBH:כֹּוכָב:000000">estrellas</a> <a name="02900401500014" ll="SDBH:אסף:000010">retraerán</a> <a name="02900401500016" ll="SDBH:נֹגַהּ:000000">su</a> <a name="02900401500016" ll="SDBH:נֹגַהּ:000000">resplandor</a>.
    
\v 16 Y <a name="02900401600004" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900401600010" ll="SDBH:שׁאג:000000">rugirá</a> desde <a name="02900401600008" ll="SDBH:צִיֹּון:000000">Sion</a>, y <a name="02900401600018">dará</a> <a name="02900401600020">su</a> <a name="02900401600020">voz</a> desde <a name="02900401600016" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a>, y <a name="02900401600024" ll="SDBH:רעשׁ:000000">temblarán</a> los <a name="02900401600026" ll="SDBH:אֶרֶץ:000002;SDBH:שָׁמַיִם:000001">cielos</a> y la <a name="02900401600030" ll="SDBH:אֶרֶץ:000000">tierra</a>; pero <a name="02900401600034" ll="SDBH:יהוה:000000">Jehová</a> será la <a name="02900401600036" ll="SDBH:מַחְסֶה:000001">esperanza</a> de <a name="02900401600040" ll="SDBH:עַם:000000">su</a> <a name="02900401600040" ll="SDBH:עַם:000000">pueblo</a>, y la <a name="02900401600044" ll="SDBH:מָעֹוז:000000">fortaleza</a> de los <a name="02900401600048" ll="SDBH:בֵּן:000000">hijos</a> de <a name="02900401600050" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>.
    
\v 17 Y <a name="02900401700004">conoceréis</a> que <a name="02900401700008">yo</a> soy <a name="02900401700010" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900401700012" ll="SDBH:אֱלֹהִים:000001">vuestro</a> <a name="02900401700012" ll="SDBH:אֱלֹהִים:000001">Dios</a>, que <a name="02900401700014" ll="SDBH:שׁכן:000000">habito</a> en <a name="02900401700018" ll="SDBH:צִיֹּון:000000">Sion</a>, <a name="02900401700022" ll="SDBH:קֹדֶשׁ:000001">mi</a> <a name="02900401700022" ll="SDBH:קֹדֶשׁ:000001">santo</a> <a name="02900401700020" ll="SDBH:הַר:000000">monte</a>; y <a name="02900401700028" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a> <a name="02900401700026">será</a> <a name="02900401700030" ll="SDBH:קֹדֶשׁ:000001">santa</a>, y extraños <a name="02900401700036" ll="SDBH:לֹא:000000">no</a> <a name="02900401700038" ll="SDBH:עבר:000000">pasarán</a> <a name="02900401700042">más</a> por ella.
    
\v 18 <a name="02900401800004">Sucederá</a> en aquel <a name="02900401800010" ll="SDBH:יֹום:000006">tiempo</a>, que los <a name="02900401800020" ll="SDBH:הַר:000000">montes</a> <a name="02900401800016" ll="SDBH:נטף:000000">destilarán</a> <a name="02900401800022">mosto</a>, y los <a name="02900401800028" ll="SDBH:גִּבְעָה:000000">collados</a> fluirán <a name="02900401800032" ll="SDBH:חָלָב:000000">leche</a>, y por <a name="02900401800036">todos</a> los <a name="02900401800038" ll="SDBH:אָפִיק:000000">arroyos</a> de <a name="02900401800040" ll="SDBH:יְהוּדָה:000000">Judá</a> correrán <a name="02900401800044" ll="SDBH:מַיִם:000000">aguas</a>; y <a name="02900401800056" ll="SDBH:יצא:000022">saldrá</a> una <a name="02900401800048" ll="SDBH:מַעְיָן:000000">fuente</a> de la <a name="02900401800052" ll="SDBH:בַּיִת:000000">casa</a> de <a name="02900401800054" ll="SDBH:יהוה:000000">Jehová</a>, y <a name="02900401800060" ll="SDBH:שׁקה:000001">regará</a> el <a name="02900401800064" ll="SDBH:נַחַל:000000">valle</a> de <a name="02900401800068" ll="SDBH:שִׁטִּים:000000">Sitim</a>.
    
\v 19 <a name="02900401900002" ll="SDBH:מִצְרַיִם:000001">Egipto</a> <a name="02900401900008">será</a> <a name="02900401900006" ll="SDBH:שְׁמָמָה:000000">destruido</a>, y <a name="02900401900012" ll="SDBH:אֱדֹום:000001">Edom</a> <a name="02900401900020">será</a> <a name="02900401900020">vuelto</a> en <a name="02900401900016" ll="SDBH:מִדְבָּר:000000">desierto</a> <a name="02900401900018" ll="SDBH:שְׁמָמָה:000000">asolado</a>, por la <a name="02900401900024" ll="SDBH:חָמָס:000000">injuria</a> hecha a los <a name="02900401900026" ll="SDBH:בֵּן:000000">hijos</a> de <a name="02900401900028" ll="SDBH:יְהוּדָה:000000">Judá</a>; <a name="02900401900030">porque</a> <a name="02900401900032" ll="SDBH:שׁפך:000009">derramaron</a> en <a name="02900401900040" ll="SDBH:אֶרֶץ:000000">su</a> <a name="02900401900040" ll="SDBH:אֶרֶץ:000000">tierra</a> <a name="02900401900034" ll="SDBH:דָּם:000003">sangre</a> <a name="02900401900036" ll="SDBH:נָקִי:000000">inocente</a>. 
\v 20 Pero <a name="02900402000004" ll="SDBH:יְהוּדָה:000000">Judá</a> será <a name="02900402000010">habitada</a> para <a name="02900402000008" ll="SDBH:עֹולָם:000000">siempre</a>, y <a name="02900402000014" ll="SDBH:יְרוּשָׁלַ͏ִם:000000">Jerusalén</a> por <a name="02900402000018" ll="SDBH:דֹּור:001000">generación</a> y <a name="02900402000022" ll="SDBH:דֹּור:001000">generación</a>. 
\v 21 Y <a name="02900402100004" ll="SDBH:נקה:000003;SDBH:נקם:000000" tl="HOTTP4204">limpiaré</a> la <a name="02900402100006" ll="SDBH:דָּם:000003" tl="HOTTP4204">sangre</a> de los que <a name="02900402100008" ll="SDBH:לֹא:000000" tl="HOTTP4204">no</a> <a name="02900402100010" ll="SDBH:נקה:000003" tl="HOTTP4204">había</a> <a name="02900402100010" ll="SDBH:נקה:000003" tl="HOTTP4204">limpiado</a>; y <a name="02900402100014" ll="SDBH:יהוה:000000">Jehová</a> <a name="02900402100016" ll="SDBH:שׁכן:000000">morará</a> en <a name="02900402100020" ll="SDBH:צִיֹּון:000000">Sion</a>.
