\v 14 Lo <a name="02002301400010">castigarás</a> con <a name="02002301400008" ll="SDBH:שֵׁבֶט:000000">vara</a>,Y <a name="02002301400020" ll="SDBH:נצל:000005">librarás</a> <a name="02002301400014" ll="SDBH:נֶפֶשׁ:000001">su</a> <a name="02002301400014" ll="SDBH:נֶפֶשׁ:000001">alma</a> del <a name="02002301400018" ll="SDBH:שְׁאֹול:000000">Seol</a>.
    
\v 15 <a name="02002301500002" ll="SDBH:בֵּן:000009">Hijo</a> <a name="02002301500002" ll="SDBH:בֵּן:000009">mío</a>, <a name="02002301500004">si</a> <a name="02002301500008" ll="SDBH:לֵב:000001">tu</a> <a name="02002301500008" ll="SDBH:לֵב:000001">corazón</a> fuere <a name="02002301500006" ll="SDBH:חכם:000000">sabio</a>,<a name="02002301500014" ll="SDBH:גַּם:000000">También</a> a mí se me <a name="02002301500010" ll="SDBH:שׂמח:000000">alegrará</a> el <a name="02002301500012" ll="SDBH:לֵב:000001">corazón</a>;
    
\v 16 <a name="02002301600006" ll="SDBH:כִּלְיָה:000001">Mis</a> <a name="02002301600006" ll="SDBH:כִּלְיָה:000001">entrañas</a> también <a name="02002301600004" ll="SDBH:עלז:000000">se</a> <a name="02002301600004" ll="SDBH:עלז:000000">alegrarán</a>Cuando <a name="02002301600012" ll="SDBH:שָׂפָה:000001">tus</a> <a name="02002301600012" ll="SDBH:שָׂפָה:000001">labios</a> <a name="02002301600010" ll="SDBH:דבר:001000">hablaren</a> cosas rectas.
    
\v 17 <a name="02002301700002" ll="SDBH:אַל:000000">No</a> tenga <a name="02002301700006" ll="SDBH:לֵב:000001">tu</a> <a name="02002301700006" ll="SDBH:לֵב:000001">corazón</a> <a name="02002301700004" ll="SDBH:קנא:000001">envidia</a> de los <a name="02002301700012" ll="SDBH:חַטָּא:000001">pecadores</a>,Antes persevera en el <a name="02002301700020" ll="SDBH:יִרְאָה:000000">temor</a> de <a name="02002301700022" ll="SDBH:יהוה:000000">Jehová</a> <a name="02002301700024">todo</a> el <a name="02002301700028" ll="SDBH:יֹום:000007">tiempo</a>;
    
\v 18 Porque <a name="02002301800004">ciertamente</a> <a name="02002301800006">hay</a> <a name="02002301800008" ll="SDBH:אַחֲרִית:000001" tl="HOTTP2786">fin</a>,Y <a name="02002301800012" ll="SDBH:תִּקְוָה:001000">tu</a> <a name="02002301800012" ll="SDBH:תִּקְוָה:001000">esperanza</a> <a name="02002301800014" ll="SDBH:לֹא:000000">no</a> será <a name="02002301800016" ll="SDBH:כרת:000006">cortada</a>.
    
\v 19 <a name="02002301900002">Oye</a>, <a name="02002301900006" ll="SDBH:בֵּן:000009">hijo</a> <a name="02002301900006" ll="SDBH:בֵּן:000009">mío</a>, y <a name="02002301900010" ll="SDBH:חכם:000000">sé</a> <a name="02002301900010" ll="SDBH:חכם:000000">sabio</a>,Y <a name="02002301900014;02002301900016;02002301900020;02002301900022" ll="SDBH:אשׁר:000002;SDBH:דֶּרֶךְ:000007;SDBH:לֵב:000001">endereza</a> <a name="02002301900014;02002301900016;02002301900020;02002301900022" ll="SDBH:אשׁר:000002;SDBH:דֶּרֶךְ:000007;SDBH:לֵב:000001">tu</a> <a name="02002301900014;02002301900016;02002301900020;02002301900022" ll="SDBH:אשׁר:000002;SDBH:דֶּרֶךְ:000007;SDBH:לֵב:000001">corazón</a> <a name="02002301900014;02002301900016;02002301900020;02002301900022" ll="SDBH:אשׁר:000002;SDBH:דֶּרֶךְ:000007;SDBH:לֵב:000001">al</a> <a name="02002301900014;02002301900016;02002301900020;02002301900022" ll="SDBH:אשׁר:000002;SDBH:דֶּרֶךְ:000007;SDBH:לֵב:000001">camino</a>.
    
\v 20 <a name="02002302000002" ll="SDBH:אַל:000000">No</a> <a name="02002302000004">estés</a> con los <a name="02002302000008" ll="SDBH:סבא:000000">bebedores</a> de <a name="02002302000010" ll="SDBH:יַיִן:000000">vino</a>,Ni con los <a name="02002302000014" ll="SDBH:זלל:000000">comedores</a> de <a name="02002302000016" ll="SDBH:בָּשָׂר:000000">carne</a>;
    
\v 21 Porque el <a name="02002302100004" ll="SDBH:סבא:000000">bebedor</a> y el <a name="02002302100008" ll="SDBH:זלל:000000">comilón</a> <a name="02002302100010" ll="SDBH:ירשׁ:000004">empobrecerán</a>,Y el <a name="02002302100018" ll="SDBH:נוּמָה:000000">sueño</a> hará <a name="02002302100016" ll="SDBH:לבשׁ:000002">vestir</a> vestidos <a name="02002302100014" ll="SDBH:קְרָעִים:000001">rotos</a>.
    
\v 22 <a name="02002302200002">Oye</a> a <a name="02002302200006" ll="SDBH:אָב:000000">tu</a> <a name="02002302200006" ll="SDBH:אָב:000000">padre</a>, a aquel que te <a name="02002302200010" ll="SDBH:ילד:000001">engendró</a>;Y cuando <a name="02002302200022" ll="SDBH:אֵם:000000">tu</a> <a name="02002302200022" ll="SDBH:אֵם:000000">madre</a> <a name="02002302200020" ll="SDBH:זקן:000000">envejeciere</a>, <a name="02002302200014" ll="SDBH:אַל:000000">no</a> la menosprecies.
    
\v 23 <a name="02002302300004" ll="SDBH:קנה:000000;SDBH:קנה:000002">Compra</a> la verdad, y <a name="02002302300008" ll="SDBH:אַל:000000">no</a> la <a name="02002302300010" ll="SDBH:מכר:000000">vendas</a>;La <a name="02002302300012" ll="SDBH:חָכְמָה:000000">sabiduría</a>, la <a name="02002302300016" ll="SDBH:מוּסָר:000000">enseñanza</a> y la <a name="02002302300020" ll="SDBH:בִּינָה:000000">inteligencia</a>.
    
\v 24 <a name="02002302400002" ll="SDBH:גיל:000000">Mucho</a> se <a name="02002302400004" ll="SDBH:גיל:000000">alegrará</a> el <a name="02002302400010" ll="SDBH:אָב:000000">padre</a> del <a name="02002302400012" ll="SDBH:צַדִּיק:000000">justo</a>,Y el que <a name="02002302400016" ll="SDBH:ילד:000001">engendra</a> <a name="02002302400022" ll="SDBH:חָכָם, קסם:000000">sabio</a> <a name="02002302400024" ll="SDBH:שׂמח:000000">se</a> <a name="02002302400024" ll="SDBH:שׂמח:000000">gozará</a> con él.
    
\v 25 <a name="02002302500002" ll="SDBH:שׂמח:000000">Alégrense</a> <a name="02002302500004" ll="SDBH:אָב:000000">tu</a> <a name="02002302500004" ll="SDBH:אָב:000000">padre</a> y <a name="02002302500008" ll="SDBH:אֵם:000000">tu</a> <a name="02002302500008" ll="SDBH:אֵם:000000">madre</a>,Y <a name="02002302500012" ll="SDBH:גיל:000000">gócese</a> la <a name="02002302500014" ll="SDBH:ילד:000000">que</a> <a name="02002302500014" ll="SDBH:ילד:000000">te</a> <a name="02002302500014" ll="SDBH:ילד:000000">dio</a> <a name="02002302500014" ll="SDBH:ילד:000000">a</a> <a name="02002302500014" ll="SDBH:ילד:000000">luz</a>.
    
\v 26 <a name="02002302600002">Dame</a>, <a name="02002302600004" ll="SDBH:בֵּן:000009">hijo</a> <a name="02002302600004" ll="SDBH:בֵּן:000009">mío</a>, <a name="02002302600006" ll="SDBH:לֵב:000001">tu</a> <a name="02002302600006" ll="SDBH:לֵב:000001">corazón</a>,Y <a name="02002302600018" ll="SDBH:נצר:000000" tl="HOTTP2787">miren</a> <a name="02002302600012" ll="SDBH:עַיִן:000000">tus</a> <a name="02002302600012" ll="SDBH:עַיִן:000000">ojos</a> por <a name="02002302600014" ll="SDBH:דֶּרֶךְ:000007">mis</a> <a name="02002302600014" ll="SDBH:דֶּרֶךְ:000007">caminos</a>.
    
\v 27 Porque <a name="02002302700004" ll="SDBH:שׁוּחָה:000000">abismo</a> <a name="02002302700006" ll="SDBH:עָמֹק:000000">profundo</a> es la <a name="02002302700008" ll="SDBH:זֹנָה:000000">ramera</a>,Y <a name="02002302700012" ll="SDBH:בְּאֵר:000002">pozo</a> angosto la <a name="02002302700016" ll="SDBH:נָכְרִי:000003">extraña</a>.
    
\v 28 <a name="02002302800002" ll="SDBH:אַף:001000">También</a> <a name="02002302800004">ella</a>, como <a name="02002302800008" ll="SDBH:חֶתֶף:000000">robador</a>, <a name="02002302800010" ll="SDBH:ארב:000000">acecha</a>,Y <a name="02002302800020">multiplica</a> entre los <a name="02002302800018" ll="SDBH:אָדָם:000000">hombres</a> los <a name="02002302800014" ll="SDBH:בגד:000000" tl="HOTTP2788">prevaricadores</a>.
    
\v 29 ¿Para <a name="02002302900004" ll="SDBH:מִי:000000">quién</a> será el <a name="02002302900006" ll="SDBH:אֹוי:000000">ay</a>? ¿Para <a name="02002302900010" ll="SDBH:מִי:000000">quién</a> el <a name="02002302900012" ll="SDBH:אֲבֹוי:000000">dolor</a>? ¿Para <a name="02002302900016" ll="SDBH:מִי:000000">quién</a> las <a name="02002302900020" ll="SDBH:מִדְיָן:000000">rencillas</a>?¿Para <a name="02002302900024" ll="SDBH:מִי:000000">quién</a> las <a name="02002302900026" ll="SDBH:שִׂיחַ:001000">quejas</a>? ¿Para <a name="02002302900030" ll="SDBH:מִי:000000">quién</a> las <a name="02002302900032" ll="SDBH:פֶּצַע:000000">heridas</a> en <a name="02002302900034" ll="SDBH:חִנָּם:000001">balde</a>?¿Para <a name="02002302900038" ll="SDBH:מִי:000000">quién</a> lo <a name="02002302900040;02002302900042" ll="SDBH:חַכְלִילוּת:000000;SDBH:עַיִן:000000">amoratado</a> <a name="02002302900040;02002302900042" ll="SDBH:חַכְלִילוּת:000000;SDBH:עַיִן:000000">de</a> <a name="02002302900040;02002302900042" ll="SDBH:חַכְלִילוּת:000000;SDBH:עַיִן:000000">los</a> <a name="02002302900040;02002302900042" ll="SDBH:חַכְלִילוּת:000000;SDBH:עַיִן:000000">ojos</a>?
    
\v 30 Para los que <a name="02002303000006" ll="SDBH:אחר:000001">se</a> <a name="02002303000006" ll="SDBH:אחר:000001">detienen</a> mucho <a name="02002303000008">en</a> el <a name="02002303000012" ll="SDBH:יַיִן:000000">vino</a>,Para los que <a name="02002303000018">van</a> <a name="02002303000022" ll="SDBH:חקר:000000">buscando</a> la <a name="02002303000024" ll="SDBH:מִמְסָךְ:000000">mistura</a>.
    
\v 31 <a name="02002303100002" ll="SDBH:אַל:000000">No</a> <a name="02002303100004" ll="SDBH:ראה:000000">mires</a> al <a name="02002303100006" ll="SDBH:יַיִן:000000">vino</a> cuando <a name="02002303100010" ll="SDBH:אדם:000000">rojea</a>,Cuando <a name="02002303100014">resplandece</a> <a name="02002303100028" ll="SDBH:עַיִן:000004">su</a> <a name="02002303100028" ll="SDBH:עַיִן:000004">color</a> en la <a name="02002303100020" ll="SDBH:כֹּוס:000000">copa</a>.<a name="02002303100030" ll="SDBH:הלך:000000;SDBH:מֵישָׁרִים:000000" tl="HOTTP2789">Se</a> <a name="02002303100030" ll="SDBH:הלך:000000;SDBH:מֵישָׁרִים:000000" tl="HOTTP2789">entra</a> suavemente;
    
\v 32 Mas al fin como <a name="02002303200006" ll="SDBH:נָחָשׁ:000000">serpiente</a> <a name="02002303200008" ll="SDBH:נשׁך:000000">morderá</a>,Y como <a name="02002303200014" ll="SDBH:צֶפַע:000000">áspid</a> dará <a name="02002303200016" ll="SDBH:פרשׁ:000002">dolor</a>.
    
\v 33 <a name="02002303300002" ll="SDBH:עַיִן:000000">Tus</a> <a name="02002303300002" ll="SDBH:עַיִן:000000">ojos</a> <a name="02002303300004" ll="SDBH:ראה:000000">mirarán</a> cosas <a name="02002303300006" ll="SDBH:זָר:000001">extrañas</a>,Y <a name="02002303300010" ll="SDBH:לֵב:000001">tu</a> <a name="02002303300010" ll="SDBH:לֵב:000001">corazón</a> <a name="02002303300012" ll="SDBH:דבר:001000">hablará</a> perversidades.
    
\v 34 <a name="02002303400004">Serás</a> como el que <a name="02002303400008" ll="SDBH:שׁכב:000000">yace</a> en <a name="02002303400012" ll="SDBH:לֵב:000004">medio</a> del <a name="02002303400014" ll="SDBH:יָם:000000">mar</a>,O como el que <a name="02002303400020" ll="SDBH:שׁכב:000000" tl="HOTTP2790">está</a> en la <a name="02002303400024" ll="SDBH:רֹאשׁ:000011" tl="HOTTP2790">punta</a> de un <a name="02002303400026" ll="SDBH:חִבֵּל:000000" tl="HOTTP2790">mastelero</a>.
    
\v 35 Y dirás: Me <a name="02002303500002">hirieron</a>, mas no me <a name="02002303500006" ll="SDBH:חלה:000000">dolió</a>;Me <a name="02002303500008" ll="SDBH:הלם:000000" tl="HOTTP2791">azotaron</a>, mas no lo <a name="02002303500012">sentí</a>;Cuando <a name="02002303500014" ll="SDBH:מָתַי:000000">despertare</a>, <a name="02002303500018">aún</a> lo <a name="02002303500022">volveré</a> a <a name="02002303500020" ll="SDBH:בקשׁ:000000">buscar</a>.
\c 24
\v 1 <a name="02002400100002" ll="SDBH:אַל:000000">No</a> tengas <a name="02002400100004" ll="SDBH:קנא:000001">envidia</a> de los hombres <a name="02002400100010" ll="SDBH:רָעָה:000000">malos</a>,<a name="02002400100014" ll="SDBH:אַל:000000">Ni</a> <a name="02002400100016" ll="SDBH:אוה:000002">desees</a> estar con ellos;
    
\v 2 Porque <a name="02002400200008" ll="SDBH:לֵב:000001">su</a> <a name="02002400200008" ll="SDBH:לֵב:000001">corazón</a> <a name="02002400200006" ll="SDBH:הגה:000002">piensa</a> en robar,E iniquidad <a name="02002400200016" ll="SDBH:דבר:001000">hablan</a> <a name="02002400200014" ll="SDBH:שָׂפָה:000001">sus</a> <a name="02002400200014" ll="SDBH:שָׂפָה:000001">labios</a>.
    
\v 3 Con <a name="02002400300004" ll="SDBH:חָכְמָה:000000">sabiduría</a> <a name="02002400300006" ll="SDBH:בנה:000000">se</a> <a name="02002400300006" ll="SDBH:בנה:000000">edificará</a> la <a name="02002400300008" ll="SDBH:בַּיִת:000000">casa</a>,Y con <a name="02002400300014" ll="SDBH:תְּבוּנָה:000000">prudencia</a> <a name="02002400300016" ll="SDBH:כון:000006">se</a> <a name="02002400300016" ll="SDBH:כון:000006">afirmará</a>;
    
\v 4 Y con <a name="02002400400006" ll="SDBH:דַּעַת:000000">ciencia</a> <a name="02002400400010" ll="SDBH:מלא:000000">se</a> <a name="02002400400010" ll="SDBH:מלא:000000">llenarán</a> las <a name="02002400400008" ll="SDBH:חֶדֶר:000000">cámaras</a>De <a name="02002400400012">todo</a> <a name="02002400400014" ll="SDBH:הֹון:000000">bien</a> <a name="02002400400016" ll="SDBH:יָקָר:000000">preciado</a> y <a name="02002400400020" ll="SDBH:נָעִים:000000">agradable</a>.
    
\v 5 El <a name="02002400500002" ll="SDBH:גֶּבֶר:001000" tl="HOTTP2792">hombre</a> <a name="02002400500004" ll="SDBH:חָכָם, קסם:000000" tl="HOTTP2792">sabio</a> es <a name="02002400500010" ll="SDBH:עֹז:000002" tl="HOTTP2792">fuerte</a>,Y de <a name="02002400500018" ll="SDBH:אמץ:000001" tl="HOTTP2793">pujante</a> <a name="02002400500020" ll="SDBH:כֹּחַ:000000" tl="HOTTP2793">vigor</a> el <a name="02002400500014">hombre</a> <a name="02002400500016" ll="SDBH:דַּעַת:000000">docto</a>.
    
\v 6 Porque con <a name="02002400600006" ll="SDBH:תַּחְבּוּלֹות:000000">ingenio</a> <a name="02002400600008">harás</a> la <a name="02002400600012" ll="SDBH:מִלְחָמָה:000000">guerra</a>,Y en la <a name="02002400600020" ll="SDBH:רֹב:000000">multitud</a> de <a name="02002400600022" ll="SDBH:יעץ:000004">consejeros</a> está la <a name="02002400600016" ll="SDBH:תְּשׁוּעָה:000000">victoria</a>.
    
\v 7 Alta está para el <a name="02002400700006" ll="SDBH:אֱוִיל:000000">insensato</a> la sabiduría;En la <a name="02002400700014" ll="SDBH:שַׁעַר:000000">puerta</a> <a name="02002400700016" ll="SDBH:לֹא:000000">no</a> <a name="02002400700018" ll="SDBH:פתח:000015">abrirá</a> él <a name="02002400700020" ll="SDBH:פֶּה:000000">su</a> <a name="02002400700020" ll="SDBH:פֶּה:000000">boca</a>.
    
\v 8 Al que <a name="02002400800002" ll="SDBH:חשׁב:000001">piensa</a> hacer el mal,Le <a name="02002400800014" ll="SDBH:קרא:000007">llamarán</a> hombre de malos <a name="02002400800012" ll="SDBH:מְזִמָּה:000000">pensamientos</a>.
    
\v 9 El <a name="02002400900002" ll="SDBH:זִמָּה:000001">pensamiento</a> del <a name="02002400900004" ll="SDBH:אִוֶּלֶת:000000">necio</a> es <a name="02002400900006" ll="SDBH:חַטָּאת:000000">pecado</a>,Y <a name="02002400900010" ll="SDBH:תֹּועֵבָה:000001">abominación</a> a los <a name="02002400900014" ll="SDBH:אָדָם:000000">hombres</a> el <a name="02002400900016" ll="SDBH:ליץ:000000">escarnecedor</a>.
    
\v 10 Si fueres <a name="02002401000002" ll="SDBH:רפה:000006">flojo</a> en el <a name="02002401000006" ll="SDBH:יֹום:000003" tl="HOTTP2794">día</a> de <a name="02002401000008" ll="SDBH:צָרָה:000000" tl="HOTTP2794">trabajo</a>,<a name="02002401000012" ll="SDBH:כֹּחַ:000000">Tu</a> <a name="02002401000012" ll="SDBH:כֹּחַ:000000">fuerza</a> será <a name="02002401000010" ll="SDBH:צַר:000001">reducida</a>.
    
\v 11 <a name="02002401100002" ll="SDBH:נצל:000005">Libra</a> a los que son <a name="02002401100004" ll="SDBH:לקח:000000">llevados</a> a la <a name="02002401100022">muerte</a>;<a name="02002401100024" ll="SDBH:חשׂך:000004">Salva</a> a los que están en peligro de <a name="02002401100020" ll="SDBH:הֶרֶג:000000">muerte</a>.
    
\v 12 Porque si <a name="02002401200004" ll="SDBH:אמר:000000">dijeres</a>: Ciertamente <a name="02002401200008" ll="SDBH:לֹא:000000" tl="HOTTP2795">no</a> lo <a name="02002401200010">supimos</a>,¿Acaso <a name="02002401200016" ll="SDBH:לֹא:000000">no</a> lo <a name="02002401200024" ll="SDBH:בין:000001">entenderá</a> el que <a name="02002401200018" ll="SDBH:תכן:000000">pesa</a> los corazones?El que <a name="02002401200028" ll="SDBH:נצר:000007">mira</a> por <a name="02002401200030" ll="SDBH:נֶפֶשׁ:000001">tu</a> <a name="02002401200030" ll="SDBH:נֶפֶשׁ:000001">alma</a>, <a name="02002401200022">él</a> lo <a name="02002401200034">conocerá</a>,Y <a name="02002401200038" ll="SDBH:שׁוב:000032">dará</a> al <a name="02002401200042" ll="SDBH:אָדָם:000000">hombre</a> según <a name="02002401200032">sus</a> <a name="02002401200046" ll="SDBH:פֹּעַל:000001">obras</a>.
    
\v 13 <a name="02002401300002" ll="SDBH:אכל:000000">Come</a>, <a name="02002401300004" ll="SDBH:בֵּן:000009">hijo</a> <a name="02002401300004" ll="SDBH:בֵּן:000009">mío</a>, de la <a name="02002401300006" ll="SDBH:דְּבַשׁ:000000">miel</a>, <a name="02002401300008">porque</a> es <a name="02002401300010" ll="SDBH:טֹוב:000000">buena</a>,Y el <a name="02002401300014" ll="SDBH:נֹפֶת:000000">panal</a> es <a name="02002401300016" ll="SDBH:מָתֹוק:000000">dulce</a> <a name="02002401300018">a</a> <a name="02002401300020" ll="SDBH:חֵךְ:000003">tu</a> <a name="02002401300020" ll="SDBH:חֵךְ:000003">paladar</a>.
    
\v 14 <a name="02002401400002">Así</a> será a <a name="02002401400010" ll="SDBH:נֶפֶשׁ:000001">tu</a> <a name="02002401400010" ll="SDBH:נֶפֶשׁ:000001">alma</a> el conocimiento de la <a name="02002401400006" ll="SDBH:חָכְמָה:000000" tl="HOTTP2796">sabiduría</a>;<a name="02002401400012">Si</a> la <a name="02002401400014" ll="SDBH:מצא:000006">hallares</a> <a name="02002401400018">tendrás</a> <a name="02002401400020" ll="SDBH:אַחֲרִית:000001" tl="HOTTP2797">recompensa</a>,Y al fin <a name="02002401400024" ll="SDBH:תִּקְוָה:001000">tu</a> <a name="02002401400024" ll="SDBH:תִּקְוָה:001000">esperanza</a> <a name="02002401400026" ll="SDBH:לֹא:000000">no</a> será <a name="02002401400028" ll="SDBH:כרת:000006">cortada</a>.
    
\v 15 Oh <a name="02002401500006" ll="SDBH:רָשָׁע:000000">impío</a>, <a name="02002401500002" ll="SDBH:אַל:000000">no</a> <a name="02002401500004" ll="SDBH:ארב:000000">aceches</a> la <a name="02002401500010" ll="SDBH:נָוֶה:000001">tienda</a> del <a name="02002401500012" ll="SDBH:צַדִּיק:000000">justo</a>,<a name="02002401500014" ll="SDBH:אַל:000000">No</a> <a name="02002401500016" ll="SDBH:שׁדד:000000">saquees</a> <a name="02002401500018" ll="SDBH:רֵבֶץ:000000">su</a> <a name="02002401500018" ll="SDBH:רֵבֶץ:000000">cámara</a>;
    
\v 16 Porque <a name="02002401600004" ll="SDBH:שֶׁבַע:000000">siete</a> veces <a name="02002401600006" ll="SDBH:נפל:000036">cae</a> el <a name="02002401600008" ll="SDBH:צַדִּיק:000000">justo</a>, y vuelve a <a name="02002401600012">levantarse</a>;Mas los <a name="02002401600016" ll="SDBH:רָשָׁע:000000">impíos</a> <a name="02002401600018" ll="SDBH:כשׁל:000001">caerán</a> en el <a name="02002401600022" ll="SDBH:רָעָה:000002">mal</a>.
    
\v 17 Cuando <a name="02002401700004" ll="SDBH:נפל:000036">cayere</a> tu enemigo, <a name="02002401700010" ll="SDBH:אַל:000000">no</a> te <a name="02002401700012" ll="SDBH:שׂמח:000000">regocijes</a>,Y cuando <a name="02002401700018" ll="SDBH:כשׁל:000001">tropezare</a>, <a name="02002401700020" ll="SDBH:אַל:000000">no</a> <a name="02002401700022" ll="SDBH:גיל:000000">se</a> <a name="02002401700022" ll="SDBH:גיל:000000">alegre</a> <a name="02002401700024" ll="SDBH:לֵב:000001">tu</a> <a name="02002401700024" ll="SDBH:לֵב:000001">corazón</a>;
    
\v 18 <a name="02002401800002">No</a> <a name="02002401800002">sea</a> <a name="02002401800002">que</a> <a name="02002401800006" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="02002401800004" ll="SDBH:ראה:000000">mire</a>, y <a name="02002401800010;02002401800012;02002401800014" ll="SDBH:עַיִן:000000;SDBH:רעע:000001">le</a> <a name="02002401800010;02002401800012;02002401800014" ll="SDBH:עַיִן:000000;SDBH:רעע:000001">desagrade</a>,Y <a name="02002401800018" ll="SDBH:שׁוב:000020">aparte</a> de <a name="02002401800022">sobre</a> <a name="02002401800022">él</a> <a name="02002401800024" ll="SDBH:אַף:000008">su</a> <a name="02002401800024" ll="SDBH:אַף:000008">enojo</a>.
    
\v 19 <a name="02002401900002" ll="SDBH:אַל:000000">No</a> te <a name="02002401900004" ll="SDBH:חרה:000002">entremetas</a> con los malignos,<a name="02002401900012" ll="SDBH:אַל:000000">Ni</a> tengas <a name="02002401900014" ll="SDBH:קנא:000001">envidia</a> de los <a name="02002401900020" ll="SDBH:רָשָׁע:000000">impíos</a>;
    
\v 20 Porque para el <a name="02002402000014" ll="SDBH:רַע:000001">malo</a> <a name="02002402000004" ll="SDBH:לֹא:000000">no</a> <a name="02002402000006">habrá</a> buen <a name="02002402000008" ll="SDBH:אַחֲרִית:000001">fin</a>,Y la lámpara de los <a name="02002402000018" ll="SDBH:רָשָׁע:000000">impíos</a> será <a name="02002402000020" ll="SDBH:דעך:000000">apagada</a>.
    
\v 21 <a name="02002402100002" ll="SDBH:ירא:000000">Teme</a> a <a name="02002402100006" ll="SDBH:יהוה:000000">Jehová</a>, <a name="02002402100008" ll="SDBH:בֵּן:000009">hijo</a> <a name="02002402100008" ll="SDBH:בֵּן:000009">mío</a>, y al <a name="02002402100012" ll="SDBH:מֶלֶךְ:000000" tl="HOTTP2798">rey</a>;<a name="02002402100018" ll="SDBH:אַל:000000" tl="HOTTP2799">No</a> <a name="02002402100020" ll="SDBH:ערב:001002" tl="HOTTP2799">te</a> <a name="02002402100020" ll="SDBH:ערב:001002" tl="HOTTP2799">entremetas</a> <a name="02002402100014">con</a> los <a name="02002402100016" ll="SDBH:שׁנה:000003" tl="HOTTP2799">veleidosos</a>;
    
\v 22 Porque <a name="02002402200008" ll="SDBH:אֵיד:000000">su</a> <a name="02002402200008" ll="SDBH:אֵיד:000000">quebrantamiento</a> <a name="02002402200006">vendrá</a> de <a name="02002402200004" ll="SDBH:פִּתְאֹם:000000">repente</a>;Y el <a name="02002402200012" ll="SDBH:פִּיד:000000">quebrantamiento</a> de <a name="02002402200014" ll="SDBH:שְׁנַיִם:000000" tl="HOTTP2800">ambos</a>,¿<a name="02002402200016" ll="SDBH:מִי:000000">quién</a> lo <a name="02002402200018">comprende</a>?
    
\v 23 <a name="02002402300002" ll="SDBH:גַּם:000000">También</a> <a name="02002402300004" ll="SDBH:אֵלֶּה:000000">éstos</a> son dichos de los <a name="02002402300008" ll="SDBH:חָכָם, קסם:000000">sabios</a>:Hacer <a name="02002402300010" ll="SDBH:נכר:000002">acepción</a> de <a name="02002402300012">personas</a> en el <a name="02002402300016" ll="SDBH:מִשְׁפָּט:000000">juicio</a> no es <a name="02002402300020" ll="SDBH:טֹוב:000002">bueno</a>.
    
\v 24 El que <a name="02002402400002" ll="SDBH:אמר:000000">dijere</a> al malo: <a name="02002402400006" ll="SDBH:רָשָׁע:000000">Justo</a> <a name="02002402400010">eres</a>,Los <a name="02002402400014" ll="SDBH:עַם:000000">pueblos</a> lo maldecirán, y le <a name="02002402400016" ll="SDBH:זעם:000001">detestarán</a> las <a name="02002402400018" ll="SDBH:לְאֹם:000000">naciones</a>;
    
\v 25 Mas los que lo <a name="02002402500008" ll="SDBH:יכח:000004">reprendieren</a> tendrán <a name="02002402500010" ll="SDBH:נעם:000002">felicidad</a>,Y <a name="02002402500014">sobre</a> <a name="02002402500014">ellos</a> <a name="02002402500016">vendrá</a> <a name="02002402500020" ll="SDBH:טֹוב:000002">gran</a> <a name="02002402500018" ll="SDBH:בְּרָכָה:000000">bendición</a>.
    
\v 26 <a name="02002402600004" ll="SDBH:נשׁק:000000">Besados</a> serán los <a name="02002402600002" ll="SDBH:שָׂפָה:000000">labios</a>Del que <a name="02002402600006" ll="SDBH:שׁוב:000050">responde</a> <a name="02002402600008" ll="SDBH:דָּבָר:000000">palabras</a> <a name="02002402600010" ll="SDBH:נָכֹחַ:000000">rectas</a>.
    
\v 27 <a name="02002402700002" ll="SDBH:כון:000003">Prepara</a> <a name="02002402700010" ll="SDBH:מְלָאכָה:000000">tus</a> <a name="02002402700010" ll="SDBH:מְלָאכָה:000000">labores</a> <a name="02002402700008" ll="SDBH:חוּץ:000006">fuera</a>,Y <a name="02002402700014" ll="SDBH:עתד:000000">disponlas</a> en tus <a name="02002402700020" ll="SDBH:שָׂדֶה:000001">campos</a>,Y <a name="02002402700024" ll="SDBH:אַחַר:002001">después</a> <a name="02002402700028" ll="SDBH:בנה:000000">edificarás</a> <a name="02002402700030" ll="SDBH:בַּיִת:000000">tu</a> <a name="02002402700030" ll="SDBH:בַּיִת:000000">casa</a>.
    
\v 28 <a name="02002402800002" ll="SDBH:אַל:000000">No</a> <a name="02002402800004">seas</a> sin <a name="02002402800008" ll="SDBH:חִנָּם:000001">causa</a> <a name="02002402800006" ll="SDBH:עֵד:000000">testigo</a> contra tu prójimo,Y no <a name="02002402800018" ll="SDBH:פתה:000001">lisonjees</a> con <a name="02002402800022" ll="SDBH:שָׂפָה:000001">tus</a> <a name="02002402800022" ll="SDBH:שָׂפָה:000001">labios</a>.
    
\v 29 <a name="02002402900002" ll="SDBH:אַל:000000">No</a> <a name="02002402900004" ll="SDBH:אמר:000000">digas</a>: Como me <a name="02002402900010">hizo</a>, <a name="02002402900014">así</a> le <a name="02002402900016">haré</a>;Daré el <a name="02002402900020" ll="SDBH:שׁוב:000032">pago</a> al <a name="02002402900026">hombre</a> según <a name="02002402900030" ll="SDBH:פֹּעַל:000001">su</a> <a name="02002402900030" ll="SDBH:פֹּעַל:000001">obra</a>.
    
\v 30 <a name="02002403000010" ll="SDBH:עבר:000000">Pasé</a> junto <a name="02002403000002">al</a> <a name="02002403000004" ll="SDBH:שָׂדֶה:000001">campo</a> del <a name="02002403000006">hombre</a> <a name="02002403000008" ll="SDBH:עָצֵל:000000">perezoso</a>,Y junto <a name="02002403000014">a</a> la <a name="02002403000016" ll="SDBH:כֶּרֶם:000000">viña</a> del <a name="02002403000018" ll="SDBH:אָדָם:000000">hombre</a> <a name="02002403000020" ll="SDBH:חָסֵר:000001">falto</a> de <a name="02002403000022" ll="SDBH:לֵב:000003">entendimiento</a>;
    
\v 31 Y <a name="02002403100004">he</a> <a name="02002403100004">aquí</a> que por <a name="02002403100008">toda</a> <a name="02002403100006" ll="SDBH:עלה:000010" tl="HOTTP2801">ella</a> <a name="02002403100006" ll="SDBH:עלה:000010" tl="HOTTP2801">habían</a> <a name="02002403100006" ll="SDBH:עלה:000010" tl="HOTTP2801">crecido</a> los <a name="02002403100010" ll="SDBH:קִמֹּושׂ:000000">espinos</a>,<a name="02002403100016" ll="SDBH:חָרוּל:000000">Ortigas</a> <a name="02002403100012" ll="SDBH:כסה:000009">habían</a> ya <a name="02002403100012" ll="SDBH:כסה:000009">cubierto</a> <a name="02002403100014" ll="SDBH:פָּנֶה:000000">su</a> <a name="02002403100014" ll="SDBH:פָּנֶה:000000">faz</a>,Y <a name="02002403100022" ll="SDBH:אֶבֶן:000007">su</a> <a name="02002403100020" ll="SDBH:גָּדֵר:000000">cerca</a> de <a name="02002403100022" ll="SDBH:אֶבֶן:000007">piedra</a> <a name="02002403100024" ll="SDBH:הרס:000000">estaba</a> ya <a name="02002403100024" ll="SDBH:הרס:000000">destruida</a>.
    
\v 32 <a name="02002403200004" ll="SDBH:חזה:000001">Miré</a>, y lo <a name="02002403200008" ll="SDBH:שׁית:000011">puse</a> en <a name="02002403200010" ll="SDBH:לֵב:000001">mi</a> <a name="02002403200010" ll="SDBH:לֵב:000001">corazón</a>;Lo <a name="02002403200012" ll="SDBH:ראה:000000">vi</a>, y <a name="02002403200014" ll="SDBH:לקח:000000">tomé</a> <a name="02002403200016" ll="SDBH:מוּסָר:000000">consejo</a>.
    
\v 33 Un <a name="02002403300002" ll="SDBH:מְעַט:000001">poco</a> de <a name="02002403300004" ll="SDBH:שֵׁנָה:000000">sueño</a>, <a name="02002403300008" ll="SDBH:תְּנוּמָה:000000">cabeceando</a> otro <a name="02002403300006" ll="SDBH:מְעַט:000001">poco</a>,<a name="02002403300012" ll="SDBH:חִבֻּק:000000">Poniendo</a> <a name="02002403300014">mano</a> sobre mano otro <a name="02002403300010" ll="SDBH:מְעַט:000001">poco</a> para <a name="02002403300018" ll="SDBH:שׁכב:000000">dormir</a>;
    
\v 34 Así <a name="02002403400004">vendrá</a> como <a name="02002403400006" ll="SDBH:הלך:000015">caminante</a> <a name="02002403400008" ll="SDBH:רֵישׁ:000000">tu</a> <a name="02002403400008" ll="SDBH:רֵישׁ:000000">necesidad</a>,Y <a name="02002403400012" ll="SDBH:מַחְסֹור:000000">tu</a> <a name="02002403400012" ll="SDBH:מַחְסֹור:000000">pobreza</a> como <a name="02002403400016">hombre</a> <a name="02002403400018" ll="SDBH:מָגֵן:000000" il="NBGmk20131008" tl="HOTTP2802">armado</a>.
\c 25
\v 1 <a name="02002500100002" ll="SDBH:גַּם:000000">También</a> <a name="02002500100004" ll="SDBH:אֵלֶּה:000000">éstos</a> son <a name="02002500100006" ll="SDBH:מָשָׁל:000000">proverbios</a> de <a name="02002500100008" ll="SDBH:שְׁלֹמֹה:000000">Salomón</a>, los <a name="02002500100010">cuales</a> <a name="02002500100012" ll="SDBH:עתק:000006">copiaron</a> los varones de <a name="02002500100016" ll="SDBH:חִזְקִיָּה:000000">Ezequías</a>, <a name="02002500100018" ll="SDBH:מֶלֶךְ:000000">rey</a> de <a name="02002500100020" ll="SDBH:יְהוּדָה:000000">Judá</a>:
    
\v 2 <a name="02002500200002" ll="SDBH:כָּבֹוד:000000">Gloria</a> de <a name="02002500200004" ll="SDBH:אֱלֹהִים:000002">Dios</a> es <a name="02002500200006" ll="SDBH:סתר:000007">encubrir</a> un <a name="02002500200008" ll="SDBH:דָּבָר:000002">asunto</a>;Pero <a name="02002500200012" ll="SDBH:כָּבֹוד:000000">honra</a> del <a name="02002500200014" ll="SDBH:מֶלֶךְ:000000">rey</a> es <a name="02002500200016" ll="SDBH:חקר:000000">escudriñarlo</a>.
    
\v 3 Para la <a name="02002500300008" ll="SDBH:רוּם:000000">altura</a> de los <a name="02002500300002" ll="SDBH:שָׁמַיִם:000001">cielos</a>, y para la <a name="02002500300018" ll="SDBH:עֹמֶק:000000">profundidad</a> de la <a name="02002500300012" ll="SDBH:אֶרֶץ:000000">tierra</a>,Y para el <a name="02002500300022" ll="SDBH:לֵב:000001">corazón</a> de los <a name="02002500300024" ll="SDBH:מֶלֶךְ:000000">reyes</a>, <a name="02002500300026">no</a> hay <a name="02002500300028" ll="SDBH:חֵקֶר:000000">investigación</a>.
    
\v 4 Quita las <a name="02002500400004" ll="SDBH:סִיג:000000">escorias</a> de la <a name="02002500400008" ll="SDBH:כֶּסֶף:000000">plata</a>,Y <a name="02002500400012" ll="SDBH:יצא:000019">saldrá</a> <a name="02002500400020" ll="SDBH:כְּלִי:000000" tl="HOTTP2803">alhaja</a> al <a name="02002500400018" ll="SDBH:צרף:000006" tl="HOTTP2803">fundidor</a>.
    
\v 5 Aparta al <a name="02002500500004" ll="SDBH:רָשָׁע:000000">impío</a> de la <a name="02002500500008">presencia</a> del <a name="02002500500010" ll="SDBH:מֶלֶךְ:000000">rey</a>,Y <a name="02002500500022" ll="SDBH:כִּסֵּא:000002">su</a> <a name="02002500500022" ll="SDBH:כִּסֵּא:000002">trono</a> <a name="02002500500014" ll="SDBH:כון:000015">se</a> <a name="02002500500014" ll="SDBH:כון:000015">afirmará</a> en <a name="02002500500020" ll="SDBH:צֶדֶק:000001">justicia</a>.
    
\v 6 <a name="02002500600002" ll="SDBH:אַל:000000">No</a> te <a name="02002500600004" ll="SDBH:הדר:000003">alabes</a> <a name="02002500600008">delante</a> del <a name="02002500600010" ll="SDBH:מֶלֶךְ:000000">rey</a>,<a name="02002500600020" ll="SDBH:אַל:000000">Ni</a> <a name="02002500600022">estés</a> en el <a name="02002500600016" ll="SDBH:מָקֹום:000000">lugar</a> de los <a name="02002500600018" ll="SDBH:גָּדֹול:000002">grandes</a>;
    
\v 7 Porque <a name="02002500700004" ll="SDBH:טֹוב:000002">mejor</a> es que se te diga: <a name="02002500700006" ll="SDBH:אמר:000000">Sube</a> <a name="02002500700010" ll="SDBH:עלה:000004">acá</a>,Y no que seas <a name="02002500700016" ll="SDBH:שׁפל:000003">humillado</a> <a name="02002500700020">delante</a> del <a name="02002500700022" ll="SDBH:נָדִיב:000001">príncipe</a>A <a name="02002500700024">quien</a> han <a name="02002500700026" ll="SDBH:ראה:000000">mirado</a> <a name="02002500700028" ll="SDBH:עַיִן:000000">tus</a> <a name="02002500700028" ll="SDBH:עַיִן:000000">ojos</a>.
    
\v 8 <a name="02002500800002" ll="SDBH:אַל:000000" tl="HOTTP2804">No</a> <a name="02002500800004" ll="SDBH:יצא:000014" tl="HOTTP2804">entres</a> apresuradamente en <a name="02002500800008" ll="SDBH:ריב:000000" tl="HOTTP2804">pleito</a>,No <a name="02002500800012">sea</a> que no sepas <a name="02002500800014" ll="SDBH:מָה:000000">qué</a> <a name="02002500800016">hacer</a> al fin,Después que tu prójimo te haya <a name="02002500800024" ll="SDBH:כלם:000001">avergonzado</a>.
    
\v 9 Trata <a name="02002500900002" ll="SDBH:רִיב:000000">tu</a> <a name="02002500900002" ll="SDBH:רִיב:000000">causa</a> con tu compañero,Y <a name="02002500900016" ll="SDBH:אַל:000000">no</a> <a name="02002500900018" ll="SDBH:גלה:000011">descubras</a> el <a name="02002500900012" ll="SDBH:סֹוד:000003">secreto</a> a <a name="02002500900014" ll="SDBH:אַחֵר:000000">otro</a>,
    
\v 10 <a name="02002501000002;02002501000012" ll="SDBH:לֹא:000000">No</a> sea que te <a name="02002501000004" ll="SDBH:חסד:001000">deshonre</a> el que lo <a name="02002501000006">oyere</a>,Y <a name="02002501000010" ll="SDBH:דִּבָּה:000000">tu</a> <a name="02002501000010" ll="SDBH:דִּבָּה:000000">infamia</a> no pueda <a name="02002501000014" ll="SDBH:שׁוב:000019">repararse</a>.
    
\v 11 <a name="02002501100002" ll="SDBH:תַּפּוּחַ:000000">Manzana</a> de <a name="02002501100004" ll="SDBH:זָהָב:000000">oro</a> con <a name="02002501100008" ll="SDBH:מַשְׂכִּית:000001">figuras</a> de <a name="02002501100010" ll="SDBH:כֶּסֶף:000000">plata</a>Es la <a name="02002501100012" ll="SDBH:דָּבָר:000000">palabra</a> <a name="02002501100014" ll="SDBH:דבר:001000">dicha</a> <a name="02002501100016">como</a> conviene.
    
\v 12 Como <a name="02002501200002" ll="SDBH:נֶזֶם:000000">zarcillo</a> de <a name="02002501200004" ll="SDBH:זָהָב:000000">oro</a> y <a name="02002501200008" ll="SDBH:חֲלִי:000000">joyel</a> de oro <a name="02002501200010" ll="SDBH:כֶּתֶם:000000">fino</a>Es el que <a name="02002501200012" ll="SDBH:יכח:000003">reprende</a> al <a name="02002501200014" ll="SDBH:חָכָם, קסם:000000">sabio</a> que tiene <a name="02002501200018" ll="SDBH:אֹזֶן:000000">oído</a> <a name="02002501200020">dócil</a>.
    
\v 13 Como <a name="02002501300004" ll="SDBH:צִנָּה:000000">frío</a> de <a name="02002501300006" ll="SDBH:שֶׁלֶג:000000">nieve</a> en <a name="02002501300010" ll="SDBH:יֹום:000003">tiempo</a> de la <a name="02002501300012" ll="SDBH:קָצִיר:000001">siega</a>,Así es el <a name="02002501300014" ll="SDBH:צִיר:001000">mensajero</a> <a name="02002501300016" ll="SDBH:אמן:000006">fiel</a> a los que lo <a name="02002501300020" ll="SDBH:שׁלח:000000">envían</a>,Pues al <a name="02002501300024" ll="SDBH:נֶפֶשׁ:000001" tl="HOTTP2806">alma</a> de <a name="02002501300026" ll="SDBH:אָדֹון:000000" tl="HOTTP2806">su</a> <a name="02002501300026" ll="SDBH:אָדֹון:000000" tl="HOTTP2806">señor</a> da <a name="02002501300028" ll="SDBH:שׁוב:000039" tl="HOTTP2806">refrigerio</a>.
    
\v 14 Como <a name="02002501400002" ll="SDBH:נָשִׂיא:000000">nubes</a> y <a name="02002501400006" ll="SDBH:רוּחַ:000004">vientos</a> <a name="02002501400012">sin</a> <a name="02002501400010" ll="SDBH:גֶּשֶׁם:000000">lluvia</a>,Así es el <a name="02002501400014">hombre</a> que <a name="02002501400016" ll="SDBH:הלל:001002">se</a> <a name="02002501400016" ll="SDBH:הלל:001002">jacta</a> de <a name="02002501400022" ll="SDBH:שֶׁקֶר:000000">falsa</a> liberalidad.
    
\v 15 <a name="02002501500002">Con</a> <a name="02002501500004;02002501500006" ll="SDBH:אַף:000007;SDBH:אֹרֶךְ:000002">larga</a> <a name="02002501500004;02002501500006" ll="SDBH:אַף:000007;SDBH:אֹרֶךְ:000002">paciencia</a> <a name="02002501500008" ll="SDBH:פתה:000001">se</a> <a name="02002501500008" ll="SDBH:פתה:000001">aplaca</a> el <a name="02002501500010" ll="SDBH:קָצִין:000000">príncipe</a>,Y la <a name="02002501500014" ll="SDBH:לָשֹׁון:000000">lengua</a> <a name="02002501500016" ll="SDBH:רַךְ:000004">blanda</a> <a name="02002501500018" ll="SDBH:שׁבר:000000">quebranta</a> los <a name="02002501500020" ll="SDBH:גֶּרֶם:000000">huesos</a>.
    
\v 16 ¿<a name="02002501600004" ll="SDBH:מצא:000012">Hallaste</a> <a name="02002501600002" ll="SDBH:דְּבַשׁ:000000">miel</a>? <a name="02002501600006" ll="SDBH:אכל:000000">Come</a> lo que te <a name="02002501600008" ll="SDBH:דַּי:000000">basta</a>,<a name="02002501600010">No</a> sea que <a name="02002501600012" ll="SDBH:שׂבע:000000">hastiado</a> de ella la vomites.
    
\v 17 <a name="02002501700002" ll="SDBH:יקר:000001">Detén</a> <a name="02002501700004" ll="SDBH:רֶגֶל:000003">tu</a> <a name="02002501700004" ll="SDBH:רֶגֶל:000003">pie</a> de la <a name="02002501700008" ll="SDBH:בַּיִת:000000">casa</a> de tu vecino,<a name="02002501700012">No</a> sea que <a name="02002501700014" ll="SDBH:שׂבע:000005">hastiado</a> de ti te <a name="02002501700018" ll="SDBH:שׂנא:000000">aborrezca</a>.
    
\v 18 <a name="02002501800002" ll="SDBH:מֵפִיץ:000000" tl="HOTTP2807">Martillo</a> y <a name="02002501800006" ll="SDBH:חֶרֶב:000001">cuchillo</a> y <a name="02002501800010" ll="SDBH:חֵץ:000003">saeta</a> <a name="02002501800012" ll="SDBH:שׁנן:000000">aguda</a>Es el <a name="02002501800014">hombre</a> que <a name="02002501800016" ll="SDBH:ענה:000001">habla</a> contra su prójimo <a name="02002501800024" ll="SDBH:שֶׁקֶר:000000">falso</a> <a name="02002501800022" ll="SDBH:עֵד:000000">testimonio</a>.
    
\v 19 Como <a name="02002501900002" ll="SDBH:שֵׁן:000000" tl="HOTTP2808">diente</a> roto y <a name="02002501900008" ll="SDBH:רֶגֶל:000000" tl="HOTTP2808">pie</a> descoyuntadoEs la <a name="02002501900012" ll="SDBH:מִבְטָח:000000">confianza</a> en el <a name="02002501900014" ll="SDBH:בגד:000000">prevaricador</a> en <a name="02002501900018" ll="SDBH:יֹום:000003">tiempo</a> de <a name="02002501900020" ll="SDBH:צָרָה:000000">angustia</a>.
    
\v 20 El que <a name="02002502000020" ll="SDBH:שׁיר:000000">canta</a> <a name="02002502000026" ll="SDBH:שִׁיר:000000">canciones</a> <a name="02002502000014">al</a> <a name="02002502000030" ll="SDBH:לֵב:000001">corazón</a> <a name="02002502000032" ll="SDBH:רַע:000003">afligido</a>Es como el que <a name="02002502000002" ll="SDBH:עדה:000003" tl="HOTTP2809">quita</a> la <a name="02002502000004" ll="SDBH:בֶּגֶד:000001" il="NBGmk20140519a;NBGmk20140519c;NBGmk20140519d" tl="HOTTP2809">ropa</a> en <a name="02002502000008" ll="SDBH:יֹום:000003" tl="HOTTP2809">tiempo</a> de <a name="02002502000010" ll="SDBH:קָרָה:000000" tl="HOTTP2809">frío</a>, o el que <a name="02002502000028">sobre</a> el <a name="02002502000016" ll="SDBH:נֶתֶק:000000;SDBH:נֶתֶר:000000" tl="HOTTP2810">jabón</a> echa <a name="02002502000012" ll="SDBH:חֹמֶץ:000000">vinagre</a>.
    
\v 21 <a name="02002502100002;02002502100014">Si</a> el que te <a name="02002502100006" ll="SDBH:שׂנא:000001">aborrece</a> tuviere hambre, dale de <a name="02002502100008" ll="SDBH:אכל:000001">comer</a> <a name="02002502100010" ll="SDBH:לֶחֶם:000000">pan</a>,Y si tuviere <a name="02002502100016" ll="SDBH:צָמֵא:000000">sed</a>, dale de <a name="02002502100018" ll="SDBH:שׁקה:000002">beber</a> <a name="02002502100020" ll="SDBH:מַיִם:000000">agua</a>;
    
\v 22 Porque <a name="02002502200004" ll="SDBH:גֶּחָל:000000">ascuas</a> <a name="02002502200008" ll="SDBH:חתה:000000">amontonarás</a> <a name="02002502200010">sobre</a> <a name="02002502200012" ll="SDBH:רֹאשׁ:000000">su</a> <a name="02002502200012" ll="SDBH:רֹאשׁ:000000">cabeza</a>,Y <a name="02002502200016" ll="SDBH:יהוה:000000">Jehová</a> te lo <a name="02002502200018" ll="SDBH:שׁלם:000007">pagará</a>.
    
\v 23 El <a name="02002502300002" ll="SDBH:רוּחַ:000004">viento</a> del <a name="02002502300004" ll="SDBH:צָפֹון:000001">norte</a> <a name="02002502300006" ll="SDBH:חיל:000007">ahuyenta</a> la lluvia,Y el <a name="02002502300012" ll="SDBH:פָּנֶה:000000">rostro</a> <a name="02002502300014" ll="SDBH:זעם:000000">airado</a> la <a name="02002502300016" ll="SDBH:לָשֹׁון:000000">lengua</a> <a name="02002502300018" ll="SDBH:סֵתֶר:000001">detractora</a>.
    
\v 24 <a name="02002502400002" ll="SDBH:טֹוב:000002">Mejor</a> es <a name="02002502400004">estar</a> <a name="02002502400006">en</a> un <a name="02002502400008" ll="SDBH:פִּנָּה:000000">rincón</a> del <a name="02002502400010" ll="SDBH:גָּג:000000">terrado</a>,Que con <a name="02002502400014" ll="SDBH:אִשָּׁה:000000">mujer</a> rencillosa en <a name="02002502400022" ll="SDBH:בַּיִת:000000">casa</a> <a name="02002502400024" ll="SDBH:חֶבֶר:000000">espaciosa</a>.
    
\v 25 Como el <a name="02002502500002" ll="SDBH:מַיִם:000000">agua</a> <a name="02002502500004" ll="SDBH:קַר:000000">fría</a> <a name="02002502500006">al</a> <a name="02002502500008" ll="SDBH:נֶפֶשׁ:000001">alma</a> <a name="02002502500010" ll="SDBH:עָיֵף:000000">sedienta</a>,Así son las <a name="02002502500016" ll="SDBH:טֹוב:000002">buenas</a> <a name="02002502500014" ll="SDBH:שְׁמוּעָה:000000">nuevas</a> de <a name="02002502500022" ll="SDBH:מֶרְחָק:000000">lejanas</a> <a name="02002502500020" ll="SDBH:אֶרֶץ:000000">tierras</a>.
    
\v 26 Como <a name="02002502600002" ll="SDBH:מַעְיָן:000000">fuente</a> turbia y <a name="02002502600008" ll="SDBH:מָקֹור:000000">manantial</a> <a name="02002502600010" ll="SDBH:שׁחת:000000">corrompido</a>,Es el <a name="02002502600012" ll="SDBH:צַדִּיק:000000">justo</a> que <a name="02002502600014" ll="SDBH:מוט:000002">cae</a> <a name="02002502600018">delante</a> del <a name="02002502600020" ll="SDBH:רָשָׁע:000000">impío</a>.
    
\v 27 <a name="02002502700002" ll="SDBH:אכל:000000">Comer</a> <a name="02002502700006">mucha</a> <a name="02002502700004" ll="SDBH:דְּבַשׁ:000000">miel</a> <a name="02002502700008" ll="SDBH:לֹא:000000">no</a> es <a name="02002502700010" ll="SDBH:טֹוב:000002">bueno</a>,Ni el <a name="02002502700014" ll="SDBH:חֵקֶר:000000" tl="HOTTP2811">buscar</a> la propia <a name="02002502700016" ll="SDBH:כָּבֹוד:000000" tl="HOTTP2811">gloria</a> es <a name="02002502700018" ll="SDBH:כָּבֹוד:000000" tl="HOTTP2811">gloria</a>.
    
\v 28 Como <a name="02002502800002" ll="SDBH:עִיר:000000">ciudad</a> <a name="02002502800004" ll="SDBH:פרץ:000001">derribada</a> y <a name="02002502800006">sin</a> <a name="02002502800008" ll="SDBH:חֹומָה:000000">muro</a>Es el <a name="02002502800010">hombre</a> cuyo <a name="02002502800020" ll="SDBH:רוּחַ:000002">espíritu</a> <a name="02002502800014">no</a> tiene rienda.
\c 26
\v 1 Como no conviene la <a name="02002600100006" ll="SDBH:שֶׁלֶג:000000">nieve</a> en el <a name="02002600100012" ll="SDBH:קַיִץ:000000">verano</a>, ni la <a name="02002600100020" ll="SDBH:מָטָר:000000">lluvia</a> en la <a name="02002600100026" ll="SDBH:קָצִיר:000003">siega</a>,<a name="02002600100028">Así</a> <a name="02002600100030" ll="SDBH:לֹא:000000">no</a> <a name="02002600100032" ll="SDBH:נָאוֶה:000001">conviene</a> al <a name="02002600100036" ll="SDBH:כְּסִיל:000000">necio</a> la <a name="02002600100038" ll="SDBH:כָּבֹוד:000000">honra</a>.
    
\v 2 Como el <a name="02002600200006" ll="SDBH:צִפֹּור:000000">gorrión</a> en su <a name="02002600200010" ll="SDBH:נוד:000006">vagar</a>, y como la golondrina en su <a name="02002600200020" ll="SDBH:עוף:000001">vuelo</a>,<a name="02002600200022">Así</a> la <a name="02002600200024" ll="SDBH:קְלָלָה:000000">maldición</a> nunca <a name="02002600200032">vendrá</a> sin <a name="02002600200026" ll="SDBH:חִנָּם:000001">causa</a>.
    
\v 3 El <a name="02002600300002" ll="SDBH:שֹׁוט:000000">látigo</a> para el <a name="02002600300008" ll="SDBH:סוּס:000000">caballo</a>, el <a name="02002600300010" ll="SDBH:מֶתֶג:000000">cabestro</a> para el <a name="02002600300016" ll="SDBH:חֲמֹור:000000">asno</a>,Y la <a name="02002600300020" ll="SDBH:שֵׁבֶט:000000">vara</a> para la <a name="02002600300024" ll="SDBH:גֵּו:000000">espalda</a> del <a name="02002600300026" ll="SDBH:כְּסִיל:000000">necio</a>.
    
\v 4 <a name="02002600400002" ll="SDBH:אַל:000000">Nunca</a> <a name="02002600400004" ll="SDBH:ענה:000000">respondas</a> al <a name="02002600400006" ll="SDBH:כְּסִיל:000000">necio</a> de acuerdo con <a name="02002600400010" ll="SDBH:אִוֶּלֶת:000000">su</a> <a name="02002600400010" ll="SDBH:אִוֶּלֶת:000000">necedad</a>,Para que <a name="02002600400012">no</a> <a name="02002600400014" ll="SDBH:שׁוה:000000">seas</a> <a name="02002600400020">tú</a> <a name="02002600400018" ll="SDBH:גַּם:000000">también</a> como él.
    
\v 5 <a name="02002600500002" ll="SDBH:ענה:000000">Responde</a> al <a name="02002600500004" ll="SDBH:כְּסִיל:000000">necio</a> como merece <a name="02002600500008" ll="SDBH:אִוֶּלֶת:000000">su</a> <a name="02002600500008" ll="SDBH:אִוֶּלֶת:000000">necedad</a>,Para que <a name="02002600500010">no</a> <a name="02002600500012">se</a> <a name="02002600500012">estime</a> <a name="02002600500014" ll="SDBH:חָכָם, קסם:000000">sabio</a> en su propia <a name="02002600500018" ll="SDBH:עַיִן:000000">opinión</a>.
    
\v 6 Como el que se corta los <a name="02002600600002" ll="SDBH:קצה:000000">pies</a> y <a name="02002600600004" ll="SDBH:רֶגֶל:000000">bebe</a> su <a name="02002600600008" ll="SDBH:שׁתה:000001" tl="HOTTP2812">daño</a>,<a name="02002600600006" ll="SDBH:חָמָס:000000" tl="HOTTP2812">Así</a> es el que <a name="02002600600010" ll="SDBH:שׁלח:000005">envía</a> <a name="02002600600012" ll="SDBH:דָּבָר:000000">recado</a> por <a name="02002600600016">mano</a> de un <a name="02002600600018" ll="SDBH:כְּסִיל:000000">necio</a>.
    
\v 7 Las piernas del cojo <a name="02002600700008" ll="SDBH:פִּסֵּחַ:000000" tl="HOTTP2813">penden</a> inútiles;<a name="02002600700004" ll="SDBH:שֹׁוק:000000" tl="HOTTP2813">Así</a> es el <a name="02002600700012" ll="SDBH:מָשָׁל:000000">proverbio</a> en la <a name="02002600700016" ll="SDBH:פֶּה:000000">boca</a> del <a name="02002600700018" ll="SDBH:כְּסִיל:000000">necio</a>.
    
\v 8 Como quien liga la <a name="02002600800006" ll="SDBH:אֶבֶן:000001">piedra</a> en la <a name="02002600800010" ll="SDBH:מַרְגֵּמָה:000000">honda</a>,<a name="02002600800012">Así</a> hace el que <a name="02002600800014">da</a> <a name="02002600800020" ll="SDBH:כָּבֹוד:000000">honra</a> al <a name="02002600800018" ll="SDBH:כְּסִיל:000000">necio</a>.
    
\v 9 <a name="02002600900002" ll="SDBH:חֹוחַ:000000">Espinas</a> <a name="02002600900004" ll="SDBH:עלה:000000">hincadas</a> en <a name="02002600900008">mano</a> del <a name="02002600900010" ll="SDBH:שִׁכֹּור:000000">embriagado</a>,Tal es el <a name="02002600900014" ll="SDBH:מָשָׁל:000000">proverbio</a> en la <a name="02002600900018" ll="SDBH:פֶּה:000000">boca</a> de los <a name="02002600900020" ll="SDBH:כְּסִיל:000000">necios</a>.
    
\v 10 Como <a name="02002601000002" ll="SDBH:רַב:000001">arquero</a> que a <a name="02002601000006">todos</a> hiere,Es el que toma a <a name="02002601000010" ll="SDBH:שׂכר:000001" tl="HOTTP2814">sueldo</a> <a name="02002601000012" ll="SDBH:כְּסִיל:000000" tl="HOTTP2814">insensatos</a> y <a name="02002601000018" ll="SDBH:עבר:000000" tl="HOTTP2814">vagabundos</a>.
    
