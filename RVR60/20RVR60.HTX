\v 14 Hay generación <a name="02003001400002" ll="SDBH:דֹּור:001001">cuyos</a> dientes <a name="02003001400006" ll="SDBH:שֵׁן:000000">son</a> <a name="02003001400004" ll="SDBH:חֶרֶב:000001">espadas</a>, y sus muelas <a name="02003001400012" ll="SDBH:מְתַלְּעֹות:000000">cuchillos</a>,<a name="02003001400010" ll="SDBH:מַאֲכֶלֶת:000000">Para</a> <a name="02003001400016" ll="SDBH:אכל:000002">devorar</a> a los <a name="02003001400018" ll="SDBH:עָנִי:000000">pobres</a> de la <a name="02003001400022" ll="SDBH:אֶרֶץ:000000">tierra</a>, y a los <a name="02003001400026" ll="SDBH:אֶבְיֹון:000000">menesterosos</a> de entre los <a name="02003001400030" ll="SDBH:אָדָם:000000" tl="HOTTP2844">hombres</a>.
    
\v 15 La <a name="02003001500004" ll="SDBH:עֲלוּקָה:000000">sanguijuela</a> tiene <a name="02003001500006" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="02003001500008" ll="SDBH:בַּת:000000">hijas</a> que dicen: ¡<a name="02003001500010;02003001500012">Dame</a>! ¡dame!<a name="02003001500014" ll="SDBH:שָׁלֹשׁ:000000">Tres</a> cosas <a name="02003001500016">hay</a> que <a name="02003001500018" ll="SDBH:לֹא:000000">nunca</a> <a name="02003001500020" ll="SDBH:שׂבע:000006">se</a> <a name="02003001500020" ll="SDBH:שׂבע:000006">sacian</a>;Aun la <a name="02003001500022" ll="SDBH:אַרְבַּע:000000">cuarta</a> <a name="02003001500024" ll="SDBH:לֹא:000000">nunca</a> <a name="02003001500026" ll="SDBH:אמר:000000">dice</a>: ¡<a name="02003001500028" ll="SDBH:הֹון:000001">Basta</a>!
    
\v 16 El <a name="02003001600002" ll="SDBH:שְׁאֹול:000000">Seol</a>, la <a name="02003001600008" ll="SDBH:רֶחֶם:000000">matriz</a> <a name="02003001600006" ll="SDBH:עֹצֶר:000000">estéril</a>,La <a name="02003001600010" ll="SDBH:אֶרֶץ:000000">tierra</a> que <a name="02003001600012" ll="SDBH:לֹא:000000">no</a> <a name="02003001600014" ll="SDBH:שׂבע:000000">se</a> <a name="02003001600014" ll="SDBH:שׂבע:000000">sacia</a> de <a name="02003001600016" ll="SDBH:מַיִם:000000">aguas</a>,Y el <a name="02003001600020" ll="SDBH:אֵשׁ:000000">fuego</a> que <a name="02003001600022" ll="SDBH:לֹא:000000">jamás</a> <a name="02003001600024" ll="SDBH:אמר:000000">dice</a>: ¡<a name="02003001600026" ll="SDBH:הֹון:000001">Basta</a>!
    
\v 17 El <a name="02003001700002" ll="SDBH:עַיִן:000000">ojo</a> que <a name="02003001700004" ll="SDBH:לעג:000000">escarnece</a> a su <a name="02003001700008" ll="SDBH:אָב:000000">padre</a>Y <a name="02003001700012" ll="SDBH:בוז:000000">menosprecia</a> la <a name="02003001700016" ll="SDBH:יְקָהָה:000000" tl="HOTTP2845">enseñanza</a> de la <a name="02003001700018" ll="SDBH:אֵם:000000" il="Motherhood1912" tl="HOTTP2845">madre</a>,Los <a name="02003001700022" ll="SDBH:עֹורֵב:000000">cuervos</a> de la <a name="02003001700024" ll="SDBH:נַחַל:000000">cañada</a> <a name="02003001700020" ll="SDBH:נקר:000001">lo</a> <a name="02003001700020" ll="SDBH:נקר:000001">saquen</a>,Y <a name="02003001700028" ll="SDBH:אכל:000000">lo</a> <a name="02003001700028" ll="SDBH:אכל:000000">devoren</a> los <a name="02003001700030" ll="SDBH:בֵּן:000001">hijos</a> del <a name="02003001700032" ll="SDBH:נֶשֶׁר:000000">águila</a>.
    
\v 18 <a name="02003001800002" ll="SDBH:שָׁלֹשׁ:000000">Tres</a> <a name="02003001800002" ll="SDBH:שָׁלֹשׁ:000000">cosas</a> <a name="02003001800008">me</a> <a name="02003001800006" ll="SDBH:פלא:000000">son</a> <a name="02003001800006" ll="SDBH:פלא:000000">ocultas</a>;<a name="02003001800018" ll="SDBH:לֹא:000000">Aun</a> <a name="02003001800018" ll="SDBH:לֹא:000000">tampoco</a> <a name="02003001800020">sé</a> la <a name="02003001800016" ll="SDBH:אַרְבַּע:000000">cuarta</a>:
    
\v 19 El <a name="02003001900002" ll="SDBH:דֶּרֶךְ:000000">rastro</a> del <a name="02003001900006" ll="SDBH:נֶשֶׁר:000000">águila</a> en el <a name="02003001900012" ll="SDBH:שָׁמַיִם:000000">aire</a>;El <a name="02003001900014" ll="SDBH:דֶּרֶךְ:000000">rastro</a> de la <a name="02003001900016" ll="SDBH:נָחָשׁ:000000">culebra</a> <a name="02003001900018">sobre</a> la <a name="02003001900020" ll="SDBH:צוּר:000000">peña</a>;El <a name="02003001900022" ll="SDBH:דֶּרֶךְ:000000">rastro</a> de la <a name="02003001900024" ll="SDBH:אֳנִיָּה:000000">nave</a> en <a name="02003001900028" ll="SDBH:לֵב:000004">medio</a> del <a name="02003001900030" ll="SDBH:יָם:000000">mar</a>;Y el <a name="02003001900034" ll="SDBH:דֶּרֶךְ:000008">rastro</a> del <a name="02003001900036" ll="SDBH:גֶּבֶר:000000">hombre</a> en la <a name="02003001900040" ll="SDBH:עַלְמָה:000000">doncella</a>.
    
\v 20 El <a name="02003002000004" ll="SDBH:דֶּרֶךְ:000007">proceder</a> de la <a name="02003002000006" ll="SDBH:אִשָּׁה:000000">mujer</a> <a name="02003002000008" ll="SDBH:נאף:000000">adúltera</a> es <a name="02003002000002">así</a>:<a name="02003002000010" ll="SDBH:אכל:000000">Come</a>, y <a name="02003002000014" ll="SDBH:מחה:000000">limpia</a> <a name="02003002000016" ll="SDBH:פֶּה:000000">su</a> <a name="02003002000016" ll="SDBH:פֶּה:000000">boca</a>Y <a name="02003002000020" ll="SDBH:אמר:000000">dice</a>: <a name="02003002000022" ll="SDBH:לֹא:000000">No</a> he hecho <a name="02003002000024" ll="SDBH:פעל:000000">maldad</a>.
    
\v 21 <a name="02003002100002">Por</a> <a name="02003002100004" ll="SDBH:שָׁלֹשׁ:000000">tres</a> cosas <a name="02003002100006" ll="SDBH:רגז:000000">se</a> <a name="02003002100006" ll="SDBH:רגז:000000">alborota</a> la <a name="02003002100008" ll="SDBH:אֶרֶץ:000000">tierra</a>,Y la <a name="02003002100014" ll="SDBH:אַרְבַּע:000000">cuarta</a> ella <a name="02003002100016" ll="SDBH:לֹא:000000">no</a> <a name="02003002100018" ll="SDBH:יכל:000000">puede</a> <a name="02003002100020" ll="SDBH:נשׂא:000015">sufrir</a>:
    
\v 22 <a name="02003002200002">Por</a> el <a name="02003002200004" ll="SDBH:עֶבֶד:000000">siervo</a> cuando <a name="02003002200008" ll="SDBH:מלך:000000">reina</a>;Por el <a name="02003002200012" ll="SDBH:נָבָל:000000">necio</a> cuando <a name="02003002200016" ll="SDBH:שׂבע:000000">se</a> <a name="02003002200016" ll="SDBH:שׂבע:000000">sacia</a> de <a name="02003002200018" ll="SDBH:לֶחֶם:000000">pan</a>;
    
\v 23 <a name="02003002300002">Por</a> la mujer <a name="02003002300004" ll="SDBH:שׂנא:000000">odiada</a> cuando <a name="02003002300008" ll="SDBH:בעל:000001">se</a> <a name="02003002300008" ll="SDBH:בעל:000001">casa</a>;Y por la <a name="02003002300012" ll="SDBH:שִׁפְחָה:000000">sierva</a> cuando <a name="02003002300016" ll="SDBH:ירשׁ:000000" tl="HOTTP2846">hereda</a> a su señora.
    
\v 24 <a name="02003002400002" ll="SDBH:אַרְבַּע:000000">Cuatro</a> <a name="02003002400004">cosas</a> son de las más <a name="02003002400006" ll="SDBH:קָטָן:000000">pequeñas</a> de la <a name="02003002400008" ll="SDBH:אֶרֶץ:000000">tierra</a>,Y las mismas <a name="02003002400012">son</a> más sabias <a name="02003002400016" ll="SDBH:חכם:000000" tl="HOTTP2847">que</a> los <a name="02003002400014" ll="SDBH:חָכָם, קסם:000000" tl="HOTTP2847">sabios</a>:
    
\v 25 Las <a name="02003002500004" ll="SDBH:נְמָלָה:000000">hormigas</a>, <a name="02003002500006" ll="SDBH:עַם:000000">pueblo</a> <a name="02003002500008" ll="SDBH:לֹא:000000">no</a> <a name="02003002500010" ll="SDBH:עַז:000000">fuerte</a>,Y en el <a name="02003002500020" ll="SDBH:קַיִץ:000000">verano</a> <a name="02003002500014" ll="SDBH:כון:000001">preparan</a> <a name="02003002500022" ll="SDBH:לֶחֶם:000000">su</a> <a name="02003002500022" ll="SDBH:לֶחֶם:000000">comida</a>;
    
\v 26 Los <a name="02003002600002" ll="SDBH:שָׁפָן:000000">conejos</a>, <a name="02003002600004" ll="SDBH:עַם:000000">pueblo</a> <a name="02003002600006" ll="SDBH:לֹא:000000">nada</a> <a name="02003002600008" ll="SDBH:עָצוּם:000002">esforzado</a>,Y <a name="02003002600012" ll="SDBH:שׂים:000006">ponen</a> <a name="02003002600020" ll="SDBH:בַּיִת:000000">su</a> <a name="02003002600020" ll="SDBH:בַּיִת:000000">casa</a> en la <a name="02003002600018" ll="SDBH:סֶלַע:000000">piedra</a>;
    
\v 27 Las <a name="02003002700010" ll="SDBH:אַרְבֶּה:000000">langostas</a>, que <a name="02003002700004">no</a> tienen <a name="02003002700002" ll="SDBH:מֶלֶךְ:000000">rey</a>,Y <a name="02003002700014" ll="SDBH:יצא:000000">salen</a> <a name="02003002700018">todas</a> por <a name="02003002700016" ll="SDBH:חצץ:000000">cuadrillas</a>;
    
\v 28 La <a name="02003002800002" ll="SDBH:שְׂמָמִית:000000">araña</a> que <a name="02003002800008" ll="SDBH:תפשׂ:000000" tl="HOTTP2848">atrapas</a> con la <a name="02003002800006">mano</a>,Y <a name="02003002800012">está</a> en <a name="02003002800016" ll="SDBH:הֵיכָל:000000">palacios</a> de <a name="02003002800018" ll="SDBH:מֶלֶךְ:000000">rey</a>.
    
\v 29 <a name="02003002900002" ll="SDBH:שָׁלֹשׁ:000000">Tres</a> <a name="02003002900004">cosas</a> hay de hermoso <a name="02003002900008" ll="SDBH:צַעַד:000000">andar</a>,Y la <a name="02003002900012" ll="SDBH:אַרְבַּע:000000">cuarta</a> pasea muy bien:
    
\v 30 El <a name="02003003000002" ll="SDBH:לַיִשׁ:000000">león</a>, <a name="02003003000004" ll="SDBH:גִּבֹּור:000000">fuerte</a> entre todos los <a name="02003003000010" ll="SDBH:בְּהֵמָה:000000">animales</a>,Que <a name="02003003000014" ll="SDBH:לֹא:000000">no</a> <a name="02003003000016" ll="SDBH:שׁוב:000023">vuelve</a> atrás <a name="02003003000020">por</a> nada;
    
\v 31 El <a name="02003003100002;02003003100004" ll="SDBH:זַרְזִיר:000000;SDBH:מָתְנַיִם:000001">ceñido</a> <a name="02003003100002;02003003100004" ll="SDBH:זַרְזִיר:000000;SDBH:מָתְנַיִם:000001">de</a> <a name="02003003100002;02003003100004" ll="SDBH:זַרְזִיר:000000;SDBH:מָתְנַיִם:000001">lomos</a>; <a name="02003003100006" ll="SDBH:אֹו:000001">asimismo</a> el <a name="02003003100008" ll="SDBH:תַּיִשׁ:000000">macho</a> <a name="02003003100008" ll="SDBH:תַּיִשׁ:000000">cabrío</a>;Y el <a name="02003003100012" ll="SDBH:מֶלֶךְ:000000" tl="HOTTP2849">rey</a>, a <a name="02003003100014" ll="SDBH:אַלְקוּם:000000" tl="HOTTP2849">quien</a> <a name="02003003100014" ll="SDBH:אַלְקוּם:000000" tl="HOTTP2849">nadie</a> <a name="02003003100014" ll="SDBH:אַלְקוּם:000000" tl="HOTTP2849">resiste</a>.
    
\v 32 <a name="02003003200002;02003003200012">Si</a> <a name="02003003200004" ll="SDBH:נבל:001000">neciamente</a> has procurado enaltecerte,O si <a name="02003003200014" ll="SDBH:זמם:000000">has</a> <a name="02003003200014" ll="SDBH:זמם:000000">pensado</a> hacer mal,Pon el <a name="02003003200016">dedo</a> sobre tu <a name="02003003200020" ll="SDBH:פֶּה:000000">boca</a>.
    
\v 33 Ciertamente el que <a name="02003003300004" ll="SDBH:מִיץ:000000">bate</a> la <a name="02003003300006" ll="SDBH:חָלָב:000000">leche</a> <a name="02003003300008" ll="SDBH:יצא:000020">sacará</a> <a name="02003003300010" ll="SDBH:חֶמְאָה:000000">mantequilla</a>,Y el que recio se <a name="02003003300014" ll="SDBH:מִיץ:000000">suena</a> las narices <a name="02003003300018" ll="SDBH:יצא:000026">sacará</a> <a name="02003003300020" ll="SDBH:דָּם:000000">sangre</a>;Y el que <a name="02003003300024" ll="SDBH:מִיץ:000001">provoca</a> la ira <a name="02003003300028" ll="SDBH:יצא:000026">causará</a> contienda.
\c 31
\v 1 <a name="02003100100002" ll="SDBH:דָּבָר:000000">Palabras</a> del <a name="02003100100006" ll="SDBH:מֶלֶךְ:000000" tl="HOTTP2850">rey</a> <a name="02003100100004" ll="SDBH:לְמוּאֵל:000000">Lemuel</a>; la profecía con <a name="02003100100010">que</a> le <a name="02003100100012" ll="SDBH:יסר:000005">enseñó</a> <a name="02003100100014" ll="SDBH:אֵם:000000">su</a> <a name="02003100100014" ll="SDBH:אֵם:000000">madre</a>.
    
\v 2 ¿<a name="02003100200002" ll="SDBH:מָה:000000">Qué</a>, <a name="02003100200004" ll="SDBH:בַּר:000000" tl="HOTTP2851">hijo</a> <a name="02003100200004" ll="SDBH:בַּר:000000" tl="HOTTP2851">mío</a>? ¿y <a name="02003100200008" ll="SDBH:מָה:000000">qué</a>, <a name="02003100200010" ll="SDBH:בַּר:000000">hijo</a> de <a name="02003100200012" ll="SDBH:בֶּטֶן:000000">mi</a> <a name="02003100200012" ll="SDBH:בֶּטֶן:000000">vientre</a>?¿Y <a name="02003100200016" ll="SDBH:מָה:000000">qué</a>, <a name="02003100200018" ll="SDBH:בַּר:000000">hijo</a> de <a name="02003100200020" ll="SDBH:נֶדֶר:000000">mis</a> <a name="02003100200020" ll="SDBH:נֶדֶר:000000">deseos</a>?
    
\v 3 <a name="02003100300002" ll="SDBH:אַל:000000">No</a> <a name="02003100300004">des</a> a las <a name="02003100300010" ll="SDBH:אִשָּׁה:000000">mujeres</a> <a name="02003100300012" ll="SDBH:חַיִל:000000">tu</a> <a name="02003100300012" ll="SDBH:חַיִל:000000">fuerza</a>,Ni <a name="02003100300016" ll="SDBH:דֶּרֶךְ:000007">tus</a> <a name="02003100300016" ll="SDBH:דֶּרֶךְ:000007">caminos</a> a lo que <a name="02003100300020" ll="SDBH:מחה:000004" tl="HOTTP2852">destruye</a> a los <a name="02003100300022" ll="SDBH:מֶלֶךְ:000000" tl="HOTTP2852">reyes</a>.
    
\v 4 <a name="02003100400002;02003100400012" ll="SDBH:אַל:000000">No</a> es de los <a name="02003100400008" ll="SDBH:מֶלֶךְ:000000">reyes</a>, oh <a name="02003100400010" ll="SDBH:לְמוּאֵל:000000">Lemuel</a>, no es de los <a name="02003100400018" ll="SDBH:מֶלֶךְ:000000">reyes</a> <a name="02003100400020" ll="SDBH:שׁתה:000000">beber</a> <a name="02003100400022" ll="SDBH:יַיִן:000000">vino</a>,Ni de los <a name="02003100400028" ll="SDBH:רזן:000000">príncipes</a> la <a name="02003100400034" ll="SDBH:שֵׁכָר:000000" tl="HOTTP2853">sidra</a>;
    
\v 5 <a name="02003100500002">No</a> sea que <a name="02003100500004" ll="SDBH:שׁתה:000000">bebiendo</a> <a name="02003100500008" ll="SDBH:שׁכח:000000">olviden</a> la <a name="02003100500010" ll="SDBH:חקק:000002">ley</a>,Y <a name="02003100500014" ll="SDBH:שׁנה:000004">perviertan</a> el <a name="02003100500016" ll="SDBH:דִּין:000001">derecho</a> de <a name="02003100500018">todos</a> los <a name="02003100500020;02003100500022" ll="SDBH:בֵּן:000007;SDBH:עֳנִי:000000">afligidos</a>.
    
\v 6 <a name="02003100600002">Dad</a> la <a name="02003100600004" ll="SDBH:שֵׁכָר:000000">sidra</a> al <a name="02003100600008" ll="SDBH:אבד:000004">desfallecido</a>,Y el <a name="02003100600012" ll="SDBH:יַיִן:000000">vino</a> a los de <a name="02003100600016" ll="SDBH:מַר:001002">amargado</a> <a name="02003100600018" ll="SDBH:נֶפֶשׁ:000001">ánimo</a>.
    
\v 7 <a name="02003100700002" ll="SDBH:שׁתה:000000">Beban</a>, y <a name="02003100700006" ll="SDBH:שׁכח:000000">olvídense</a> de <a name="02003100700008" ll="SDBH:רֵישׁ:000000">su</a> <a name="02003100700008" ll="SDBH:רֵישׁ:000000">necesidad</a>,Y de su miseria <a name="02003100700014" ll="SDBH:לֹא:000000">no</a> <a name="02003100700016" ll="SDBH:זכר:000000">se</a> <a name="02003100700016" ll="SDBH:זכר:000000">acuerden</a> <a name="02003100700018">más</a>.
    
\v 8 <a name="02003100800002" ll="SDBH:פתח:000015">Abre</a> <a name="02003100800004" ll="SDBH:פֶּה:000000">tu</a> <a name="02003100800004" ll="SDBH:פֶּה:000000">boca</a> por el <a name="02003100800008" ll="SDBH:אִלֵּם:000000">mudo</a><a name="02003100800010">En</a> el <a name="02003100800012" ll="SDBH:דִּין:000001">juicio</a> de <a name="02003100800014">todos</a> los <a name="02003100800016;02003100800018" ll="SDBH:בֵּן:000007;SDBH:חֲלֹוף:000000">desvalidos</a>.
    
\v 9 <a name="02003100900002" ll="SDBH:פתח:000015">Abre</a> <a name="02003100900004" ll="SDBH:פֶּה:000000">tu</a> <a name="02003100900004" ll="SDBH:פֶּה:000000">boca</a>, <a name="02003100900006" ll="SDBH:שׁפט:000000">juzga</a> con <a name="02003100900008" ll="SDBH:צֶדֶק:000001">justicia</a>,Y <a name="02003100900012" ll="SDBH:דין:000000">defiende</a> la causa del <a name="02003100900014" ll="SDBH:עָנִי:000000">pobre</a> y del <a name="02003100900018" ll="SDBH:אֶבְיֹון:000000">menesteroso</a>.
    
\v 10 <a name="02003101000002" ll="SDBH:אִשָּׁה:000000">Mujer</a> <a name="02003101000004" ll="SDBH:חַיִל:000000">virtuosa</a>, ¿<a name="02003101000006" ll="SDBH:מִי:000000">quién</a> la <a name="02003101000008" ll="SDBH:מצא:000000">hallará</a>?Porque <a name="02003101000018" ll="SDBH:מֶכֶר:000001">su</a> <a name="02003101000018" ll="SDBH:מֶכֶר:000001">estima</a> <a name="02003101000012" ll="SDBH:רָחֹוק:000002">sobrepasa</a> largamente a la de las piedras <a name="02003101000016" ll="SDBH:פְּנִינִים:000000">preciosas</a>.
    
