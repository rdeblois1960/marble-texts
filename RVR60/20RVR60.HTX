\v 13 <a name="02001901300002" ll="SDBH:הַוָּה:001000">Dolor</a> es para <a name="02001901300006" ll="SDBH:אָב:000000">su</a> <a name="02001901300006" ll="SDBH:אָב:000000">padre</a> el <a name="02001901300008" ll="SDBH:בֵּן:000000">hijo</a> <a name="02001901300010" ll="SDBH:כְּסִיל:000000">necio</a>,Y <a name="02001901300014" ll="SDBH:דֶּלֶף:000000">gotera</a> <a name="02001901300016" ll="SDBH:טרד:000000">continua</a> las <a name="02001901300018" ll="SDBH:מִדְיָן:000000">contiendas</a> de la <a name="02001901300020" ll="SDBH:אִשָּׁה:000000">mujer</a>.
    
\v 14 La <a name="02001901400002" ll="SDBH:בַּיִת:000000">casa</a> y las riquezas <a name="02001901400006" ll="SDBH:הֹון:000000">son</a> herencia de los <a name="02001901400010" ll="SDBH:אָב:000001">padres</a>;Mas de <a name="02001901400016" ll="SDBH:יהוה:000000">Jehová</a> la <a name="02001901400018" ll="SDBH:אִשָּׁה:000000">mujer</a> <a name="02001901400020" ll="SDBH:שׂכל:001000">prudente</a>.
    
\v 15 La pereza <a name="02001901500002" ll="SDBH:עַצְלָה:000000">hace</a> <a name="02001901500004" ll="SDBH:נפל:000022">caer</a> en profundo <a name="02001901500006" ll="SDBH:תַּרְדֵּמָה:000000">sueño</a>,Y el <a name="02001901500010" ll="SDBH:נֶפֶשׁ:000004">alma</a> <a name="02001901500012" ll="SDBH:רְמִיָּה:000000">negligente</a> padecerá <a name="02001901500014" ll="SDBH:רעב:000000">hambre</a>.
    
\v 16 El que <a name="02001901600002" ll="SDBH:שׁמר:000003">guarda</a> el <a name="02001901600004" ll="SDBH:מִצְוָה:000000">mandamiento</a> <a name="02001901600006" ll="SDBH:שׁמר:000007">guarda</a> <a name="02001901600008" ll="SDBH:נֶפֶשׁ:000001">su</a> <a name="02001901600008" ll="SDBH:נֶפֶשׁ:000001">alma</a>;Mas el que <a name="02001901600010" ll="SDBH:בזה:000000">menosprecia</a> <a name="02001901600012" ll="SDBH:דֶּרֶךְ:000007" tl="HOTTP2754">sus</a> <a name="02001901600012" ll="SDBH:דֶּרֶךְ:000007" tl="HOTTP2754">caminos</a> <a name="02001901600014" ll="SDBH:מות:000000">morirá</a>.
    
\v 17 A <a name="02001901700004" ll="SDBH:יהוה:000000">Jehová</a> <a name="02001901700002" ll="SDBH:לוה:001001">presta</a> el <a name="02001901700006" ll="SDBH:חנן:000000">que</a> <a name="02001901700006" ll="SDBH:חנן:000000">da</a> al <a name="02001901700008" ll="SDBH:דַּל:001000">pobre</a>,Y el <a name="02001901700012" ll="SDBH:גְּמוּל:000000">bien</a> <a name="02001901700012" ll="SDBH:גְּמוּל:000000">que</a> <a name="02001901700012" ll="SDBH:גְּמוּל:000000">ha</a> <a name="02001901700012" ll="SDBH:גְּמוּל:000000">hecho</a>, <a name="02001901700014" ll="SDBH:שׁלם:000007">se</a> <a name="02001901700016">lo</a> <a name="02001901700014" ll="SDBH:שׁלם:000007">volverá</a> <a name="02001901700014" ll="SDBH:שׁלם:000007">a</a> <a name="02001901700014" ll="SDBH:שׁלם:000007">pagar</a>.
    
\v 18 <a name="02001901800002" ll="SDBH:יסר:000000">Castiga</a> a <a name="02001901800004" ll="SDBH:בֵּן:000000">tu</a> <a name="02001901800004" ll="SDBH:בֵּן:000000">hijo</a> en tanto que <a name="02001901800008">hay</a> <a name="02001901800010" ll="SDBH:תִּקְוָה:001000">esperanza</a>;Mas <a name="02001901800018" ll="SDBH:אַל:000000">no</a> <a name="02001901800020" ll="SDBH:נשׂא:000052">se</a> <a name="02001901800020" ll="SDBH:נשׂא:000052">apresure</a> <a name="02001901800022" ll="SDBH:נֶפֶשׁ:000001">tu</a> <a name="02001901800022" ll="SDBH:נֶפֶשׁ:000001">alma</a> para <a name="02001901800016" ll="SDBH:מות:000001">destruirlo</a>.
    
\v 19 El de <a name="02001901900002" ll="SDBH:גֹּורָל:000004;SDBH:גרל:000000">grande</a> <a name="02001901900006" ll="SDBH:חֵמָה:001001">ira</a> <a name="02001901900008" ll="SDBH:נשׂא:000015">llevará</a> la <a name="02001901900010" ll="SDBH:עֹנֶשׁ:000000">pena</a>;Y <a name="02001901900014">si</a> usa de violencias, <a name="02001901900022">añadirá</a> nuevos males.
    
\v 20 <a name="02001902000002">Escucha</a> el <a name="02001902000004" ll="SDBH:עֵצָה:000001">consejo</a>, y <a name="02001902000008" ll="SDBH:קבל:000002">recibe</a> la <a name="02001902000010" ll="SDBH:מוּסָר:000000">corrección</a>,Para que seas <a name="02001902000014" ll="SDBH:חכם:000000">sabio</a> en <a name="02001902000018" ll="SDBH:אַחֲרִית:000001">tu</a> <a name="02001902000018" ll="SDBH:אַחֲרִית:000001">vejez</a>.
    
\v 21 <a name="02001902100002" ll="SDBH:רַב:000001">Muchos</a> <a name="02001902100004" ll="SDBH:מַחֲשָׁבָה:000000">pensamientos</a> hay en el <a name="02001902100008" ll="SDBH:לֵב:000001">corazón</a> del <a name="02001902100010">hombre</a>;Mas el <a name="02001902100014" ll="SDBH:עֵצָה:000002">consejo</a> de <a name="02001902100016" ll="SDBH:יהוה:000000">Jehová</a> <a name="02001902100020">permanecerá</a>.
    
\v 22 <a name="02001902200002" ll="SDBH:תַּאֲוָה:000001">Contentamiento</a> es a los <a name="02001902200004" ll="SDBH:אָדָם:000000">hombres</a> hacer misericordia;Pero <a name="02001902200010" ll="SDBH:טֹוב:000002">mejor</a> es el <a name="02001902200012" ll="SDBH:רושׁ:000001">pobre</a> que el <a name="02001902200016;02001902200018" ll="SDBH:כָּזָב:000001">mentiroso</a>.
    
\v 23 El <a name="02001902300002" ll="SDBH:יִרְאָה:000000">temor</a> de <a name="02001902300004" ll="SDBH:יהוה:000000">Jehová</a> es para <a name="02001902300008" ll="SDBH:חַיִּים:000000">vida</a>,Y con él <a name="02001902300014" ll="SDBH:לין:000001">vivirá</a> <a name="02001902300012" ll="SDBH:שָׂבֵעַ:000003">lleno</a> <a name="02001902300012" ll="SDBH:שָׂבֵעַ:000003">de</a> <a name="02001902300012" ll="SDBH:שָׂבֵעַ:000003">reposo</a> el hombre;No será <a name="02001902300018" ll="SDBH:פקד:000011">visitado</a> de <a name="02001902300020" ll="SDBH:רַע:000004">mal</a>.
    
\v 24 El <a name="02001902400004" ll="SDBH:עָצֵל:000000">perezoso</a> <a name="02001902400002" ll="SDBH:טמן:000001">mete</a> <a name="02001902400006">su</a> <a name="02001902400006">mano</a> en el <a name="02001902400012" ll="SDBH:צַלַּחַת:000000">plato</a>,Y <a name="02001902400020" ll="SDBH:לֹא:000000">ni</a> <a name="02001902400014" ll="SDBH:גַּם:000000">aun</a> <a name="02001902400016">a</a> <a name="02001902400018" ll="SDBH:פֶּה:000000">su</a> <a name="02001902400018" ll="SDBH:פֶּה:000000">boca</a> la <a name="02001902400022" ll="SDBH:שׁוב:000051">llevará</a>.
    
\v 25 <a name="02001902500004">Hiere</a> al <a name="02001902500002" ll="SDBH:ליץ:000000">escarnecedor</a>, y el <a name="02001902500008" ll="SDBH:פֶּתִי:000000">simple</a> se hará <a name="02001902500010" ll="SDBH:ערם:001000">avisado</a>;Y <a name="02001902500014" ll="SDBH:יכח:000004">corrigiendo</a> al <a name="02001902500018" ll="SDBH:בין:000000">entendido</a>, entenderá <a name="02001902500022" ll="SDBH:דַּעַת:000000">ciencia</a>.
    
\v 26 El que <a name="02001902600002" ll="SDBH:שׁדד:000000">roba</a> a su <a name="02001902600004" ll="SDBH:אָב:000000">padre</a> y <a name="02001902600006" ll="SDBH:ברח:000001">ahuyenta</a> a su <a name="02001902600008" ll="SDBH:אֵם:000000">madre</a>,Es <a name="02001902600010" ll="SDBH:בֵּן:000000">hijo</a> que <a name="02001902600012" ll="SDBH:בושׁ:000002">causa</a> <a name="02001902600012" ll="SDBH:בושׁ:000002">vergüenza</a> y <a name="02001902600016" ll="SDBH:חפר:001001">acarrea</a> <a name="02001902600016" ll="SDBH:חפר:001001">oprobio</a>.
    
\v 27 <a name="02001902700002" ll="SDBH:חדל:000003" tl="HOTTP2755">Cesa</a>, <a name="02001902700004" ll="SDBH:בֵּן:000009" tl="HOTTP2755">hijo</a> <a name="02001902700004" ll="SDBH:בֵּן:000009" tl="HOTTP2755">mío</a>, de <a name="02001902700008">oír</a> las <a name="02001902700010" ll="SDBH:מוּסָר:000000">enseñanzas</a>Que te <a name="02001902700014" ll="SDBH:שׁגה:000002">hacen</a> <a name="02001902700014" ll="SDBH:שׁגה:000002">divagar</a> de las <a name="02001902700018" ll="SDBH:אֵמֶר:000000">razones</a> de <a name="02001902700020" ll="SDBH:דַּעַת:000000">sabiduría</a>.
    
\v 28 El <a name="02001902800002" ll="SDBH:עֵד:000000">testigo</a> <a name="02001902800004" ll="SDBH:בְּלִיַּעַל:000000">perverso</a> <a name="02001902800006" ll="SDBH:ליץ:000003">se</a> <a name="02001902800006" ll="SDBH:ליץ:000003">burlará</a> del <a name="02001902800008" ll="SDBH:מִשְׁפָּט:000007">juicio</a>,Y la <a name="02001902800012" ll="SDBH:פֶּה:000000">boca</a> de los <a name="02001902800014" ll="SDBH:רָשָׁע:000000">impíos</a> <a name="02001902800016" ll="SDBH:בלע:000000">encubrirá</a> la <a name="02001902800018" ll="SDBH:אָוֶן:000000">iniquidad</a>.
    
\v 29 <a name="02001902900002" ll="SDBH:כון:000012">Preparados</a> están <a name="02001902900010" ll="SDBH:שֶׁפֶט:000000" tl="HOTTP2756">juicios</a> para los escarnecedores,Y <a name="02001902900014" ll="SDBH:מַהֲלֻמֹות:000000">azotes</a> para las <a name="02001902900018" ll="SDBH:גֵּו:000000">espaldas</a> de los <a name="02001902900020" ll="SDBH:כְּסִיל:000000">necios</a>.
\c 20
\v 1 El <a name="02002000100006" ll="SDBH:יַיִן:000000">vino</a> es <a name="02002000100002" ll="SDBH:ליץ:000000">escarnecedor</a>, la <a name="02002000100010" ll="SDBH:שֵׁכָר:000000">sidra</a> <a name="02002000100008" ll="SDBH:המה:000001">alborotadora</a>,Y <a name="02002000100014">cualquiera</a> que por ellos <a name="02002000100016" ll="SDBH:שׁגה:000002">yerra</a> <a name="02002000100020" ll="SDBH:לֹא:000000">no</a> es <a name="02002000100022" ll="SDBH:חכם:000000">sabio</a>.
    
\v 2 Como <a name="02002000200002" ll="SDBH:נַהַם:000000">rugido</a> de cachorro de <a name="02002000200008" ll="SDBH:כְּפִיר:000000">león</a> es el <a name="02002000200010" ll="SDBH:אֵימָה:000001">terror</a> del <a name="02002000200012" ll="SDBH:מֶלֶךְ:000000">rey</a>;El que lo <a name="02002000200014">enfurece</a> <a name="02002000200016" ll="SDBH:חטא:001000">peca</a> contra sí <a name="02002000200018" ll="SDBH:נֶפֶשׁ:000001">mismo</a>.
    
\v 3 <a name="02002000300002" ll="SDBH:כָּבֹוד:000000">Honra</a> es del <a name="02002000300008">hombre</a> dejar la contienda;Mas <a name="02002000300018">todo</a> <a name="02002000300020" ll="SDBH:אֱוִיל:000000">insensato</a> <a name="02002000300022" ll="SDBH:גלע:000000">se</a> <a name="02002000300022" ll="SDBH:גלע:000000">envolverá</a> en ella.
    
\v 4 El <a name="02002000400006" ll="SDBH:עָצֵל:000000">perezoso</a> <a name="02002000400008" ll="SDBH:לֹא:000000">no</a> <a name="02002000400010" ll="SDBH:חרשׁ:000001">ara</a> a causa del <a name="02002000400004" ll="SDBH:חֹרֶף:000000">invierno</a>;<a name="02002000400014" ll="SDBH:שׁאל:000000">Pedirá</a>, pues, en la <a name="02002000400024" ll="SDBH:קָצִיר:000003">siega</a>, y <a name="02002000400028">no</a> hallará.
    
\v 5 Como <a name="02002000500002" ll="SDBH:מַיִם:000000">aguas</a> profundas <a name="02002000500004" ll="SDBH:עָמֹק:000000">es</a> el <a name="02002000500006" ll="SDBH:עֵצָה:000002">consejo</a> en el <a name="02002000500010" ll="SDBH:לֵב:000001">corazón</a> del <a name="02002000500012">hombre</a>;Mas el <a name="02002000500016">hombre</a> <a name="02002000500018" ll="SDBH:תְּבוּנָה:000000">entendido</a> lo <a name="02002000500020" ll="SDBH:דלה:000000">alcanzará</a>.
    
\v 6 <a name="02002000600002" ll="SDBH:רֹב:000000">Muchos</a> <a name="02002000600004" ll="SDBH:אָדָם:000000">hombres</a> <a name="02002000600006" ll="SDBH:קרא:000006">proclaman</a> cada <a name="02002000600008">uno</a> su propia <a name="02002000600010" ll="SDBH:חֶסֶד:000000">bondad</a>,Pero <a name="02002000600014">hombre</a> de verdad, ¿<a name="02002000600018" ll="SDBH:מִי:000000">quién</a> lo <a name="02002000600020" ll="SDBH:מצא:000012">hallará</a>?
    
\v 7 <a name="02002000700002" ll="SDBH:הלך:000004">Camina</a> en <a name="02002000700006" ll="SDBH:תֹּם:000001">su</a> <a name="02002000700006" ll="SDBH:תֹּם:000001">integridad</a> el <a name="02002000700008" ll="SDBH:צַדִּיק:000000">justo</a>;<a name="02002000700012" ll="SDBH:בֵּן:000000">Sus</a> <a name="02002000700012" ll="SDBH:בֵּן:000000">hijos</a> son <a name="02002000700010" ll="SDBH:אַשְׁרֵי:000000">dichosos</a> <a name="02002000700014" ll="SDBH:אַחַר:001002">después</a> <a name="02002000700014" ll="SDBH:אַחַר:001002">de</a> <a name="02002000700014" ll="SDBH:אַחַר:001002">él</a>.
    
\v 8 El <a name="02002000800002" ll="SDBH:מֶלֶךְ:000000">rey</a> que <a name="02002000800004">se</a> <a name="02002000800004">sienta</a> <a name="02002000800006">en</a> el <a name="02002000800008" ll="SDBH:כִּסֵּא:000000">trono</a> de <a name="02002000800010" ll="SDBH:דִּין:000000">juicio</a>,Con <a name="02002000800016" ll="SDBH:עַיִן:000000">su</a> <a name="02002000800016" ll="SDBH:עַיִן:000000">mirar</a> <a name="02002000800012" ll="SDBH:זרה:000004">disipa</a> <a name="02002000800018">todo</a> <a name="02002000800020" ll="SDBH:רַע:000005">mal</a>.
    
\v 9 ¿<a name="02002000900002" ll="SDBH:מִי:000000">Quién</a> podrá <a name="02002000900004" ll="SDBH:אמר:000000">decir</a>: Yo <a name="02002000900006" ll="SDBH:זכה:000002">he</a> <a name="02002000900006" ll="SDBH:זכה:000002">limpiado</a> <a name="02002000900008" ll="SDBH:לֵב:000001">mi</a> <a name="02002000900008" ll="SDBH:לֵב:000001">corazón</a>,<a name="02002000900010" ll="SDBH:טהר:000001">Limpio</a> estoy de <a name="02002000900014" ll="SDBH:חַטָּאת:000000">mi</a> <a name="02002000900014" ll="SDBH:חַטָּאת:000000">pecado</a>?
    
\v 10 <a name="02002001000002" ll="SDBH:אֶבֶן:000001">Pesa</a> falsa y <a name="02002001000008" ll="SDBH:אֵיפָה:000000">medida</a> falsa,<a name="02002001000020" ll="SDBH:שְׁנַיִם:000000">Ambas</a> cosas son <a name="02002001000014" ll="SDBH:תֹּועֵבָה:000000">abominación</a> a <a name="02002001000016" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 11 <a name="02002001100002" ll="SDBH:גַּם:000000">Aun</a> el <a name="02002001100010" ll="SDBH:נַעַר:000000">muchacho</a> es <a name="02002001100008" ll="SDBH:נכר:000000">conocido</a> por <a name="02002001100006" ll="SDBH:מַעֲלָל:000001">sus</a> <a name="02002001100006" ll="SDBH:מַעֲלָל:000001">hechos</a>,<a name="02002001100012">Si</a> <a name="02002001100022" ll="SDBH:פֹּעַל:000001">su</a> <a name="02002001100022" ll="SDBH:פֹּעַל:000001">conducta</a> fuere <a name="02002001100014" ll="SDBH:זַךְ:000001">limpia</a> y <a name="02002001100020" ll="SDBH:יָשָׁר:000003">recta</a>.
    
\v 12 El oído <a name="02002001200002" ll="SDBH:אֹזֶן:000000">que</a> <a name="02002001200004">oye</a>, y el <a name="02002001200008" ll="SDBH:עַיִן:000000">ojo</a> que <a name="02002001200010" ll="SDBH:ראה:000000">ve</a>,<a name="02002001200018" ll="SDBH:שְׁנַיִם:000000">Ambas</a> cosas <a name="02002001200016" ll="SDBH:גַּם:000000">igualmente</a> <a name="02002001200014">ha</a> <a name="02002001200014">hecho</a> <a name="02002001200012" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 13 <a name="02002001300002;02002001300008" ll="SDBH:אַל:000000">No</a> <a name="02002001300004" ll="SDBH:אהב:000000">ames</a> el <a name="02002001300006" ll="SDBH:שֵׁנָה:000000">sueño</a>, para que no te <a name="02002001300010" ll="SDBH:ירשׁ:000004">empobrezcas</a>;<a name="02002001300012" ll="SDBH:פקח:000002">Abre</a> <a name="02002001300014" ll="SDBH:עַיִן:000000">tus</a> <a name="02002001300014" ll="SDBH:עַיִן:000000">ojos</a>, y te <a name="02002001300016" ll="SDBH:שׂבע:000000">saciarás</a> de <a name="02002001300018" ll="SDBH:לֶחֶם:000000">pan</a>.
    
\v 14 El que <a name="02002001400010" ll="SDBH:קנה:000000">compra</a> <a name="02002001400006" ll="SDBH:אמר:000000">dice</a>: <a name="02002001400002;02002001400004" ll="SDBH:רַע:000004">Malo</a> es, malo es;Mas <a name="02002001400018" ll="SDBH:אָז:000000">cuando</a> <a name="02002001400014" ll="SDBH:אזל:000000">se</a> <a name="02002001400014" ll="SDBH:אזל:000000">aparta</a>, <a name="02002001400020" ll="SDBH:הלל:001002">se</a> <a name="02002001400020" ll="SDBH:הלל:001002">alaba</a>.
    
\v 15 <a name="02002001500002">Hay</a> <a name="02002001500004" ll="SDBH:זָהָב:000000">oro</a> y <a name="02002001500008" ll="SDBH:רֹב:000000">multitud</a> de piedras <a name="02002001500010" ll="SDBH:פְּנִינִים:000000">preciosas</a>;Mas los <a name="02002001500018" ll="SDBH:שָׂפָה:000001">labios</a> prudentes son <a name="02002001500014" ll="SDBH:כְּלִי:000000">joya</a> <a name="02002001500016" ll="SDBH:יְקָר:000000">preciosa</a>.
    
\v 16 <a name="02002001600002" ll="SDBH:לקח:000000">Quítale</a> <a name="02002001600004" ll="SDBH:בֶּגֶד:000001">su</a> <a name="02002001600004" ll="SDBH:בֶּגֶד:000001">ropa</a> al que salió por <a name="02002001600008" ll="SDBH:ערב:000000">fiador</a> del <a name="02002001600010" ll="SDBH:זָר:000000">extraño</a>,Y toma <a name="02002001600020" ll="SDBH:חבל:000000">prenda</a> del que sale fiador <a name="02002001600014" ll="SDBH:בַּעַד:000002" tl="HOTTP2757">por</a> los <a name="02002001600016" ll="SDBH:נָכְרִי:000002" tl="HOTTP2757">extraños</a>.
    
\v 17 Sabroso es al <a name="02002001700008">hombre</a> el <a name="02002001700010" ll="SDBH:לֶחֶם:000000">pan</a> de <a name="02002001700012" ll="SDBH:שֶׁקֶר:000000">mentira</a>;Pero <a name="02002001700016" ll="SDBH:אַחַר:002001">después</a> <a name="02002001700020" ll="SDBH:פֶּה:000000">su</a> <a name="02002001700020" ll="SDBH:פֶּה:000000">boca</a> será <a name="02002001700018" ll="SDBH:מלא:000000">llena</a> de <a name="02002001700022" ll="SDBH:חָצָץ:000000">cascajo</a>.
    
\v 18 Los <a name="02002001800002" ll="SDBH:מַחֲשָׁבָה:000000">pensamientos</a> con el <a name="02002001800006" ll="SDBH:עֵצָה:000001">consejo</a> <a name="02002001800008" ll="SDBH:כון:000027">se</a> <a name="02002001800008" ll="SDBH:כון:000027">ordenan</a>;Y con dirección <a name="02002001800014" ll="SDBH:תַּחְבּוּלֹות:000000">sabia</a> <a name="02002001800016">se</a> <a name="02002001800016">hace</a> la <a name="02002001800018" ll="SDBH:מִלְחָמָה:000000">guerra</a>.
    
\v 19 El que <a name="02002001900006" ll="SDBH:הלך:000004">anda</a> en <a name="02002001900008" ll="SDBH:רָכִיל:000000">chismes</a> <a name="02002001900002" ll="SDBH:גלה:000011">descubre</a> el <a name="02002001900004" ll="SDBH:סֹוד:000003">secreto</a>;<a name="02002001900018" ll="SDBH:לֹא:000000">No</a> te <a name="02002001900020" ll="SDBH:ערב:001002">entremetas</a>, pues, con el <a name="02002001900014" ll="SDBH:פתה:001001">suelto</a> de <a name="02002001900016" ll="SDBH:שָׂפָה:000001">lengua</a>.
    
\v 20 Al que <a name="02002002000002" ll="SDBH:קלל:000005">maldice</a> a <a name="02002002000004" ll="SDBH:אָב:000000">su</a> <a name="02002002000004" ll="SDBH:אָב:000000">padre</a> o a <a name="02002002000008" ll="SDBH:אֵם:000000">su</a> <a name="02002002000008" ll="SDBH:אֵם:000000">madre</a>,Se le <a name="02002002000010" ll="SDBH:דעך:000000">apagará</a> <a name="02002002000012" ll="SDBH:נֵר:000000">su</a> <a name="02002002000012" ll="SDBH:נֵר:000000">lámpara</a> en <a name="02002002000020;02002002000022" ll="SDBH:אֱשׁוּן:000000;SDBH:חֹשֶׁךְ:000000" tl="HOTTP2758">oscuridad</a> <a name="02002002000020;02002002000022" ll="SDBH:אֱשׁוּן:000000;SDBH:חֹשֶׁךְ:000000" tl="HOTTP2758">tenebrosa</a>.
    
\v 21 Los bienes que se adquieren de prisa al principio,<a name="02002002100018" ll="SDBH:לֹא:000000">No</a> <a name="02002002100012" ll="SDBH:רִאשֹׁון:000000">serán</a> al <a name="02002002100020" ll="SDBH:ברך:001001">final</a> <a name="02002002100016" ll="SDBH:אַחֲרִית:000001">bendecidos</a>.
    
\v 22 <a name="02002002200002" ll="SDBH:אַל:000000">No</a> <a name="02002002200004" ll="SDBH:אמר:000000">digas</a>: Yo me <a name="02002002200006;02002002200008" ll="SDBH:רַע:000005;SDBH:שׁלם:000007">vengaré</a>;<a name="02002002200010" ll="SDBH:קוה:000000">Espera</a> a <a name="02002002200014" ll="SDBH:יהוה:000000">Jehová</a>, y él te <a name="02002002200018" ll="SDBH:ישׁע:000001">salvará</a>.
    
\v 23 <a name="02002002300002" ll="SDBH:תֹּועֵבָה:000000">Abominación</a> son a <a name="02002002300004" ll="SDBH:יהוה:000000">Jehová</a> las pesas <a name="02002002300006" ll="SDBH:אֶבֶן:000001">falsas</a>,Y la <a name="02002002300014" ll="SDBH:מֹאזְנַיִם:000000">balanza</a> <a name="02002002300016" ll="SDBH:מִרְמָה:000000">falsa</a> <a name="02002002300018" ll="SDBH:לֹא:000000" tl="HOTTP2760">no</a> es <a name="02002002300020" ll="SDBH:טֹוב:000002" tl="HOTTP2760">buena</a>.
    
\v 24 De <a name="02002002400004" ll="SDBH:יהוה:000000">Jehová</a> son los <a name="02002002400006" ll="SDBH:מִצְעָד:000000">pasos</a> del <a name="02002002400008" ll="SDBH:גֶּבֶר:001000">hombre</a>;¿<a name="02002002400014" ll="SDBH:מָה:000000">Cómo</a>, pues, <a name="02002002400016" ll="SDBH:בין:000001">entenderá</a> el <a name="02002002400012" ll="SDBH:אָדָם:000000">hombre</a> <a name="02002002400018" ll="SDBH:דֶּרֶךְ:000007">su</a> <a name="02002002400018" ll="SDBH:דֶּרֶךְ:000007">camino</a>?
    
\v 25 <a name="02002002500002" ll="SDBH:מֹוקֵשׁ:000000">Lazo</a> es al <a name="02002002500004" ll="SDBH:אָדָם:000000">hombre</a> hacer apresuradamente <a name="02002002500014" ll="SDBH:נֶדֶר:000000">voto</a> de <a name="02002002500008" ll="SDBH:קֹדֶשׁ:000001">consagración</a>,Y <a name="02002002500012" ll="SDBH:אַחַר:001002">después</a> de hacerlo, <a name="02002002500018" ll="SDBH:בקר:000001">reflexionar</a>.
    
\v 26 El <a name="02002002600006" ll="SDBH:מֶלֶךְ:000000">rey</a> <a name="02002002600008" ll="SDBH:חָכָם, קסם:000000">sabio</a> <a name="02002002600002" ll="SDBH:זרה:000004">avienta</a> a los <a name="02002002600004" ll="SDBH:רָשָׁע:000000">impíos</a>,Y <a name="02002002600014">sobre</a> <a name="02002002600014">ellos</a> hace <a name="02002002600012" ll="SDBH:שׁוב:000003">rodar</a> la <a name="02002002600016" ll="SDBH:אֹופַן:000000">rueda</a>.
    
\v 27 Lámpara de <a name="02002002700004" ll="SDBH:יהוה:000000">Jehová</a> es el <a name="02002002700006" ll="SDBH:נְשָׁמָה:000001">espíritu</a> del <a name="02002002700008" ll="SDBH:אָדָם:000000">hombre</a>,La cual <a name="02002002700010" ll="SDBH:חפשׂ:000000">escudriña</a> lo más <a name="02002002700014" ll="SDBH:חֶדֶר:000002">profundo</a> del <a name="02002002700016" ll="SDBH:בֶּטֶן:000000">corazón</a>.
    
\v 28 <a name="02002002800002" ll="SDBH:חֶסֶד:000000">Misericordia</a> y verdad <a name="02002002800008" ll="SDBH:נצר:000007">guardan</a> al <a name="02002002800010" ll="SDBH:מֶלֶךְ:000000">rey</a>,Y con <a name="02002002800020" ll="SDBH:חֶסֶד:000000" tl="HOTTP2762">clemencia</a> <a name="02002002800014" ll="SDBH:סעד:000000">se</a> <a name="02002002800014" ll="SDBH:סעד:000000">sustenta</a> <a name="02002002800022" ll="SDBH:כִּסֵּא:000002">su</a> <a name="02002002800022" ll="SDBH:כִּסֵּא:000002">trono</a>.
    
\v 29 La <a name="02002002900002" ll="SDBH:תִּפְאֶרֶת:000000">gloria</a> de los <a name="02002002900004" ll="SDBH:בָּחוּר:000000">jóvenes</a> es <a name="02002002900006" ll="SDBH:כֹּחַ:000000">su</a> <a name="02002002900006" ll="SDBH:כֹּחַ:000000">fuerza</a>,Y la hermosura de los <a name="02002002900012" ll="SDBH:זָקֵן:000001">ancianos</a> es su <a name="02002002900014" ll="SDBH:שֵׂיבָה:000000">vejez</a>.
    
\v 30 Los <a name="02002003000002" ll="SDBH:חַבּוּרָה:000000">azotes</a> que <a name="02002003000004" ll="SDBH:פֶּצַע:000000">hieren</a> son medicina para el <a name="02002003000012" ll="SDBH:רַע:000005" tl="HOTTP2763">malo</a>,Y el <a name="02002003000016" ll="SDBH:מַכָּה:000000">castigo</a> purifica el <a name="02002003000018" ll="SDBH:חֶדֶר:000002">corazón</a>.
\c 21
\v 1 Como los <a name="02002100100002" ll="SDBH:פֶּלֶג:000000">repartimientos</a> de las <a name="02002100100004" ll="SDBH:מַיִם:000000">aguas</a>,Así está el <a name="02002100100006" ll="SDBH:לֵב:000001">corazón</a> del <a name="02002100100008" ll="SDBH:מֶלֶךְ:000000">rey</a> en la <a name="02002100100012">mano</a> de <a name="02002100100014" ll="SDBH:יהוה:000000">Jehová</a>;<a name="02002100100016">A</a> <a name="02002100100018">todo</a> lo <a name="02002100100020">que</a> <a name="02002100100022" ll="SDBH:חפץ:000002">quiere</a> lo <a name="02002100100024" ll="SDBH:נטה:002013">inclina</a>.
    
\v 2 <a name="02002100200002">Todo</a> <a name="02002100200004" ll="SDBH:דֶּרֶךְ:000007">camino</a> del <a name="02002100200006">hombre</a> es <a name="02002100200008" ll="SDBH:יָשָׁר:000003">recto</a> en su propia <a name="02002100200012" ll="SDBH:עַיִן:000000">opinión</a>;Pero <a name="02002100200020" ll="SDBH:יהוה:000000">Jehová</a> <a name="02002100200016" ll="SDBH:תכן:000000">pesa</a> los corazones.
    
\v 3 <a name="02002100300002">Hacer</a> <a name="02002100300004" ll="SDBH:צְדָקָה:000001">justicia</a> y <a name="02002100300008" ll="SDBH:מִשְׁפָּט:000007">juicio</a> es a <a name="02002100300014" ll="SDBH:יהוה:000000">Jehová</a>Más <a name="02002100300010" ll="SDBH:בחר:000002">agradable</a> que <a name="02002100300018" ll="SDBH:זֶבַח:000000">sacrificio</a>.
    
\v 4 Altivez de <a name="02002100400004" ll="SDBH:עַיִן:000000">ojos</a>, y orgullo de <a name="02002100400010" ll="SDBH:לֵב:000001">corazón</a>,Y pensamiento de <a name="02002100400014" ll="SDBH:רָשָׁע:000000">impíos</a>, son <a name="02002100400016" ll="SDBH:חַטָּאת:000000">pecado</a>.
    
\v 5 Los <a name="02002100500002" ll="SDBH:מַחֲשָׁבָה:000000" tl="HOTTP2765">pensamientos</a> del <a name="02002100500004" ll="SDBH:חָרוּץ:003000" tl="HOTTP2765">diligente</a> ciertamente <a name="02002100500006" ll="SDBH:אַךְ:000003" tl="HOTTP2765">tienden</a> a la <a name="02002100500010" ll="SDBH:מֹותָר:000000" tl="HOTTP2765">abundancia</a>;Mas <a name="02002100500014">todo</a> el que <a name="02002100500016" ll="SDBH:אוץ:000000" tl="HOTTP2765">se</a> <a name="02002100500016" ll="SDBH:אוץ:000000" tl="HOTTP2765">apresura</a> alocadamente, de <a name="02002100500018" ll="SDBH:אַךְ:000003" tl="HOTTP2765">cierto</a> va a la <a name="02002100500022" ll="SDBH:מַחְסֹור:000000" tl="HOTTP2765">pobreza</a>.
    
\v 6 <a name="02002100600002" ll="SDBH:פֹּעַל:000004" tl="HOTTP2766">Amontonar</a> <a name="02002100600004" ll="SDBH:אֹוצָר:000000">tesoros</a> con <a name="02002100600008" ll="SDBH:לָשֹׁון:000000">lengua</a> <a name="02002100600010" ll="SDBH:שֶׁקֶר:000000">mentirosa</a>Es <a name="02002100600012" ll="SDBH:הֶבֶל:000002" tl="HOTTP2767">aliento</a> <a name="02002100600014" ll="SDBH:נדף:000000" tl="HOTTP2767">fugaz</a> de aquellos que <a name="02002100600016" ll="SDBH:בקשׁ:000007" tl="HOTTP2768">buscan</a> la <a name="02002100600018" ll="SDBH:מָוֶת:000000" tl="HOTTP2768">muerte</a>.
    
\v 7 La rapiña de los <a name="02002100700004" ll="SDBH:רָשָׁע:000000">impíos</a> los <a name="02002100700006" ll="SDBH:גרר:000000">destruirá</a>,Por cuanto no <a name="02002100700010" ll="SDBH:מאן:000000">quisieron</a> <a name="02002100700014">hacer</a> <a name="02002100700016" ll="SDBH:מִשְׁפָּט:000007">juicio</a>.
    
\v 8 El <a name="02002100800004" ll="SDBH:דֶּרֶךְ:000007">camino</a> del <a name="02002100800006">hombre</a> <a name="02002100800008" ll="SDBH:וָזָר:000000">perverso</a> es <a name="02002100800002" ll="SDBH:הֲפַכְפַּךְ:000000">torcido</a> y extraño;Mas los <a name="02002100800016" ll="SDBH:פֹּעַל:000001">hechos</a> del <a name="02002100800012" ll="SDBH:זַךְ:000001">limpio</a> son <a name="02002100800014" ll="SDBH:יָשָׁר:000004">rectos</a>.
    
\v 9 <a name="02002100900002" ll="SDBH:טֹוב:000002">Mejor</a> es <a name="02002100900006">vivir</a> <a name="02002100900008">en</a> un <a name="02002100900010" ll="SDBH:פִּנָּה:000000">rincón</a> del <a name="02002100900012" ll="SDBH:גָּג:000000">terrado</a>Que con <a name="02002100900016" ll="SDBH:אִשָּׁה:000000">mujer</a> <a name="02002100900018" ll="SDBH:מִדְיָן:000000">rencillosa</a> en <a name="02002100900022" ll="SDBH:בַּיִת:000000">casa</a> <a name="02002100900024" ll="SDBH:חֶבֶר:000000">espaciosa</a>.
    
\v 10 El <a name="02002101000002" ll="SDBH:נֶפֶשׁ:000001">alma</a> del <a name="02002101000004" ll="SDBH:רָשָׁע:000000">impío</a> <a name="02002101000006" ll="SDBH:אוה:000002">desea</a> el <a name="02002101000008" ll="SDBH:רַע:000005">mal</a>;<a name="02002101000018" ll="SDBH:רֵעַ:001000">Su</a> <a name="02002101000018" ll="SDBH:רֵעַ:001000">prójimo</a> <a name="02002101000010" ll="SDBH:לֹא:000000">no</a> <a name="02002101000012" ll="SDBH:חנן:000000">halla</a> <a name="02002101000012" ll="SDBH:חנן:000000">favor</a> en <a name="02002101000016" ll="SDBH:עַיִן:000000">sus</a> <a name="02002101000016" ll="SDBH:עַיִן:000000">ojos</a>.
    
\v 11 Cuando el escarnecedor es <a name="02002101100004" ll="SDBH:ענשׁ:000000">castigado</a>, el <a name="02002101100010" ll="SDBH:פֶּתִי:000000">simple</a> se hace <a name="02002101100020" ll="SDBH:חָכָם, קסם:000000">sabio</a>;Y cuando se le <a name="02002101100016" ll="SDBH:שׂכל:001001">amonesta</a> al <a name="02002101100008" ll="SDBH:חכם:000000">sabio</a>, <a name="02002101100022" ll="SDBH:לקח:000000">aprende</a> <a name="02002101100024" ll="SDBH:דַּעַת:000000">ciencia</a>.
    
