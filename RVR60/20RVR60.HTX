\id PRO
\c 1
\v 1 Los <a name="02000100100002" ll="SDBH:מָשָׁל:000000">proverbios</a> de <a name="02000100100004" ll="SDBH:שְׁלֹמֹה:000000">Salomón</a>, <a name="02000100100006" ll="SDBH:בֵּן:000000">hijo</a> de <a name="02000100100008" ll="SDBH:דָּוִד:000000">David</a>, <a name="02000100100010" ll="SDBH:מֶלֶךְ:000000">rey</a> de <a name="02000100100012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>.
    
\v 2 Para <a name="02000100200004">entender</a> <a name="02000100200006" ll="SDBH:חָכְמָה:000000">sabiduría</a> y <a name="02000100200010" ll="SDBH:מוּסָר:000000">doctrina</a>,Para <a name="02000100200014" ll="SDBH:בין:000001">conocer</a> <a name="02000100200016" ll="SDBH:אֵמֶר:000000">razones</a> <a name="02000100200018" ll="SDBH:בִּינָה:000000">prudentes</a>,
    
\v 3 Para <a name="02000100300004" ll="SDBH:לקח:000000">recibir</a> el <a name="02000100300006" ll="SDBH:מוּסָר:000000">consejo</a> de <a name="02000100300008" ll="SDBH:שׂכל:001000">prudencia</a>,<a name="02000100300010" ll="SDBH:צֶדֶק:000001">Justicia</a>, <a name="02000100300014" ll="SDBH:מִשְׁפָּט:000007">juicio</a> y equidad;
    
\v 4 Para <a name="02000100400004">dar</a> sagacidad a los <a name="02000100400008" ll="SDBH:פֶּתִי:000000">simples</a>,Y a los <a name="02000100400014" ll="SDBH:נַעַר:000000">jóvenes</a> inteligencia y <a name="02000100400020" ll="SDBH:מְזִמָּה:000001">cordura</a>.
    
\v 5 <a name="02000100500002">Oirá</a> el <a name="02000100500004" ll="SDBH:חָכָם, קסם:000000">sabio</a>, y <a name="02000100500008">aumentará</a> el <a name="02000100500010" ll="SDBH:לֶקַח:000000">saber</a>,Y el <a name="02000100500014" ll="SDBH:בין:000000">entendido</a> <a name="02000100500018" ll="SDBH:קנה:000002">adquirirá</a> <a name="02000100500016" ll="SDBH:תַּחְבּוּלֹות:000000">consejo</a>,
    
\v 6 Para <a name="02000100600004" ll="SDBH:בין:000001">entender</a> <a name="02000100600006" ll="SDBH:מָשָׁל:000000">proverbio</a> y <a name="02000100600010" ll="SDBH:מְלִיצָה:000001">declaración</a>,<a name="02000100600012" ll="SDBH:דָּבָר:000000">Palabras</a> de <a name="02000100600014" ll="SDBH:חָכָם, קסם:000000">sabios</a>, y sus dichos <a name="02000100600018" ll="SDBH:חִידָה:000000">profundos</a>.
    
\v 7 El <a name="02000100700006" ll="SDBH:רֵאשִׁית:000000">principio</a> de la <a name="02000100700008" ll="SDBH:דַּעַת:000000">sabiduría</a> es el <a name="02000100700002" ll="SDBH:יִרְאָה:000000">temor</a> de <a name="02000100700004" ll="SDBH:יהוה:000000">Jehová</a>;Los <a name="02000100700016" ll="SDBH:אֱוִיל:000000">insensatos</a> <a name="02000100700018" ll="SDBH:בוז:000000">desprecian</a> la <a name="02000100700010" ll="SDBH:חָכְמָה:000000">sabiduría</a> y la <a name="02000100700014" ll="SDBH:מוּסָר:000000">enseñanza</a>.
    
\v 8 <a name="02000100800002">Oye</a>, <a name="02000100800004" ll="SDBH:בֵּן:000009">hijo</a> <a name="02000100800004" ll="SDBH:בֵּן:000009">mío</a>, la <a name="02000100800006" ll="SDBH:מוּסָר:000000">instrucción</a> de <a name="02000100800008" ll="SDBH:אָב:000000">tu</a> <a name="02000100800008" ll="SDBH:אָב:000000">padre</a>,Y <a name="02000100800012" ll="SDBH:אַל:000000">no</a> <a name="02000100800014" ll="SDBH:נטשׁ:000006">desprecies</a> la <a name="02000100800016" ll="SDBH:תֹּורָה:000000">dirección</a> de <a name="02000100800018" ll="SDBH:אֵם:000000">tu</a> <a name="02000100800018" ll="SDBH:אֵם:000000">madre</a>;
    
\v 9 Porque <a name="02000100900004" ll="SDBH:לִוְיָה:000000">adorno</a> de <a name="02000100900006" ll="SDBH:חֵן:000000">gracia</a> serán a <a name="02000100900012" ll="SDBH:רֹאשׁ:000000">tu</a> <a name="02000100900012" ll="SDBH:רֹאשׁ:000000">cabeza</a>,Y <a name="02000100900016" ll="SDBH:עֲנָק:000000">collares</a> a tu cuello.
    
\v 10 <a name="02000101000002" ll="SDBH:בֵּן:000009">Hijo</a> <a name="02000101000002" ll="SDBH:בֵּן:000009">mío</a>, <a name="02000101000004">si</a> los <a name="02000101000008" ll="SDBH:חַטָּא:000001">pecadores</a> <a name="02000101000006" ll="SDBH:פתה:000001">te</a> <a name="02000101000006" ll="SDBH:פתה:000001">quisieren</a> <a name="02000101000006" ll="SDBH:פתה:000001">engañar</a>,<a name="02000101000010" ll="SDBH:אַל:000000" tl="HOTTP2636">No</a> <a name="02000101000012" ll="SDBH:אבה:000000" tl="HOTTP2636">consientas</a>.
    
\v 11 <a name="02000101100002">Si</a> <a name="02000101100004" ll="SDBH:אמר:000000">dijeren</a>: <a name="02000101100006" ll="SDBH:הלך:000001">Ven</a> <a name="02000101100008">con</a> <a name="02000101100008">nosotros</a>;<a name="02000101100010" ll="SDBH:ארב:000000">Pongamos</a> <a name="02000101100010" ll="SDBH:ארב:000000">asechanzas</a> para derramar <a name="02000101100014" ll="SDBH:דָּם:000003">sangre</a>,<a name="02000101100016" ll="SDBH:צפן:000000">Acechemos</a> <a name="02000101100022" ll="SDBH:חִנָּם:000001">sin</a> <a name="02000101100022" ll="SDBH:חִנָּם:000001">motivo</a> al <a name="02000101100020" ll="SDBH:נָקִי:000000">inocente</a>;
    
\v 12 Los <a name="02000101200002" ll="SDBH:בלע:000000">tragaremos</a> <a name="02000101200008" ll="SDBH:חַי:000000">vivos</a> como el <a name="02000101200006" ll="SDBH:שְׁאֹול:000000">Seol</a>,Y <a name="02000101200012" ll="SDBH:תָּמִים:000000">enteros</a>, como <a name="02000101200016" ll="SDBH:ירד:000004">los</a> <a name="02000101200016" ll="SDBH:ירד:000004">que</a> <a name="02000101200016" ll="SDBH:ירד:000004">caen</a> en un <a name="02000101200018" ll="SDBH:בֹּור:000002">abismo</a>;
    
\v 13 <a name="02000101300008" ll="SDBH:מצא:000012">Hallaremos</a> <a name="02000101300004" ll="SDBH:הֹון:000000">riquezas</a> de <a name="02000101300002">toda</a> <a name="02000101300006" ll="SDBH:יָקָר:000000">clase</a>,<a name="02000101300010" ll="SDBH:מלא:000002">Llenaremos</a> <a name="02000101300012" ll="SDBH:בַּיִת:000000">nuestras</a> <a name="02000101300012" ll="SDBH:בַּיִת:000000">casas</a> de <a name="02000101300014" ll="SDBH:שָׁלָל:000000">despojos</a>;
    
\v 14 <a name="02000101400004" ll="SDBH:נפל:000034">Echa</a> <a name="02000101400002" ll="SDBH:גֹּורָל:000001">tu</a> <a name="02000101400002" ll="SDBH:גֹּורָל:000001">suerte</a> <a name="02000101400006;02000101400008" ll="SDBH:תָּוֶךְ:000000">entre</a> <a name="02000101400006;02000101400008" ll="SDBH:תָּוֶךְ:000000">nosotros</a>;<a name="02000101400014">Tengamos</a> <a name="02000101400018">todos</a> <a name="02000101400012" ll="SDBH:אֶחָד:000000">una</a> <a name="02000101400010" ll="SDBH:כִּיס:000000">bolsa</a>.
    
\v 15 <a name="02000101500002" ll="SDBH:בֵּן:000009">Hijo</a> <a name="02000101500002" ll="SDBH:בֵּן:000009">mío</a>, <a name="02000101500004" ll="SDBH:אַל:000000">no</a> <a name="02000101500006" ll="SDBH:הלך:000001">andes</a> en <a name="02000101500010" ll="SDBH:דֶּרֶךְ:000007">camino</a> con ellos.<a name="02000101500014" ll="SDBH:מנע:000001">Aparta</a> <a name="02000101500016" ll="SDBH:רֶגֶל:000003">tu</a> <a name="02000101500016" ll="SDBH:רֶגֶל:000003">pie</a> de <a name="02000101500020" ll="SDBH:נְתִיבָה:000000">sus</a> <a name="02000101500020" ll="SDBH:נְתִיבָה:000000">veredas</a>,
    
\v 16 Porque <a name="02000101600004" ll="SDBH:רֶגֶל:000003" tl="HOTTP2637">sus</a> <a name="02000101600004" ll="SDBH:רֶגֶל:000003" tl="HOTTP2637">pies</a> <a name="02000101600012" ll="SDBH:רוץ:000000" tl="HOTTP2637">corren</a> hacia el <a name="02000101600010" ll="SDBH:רַע:000005" tl="HOTTP2637">mal</a>,Y van <a name="02000101600016" ll="SDBH:מהר:000000" tl="HOTTP2637">presurosos</a> a <a name="02000101600020" ll="SDBH:שׁפך:000009" tl="HOTTP2637">derramar</a> <a name="02000101600022" ll="SDBH:דָּם:000003" tl="HOTTP2637">sangre</a>.
    
\v 17 Porque en <a name="02000101700004" ll="SDBH:חִנָּם:000002">vano</a> <a name="02000101700006" ll="SDBH:זרה:000007">se</a> <a name="02000101700006" ll="SDBH:זרה:000007">tenderá</a> la <a name="02000101700010" ll="SDBH:רֶשֶׁת:000000">red</a>Ante los <a name="02000101700014" ll="SDBH:עַיִן:000000">ojos</a> de <a name="02000101700016">toda</a> <a name="02000101700018" ll="SDBH:בַּעַל:000003">ave</a>;
    
\v 18 Pero <a name="02000101800004">ellos</a> a su propia <a name="02000101800008" ll="SDBH:דָּם:000003">sangre</a> ponen <a name="02000101800010" ll="SDBH:ארב:000000">asechanzas</a>,Y a <a name="02000101800016" ll="SDBH:נֶפֶשׁ:000001">sus</a> <a name="02000101800016" ll="SDBH:נֶפֶשׁ:000001">almas</a> tienden <a name="02000101800012" ll="SDBH:צפן:000000">lazo</a>.
    
\v 19 <a name="02000101900002">Tales</a> son las sendas de <a name="02000101900006">todo</a> el que es <a name="02000101900008" ll="SDBH:בצע:000005">dado</a> a la <a name="02000101900010" ll="SDBH:בֶּצַע:000001">codicia</a>,La cual <a name="02000101900018" ll="SDBH:לקח:000000">quita</a> la <a name="02000101900014" ll="SDBH:נֶפֶשׁ:000001">vida</a> de <a name="02000101900016" ll="SDBH:בַּעַל:000000">sus</a> <a name="02000101900016" ll="SDBH:בַּעַל:000000">poseedores</a>.
    
\v 20 La sabiduría <a name="02000102000010" ll="SDBH:רנן:000000">clama</a> en las <a name="02000102000008" ll="SDBH:חוּץ:000004">calles</a>,<a name="02000102000018">Alza</a> <a name="02000102000020">su</a> <a name="02000102000020">voz</a> en las <a name="02000102000016" ll="SDBH:רְחֹוב:000000">plazas</a>;
    
\v 21 <a name="02000102100008" ll="SDBH:קרא:000000">Clama</a> en los <a name="02000102100004" ll="SDBH:רֹאשׁ:000011" tl="HOTTP2639">principales</a> lugares de <a name="02000102100006" ll="SDBH:המה:000002" tl="HOTTP2639">reunión</a>;En las <a name="02000102100012" ll="SDBH:פֶּתַח:000000">entradas</a> de las <a name="02000102100014" ll="SDBH:שַׁעַר:000000">puertas</a> de la <a name="02000102100020" ll="SDBH:עִיר:000000">ciudad</a> <a name="02000102100022" ll="SDBH:אֵמֶר:000000">dice</a> sus <a name="02000102100024" ll="SDBH:אמר:000001">razones</a>.
    
\v 22 ¿Hasta <a name="02000102200004" ll="SDBH:מָתַי:000000">cuándo</a>, oh <a name="02000102200006" ll="SDBH:פֶּתִי:000000">simples</a>, <a name="02000102200008" ll="SDBH:אהב:000000">amaréis</a> la <a name="02000102200010" ll="SDBH:פֶּתִי:000000">simpleza</a>,Y los burladores <a name="02000102200018" ll="SDBH:חמד:000000">desearán</a> el <a name="02000102200016" ll="SDBH:לָצֹון:000000">burlar</a>,Y los <a name="02000102200024" ll="SDBH:כְּסִיל:000000">insensatos</a> <a name="02000102200026" ll="SDBH:שׂנא:000000">aborrecerán</a> la <a name="02000102200028" ll="SDBH:דַּעַת:000000">ciencia</a>?
    
\v 23 <a name="02000102300002" ll="SDBH:שׁוב:000026">Volveos</a> a <a name="02000102300006" ll="SDBH:תֹּוכַחַת:000000">mi</a> <a name="02000102300006" ll="SDBH:תֹּוכַחַת:000000">reprensión</a>;<a name="02000102300008">He</a> <a name="02000102300008">aquí</a> <a name="02000102300010" ll="SDBH:נבע:000002">yo</a> <a name="02000102300010" ll="SDBH:נבע:000002">derramaré</a> <a name="02000102300014" ll="SDBH:רוּחַ:000002">mi</a> <a name="02000102300014" ll="SDBH:רוּחַ:000002">espíritu</a> <a name="02000102300012">sobre</a> <a name="02000102300012">vosotros</a>,Y <a name="02000102300020">os</a> <a name="02000102300016">haré</a> <a name="02000102300016">saber</a> <a name="02000102300018" ll="SDBH:דָּבָר:000000">mis</a> <a name="02000102300018" ll="SDBH:דָּבָר:000000">palabras</a>.
    
\v 24 <a name="02000102400002">Por</a> cuanto <a name="02000102400004" ll="SDBH:קרא:000000">llamé</a>, y no <a name="02000102400008" ll="SDBH:מאן:000000">quisisteis</a> oír,<a name="02000102400010" ll="SDBH:נטה:001006">Extendí</a> <a name="02000102400012">mi</a> <a name="02000102400012">mano</a>, y <a name="02000102400016">no</a> hubo quien <a name="02000102400018" ll="SDBH:קשׁב:000000">atendiese</a>,
    
\v 25 Sino que <a name="02000102500004" ll="SDBH:פרע:000005">desechasteis</a> <a name="02000102500006">todo</a> <a name="02000102500008" ll="SDBH:עֵצָה:000001">consejo</a> <a name="02000102500008" ll="SDBH:עֵצָה:000001">mío</a>Y <a name="02000102500012" ll="SDBH:תֹּוכַחַת:000000">mi</a> <a name="02000102500012" ll="SDBH:תֹּוכַחַת:000000">reprensión</a> <a name="02000102500014" ll="SDBH:לֹא:000000">no</a> <a name="02000102500016" ll="SDBH:אבה:000002">quisisteis</a>,
    
\v 26 <a name="02000102600002" ll="SDBH:גַּם:000003">También</a> <a name="02000102600004">yo</a> <a name="02000102600010" ll="SDBH:שׂחק:000000">me</a> <a name="02000102600010" ll="SDBH:שׂחק:000000">reiré</a> en <a name="02000102600008" ll="SDBH:אֵיד:000000">vuestra</a> <a name="02000102600008" ll="SDBH:אֵיד:000000">calamidad</a>,Y <a name="02000102600012" ll="SDBH:לעג:000000">me</a> <a name="02000102600012" ll="SDBH:לעג:000000">burlaré</a> cuando os <a name="02000102600016">viniere</a> lo <a name="02000102600018" ll="SDBH:פַּחַד:000000">que</a> <a name="02000102600018" ll="SDBH:פַּחַד:000000">teméis</a>;
    
\v 27 Cuando <a name="02000102700004">viniere</a> <a name="02000102700010">como</a> una <a name="02000102700012" ll="SDBH:שֹׁואָה:000000">destrucción</a> lo <a name="02000102700014" ll="SDBH:פַּחַד:000000">que</a> <a name="02000102700014" ll="SDBH:פַּחַד:000000">teméis</a>,Y <a name="02000102700018" ll="SDBH:אֵיד:000000">vuestra</a> <a name="02000102700018" ll="SDBH:אֵיד:000000">calamidad</a> <a name="02000102700024" ll="SDBH:אתה:000002">llegare</a> como un <a name="02000102700022" ll="SDBH:סוּפָה:000000">torbellino</a>;Cuando <a name="02000102700030">sobre</a> <a name="02000102700030">vosotros</a> <a name="02000102700028">viniere</a> <a name="02000102700032" ll="SDBH:צָרָה:000000" tl="HOTTP2640">tribulación</a> y <a name="02000102700036">angustia</a>.
    
\v 28 <a name="02000102800002" ll="SDBH:אָז:000000">Entonces</a> me <a name="02000102800004" ll="SDBH:קרא:000000">llamarán</a>, y <a name="02000102800008" ll="SDBH:לֹא:000000">no</a> <a name="02000102800010" ll="SDBH:ענה:000000">responderé</a>;Me buscarán de mañana, y <a name="02000102800016" ll="SDBH:לֹא:000000">no</a> me <a name="02000102800018" ll="SDBH:מצא:000000">hallarán</a>.
    
\v 29 <a name="02000102900002">Por</a> cuanto <a name="02000102900006" ll="SDBH:שׂנא:000000">aborrecieron</a> la <a name="02000102900008" ll="SDBH:דַּעַת:000000">sabiduría</a>,Y <a name="02000102900016" ll="SDBH:לֹא:000000">no</a> <a name="02000102900018" ll="SDBH:בחר:000000">escogieron</a> el <a name="02000102900012" ll="SDBH:יִרְאָה:000000">temor</a> de <a name="02000102900014" ll="SDBH:יהוה:000000">Jehová</a>,
    
\v 30 <a name="02000103000002" ll="SDBH:לֹא:000000">Ni</a> <a name="02000103000004" ll="SDBH:אבה:000002">quisieron</a> <a name="02000103000008" ll="SDBH:עֵצָה:000001">mi</a> <a name="02000103000008" ll="SDBH:עֵצָה:000001">consejo</a>,Y <a name="02000103000010" ll="SDBH:נאץ:000000">menospreciaron</a> <a name="02000103000012">toda</a> <a name="02000103000014" ll="SDBH:תֹּוכַחַת:000000">reprensión</a> <a name="02000103000014" ll="SDBH:תֹּוכַחַת:000000">mía</a>,
    
\v 31 <a name="02000103100004;02000103100008" ll="SDBH:אכל:000008;SDBH:פְּרִי:000003">Comerán</a> del <a name="02000103100004;02000103100008" ll="SDBH:אכל:000008;SDBH:פְּרִי:000003">fruto</a> de <a name="02000103100010" ll="SDBH:דֶּרֶךְ:000007">su</a> <a name="02000103100010" ll="SDBH:דֶּרֶךְ:000007">camino</a>,Y <a name="02000103100018" ll="SDBH:שׂבע:000000">serán</a> <a name="02000103100018" ll="SDBH:שׂבע:000000">hastiados</a> de sus propios <a name="02000103100016" ll="SDBH:מֹועֵצָה:000000">consejos</a>.
    
\v 32 Porque el <a name="02000103200004" ll="SDBH:מְשׁוּבָה:000000">desvío</a> de los <a name="02000103200006" ll="SDBH:פֶּתִי:000000">ignorantes</a> los <a name="02000103200008" ll="SDBH:הרג:000000">matará</a>,Y la <a name="02000103200012" ll="SDBH:שַׁלְוָה:000000">prosperidad</a> de los <a name="02000103200014" ll="SDBH:כְּסִיל:000000">necios</a> los echará a <a name="02000103200016" ll="SDBH:אבד:000005">perder</a>;
    
\v 33 Mas el que me <a name="02000103300004">oyere</a>, <a name="02000103300008" ll="SDBH:שׁכן:000000">habitará</a> <a name="02000103300010" ll="SDBH:בֶּטַח:000000">confiadamente</a>Y vivirá <a name="02000103300014" ll="SDBH:שׁאן:000000">tranquilo</a>, sin <a name="02000103300018" ll="SDBH:פַּחַד:000000">temor</a> del <a name="02000103300020" ll="SDBH:רָעָה:000002">mal</a>.
\c 2
\v 1 <a name="02000200100002" ll="SDBH:בֵּן:000009">Hijo</a> <a name="02000200100002" ll="SDBH:בֵּן:000009">mío</a>, <a name="02000200100004">si</a> <a name="02000200100006" ll="SDBH:לקח:000000">recibieres</a> <a name="02000200100008" ll="SDBH:אֵמֶר:000000">mis</a> <a name="02000200100008" ll="SDBH:אֵמֶר:000000">palabras</a>,Y <a name="02000200100012" ll="SDBH:מִצְוָה:000000">mis</a> <a name="02000200100012" ll="SDBH:מִצְוָה:000000">mandamientos</a> <a name="02000200100014" ll="SDBH:צפן:000006">guardares</a> dentro de ti,
    
\v 2 Haciendo estar <a name="02000200200004" ll="SDBH:קשׁב:000000">atento</a> <a name="02000200200012" ll="SDBH:אֹזֶן:000000">tu</a> <a name="02000200200012" ll="SDBH:אֹזֶן:000000">oído</a> a la <a name="02000200200010" ll="SDBH:חָכְמָה:000000">sabiduría</a>;Si <a name="02000200200014" ll="SDBH:נטה:002012">inclinares</a> <a name="02000200200016" ll="SDBH:לֵב:000001">tu</a> <a name="02000200200016" ll="SDBH:לֵב:000001">corazón</a> a la <a name="02000200200022" ll="SDBH:תְּבוּנָה:000000">prudencia</a>,
    
\v 3 Si <a name="02000200300012" ll="SDBH:קרא:000003">clamares</a> a la <a name="02000200300010" ll="SDBH:בִּינָה:000000">inteligencia</a>,Y a la <a name="02000200300018" ll="SDBH:תְּבוּנָה:000000">prudencia</a> <a name="02000200300020">dieres</a> <a name="02000200300022">tu</a> <a name="02000200300022">voz</a>;
    
\v 4 <a name="02000200400002">Si</a> como a la <a name="02000200400010" ll="SDBH:כֶּסֶף:000000">plata</a> la <a name="02000200400004" ll="SDBH:בקשׁ:000007">buscares</a>,Y la <a name="02000200400020" ll="SDBH:חפשׂ:000004">escudriñares</a> como a <a name="02000200400018" ll="SDBH:מַטְמֹון:000000">tesoros</a>,
    
\v 5 <a name="02000200500002" ll="SDBH:אָז:000002">Entonces</a> <a name="02000200500004" ll="SDBH:בין:000001">entenderás</a> el <a name="02000200500006" ll="SDBH:יִרְאָה:000000">temor</a> de <a name="02000200500008" ll="SDBH:יהוה:000000">Jehová</a>,Y <a name="02000200500016" ll="SDBH:מצא:000011">hallarás</a> el <a name="02000200500012" ll="SDBH:דַּעַת:000000">conocimiento</a> de <a name="02000200500014" ll="SDBH:אֱלֹהִים:000002">Dios</a>.
    
\v 6 Porque <a name="02000200600004" ll="SDBH:יהוה:000000">Jehová</a> <a name="02000200600006">da</a> la <a name="02000200600008" ll="SDBH:חָכְמָה:000000">sabiduría</a>,Y de <a name="02000200600012" ll="SDBH:פֶּה:000000">su</a> <a name="02000200600012" ll="SDBH:פֶּה:000000">boca</a> viene el <a name="02000200600014" ll="SDBH:דַּעַת:000000">conocimiento</a> y la <a name="02000200600018" ll="SDBH:תְּבוּנָה:000000">inteligencia</a>.
    
\v 7 Él <a name="02000200700002" ll="SDBH:צפן:000005">provee</a> de sana <a name="02000200700012" ll="SDBH:תּוּשִׁיָּה:000000">sabiduría</a> a los <a name="02000200700010" ll="SDBH:יָשָׁר:000004">rectos</a>;Es <a name="02000200700014" ll="SDBH:מָגֵן:000000">escudo</a> a los que <a name="02000200700018" ll="SDBH:הלך:000004">caminan</a> rectamente.
    
\v 8 Es el que <a name="02000200800004" ll="SDBH:נצר:000007">guarda</a> las <a name="02000200800006" ll="SDBH:אֹרַח:000002">veredas</a> del <a name="02000200800008" ll="SDBH:מִשְׁפָּט:000007">juicio</a>,Y <a name="02000200800018" ll="SDBH:שׁמר:000007">preserva</a> el <a name="02000200800012" ll="SDBH:דֶּרֶךְ:000007">camino</a> de sus <a name="02000200800014" ll="SDBH:חָסִיד:000000">santos</a>.
    
\v 9 <a name="02000200900002" ll="SDBH:אָז:000002">Entonces</a> <a name="02000200900004" ll="SDBH:בין:000001">entenderás</a> <a name="02000200900006" ll="SDBH:צֶדֶק:000001">justicia</a>, <a name="02000200900010" ll="SDBH:מִשְׁפָּט:000007">juicio</a>Y equidad, y <a name="02000200900016">todo</a> <a name="02000200900020" ll="SDBH:טֹוב:000002">buen</a> <a name="02000200900018" ll="SDBH:מַעְגָּל:000002">camino</a>.
    
\v 10 Cuando la <a name="02000201000006" ll="SDBH:חָכְמָה:000000">sabiduría</a> <a name="02000201000004">entrare</a> en <a name="02000201000010" ll="SDBH:לֵב:000001">tu</a> <a name="02000201000010" ll="SDBH:לֵב:000001">corazón</a>,Y la <a name="02000201000014" ll="SDBH:דַּעַת:000000">ciencia</a> fuere <a name="02000201000020" ll="SDBH:נעם:000001">grata</a> a <a name="02000201000018" ll="SDBH:נֶפֶשׁ:000001">tu</a> <a name="02000201000018" ll="SDBH:נֶפֶשׁ:000001">alma</a>,
    
\v 11 La <a name="02000201100002" ll="SDBH:מְזִמָּה:000001">discreción</a> te <a name="02000201100004" ll="SDBH:שׁמר:000007">guardará</a>;Te <a name="02000201100010" ll="SDBH:נצר:000007">preservará</a> la <a name="02000201100008" ll="SDBH:תְּבוּנָה:000000">inteligencia</a>,
    
\v 12 Para <a name="02000201200004" ll="SDBH:נצל:000005">librarte</a> del <a name="02000201200010" ll="SDBH:רַע:000005">mal</a> <a name="02000201200008" ll="SDBH:דֶּרֶךְ:000007">camino</a>,De los <a name="02000201200014">hombres</a> que <a name="02000201200016" ll="SDBH:דבר:001000">hablan</a> perversidades,
    
\v 13 Que <a name="02000201300004" ll="SDBH:עזב:000006">dejan</a> los caminos <a name="02000201300008" ll="SDBH:יֹשֶׁר:000000">derechos</a>,Para andar por <a name="02000201300016" ll="SDBH:דֶּרֶךְ:000007">sendas</a> <a name="02000201300018" ll="SDBH:חֹשֶׁךְ:000000">tenebrosas</a>;
    
\v 14 Que se <a name="02000201400004" ll="SDBH:שָׂמֵחַ:000000">alegran</a> <a name="02000201400008">haciendo</a> el <a name="02000201400010" ll="SDBH:רַע:000005">mal</a>,Que <a name="02000201400012" ll="SDBH:גיל:000000">se</a> <a name="02000201400012" ll="SDBH:גיל:000000">huelgan</a> en las perversidades del <a name="02000201400018" ll="SDBH:רַע:000005">vicio</a>;
    
\v 15 Cuyas <a name="02000201500004" ll="SDBH:אֹרַח:000002">veredas</a> son <a name="02000201500006" ll="SDBH:עִקֵּשׁ:000001">torcidas</a>,Y torcidos <a name="02000201500014" ll="SDBH:מַעְגָּל:000002">sus</a> <a name="02000201500014" ll="SDBH:מַעְגָּל:000002">caminos</a>.
    
\v 16 Serás <a name="02000201600004" ll="SDBH:נצל:000005">librado</a> de la <a name="02000201600008" ll="SDBH:אִשָּׁה:000000">mujer</a> <a name="02000201600010" ll="SDBH:זָר:000000">extraña</a>,De la <a name="02000201600014" ll="SDBH:נָכְרִי:000003">ajena</a> que <a name="02000201600018" ll="SDBH:חלק:001002">halaga</a> con <a name="02000201600016" ll="SDBH:אֵמֶר:000000">sus</a> <a name="02000201600016" ll="SDBH:אֵמֶר:000000">palabras</a>,
    
\v 17 La cual <a name="02000201700004" ll="SDBH:עזב:000003">abandona</a> al <a name="02000201700006" ll="SDBH:אַלּוּף:002000" tl="HOTTP2642">compañero</a> de <a name="02000201700008" ll="SDBH:נְעוּרִים:000000">su</a> <a name="02000201700008" ll="SDBH:נְעוּרִים:000000">juventud</a>,Y <a name="02000201700018" ll="SDBH:שׁכח:000000">se</a> <a name="02000201700018" ll="SDBH:שׁכח:000000">olvida</a> del <a name="02000201700014" ll="SDBH:בְּרִית:000000">pacto</a> de <a name="02000201700016" ll="SDBH:אֱלֹהִים:000001">su</a> <a name="02000201700016" ll="SDBH:אֱלֹהִים:000001">Dios</a>.
    
\v 18 Por lo cual <a name="02000201800010" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917" tl="HOTTP2643">su</a> <a name="02000201800010" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917" tl="HOTTP2643">casa</a> está inclinada <a name="02000201800006">a</a> la <a name="02000201800008" ll="SDBH:מָוֶת:000002">muerte</a>,Y <a name="02000201800018" ll="SDBH:מַעְגָּל:000002">sus</a> <a name="02000201800018" ll="SDBH:מַעְגָּל:000002">veredas</a> <a name="02000201800014">hacia</a> los <a name="02000201800016" ll="SDBH:רְפָאִים:000000">muertos</a>;
    
\v 19 <a name="02000201900002">Todos</a> <a name="02000201900004">los</a> <a name="02000201900004">que</a> <a name="02000201900004">a</a> <a name="02000201900004">ella</a> <a name="02000201900004">se</a> <a name="02000201900004">lleguen</a>, <a name="02000201900006" ll="SDBH:לֹא:000000">no</a> <a name="02000201900008" ll="SDBH:שׁוב:000023">volverán</a>,<a name="02000201900012" ll="SDBH:לֹא:000000">Ni</a> <a name="02000201900014">seguirán</a> otra vez los <a name="02000201900016" ll="SDBH:אֹרַח:000002">senderos</a> de la <a name="02000201900018" ll="SDBH:חַיִּים:000000">vida</a>.
    
\v 20 Así andarás por el <a name="02000202000008" ll="SDBH:דֶּרֶךְ:000007">camino</a> de los <a name="02000202000010" ll="SDBH:טֹוב:000002">buenos</a>,Y <a name="02000202000018" ll="SDBH:שׁמר:000003">seguirás</a> las veredas de los <a name="02000202000016" ll="SDBH:צַדִּיק:000000">justos</a>;
    
\v 21 <a name="02000202100002">Porque</a> los <a name="02000202100004" ll="SDBH:יָשָׁר:000004">rectos</a> <a name="02000202100006" ll="SDBH:שׁכן:000000">habitarán</a> la <a name="02000202100008" ll="SDBH:אֶרֶץ:000000">tierra</a>,Y los <a name="02000202100012" ll="SDBH:תָּמִים:000004">perfectos</a> <a name="02000202100014">permanecerán</a> en ella,
    
\v 22 Mas los <a name="02000202200004" ll="SDBH:רָשָׁע:000000">impíos</a> serán <a name="02000202200010" ll="SDBH:כרת:000005">cortados</a> <a name="02000202200006">de</a> la <a name="02000202200008" ll="SDBH:אֶרֶץ:000000">tierra</a>,Y los <a name="02000202200014" ll="SDBH:בגד:000000">prevaricadores</a> serán <a name="02000202200018">de</a> <a name="02000202200018">ella</a> <a name="02000202200016" ll="SDBH:נסח:000002" tl="HOTTP2644">desarraigados</a>.
