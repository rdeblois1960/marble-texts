\v 30 <a name="02001603000002" ll="SDBH:עצה:000000">Cierra</a> <a name="02001603000004" ll="SDBH:עַיִן:000000">sus</a> <a name="02001603000004" ll="SDBH:עַיִן:000000">ojos</a> para <a name="02001603000008" ll="SDBH:חשׁב:000001" tl="HOTTP2743">pensar</a> perversidades;<a name="02001603000012" ll="SDBH:קרץ:000000">Mueve</a> <a name="02001603000014" ll="SDBH:שָׂפָה:000000">sus</a> <a name="02001603000014" ll="SDBH:שָׂפָה:000000">labios</a>, <a name="02001603000016" ll="SDBH:כלה:000006">efectúa</a> el <a name="02001603000018" ll="SDBH:רָעָה:000000">mal</a>.
    
\v 31 Corona de <a name="02001603100004" ll="SDBH:תִּפְאֶרֶת:000000">honra</a> es la <a name="02001603100006" ll="SDBH:שֵׂיבָה:000000">vejez</a>Que <a name="02001603100014" ll="SDBH:מצא:000006">se</a> <a name="02001603100014" ll="SDBH:מצא:000006">halla</a> en el <a name="02001603100010" ll="SDBH:דֶּרֶךְ:000007">camino</a> de <a name="02001603100012" ll="SDBH:צְדָקָה:000001">justicia</a>.
    
\v 32 <a name="02001603200002" ll="SDBH:טֹוב:000002">Mejor</a> es el que <a name="02001603200004;02001603200006" ll="SDBH:אַף:000007;SDBH:אָרֵךְ:000001">tarda</a> <a name="02001603200004;02001603200006" ll="SDBH:אַף:000007;SDBH:אָרֵךְ:000001">en</a> <a name="02001603200004;02001603200006" ll="SDBH:אַף:000007;SDBH:אָרֵךְ:000001">airarse</a> que el <a name="02001603200010" ll="SDBH:גִּבֹּור:000000">fuerte</a>;Y el que <a name="02001603200014" ll="SDBH:משׁל:001001">se</a> <a name="02001603200014" ll="SDBH:משׁל:001001">enseñorea</a> de <a name="02001603200018" ll="SDBH:רוּחַ:000002">su</a> <a name="02001603200018" ll="SDBH:רוּחַ:000002">espíritu</a>, que el que <a name="02001603200022" ll="SDBH:לכד:000000">toma</a> una <a name="02001603200024" ll="SDBH:עִיר:000000">ciudad</a>.
    
\v 33 La <a name="02001603300014" ll="SDBH:גֹּורָל:000001">suerte</a> se echa en el <a name="02001603300006" ll="SDBH:חֵיק:000000">regazo</a>;Mas de <a name="02001603300020" ll="SDBH:יהוה:000000">Jehová</a> es la <a name="02001603300024" ll="SDBH:מִשְׁפָּט:000001">decisión</a> de ella.
\c 17
\v 1 <a name="02001700100002" ll="SDBH:טֹוב:000002">Mejor</a> es un <a name="02001700100004" ll="SDBH:פַּת:000000">bocado</a> <a name="02001700100006" ll="SDBH:חָרֵב:000000">seco</a>, y en <a name="02001700100010" ll="SDBH:שַׁלְוָה:000000">paz</a>,Que <a name="02001700100016" ll="SDBH:בַּיִת:000000">casa</a> de contiendas llena de <a name="02001700100020" ll="SDBH:זֶבַח:000000">provisiones</a>.
    
\v 2 El <a name="02001700200002" ll="SDBH:עֶבֶד:000000">siervo</a> <a name="02001700200004" ll="SDBH:שׂכל:001000">prudente</a> <a name="02001700200006" ll="SDBH:משׁל:001000">se</a> <a name="02001700200006" ll="SDBH:משׁל:001000">enseñoreará</a> del <a name="02001700200010" ll="SDBH:בֵּן:000000">hijo</a> que <a name="02001700200012" ll="SDBH:בושׁ:000002">deshonra</a>,Y <a name="02001700200018" ll="SDBH:תָּוֶךְ:000000">con</a> los <a name="02001700200020" ll="SDBH:אָח:000000">hermanos</a> <a name="02001700200022" ll="SDBH:חלק:000005">compartirá</a> la <a name="02001700200024" ll="SDBH:נַחֲלָה:000000">herencia</a>.
    
\v 3 El <a name="02001700300002" ll="SDBH:מַצְרֵף:000000">crisol</a> para la <a name="02001700300008" ll="SDBH:כֶּסֶף:000000">plata</a>, y la <a name="02001700300012" ll="SDBH:כּוּר:000000">hornaza</a> para el <a name="02001700300018" ll="SDBH:זָהָב:000000">oro</a>;Pero <a name="02001700300026" ll="SDBH:יהוה:000000">Jehová</a> <a name="02001700300022" ll="SDBH:בחן:000000">prueba</a> los corazones.
    
\v 4 El malo está <a name="02001700400004" ll="SDBH:קשׁב:000000">atento</a> <a name="02001700400006">al</a> <a name="02001700400008" ll="SDBH:שָׂפָה:000001">labio</a> <a name="02001700400010" ll="SDBH:אָוֶן:000000">inicuo</a>;Y el <a name="02001700400012" ll="SDBH:שֶׁקֶר:000000">mentiroso</a> <a name="02001700400014" ll="SDBH:אזן:000001">escucha</a> <a name="02001700400016">la</a> <a name="02001700400018" ll="SDBH:לָשֹׁון:000000">lengua</a> <a name="02001700400020" ll="SDBH:הַוָּה:001000">detractora</a>.
    
\v 5 <a name="02001700500002" ll="SDBH:לעג:000000">El</a> <a name="02001700500002" ll="SDBH:לעג:000000">que</a> <a name="02001700500002" ll="SDBH:לעג:000000">escarnece</a> al <a name="02001700500008" ll="SDBH:רושׁ:000001">pobre</a> <a name="02001700500010" ll="SDBH:חרף:000000">afrenta</a> a <a name="02001700500012">su</a> <a name="02001700500012">Hacedor</a>;Y <a name="02001700500014" ll="SDBH:שָׂמֵחַ:000000">el</a> <a name="02001700500014" ll="SDBH:שָׂמֵחַ:000000">que</a> <a name="02001700500014" ll="SDBH:שָׂמֵחַ:000000">se</a> <a name="02001700500014" ll="SDBH:שָׂמֵחַ:000000">alegra</a> de la <a name="02001700500018" ll="SDBH:אֵיד:000000">calamidad</a> <a name="02001700500020" ll="SDBH:לֹא:000000">no</a> <a name="02001700500022" ll="SDBH:נקה:000002">quedará</a> <a name="02001700500022" ll="SDBH:נקה:000002">sin</a> <a name="02001700500022" ll="SDBH:נקה:000002">castigo</a>.
    
\v 6 <a name="02001700600002" ll="SDBH:עֲטָרָה:000000">Corona</a> de los <a name="02001700600004" ll="SDBH:זָקֵן:000001">viejos</a> son los <a name="02001700600006;02001700600008" ll="SDBH:בֵּן:000000">nietos</a>,Y la <a name="02001700600012" ll="SDBH:תִּפְאֶרֶת:000000">honra</a> de los <a name="02001700600014" ll="SDBH:בֵּן:000000">hijos</a>, <a name="02001700600016" ll="SDBH:אָב:000000">sus</a> <a name="02001700600016" ll="SDBH:אָב:000000">padres</a>.
    
\v 7 <a name="02001700700002" ll="SDBH:לֹא:000000">No</a> <a name="02001700700004" ll="SDBH:נָאוֶה:000001">conviene</a> al <a name="02001700700008" ll="SDBH:נָבָל:000000">necio</a> la <a name="02001700700010;02001700700012" ll="SDBH:יֶתֶר:000005;SDBH:שָׂפָה:000001">altilocuencia</a>;¡<a name="02001700700014;02001700700016" ll="SDBH:אַף:001001">Cuánto</a> <a name="02001700700014;02001700700016" ll="SDBH:אַף:001001">menos</a> al <a name="02001700700020" ll="SDBH:נָדִיב:000001">príncipe</a> el <a name="02001700700022" ll="SDBH:שָׂפָה:000001">labio</a> <a name="02001700700024" ll="SDBH:שֶׁקֶר:000000">mentiroso</a>!
    
\v 8 <a name="02001700800002" ll="SDBH:אֶבֶן:000008">Piedra</a> <a name="02001700800004" ll="SDBH:חֵן:000000">preciosa</a> es el <a name="02001700800008" ll="SDBH:שֹׁחַד:000000">soborno</a> <a name="02001700800012" ll="SDBH:עַיִן:000000">para</a> el que lo <a name="02001700800014" ll="SDBH:בַּעַל:000000">practica</a>;<a name="02001700800016">Adondequiera</a> <a name="02001700800020">que</a> <a name="02001700800022" ll="SDBH:פנה:000002">se</a> <a name="02001700800022" ll="SDBH:פנה:000002">vuelve</a>, halla <a name="02001700800024" ll="SDBH:שׂכל:000000">prosperidad</a>.
    
\v 9 El que <a name="02001700900002" ll="SDBH:כסה:000000;SDBH:כסה:000004">cubre</a> la <a name="02001700900004" ll="SDBH:פֶּשַׁע:000000">falta</a> <a name="02001700900006" ll="SDBH:בקשׁ:000007">busca</a> amistad;Mas el que la <a name="02001700900016" ll="SDBH:דָּבָר:000002">divulga</a>, <a name="02001700900018" ll="SDBH:פרד:000003">aparta</a> al <a name="02001700900020" ll="SDBH:אַלּוּף:002000">amigo</a>.
    
\v 10 La <a name="02001701000004" ll="SDBH:גְּעָרָה:000000">reprensión</a> <a name="02001701000002" ll="SDBH:נחת:000002">aprovecha</a> al <a name="02001701000008" ll="SDBH:בין:000000">entendido</a>,Más que <a name="02001701000016" ll="SDBH:מֵאָה:000000">cien</a> <a name="02001701000012">azotes</a> al <a name="02001701000014" ll="SDBH:כְּסִיל:000000">necio</a>.
    
\v 11 El <a name="02001701100004" ll="SDBH:מְרִי:000000">rebelde</a> no <a name="02001701100006" ll="SDBH:בקשׁ:000007">busca</a> <a name="02001701100002" ll="SDBH:אַךְ:000003">sino</a> el <a name="02001701100008" ll="SDBH:רַע:000001">mal</a>,Y <a name="02001701100012" ll="SDBH:מַלְאָךְ:000000">mensajero</a> <a name="02001701100014" ll="SDBH:אַכְזָרִי:000000">cruel</a> será <a name="02001701100016" ll="SDBH:שׁלח:000002">enviado</a> contra él.
    
\v 12 Mejor es <a name="02001701200002" ll="SDBH:פגשׁ:000000">encontrarse</a> con una <a name="02001701200004" ll="SDBH:דֹּב:000000">osa</a> a la cual han robado sus <a name="02001701200006" ll="SDBH:שַׁכּוּל:000000">cachorros</a>,Que con un <a name="02001701200016" ll="SDBH:כְּסִיל:000000">fatuo</a> en <a name="02001701200020" ll="SDBH:אִוֶּלֶת:000000">su</a> <a name="02001701200020" ll="SDBH:אִוֶּלֶת:000000">necedad</a>.
    
\v 13 El que <a name="02001701300002" ll="SDBH:שׁוב:000032">da</a> <a name="02001701300004" ll="SDBH:רָעָה:000000">mal</a> <a name="02001701300006">por</a> <a name="02001701300008" ll="SDBH:טֹובָה:000002">bien</a>,<a name="02001701300010" ll="SDBH:לֹא:000000">No</a> se apartará el <a name="02001701300016" ll="SDBH:רָעָה:000000">mal</a> de <a name="02001701300020" ll="SDBH:בַּיִת:000003">su</a> <a name="02001701300020" ll="SDBH:בַּיִת:000003">casa</a>.
    
\v 14 El que <a name="02001701400006" ll="SDBH:רֵאשִׁית:000000">comienza</a> la discordia es como quien <a name="02001701400002" ll="SDBH:פטר:000002">suelta</a> las <a name="02001701400004" ll="SDBH:מַיִם:000000">aguas</a>;Deja, <a name="02001701400022" ll="SDBH:נטשׁ:000006">pues</a>, la contienda, <a name="02001701400014">antes</a> que <a name="02001701400016" ll="SDBH:גלע:000001">se</a> <a name="02001701400016" ll="SDBH:גלע:000001">enrede</a>.
    
\v 15 El que <a name="02001701500002" ll="SDBH:צדק:000003">justifica</a> al <a name="02001701500004" ll="SDBH:רָשָׁע:000000">impío</a>, y el que <a name="02001701500008" ll="SDBH:רשׁע:000001">condena</a> al <a name="02001701500010" ll="SDBH:צַדִּיק:000000">justo</a>,<a name="02001701500018" ll="SDBH:שְׁנַיִם:000000">Ambos</a> son <a name="02001701500016" ll="SDBH:גַּם:000000">igualmente</a> <a name="02001701500012" ll="SDBH:תֹּועֵבָה:000001">abominación</a> a <a name="02001701500014" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 16 ¿De qué sirve el <a name="02001701600006" ll="SDBH:מְחִיר:000000">precio</a> en la <a name="02001701600010">mano</a> del <a name="02001701600012" ll="SDBH:כְּסִיל:000000">necio</a> para <a name="02001701600016" ll="SDBH:קנה:000000;SDBH:קנה:000002">comprar</a> <a name="02001701600018" ll="SDBH:חָכְמָה:000000">sabiduría</a>,<a name="02001701600024">No</a> teniendo <a name="02001701600022" ll="SDBH:לֵב:000003">entendimiento</a>?
    
\v 17 En <a name="02001701700004">todo</a> <a name="02001701700006" ll="SDBH:עֵת:000001">tiempo</a> <a name="02001701700008" ll="SDBH:אהב:000001">ama</a> el amigo,Y es como un <a name="02001701700016" ll="SDBH:אָח:000003">hermano</a> en tiempo de <a name="02001701700020" ll="SDBH:צָרָה:000000">angustia</a>.
    
\v 18 El <a name="02001701800002" ll="SDBH:אָדָם:000000">hombre</a> <a name="02001701800004" ll="SDBH:חָסֵר:000001">falto</a> de <a name="02001701800006" ll="SDBH:לֵב:000003">entendimiento</a> presta <a name="02001701800008" ll="SDBH:תקע:000002">fianzas</a>,Y <a name="02001701800012" ll="SDBH:ערב:000000">sale</a> por <a name="02001701800014" ll="SDBH:עֲרֻבָּה:000000">fiador</a> en <a name="02001701800018">presencia</a> de su amigo.
    
\v 19 El <a name="02001701900006" ll="SDBH:אהב:000000">que</a> <a name="02001701900006" ll="SDBH:אהב:000000">ama</a> la <a name="02001701900008" ll="SDBH:מַצָּה:001000">disputa</a>, <a name="02001701900002" ll="SDBH:אהב:000000">ama</a> la <a name="02001701900004" ll="SDBH:פֶּשַׁע:000000">transgresión</a>;Y el que abre demasiado la <a name="02001701900012" ll="SDBH:פֶּתַח:000000">puerta</a> <a name="02001701900014" ll="SDBH:בקשׁ:000007">busca</a> su <a name="02001701900016" ll="SDBH:שֶׁבֶר:000000;SDBH:שֶׁבֶר:000001">ruina</a>.
    
\v 20 El <a name="02001702000002" ll="SDBH:עִקֵּשׁ:000001">perverso</a> de <a name="02001702000004" ll="SDBH:לֵב:000001">corazón</a> <a name="02001702000006" ll="SDBH:לֹא:000000">nunca</a> <a name="02001702000008" ll="SDBH:מצא:000011">hallará</a> el <a name="02001702000010" ll="SDBH:טֹוב:000002">bien</a>,Y el que <a name="02001702000014" ll="SDBH:הפך:000008">revuelve</a> con <a name="02001702000018" ll="SDBH:לָשֹׁון:000000">su</a> <a name="02001702000018" ll="SDBH:לָשֹׁון:000000">lengua</a> <a name="02001702000020" ll="SDBH:נפל:000012">caerá</a> en el <a name="02001702000024" ll="SDBH:רָעָה:000002">mal</a>.
    
\v 21 El que <a name="02001702100002" ll="SDBH:ילד:000001">engendra</a> al <a name="02001702100004" ll="SDBH:כְּסִיל:000000">insensato</a>, para su <a name="02001702100008" ll="SDBH:תּוּגָה:000000">tristeza</a> lo engendra;Y el <a name="02001702100018" ll="SDBH:אָב:000000">padre</a> del <a name="02001702100020" ll="SDBH:נָבָל:000000">necio</a> <a name="02001702100014" ll="SDBH:לֹא:000000">no</a> <a name="02001702100016" ll="SDBH:שׂמח:000000">se</a> <a name="02001702100016" ll="SDBH:שׂמח:000000">alegrará</a>.
    
\v 22 El <a name="02001702200002" ll="SDBH:לֵב:000001">corazón</a> <a name="02001702200004" ll="SDBH:שָׂמֵחַ:000000">alegre</a> <a name="02001702200006" ll="SDBH:יטב:000002" tl="HOTTP2744">constituye</a> buen <a name="02001702200008" ll="SDBH:גֵּהָה:000000" tl="HOTTP2744">remedio</a>;Mas el <a name="02001702200012" ll="SDBH:רוּחַ:000002">espíritu</a> <a name="02001702200014" ll="SDBH:נָכֵא:000000">triste</a> seca los <a name="02001702200018" ll="SDBH:גֶּרֶם:000000">huesos</a>.
    
\v 23 El <a name="02001702300008" ll="SDBH:רָשָׁע:000000">impío</a> <a name="02001702300010" ll="SDBH:לקח:000000">toma</a> <a name="02001702300002" ll="SDBH:שֹׁחַד:000000">soborno</a> del <a name="02001702300006" ll="SDBH:חֵיק:000000">seno</a>Para <a name="02001702300014" ll="SDBH:נטה:000010">pervertir</a> las <a name="02001702300016" ll="SDBH:אֹרַח:000002">sendas</a> de la <a name="02001702300018" ll="SDBH:מִשְׁפָּט:000007">justicia</a>.
    
\v 24 En el <a name="02001702400004" ll="SDBH:פָּנֶה:000000">rostro</a> del <a name="02001702400006" ll="SDBH:בין:000000">entendido</a> aparece la <a name="02001702400008" ll="SDBH:חָכְמָה:000000">sabiduría</a>;Mas los <a name="02001702400012" ll="SDBH:עַיִן:000000">ojos</a> del <a name="02001702400014" ll="SDBH:כְּסִיל:000000">necio</a> vagan hasta el <a name="02001702400018" ll="SDBH:קָצֶה:000002">extremo</a> de la <a name="02001702400020" ll="SDBH:אֶרֶץ:000000">tierra</a>.
    
\v 25 El <a name="02001702500008" ll="SDBH:בֵּן:000000">hijo</a> <a name="02001702500010" ll="SDBH:כְּסִיל:000000">necio</a> es <a name="02001702500002" ll="SDBH:כַּעַס:000000;SDBH:כַּעַס:000001">pesadumbre</a> de <a name="02001702500006" ll="SDBH:אָב:000000">su</a> <a name="02001702500006" ll="SDBH:אָב:000000">padre</a>,Y <a name="02001702500014" ll="SDBH:מֶמֶר:000000">amargura</a> a la <a name="02001702500018" ll="SDBH:ילד:000000">que</a> <a name="02001702500018" ll="SDBH:ילד:000000">lo</a> <a name="02001702500018" ll="SDBH:ילד:000000">dio</a> <a name="02001702500018" ll="SDBH:ילד:000000">a</a> <a name="02001702500018" ll="SDBH:ילד:000000">luz</a>.
    
\v 26 <a name="02001702600002" ll="SDBH:גַּם:000000">Ciertamente</a> <a name="02001702600012" ll="SDBH:לֹא:000000">no</a> es <a name="02001702600014" ll="SDBH:טֹוב:000002">bueno</a> <a name="02001702600004" ll="SDBH:ענשׁ:000000">condenar</a> al <a name="02001702600010" ll="SDBH:צַדִּיק:000000">justo</a>,Ni <a name="02001702600018">herir</a> a los <a name="02001702600020" ll="SDBH:נָדִיב:000001">nobles</a> <a name="02001702600024" ll="SDBH:יֹשֶׁר:000000">que</a> hacen lo <a name="02001702600022">recto</a>.
    
\v 27 El que <a name="02001702700002" ll="SDBH:חשׂך:000001">ahorra</a> <a name="02001702700004" ll="SDBH:אֵמֶר:000000">sus</a> <a name="02001702700004" ll="SDBH:אֵמֶר:000000">palabras</a> tiene <a name="02001702700006">sabiduría</a>;De <a name="02001702700014" ll="SDBH:רוּחַ:000002">espíritu</a> <a name="02001702700010" ll="SDBH:יָקָר:000000;SDBH:קַר:000001">prudente</a> es el <a name="02001702700016">hombre</a> <a name="02001702700018" ll="SDBH:תְּבוּנָה:000000">entendido</a>.
    
\v 28 <a name="02001702800002" ll="SDBH:גַּם:000000">Aun</a> el <a name="02001702800004" ll="SDBH:אֱוִיל:000000">necio</a>, cuando <a name="02001702800006" ll="SDBH:חרשׁ:001001">calla</a>, es <a name="02001702800010" ll="SDBH:חשׁב:000007">contado</a> por <a name="02001702800008" ll="SDBH:חָכָם, קסם:000000">sabio</a>;El que <a name="02001702800012" ll="SDBH:אטם:000001">cierra</a> <a name="02001702800014" ll="SDBH:שָׂפָה:000001">sus</a> <a name="02001702800014" ll="SDBH:שָׂפָה:000001">labios</a> es <a name="02001702800016" ll="SDBH:בין:000000">entendido</a>.
\c 18
\v 1 Su <a name="02001800100004" ll="SDBH:תַּאֲוָה:000001" tl="HOTTP2745">deseo</a> <a name="02001800100006" ll="SDBH:בקשׁ:000007">busca</a> el que <a name="02001800100008" ll="SDBH:פרד:000002">se</a> <a name="02001800100008" ll="SDBH:פרד:000002">desvía</a>,Y <a name="02001800100016" ll="SDBH:גלע:000000">se</a> <a name="02001800100016" ll="SDBH:גלע:000000">entremete</a> en <a name="02001800100012">todo</a> <a name="02001800100014" ll="SDBH:תּוּשִׁיָּה:000000">negocio</a>.
    
\v 2 <a name="02001800200002" ll="SDBH:לֹא:000000">No</a> toma <a name="02001800200004" ll="SDBH:חפץ:000002">placer</a> el <a name="02001800200006" ll="SDBH:כְּסִיל:000000">necio</a> en la <a name="02001800200010" ll="SDBH:תְּבוּנָה:000000">inteligencia</a>,Sino en que <a name="02001800200020" ll="SDBH:לֵב:000001">su</a> <a name="02001800200020" ll="SDBH:לֵב:000001">corazón</a> <a name="02001800200018" ll="SDBH:גלה:000011">se</a> <a name="02001800200018" ll="SDBH:גלה:000011">descubra</a>.
    
\v 3 Cuando <a name="02001800300004">viene</a> el <a name="02001800300006" ll="SDBH:רָשָׁע:000000" tl="HOTTP2746">impío</a>, <a name="02001800300008">viene</a> <a name="02001800300010" ll="SDBH:גַּם:000000">también</a> el <a name="02001800300012" ll="SDBH:בּוּז:000000">menosprecio</a>,Y <a name="02001800300016">con</a> el <a name="02001800300018" ll="SDBH:קָלֹון:000000">deshonrador</a> la <a name="02001800300020" ll="SDBH:חֶרְפָּה:000000">afrenta</a>.
    
\v 4 <a name="02001800400002" ll="SDBH:מַיִם:000000">Aguas</a> <a name="02001800400004" ll="SDBH:עָמֹק:000000">profundas</a> son las <a name="02001800400006" ll="SDBH:דָּבָר:000000">palabras</a> de la <a name="02001800400008" ll="SDBH:פֶּה:000000">boca</a> del <a name="02001800400010">hombre</a>;Y <a name="02001800400012" ll="SDBH:נַחַל:000000">arroyo</a> que <a name="02001800400014" ll="SDBH:נבע:000000">rebosa</a>, la <a name="02001800400016" ll="SDBH:מָקֹור:000000">fuente</a> de la <a name="02001800400018" ll="SDBH:חָכְמָה:000000">sabiduría</a>.
    
\v 5 Tener <a name="02001800500002" ll="SDBH:נשׂא:000050">respeto</a> a la <a name="02001800500004" ll="SDBH:פָּנֶה:000000">persona</a> del <a name="02001800500006" ll="SDBH:רָשָׁע:000000">impío</a>,Para <a name="02001800500014" ll="SDBH:נטה:002008">pervertir</a> el <a name="02001800500022" ll="SDBH:מִשְׁפָּט:000000">derecho</a> del <a name="02001800500016" ll="SDBH:צַדִּיק:000000">justo</a>, <a name="02001800500008" ll="SDBH:לֹא:000000">no</a> es <a name="02001800500010" ll="SDBH:טֹוב:000002">bueno</a>.
    
\v 6 Los <a name="02001800600002" ll="SDBH:שָׂפָה:000001">labios</a> del <a name="02001800600004" ll="SDBH:כְּסִיל:000000">necio</a> <a name="02001800600006">traen</a> contienda;Y <a name="02001800600014" ll="SDBH:פֶּה:000000">su</a> <a name="02001800600014" ll="SDBH:פֶּה:000000">boca</a> los <a name="02001800600018" ll="SDBH:מַהֲלֻמֹות:000000">azotes</a> <a name="02001800600020" ll="SDBH:קרא:000003">llama</a>.
    
\v 7 La <a name="02001800700002" ll="SDBH:פֶּה:000000">boca</a> del <a name="02001800700004" ll="SDBH:כְּסִיל:000000">necio</a> es <a name="02001800700006" ll="SDBH:מְחִתָּה:000001">quebrantamiento</a> para sí,Y <a name="02001800700012" ll="SDBH:שָׂפָה:000001">sus</a> <a name="02001800700012" ll="SDBH:שָׂפָה:000001">labios</a> son <a name="02001800700014" ll="SDBH:מֹוקֵשׁ:000000">lazos</a> para <a name="02001800700016" ll="SDBH:נֶפֶשׁ:000001">su</a> <a name="02001800700016" ll="SDBH:נֶפֶשׁ:000001">alma</a>.
    
\v 8 Las <a name="02001800800002" ll="SDBH:דָּבָר:000000">palabras</a> del chismoso <a name="02001800800004" ll="SDBH:רגן:000002">son</a> como bocados <a name="02001800800008" ll="SDBH:להם:000000">suaves</a>,Y <a name="02001800800012">penetran</a> <a name="02001800800014" ll="SDBH:ירד:000000">hasta</a> las <a name="02001800800016" ll="SDBH:חֶדֶר:000002">entrañas</a>.
    
\v 9 <a name="02001800900002" ll="SDBH:גַּם:000000">También</a> el que es <a name="02001800900004" ll="SDBH:רפה:000006">negligente</a> en <a name="02001800900012">su</a> <a name="02001800900008" ll="SDBH:מְלָאכָה:000000">trabajo</a>Es <a name="02001800900010" ll="SDBH:אָח:000003">hermano</a> del hombre <a name="02001800900016" ll="SDBH:בַּעַל:000005">disipador</a>.
    
\v 10 <a name="02001801000002" ll="SDBH:מִגְדָּל:000000">Torre</a> <a name="02001801000004" ll="SDBH:עֹז:000000">fuerte</a> es el <a name="02001801000006" ll="SDBH:שֵׁם:000003">nombre</a> de <a name="02001801000008" ll="SDBH:יהוה:000000">Jehová</a>;A él <a name="02001801000012" ll="SDBH:רוץ:000000">correrá</a> el <a name="02001801000014" ll="SDBH:צַדִּיק:000000">justo</a>, y será <a name="02001801000018" ll="SDBH:שׂגב:000004">levantado</a>.
    
\v 11 Las <a name="02001801100002" ll="SDBH:הֹון:000000">riquezas</a> del <a name="02001801100004" ll="SDBH:עָשִׁיר:000000">rico</a> son su <a name="02001801100006" ll="SDBH:קִרְיָה:000000">ciudad</a> <a name="02001801100008" ll="SDBH:עֹז:000000">fortificada</a>,Y como un <a name="02001801100014" ll="SDBH:חֹומָה:000000">muro</a> <a name="02001801100016" ll="SDBH:שׂגב:000000">alto</a> en <a name="02001801100020" ll="SDBH:מַשְׂכִּית:000002">su</a> <a name="02001801100020" ll="SDBH:מַשְׂכִּית:000002">imaginación</a>.
    
\v 12 <a name="02001801200004;02001801200018">Antes</a> del <a name="02001801200006" ll="SDBH:שֶׁבֶר:000000;SDBH:שֶׁבֶר:000001">quebrantamiento</a> se eleva el <a name="02001801200010" ll="SDBH:לֵב:000001">corazón</a> del <a name="02001801200012">hombre</a>,Y antes de la <a name="02001801200020" ll="SDBH:כָּבֹוד:000000">honra</a> es el abatimiento.
    
\v 13 Al que <a name="02001801300002" ll="SDBH:שׁוב:000049">responde</a> <a name="02001801300004" ll="SDBH:דָּבָר:000000">palabra</a> antes de <a name="02001801300010">oír</a>,<a name="02001801300014">Le</a> es <a name="02001801300012" ll="SDBH:אִוֶּלֶת:000000">fatuidad</a> y <a name="02001801300020" ll="SDBH:כְּלִמָּה:000000">oprobio</a>.
    
\v 14 El <a name="02001801400002" ll="SDBH:רוּחַ:000002">ánimo</a> del <a name="02001801400004">hombre</a> <a name="02001801400006" ll="SDBH:כול:000001">soportará</a> <a name="02001801400008" ll="SDBH:מַחֲלֶה:000000">su</a> <a name="02001801400008" ll="SDBH:מַחֲלֶה:000000">enfermedad</a>;Mas ¿<a name="02001801400016" ll="SDBH:מִי:000000">quién</a> <a name="02001801400018" ll="SDBH:נשׂא:000015">soportará</a> al <a name="02001801400012" ll="SDBH:רוּחַ:000002">ánimo</a> <a name="02001801400014" ll="SDBH:נָכֵא:000000">angustiado</a>?
    
\v 15 El <a name="02001801500002" ll="SDBH:לֵב:000001">corazón</a> del <a name="02001801500004" ll="SDBH:בין:000000">entendido</a> adquiere <a name="02001801500006" ll="SDBH:קנה:000002">sabiduría</a>;Y el <a name="02001801500012" ll="SDBH:אֹזֶן:000000">oído</a> de los <a name="02001801500014" ll="SDBH:חָכָם, קסם:000000">sabios</a> <a name="02001801500016" ll="SDBH:בקשׁ:000007">busca</a> la <a name="02001801500008" ll="SDBH:דַּעַת:000000">ciencia</a>.
    
\v 16 La <a name="02001801600002" ll="SDBH:מַתָּן:000000" tl="HOTTP2747">dádiva</a> del <a name="02001801600004" ll="SDBH:אָדָם:000000" tl="HOTTP2747">hombre</a> le <a name="02001801600006" ll="SDBH:רחב:000006">ensancha</a> el caminoY le lleva <a name="02001801600014">delante</a> de los <a name="02001801600016" ll="SDBH:גָּדֹול:000002">grandes</a>.
    
\v 17 <a name="02001801700002" ll="SDBH:צַדִּיק:000000">Justo</a> parece el <a name="02001801700006" ll="SDBH:רִאשֹׁון:000000">primero</a> que aboga por su causa;Pero <a name="02001801700014">viene</a> su adversario, y le <a name="02001801700024">descubre</a>.
    
\v 18 La <a name="02001801800008" ll="SDBH:גֹּורָל:000001">suerte</a> pone <a name="02001801800004" ll="SDBH:שׁבת:000003">fin</a> a los <a name="02001801800002" ll="SDBH:מִדְיָן:000000">pleitos</a>,Y <a name="02001801800016" ll="SDBH:פרד:000003">decide</a> <a name="02001801800012">entre</a> los <a name="02001801800014" ll="SDBH:עָצוּם:000002" tl="HOTTP2748">poderosos</a>.
    
\v 19 El <a name="02001801900002" ll="SDBH:אָח:000001" tl="HOTTP2749">hermano</a> <a name="02001801900004" ll="SDBH:פשׁע:000000" tl="HOTTP2749">ofendido</a> es más tenaz que una <a name="02001801900008" ll="SDBH:קִרְיָה:000000" tl="HOTTP2749">ciudad</a> <a name="02001801900010" ll="SDBH:עֹז:000000" tl="HOTTP2749">fuerte</a>,Y las <a name="02001801900014" ll="SDBH:מִדְיָן:000000">contiendas</a> de los hermanos son como <a name="02001801900022" ll="SDBH:בְּרִיחַ:000001">cerrojos</a> de <a name="02001801900024" ll="SDBH:אַרְמֹון:000000">alcázar</a>.
    
\v 20 Del <a name="02001802000004" ll="SDBH:פְּרִי:000003">fruto</a> de la <a name="02001802000006" ll="SDBH:פֶּה:000000">boca</a> del <a name="02001802000008">hombre</a> <a name="02001802000010" ll="SDBH:שׂבע:000000">se</a> <a name="02001802000010" ll="SDBH:שׂבע:000000">llenará</a> <a name="02001802000012" ll="SDBH:בֶּטֶן:000000">su</a> <a name="02001802000012" ll="SDBH:בֶּטֶן:000000">vientre</a>;<a name="02001802000018" ll="SDBH:שׂבע:000000">Se</a> <a name="02001802000018" ll="SDBH:שׂבע:000000">saciará</a> del <a name="02001802000014" ll="SDBH:תְּבוּאָה:000001">producto</a> de <a name="02001802000016" ll="SDBH:שָׂפָה:000001">sus</a> <a name="02001802000016" ll="SDBH:שָׂפָה:000001">labios</a>.
    
\v 21 La <a name="02001802100002" ll="SDBH:מָוֶת:000000">muerte</a> y la <a name="02001802100006" ll="SDBH:חַיִּים:000000">vida</a> están en <a name="02001802100010">poder</a> de la <a name="02001802100012" ll="SDBH:לָשֹׁון:000000">lengua</a>,Y el que la <a name="02001802100016" ll="SDBH:אהב:000000">ama</a> <a name="02001802100018;02001802100020" ll="SDBH:אכל:000008;SDBH:פְּרִי:000003">comerá</a> <a name="02001802100018;02001802100020" ll="SDBH:אכל:000008;SDBH:פְּרִי:000003">de</a> <a name="02001802100018;02001802100020" ll="SDBH:אכל:000008;SDBH:פְּרִי:000003">sus</a> <a name="02001802100018;02001802100020" ll="SDBH:אכל:000008;SDBH:פְּרִי:000003">frutos</a>.
    
\v 22 El que <a name="02001802200002" ll="SDBH:מצא:000000">halla</a> <a name="02001802200004" ll="SDBH:אִשָּׁה:000000">esposa</a> <a name="02001802200006" ll="SDBH:מצא:000000">halla</a> el <a name="02001802200008" ll="SDBH:טֹוב:000002">bien</a>,Y alcanza la <a name="02001802200014" ll="SDBH:רָצֹון:000000">benevolencia</a> de <a name="02001802200018" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 23 El <a name="02001802300006" ll="SDBH:רושׁ:000001">pobre</a> <a name="02001802300004" ll="SDBH:דבר:001000">habla</a> con <a name="02001802300002" ll="SDBH:תַּחֲנוּן:000000">ruegos</a>,Mas el <a name="02001802300010" ll="SDBH:עָשִׁיר:000000">rico</a> <a name="02001802300012" ll="SDBH:ענה:000000">responde</a> <a name="02001802300014" ll="SDBH:עַז:000002">durezas</a>.
    
\v 24 El <a name="02001802400002" ll="SDBH:אִשׁ:000000" tl="HOTTP2750">hombre</a> que tiene amigos ha de mostrarse amigo;Y <a name="02001802400014" ll="SDBH:אהב:000001">amigo</a> <a name="02001802400012">hay</a> más <a name="02001802400016" ll="SDBH:דָּבֵק:000001">unido</a> que un <a name="02001802400020" ll="SDBH:אָח:000000">hermano</a>.
\c 19
\v 1 <a name="02001900100002" ll="SDBH:טֹוב:000002">Mejor</a> es el pobre que <a name="02001900100006" ll="SDBH:הלך:000004">camina</a> en <a name="02001900100010" ll="SDBH:תֹּם:000001">integridad</a>,Que el de <a name="02001900100014" ll="SDBH:עִקֵּשׁ:000001">perversos</a> <a name="02001900100016" ll="SDBH:שָׂפָה:000001">labios</a> y <a name="02001900100022" ll="SDBH:כְּסִיל:000000">fatuo</a>.
    
\v 2 El <a name="02001900200010" ll="SDBH:נֶפֶשׁ:000001">alma</a> sin <a name="02001900200008" ll="SDBH:דַּעַת:000000">ciencia</a> <a name="02001900200006" ll="SDBH:לֹא:000000">no</a> es <a name="02001900200014" ll="SDBH:טֹוב:000002">buena</a>,Y aquel que <a name="02001900200018" ll="SDBH:אוץ:000000">se</a> <a name="02001900200018" ll="SDBH:אוץ:000000">apresura</a> con los <a name="02001900200022" ll="SDBH:רֶגֶל:000003">pies</a>, <a name="02001900200024" ll="SDBH:חטא:001000">peca</a>.
    
\v 3 La <a name="02001900300002" ll="SDBH:אִוֶּלֶת:000000">insensatez</a> del <a name="02001900300004" ll="SDBH:אָדָם:000000">hombre</a> <a name="02001900300006" ll="SDBH:סלף:000000">tuerce</a> <a name="02001900300008" ll="SDBH:דֶּרֶךְ:000007">su</a> <a name="02001900300008" ll="SDBH:דֶּרֶךְ:000007">camino</a>,Y luego <a name="02001900300012">contra</a> <a name="02001900300014" ll="SDBH:יהוה:000000">Jehová</a> <a name="02001900300016" ll="SDBH:זעף:000000">se</a> <a name="02001900300016" ll="SDBH:זעף:000000">irrita</a> <a name="02001900300018" ll="SDBH:לֵב:000001">su</a> <a name="02001900300018" ll="SDBH:לֵב:000001">corazón</a>.
    
\v 4 Las riquezas <a name="02001900400002" ll="SDBH:הֹון:000000">traen</a> <a name="02001900400004">muchos</a> <a name="02001900400008" ll="SDBH:רַב:000001">amigos</a>;Mas el <a name="02001900400012" ll="SDBH:דַּל:001000">pobre</a> es <a name="02001900400018" ll="SDBH:פרד:000003">apartado</a> de su amigo.
    
\v 5 El <a name="02001900500002" ll="SDBH:עֵד:000000">testigo</a> <a name="02001900500004" ll="SDBH:שֶׁקֶר:000000">falso</a> <a name="02001900500006" ll="SDBH:לֹא:000000">no</a> <a name="02001900500008" ll="SDBH:נקה:000002">quedará</a> <a name="02001900500008" ll="SDBH:נקה:000002">sin</a> <a name="02001900500008" ll="SDBH:נקה:000002">castigo</a>,Y <a name="02001900500012" ll="SDBH:פוח:000003">el</a> <a name="02001900500012" ll="SDBH:פוח:000003">que</a> <a name="02001900500012" ll="SDBH:פוח:000003">habla</a> <a name="02001900500014" ll="SDBH:כָּזָב:000000">mentiras</a> <a name="02001900500016" ll="SDBH:לֹא:000000">no</a> <a name="02001900500018" ll="SDBH:מלט:000000">escapará</a>.
    
\v 6 <a name="02001900600002" ll="SDBH:רַב:000001">Muchos</a> <a name="02001900600004;02001900600006" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">buscan</a> <a name="02001900600004;02001900600006" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">el</a> <a name="02001900600004;02001900600006" ll="SDBH:חלה:000003;SDBH:פָּנֶה:000000">favor</a> del <a name="02001900600008" ll="SDBH:נָדִיב:000001">generoso</a>,Y <a name="02001900600012">cada</a> uno es <a name="02001900600016" ll="SDBH:רֵעַ:001000">amigo</a> del <a name="02001900600020;02001900600022" ll="SDBH:מַתָּן:000000">hombre</a> <a name="02001900600020;02001900600022" ll="SDBH:מַתָּן:000000">que</a> <a name="02001900600020;02001900600022" ll="SDBH:מַתָּן:000000">da</a>.
    
\v 7 <a name="02001900700002">Todos</a> los <a name="02001900700004" ll="SDBH:אָח:000001">hermanos</a> del <a name="02001900700006" ll="SDBH:רושׁ:000001">pobre</a> le <a name="02001900700008" ll="SDBH:שׂנא:000000">aborrecen</a>;¡<a name="02001900700010;02001900700012" ll="SDBH:אַף:001001">Cuánto</a> <a name="02001900700010;02001900700012" ll="SDBH:אַף:001001">más</a> <a name="02001900700014" ll="SDBH:מֵרֵעַ:000000">sus</a> <a name="02001900700014" ll="SDBH:מֵרֵעַ:000000">amigos</a> <a name="02001900700016" ll="SDBH:רחק:000000" tl="HOTTP2752">se</a> <a name="02001900700016" ll="SDBH:רחק:000000" tl="HOTTP2752">alejarán</a> <a name="02001900700018">de</a> <a name="02001900700018">él</a>!<a name="02001900700020" ll="SDBH:רדף:000004">Buscará</a> la <a name="02001900700022" ll="SDBH:אֵמֶר:000000">palabra</a>, y <a name="02001900700026;02001900700028">no</a> <a name="02001900700026;02001900700028">la</a> <a name="02001900700026;02001900700028">hallará</a>.
    
\v 8 El que <a name="02001900800002" ll="SDBH:קנה:000002">posee</a> <a name="02001900800004" ll="SDBH:לֵב:000003">entendimiento</a> <a name="02001900800006" ll="SDBH:אהב:000000">ama</a> <a name="02001900800008" ll="SDBH:נֶפֶשׁ:000001">su</a> <a name="02001900800008" ll="SDBH:נֶפֶשׁ:000001">alma</a>;El que <a name="02001900800010" ll="SDBH:שׁמר:000002">guarda</a> la <a name="02001900800012" ll="SDBH:תְּבוּנָה:000000">inteligencia</a> <a name="02001900800016" ll="SDBH:מצא:000006">hallará</a> el <a name="02001900800018" ll="SDBH:טֹוב:000002">bien</a>.
    
\v 9 El <a name="02001900900002" ll="SDBH:עֵד:000000">testigo</a> <a name="02001900900004" ll="SDBH:שֶׁקֶר:000000">falso</a> <a name="02001900900006" ll="SDBH:לֹא:000000">no</a> <a name="02001900900008" ll="SDBH:נקה:000002">quedará</a> <a name="02001900900008" ll="SDBH:נקה:000002">sin</a> <a name="02001900900008" ll="SDBH:נקה:000002">castigo</a>,Y <a name="02001900900012" ll="SDBH:פוח:000003">el</a> <a name="02001900900012" ll="SDBH:פוח:000003">que</a> <a name="02001900900012" ll="SDBH:פוח:000003">habla</a> <a name="02001900900014" ll="SDBH:כָּזָב:000000">mentiras</a> <a name="02001900900016" ll="SDBH:אבד:000004">perecerá</a>.
    
\v 10 <a name="02001901000002" ll="SDBH:לֹא:000000">No</a> <a name="02001901000004" ll="SDBH:נָאוֶה:000001">conviene</a> al <a name="02001901000008" ll="SDBH:כְּסִיל:000000">necio</a> el <a name="02001901000010" ll="SDBH:תַּעֲנוּג:000000">deleite</a>;¡<a name="02001901000012;02001901000014" ll="SDBH:אַף:001001">Cuánto</a> <a name="02001901000012;02001901000014" ll="SDBH:אַף:001001">menos</a> al <a name="02001901000018" ll="SDBH:עֶבֶד:000000">siervo</a> ser <a name="02001901000020" ll="SDBH:משׁל:001000">señor</a> de los <a name="02001901000024" ll="SDBH:שַׂר:000000">príncipes</a>!
    
\v 11 La <a name="02001901100002" ll="SDBH:שֶׂכֶל:000000">cordura</a> del <a name="02001901100004" ll="SDBH:אָדָם:000000">hombre</a> <a name="02001901100006;02001901100008" ll="SDBH:אַף:000008;SDBH:ארך:000007">detiene</a> <a name="02001901100006;02001901100008" ll="SDBH:אַף:000008;SDBH:ארך:000007">su</a> <a name="02001901100006;02001901100008" ll="SDBH:אַף:000008;SDBH:ארך:000007">furor</a>,Y <a name="02001901100012" ll="SDBH:תִּפְאֶרֶת:000000">su</a> <a name="02001901100012" ll="SDBH:תִּפְאֶרֶת:000000">honra</a> es <a name="02001901100014">pasar</a> <a name="02001901100016">por</a> <a name="02001901100016">alto</a> la <a name="02001901100018" ll="SDBH:פֶּשַׁע:000000">ofensa</a>.
    
