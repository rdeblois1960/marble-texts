\v 17 <a name="00501701700004;00501701700028" ll="SDBH:לֹא:000000">Ni</a> <a name="00501701700006">tomará</a> para sí muchas <a name="00501701700010" ll="SDBH:אִשָּׁה:000000">mujeres</a>, para que <a name="00501701700018" ll="SDBH:לֵבָב:000001">su</a> <a name="00501701700018" ll="SDBH:לֵבָב:000001">corazón</a> <a name="00501701700014" ll="SDBH:לֹא:000000">no</a> <a name="00501701700016" ll="SDBH:סור:000002">se</a> <a name="00501701700016" ll="SDBH:סור:000002">desvíe</a>; ni <a name="00501701700022" ll="SDBH:כֶּסֶף:000000">plata</a> ni <a name="00501701700026" ll="SDBH:זָהָב:000000">oro</a> <a name="00501701700030">amontonará</a> para sí en <a name="00501701700034" ll="SDBH:מְאֹד:000001">abundancia</a>. 
\v 18 Y cuando <a name="00501701800008">se</a> <a name="00501701800008">siente</a> <a name="00501701800010">sobre</a> el <a name="00501701800012" ll="SDBH:כִּסֵּא:000002">trono</a> de <a name="00501701800014">su</a> <a name="00501701800014">reino</a>, entonces <a name="00501701800018" ll="SDBH:כתב:000000">escribirá</a> <a name="00501701800020">para</a> <a name="00501701800020">sí</a> <a name="00501701800034">en</a> un <a name="00501701800036" ll="SDBH:סֵפֶר:000000">libro</a> una <a name="00501701800024" ll="SDBH:מִשְׁנֶה:000003">copia</a> de <a name="00501701800032" ll="SDBH:זֶה:000000;SDBH:זֹה:000000">esta</a> <a name="00501701800028" ll="SDBH:תֹּורָה:000000">ley</a>, <a name="00501701800038">del</a> original que está <a name="00501701800040;00501701800042">al</a> <a name="00501701800040;00501701800042">cuidado</a> <a name="00501701800040;00501701800042">de</a> los <a name="00501701800046" ll="SDBH:כֹּהֵן:000000">sacerdotes</a> <a name="00501701800050" ll="SDBH:לֵוִי:000001">levitas</a>; 
