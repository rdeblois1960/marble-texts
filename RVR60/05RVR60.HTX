\v 57 al recién <a name="00502805700006" ll="SDBH:שִׁלְיָה:000000">nacido</a> que <a name="00502805700010" ll="SDBH:יצא:000008">sale</a> de <a name="00502805700014">entre</a> <a name="00502805700016" ll="SDBH:רֶגֶל:000001">sus</a> <a name="00502805700016" ll="SDBH:רֶגֶל:000001">pies</a>, y a <a name="00502805700022" ll="SDBH:בֵּן:000000" tl="HOTTP669">sus</a> <a name="00502805700022" ll="SDBH:בֵּן:000000" tl="HOTTP669">hijos</a> <a name="00502805700024">que</a> <a name="00502805700026" ll="SDBH:ילד:000000">diere</a> <a name="00502805700026" ll="SDBH:ילד:000000">a</a> <a name="00502805700026" ll="SDBH:ילד:000000">luz</a>; pues los <a name="00502805700030" ll="SDBH:אכל:000000">comerá</a> <a name="00502805700038;00502805700042" ll="SDBH:סֵתֶר:000001">ocultamente</a>, por la <a name="00502805700034" ll="SDBH:חֹסֶר:000000">carencia</a> de <a name="00502805700036">todo</a>, en el <a name="00502805700046" ll="SDBH:מָצֹור:000000;SDBH:מָצֹור:001000">asedio</a> y en el <a name="00502805700052" ll="SDBH:מָצֹוק:000000">apuro</a> con <a name="00502805700054">que</a> <a name="00502805700060" ll="SDBH:איב:000001">tu</a> <a name="00502805700060" ll="SDBH:איב:000001">enemigo</a> <a name="00502805700058">te</a> <a name="00502805700056" ll="SDBH:צוק:000000">oprimirá</a> en <a name="00502805700064" ll="SDBH:שַׁעַר:000000">tus</a> <a name="00502805700064" ll="SDBH:שַׁעַר:000000">ciudades</a>.
    
\v 58 <a name="00502805800002">Si</a> <a name="00502805800004" ll="SDBH:לֹא:000000">no</a> <a name="00502805800006" ll="SDBH:שׁמר:000006">cuidares</a> de <a name="00502805800010">poner</a> <a name="00502805800010">por</a> <a name="00502805800010">obra</a> <a name="00502805800014">todas</a> las <a name="00502805800016" ll="SDBH:דָּבָר:000003">palabras</a> de <a name="00502805800024" ll="SDBH:זֶה:000000;SDBH:זֹה:000000">esta</a> <a name="00502805800020" ll="SDBH:תֹּורָה:000000">ley</a> que están <a name="00502805800028" ll="SDBH:כתב:000000">escritas</a> en <a name="00502805800038" ll="SDBH:זֶה:000000">este</a> <a name="00502805800034" ll="SDBH:סֵפֶר:000000">libro</a>, <a name="00502805800042" ll="SDBH:ירא:000000">temiendo</a> <a name="00502805800062" ll="SDBH:זֶה:000000">este</a> <a name="00502805800048" ll="SDBH:שֵׁם:000002">nombre</a> <a name="00502805800052" ll="SDBH:כבד:000006">glorioso</a> y <a name="00502805800058" ll="SDBH:ירא:000001">temible</a>: <a name="00502805800066" ll="SDBH:יהוה:000000">JEHOVÁ</a> <a name="00502805800068" ll="SDBH:אֱלֹהִים:000001">TU</a> <a name="00502805800068" ll="SDBH:אֱלֹהִים:000001">DIOS</a>, 
\v 59 entonces <a name="00502805900006" ll="SDBH:יהוה:000000">Jehová</a> aumentará <a name="00502805900004" ll="SDBH:פלא:000002">maravillosamente</a> <a name="00502805900010" ll="SDBH:מַכָּה:000000">tus</a> <a name="00502805900010" ll="SDBH:מַכָּה:000000">plagas</a> y las <a name="00502805900016" ll="SDBH:מַכָּה:000000">plagas</a> de <a name="00502805900018" ll="SDBH:זֶרַע:000004">tu</a> <a name="00502805900018" ll="SDBH:זֶרַע:000004">descendencia</a>, <a name="00502805900020" ll="SDBH:מַכָּה:000000">plagas</a> <a name="00502805900022" ll="SDBH:גָּדֹול:000003">grandes</a> y <a name="00502805900026" ll="SDBH:אמן:000002">permanentes</a>, y <a name="00502805900030" ll="SDBH:חֳלִי:000000">enfermedades</a> <a name="00502805900032" ll="SDBH:רַע:000004">malignas</a> y <a name="00502805900036" ll="SDBH:אמן:000002">duraderas</a>; 
\v 60 y <a name="00502806000004" ll="SDBH:שׁוב:000043">traerá</a> sobre ti <a name="00502806000010">todos</a> los <a name="00502806000012" ll="SDBH:מַדְוֶה:000000">males</a> de <a name="00502806000014" ll="SDBH:מִצְרַיִם:000001">Egipto</a>, <a name="00502806000022">delante</a> de los <a name="00502806000016">cuales</a> <a name="00502806000018" ll="SDBH:יגר:000000">temiste</a>, y no te <a name="00502806000026" ll="SDBH:דבק:000003">dejarán</a>. 
\v 61 <a name="00502806100002" ll="SDBH:גַּם:000000">Asimismo</a> <a name="00502806100004">toda</a> <a name="00502806100006" ll="SDBH:חֳלִי:000000">enfermedad</a> y <a name="00502806100010">toda</a> <a name="00502806100012" ll="SDBH:מַכָּה:000000">plaga</a> <a name="00502806100014">que</a> <a name="00502806100016" ll="SDBH:לֹא:000000">no</a> está <a name="00502806100018" ll="SDBH:כתב:000000">escrita</a> en el <a name="00502806100022" ll="SDBH:סֵפֶר:000000">libro</a> de esta <a name="00502806100026" ll="SDBH:תֹּורָה:000000">ley</a>, <a name="00502806100034" ll="SDBH:יהוה:000000">Jehová</a> la <a name="00502806100032" ll="SDBH:עלה:000018">enviará</a> <a name="00502806100036">sobre</a> <a name="00502806100036">ti</a>, <a name="00502806100038" ll="SDBH:עַד:000002">hasta</a> <a name="00502806100038" ll="SDBH:עַד:000002">que</a> <a name="00502806100040" ll="SDBH:שׁמד:000000">seas</a> <a name="00502806100040" ll="SDBH:שׁמד:000000">destruido</a>. 
\v 62 Y <a name="00502806200004" ll="SDBH:שׁאר:000000">quedaréis</a> <a name="00502806200010" ll="SDBH:מְעַט:000000">pocos</a> en número, en <a name="00502806200012">lugar</a> de haber <a name="00502806200016">sido</a> como las <a name="00502806200020" ll="SDBH:כֹּוכָב:000000">estrellas</a> del <a name="00502806200024" ll="SDBH:שָׁמַיִם:000000">cielo</a> en <a name="00502806200028" ll="SDBH:רֹב:000000">multitud</a>, por cuanto <a name="00502806200032" ll="SDBH:לֹא:000000">no</a> <a name="00502806200034">obedecisteis</a> a la <a name="00502806200038">voz</a> de <a name="00502806200040" ll="SDBH:יהוה:000000">Jehová</a> <a name="00502806200042" ll="SDBH:אֱלֹהִים:000001">tu</a> <a name="00502806200042" ll="SDBH:אֱלֹהִים:000001">Dios</a>. 
\v 63 <a name="00502806300030">Así</a> como <a name="00502806300012" ll="SDBH:יהוה:000000">Jehová</a> <a name="00502806300010" ll="SDBH:שׂושׂ:000000">se</a> <a name="00502806300010" ll="SDBH:שׂושׂ:000000">gozaba</a> en haceros <a name="00502806300018" ll="SDBH:יטב:000007">bien</a> y en <a name="00502806300026">multiplicaros</a>, así <a name="00502806300032" ll="SDBH:שׂושׂ:000000">se</a> <a name="00502806300032" ll="SDBH:שׂושׂ:000000">gozará</a> <a name="00502806300034" ll="SDBH:יהוה:000000">Jehová</a> en <a name="00502806300040" ll="SDBH:אבד:000005">arruinaros</a> y en <a name="00502806300048" ll="SDBH:שׁמד:000000">destruiros</a>; y seréis <a name="00502806300054" ll="SDBH:נסח:000002">arrancados</a> de <a name="00502806300014">sobre</a> la <a name="00502806300062" ll="SDBH:אֲדָמָה:000000">tierra</a> a la <a name="00502806300008">cual</a> <a name="00502806300068">entráis</a> para tomar <a name="00502806300074" ll="SDBH:ירשׁ:000002">posesión</a> de ella. 
\v 64 Y <a name="00502806400006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00502806400050">te</a> esparcirá por <a name="00502806400010">todos</a> los <a name="00502806400014" ll="SDBH:עַם:000000">pueblos</a>, desde un <a name="00502806400018" ll="SDBH:קָצֶה:000002">extremo</a> de la <a name="00502806400022" ll="SDBH:אֶרֶץ:000000">tierra</a> hasta el otro <a name="00502806400028" ll="SDBH:קָצֶה:000002">extremo</a>; y <a name="00502806400038" ll="SDBH:שָׁם:000000">allí</a> <a name="00502806400036" ll="SDBH:עבד:000002">servirás</a> a <a name="00502806400040" ll="SDBH:אֱלֹהִים:000001">dioses</a> <a name="00502806400042" ll="SDBH:אַחֵר:000000">ajenos</a> <a name="00502806400044">que</a> <a name="00502806400046" ll="SDBH:לֹא:000000">no</a> <a name="00502806400048">conociste</a> tú ni <a name="00502806400054" ll="SDBH:אָב:000001">tus</a> <a name="00502806400054" ll="SDBH:אָב:000001">padres</a>, al <a name="00502806400056" ll="SDBH:עֵץ:000001">leño</a> y a la <a name="00502806400060" ll="SDBH:אֶבֶן:000000">piedra</a>. 
\v 65 Y <a name="00502806500014" ll="SDBH:לֹא:000000">ni</a> aun entre <a name="00502806500012">estas</a> <a name="00502806500008" ll="SDBH:גֹּוי:000000">naciones</a> <a name="00502806500016" ll="SDBH:רגע:002002">descansarás</a>, <a name="00502806500020" ll="SDBH:לֹא:000000">ni</a> la <a name="00502806500028" ll="SDBH:כַּף:000011">planta</a> de <a name="00502806500030" ll="SDBH:רֶגֶל:000000">tu</a> <a name="00502806500030" ll="SDBH:רֶגֶל:000000">pie</a> <a name="00502806500022">tendrá</a> reposo; pues <a name="00502806500040" ll="SDBH:שָׁם:000000">allí</a> te <a name="00502806500034">dará</a> <a name="00502806500036" ll="SDBH:יהוה:000000">Jehová</a> <a name="00502806500042" ll="SDBH:לֵב:000001">corazón</a> <a name="00502806500044" ll="SDBH:רַגָּז:000000">temeroso</a>, y <a name="00502806500048" ll="SDBH:כִּלָּיֹון:000001">desfallecimiento</a> de <a name="00502806500050" ll="SDBH:עַיִן:000000">ojos</a>, y tristeza de <a name="00502806500056" ll="SDBH:נֶפֶשׁ:000001">alma</a>; 
\v 66 y <a name="00502806600004">tendrás</a> <a name="00502806600006" ll="SDBH:חַיִּים:000000">tu</a> <a name="00502806600006" ll="SDBH:חַיִּים:000000">vida</a> como algo que <a name="00502806600008" ll="SDBH:תלא:000001">pende</a> <a name="00502806600014" ll="SDBH:נֶגֶד:000000">delante</a> de ti, y estarás <a name="00502806600018" ll="SDBH:פחד:000000">temeroso</a> de <a name="00502806600020" ll="SDBH:לַיְלָה:000000">noche</a> y de <a name="00502806600024" ll="SDBH:יֹומָם:000000">día</a>, y <a name="00502806600028" ll="SDBH:לֹא:000000">no</a> tendrás <a name="00502806600030" ll="SDBH:אמן:000005">seguridad</a> de <a name="00502806600034" ll="SDBH:חַיִּים:000000">tu</a> <a name="00502806600034" ll="SDBH:חַיִּים:000000">vida</a>. 
\v 67 Por la <a name="00502806700006" ll="SDBH:בֹּקֶר:000000">mañana</a> <a name="00502806700008" ll="SDBH:אמר:000000">dirás</a>: ¡<a name="00502806700010" ll="SDBH:מִי:000000">Quién</a> <a name="00502806700012">diera</a> que fuese la <a name="00502806700014" ll="SDBH:עֶרֶב:000000">tarde</a>! y a la <a name="00502806700022" ll="SDBH:עֶרֶב:000000">tarde</a> <a name="00502806700024" ll="SDBH:אמר:000000">dirás</a>: ¡<a name="00502806700026" ll="SDBH:מִי:000000">Quién</a> <a name="00502806700028">diera</a> que fuese la <a name="00502806700030" ll="SDBH:בֹּקֶר:000000">mañana</a>! por el <a name="00502806700034" ll="SDBH:פַּחַד:000000">miedo</a> de <a name="00502806700036" ll="SDBH:לֵבָב:000001">tu</a> <a name="00502806700036" ll="SDBH:לֵבָב:000001">corazón</a> con <a name="00502806700038">que</a> estarás <a name="00502806700040" ll="SDBH:פחד:000000">amedrentado</a>, y por lo <a name="00502806700050">que</a> <a name="00502806700052" ll="SDBH:ראה:000000">verán</a> <a name="00502806700048" ll="SDBH:עַיִן:000000">tus</a> <a name="00502806700048" ll="SDBH:עַיִן:000000">ojos</a>. 
\v 68 Y <a name="00502806800006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00502806800004" ll="SDBH:שׁוב:000051">te</a> <a name="00502806800004" ll="SDBH:שׁוב:000051">hará</a> <a name="00502806800004" ll="SDBH:שׁוב:000051">volver</a> a <a name="00502806800008" ll="SDBH:מִצְרַיִם:000001">Egipto</a> en <a name="00502806800012" ll="SDBH:אֳנִיָּה:000000">naves</a>, por el <a name="00502806800018" ll="SDBH:דֶּרֶךְ:000003">camino</a> del <a name="00502806800020">cual</a> <a name="00502806800024">te</a> <a name="00502806800022" ll="SDBH:אמר:000000">ha</a> <a name="00502806800022" ll="SDBH:אמר:000000">dicho</a>: <a name="00502806800026" ll="SDBH:לֹא:000000">Nunca</a> <a name="00502806800028;00502806800030">más</a> <a name="00502806800034" ll="SDBH:ראה:000000">volverás</a>; y <a name="00502806800040" ll="SDBH:שָׁם:000000">allí</a> <a name="00502806800038" ll="SDBH:מכר:000001">seréis</a> <a name="00502806800038" ll="SDBH:מכר:000001">vendidos</a> a <a name="00502806800044" ll="SDBH:איב:000001">vuestros</a> <a name="00502806800044" ll="SDBH:איב:000001">enemigos</a> por <a name="00502806800048" ll="SDBH:עֶבֶד:000000">esclavos</a> y por <a name="00502806800054" ll="SDBH:שִׁפְחָה:000000">esclavas</a>, y <a name="00502806800058">no</a> <a name="00502806800058">habrá</a> <a name="00502806800060" ll="SDBH:קנה:000000">quien</a> os <a name="00502806800060" ll="SDBH:קנה:000000">compre</a>.
\v 69 (1) <a name="00502806900002" ll="SDBH:אֵלֶּה:000000">Éstas</a> son las <a name="00502806900004" ll="SDBH:דָּבָר:000003">palabras</a> del <a name="00502806900008" ll="SDBH:בְּרִית:000000">pacto</a> <a name="00502806900010">que</a> <a name="00502806900014" ll="SDBH:יהוה:000000">Jehová</a> <a name="00502806900012" ll="SDBH:צוה:000000">mandó</a> a <a name="00502806900018" ll="SDBH:מֹשֶׁה:000000">Moisés</a> que <a name="00502806900022" ll="SDBH:כרת:000001">celebrase</a> <a name="00502806900024">con</a> los <a name="00502806900026" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00502806900028" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> en la <a name="00502806900032" ll="SDBH:אֶרֶץ:000000">tierra</a> de <a name="00502806900034" ll="SDBH:מֹואָב:000001">Moab</a>, además del <a name="00502806900044" ll="SDBH:בְּרִית:000000">pacto</a> <a name="00502806900046">que</a> <a name="00502806900048" ll="SDBH:כרת:000001">concertó</a> <a name="00502806900050">con</a> <a name="00502806900050">ellos</a> en <a name="00502806900054" ll="SDBH:חֹרֵב:000000">Horeb</a>.
    
\c 29
\v 1 (2) <a name="00502900100006" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, pues, <a name="00502900100004" ll="SDBH:קרא:000000">llamó</a> <a name="00502900100008">a</a> <a name="00502900100010">todo</a> <a name="00502900100012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, y <a name="00502900100018">les</a> <a name="00502900100016" ll="SDBH:אמר:000000">dijo</a>: <a name="00502900100020">Vosotros</a> <a name="00502900100022" ll="SDBH:ראה:000000">habéis</a> <a name="00502900100022" ll="SDBH:ראה:000000">visto</a> <a name="00502900100026">todo</a> lo <a name="00502900100028">que</a> <a name="00502900100032" ll="SDBH:יהוה:000000">Jehová</a> <a name="00502900100030">ha</a> <a name="00502900100030">hecho</a> delante de <a name="00502900100036" ll="SDBH:עַיִן:000000">vuestros</a> <a name="00502900100036" ll="SDBH:עַיִן:000000">ojos</a> en la <a name="00502900100040" ll="SDBH:אֶרֶץ:000000">tierra</a> de <a name="00502900100042" ll="SDBH:מִצְרַיִם:000001">Egipto</a> a <a name="00502900100046" ll="SDBH:פַּרְעֹה:000000">Faraón</a> y a <a name="00502900100052">todos</a> <a name="00502900100054" ll="SDBH:עֶבֶד:000000">sus</a> <a name="00502900100054" ll="SDBH:עֶבֶד:000000">siervos</a>, y a <a name="00502900100060">toda</a> <a name="00502900100062" ll="SDBH:אֶרֶץ:000000">su</a> <a name="00502900100062" ll="SDBH:אֶרֶץ:000000">tierra</a>, 
