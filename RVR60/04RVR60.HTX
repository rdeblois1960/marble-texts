\v 84 <a name="00400708400002" ll="SDBH:זֶה:000000;SDBH:זֹה:000000">Ésta</a> fue la <a name="00400708400004" ll="SDBH:חֲנֻכָּה:000000">ofrenda</a> <a name="00400708400016">que</a> los <a name="00400708400022" ll="SDBH:נָשִׂיא:000001">príncipes</a> de <a name="00400708400024" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> ofrecieron para la dedicación del <a name="00400708400008" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, el <a name="00400708400012" ll="SDBH:יֹום:000003">día</a> en que fue <a name="00400708400014" ll="SDBH:משׁח:000001">ungido</a>: <a name="00400708400030;00400708400032" ll="SDBH:שְׁנַיִם:000000">doce</a> <a name="00400708400026" ll="SDBH:קְעָרָה:000000">platos</a> de <a name="00400708400028" ll="SDBH:כֶּסֶף:000000">plata</a>, <a name="00400708400038;00400708400040" ll="SDBH:עָשָׂר:000000;SDBH:שְׁנַיִם:000000">doce</a> <a name="00400708400034" ll="SDBH:מִזְרָק:000000">jarros</a> de <a name="00400708400036" ll="SDBH:כֶּסֶף:000000">plata</a>, <a name="00400708400046;00400708400048" ll="SDBH:שְׁנַיִם:000000">doce</a> <a name="00400708400042" ll="SDBH:כַּף:000003">cucharas</a> de <a name="00400708400044" ll="SDBH:זָהָב:000000">oro</a>. 
\v 85 <a name="00400708500014" ll="SDBH:אֶחָד:000001">Cada</a> <a name="00400708500010" ll="SDBH:קְעָרָה:000000">plato</a> de <a name="00400708500006" ll="SDBH:מֵאָה:000000">ciento</a> <a name="00400708500002" ll="SDBH:שָׁלֹשׁ:000001">treinta</a> siclos, y <a name="00400708500028" ll="SDBH:אֶחָד:000001">cada</a> <a name="00400708500024" ll="SDBH:מִזְרָק:000000">jarro</a> de <a name="00400708500020" ll="SDBH:שֶׁבַע:000001">setenta</a>; <a name="00400708500030">toda</a> la <a name="00400708500032" ll="SDBH:כֶּסֶף:000000">plata</a> de la <a name="00400708500036" ll="SDBH:כְּלִי:000000">vajilla</a>, <a name="00400708500038" ll="SDBH:אֶלֶף:001000">dos</a> <a name="00400708500038" ll="SDBH:אֶלֶף:001000">mil</a> <a name="00400708500042;00400708500044" ll="SDBH:אַרְבַּע:000000;SDBH:מֵאָה:000000">cuatrocientos</a> siclos, al <a name="00400708500048" ll="SDBH:שֶׁקֶל:000000">siclo</a> del <a name="00400708500052" ll="SDBH:קֹדֶשׁ:000002">santuario</a>. 
\v 86 Las <a name="00400708600006;00400708600008" ll="SDBH:שְׁנַיִם:000000">doce</a> <a name="00400708600002" ll="SDBH:כַּף:000003">cucharas</a> de <a name="00400708600004" ll="SDBH:זָהָב:000000">oro</a> <a name="00400708600010" ll="SDBH:מלא:000000">llenas</a> de <a name="00400708600012" ll="SDBH:קְטֹרֶת:000001">incienso</a>, de <a name="00400708600014;00400708600016" ll="SDBH:עֶשֶׂר:000000">diez</a> siclos <a name="00400708600014;00400708600016" ll="SDBH:עֶשֶׂר:000000">cada</a> <a name="00400708600020" ll="SDBH:כַּף:000003">cuchara</a>, al <a name="00400708600024" ll="SDBH:שֶׁקֶל:000000">siclo</a> del <a name="00400708600028" ll="SDBH:קֹדֶשׁ:000002">santuario</a>; <a name="00400708600030">todo</a> el <a name="00400708600032" ll="SDBH:זָהָב:000000">oro</a> de las <a name="00400708600036" ll="SDBH:כַּף:000003">cucharas</a>, <a name="00400708600042" ll="SDBH:מֵאָה:000000">ciento</a> <a name="00400708600038" ll="SDBH:עֶשֶׂר:000001">veinte</a> siclos. 
\v 87 <a name="00400708700002">Todos</a> los <a name="00400708700006" ll="SDBH:בָּקָר:000000">bueyes</a> para <a name="00400708700012" ll="SDBH:עֹלָה:000000">holocausto</a>, <a name="00400708700014;00400708700016" ll="SDBH:עָשָׂר:000000;SDBH:שְׁנַיִם:000000">doce</a> <a name="00400708700018" ll="SDBH:פַּר:000000">becerros</a>; <a name="00400708700022;00400708700024" ll="SDBH:עָשָׂר:000000;SDBH:שְׁנַיִם:000000">doce</a> los <a name="00400708700020" ll="SDBH:אַיִל:000000">carneros</a>, <a name="00400708700032;00400708700034" ll="SDBH:עָשָׂר:000000;SDBH:שְׁנַיִם:000000">doce</a> los <a name="00400708700026" ll="SDBH:כֶּבֶשׂ:000000">corderos</a> de un <a name="00400708700030" ll="SDBH:שָׁנָה:000000">año</a>, con <a name="00400708700038" ll="SDBH:מִנְחָה:000000">su</a> <a name="00400708700038" ll="SDBH:מִנְחָה:000000">ofrenda</a>, y <a name="00400708700046;00400708700048" ll="SDBH:עָשָׂר:000000;SDBH:שְׁנַיִם:000000">doce</a> los <a name="00400708700042;00400708700044" ll="SDBH:עֵז:000000;SDBH:שָׂעִיר:000000">machos</a> <a name="00400708700042;00400708700044" ll="SDBH:עֵז:000000;SDBH:שָׂעִיר:000000">cabríos</a> para <a name="00400708700052" ll="SDBH:חַטָּאת:000004">expiación</a>. 
\v 88 Y <a name="00400708800004">todos</a> los <a name="00400708800006" ll="SDBH:בָּקָר:000000">bueyes</a> de la <a name="00400708800008;00400708800012" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">ofrenda</a> <a name="00400708800008;00400708800012" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">de</a> <a name="00400708800008;00400708800012" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">paz</a>, <a name="00400708800014;00400708800018" ll="SDBH:אַרְבַּע:000000;SDBH:עֶשֶׂר:000001">veinticuatro</a> <a name="00400708800020" ll="SDBH:פַּר:000000">novillos</a>, <a name="00400708800024" ll="SDBH:שֵׁשׁ:000001">sesenta</a> los <a name="00400708800022" ll="SDBH:אַיִל:000000">carneros</a>, <a name="00400708800028" ll="SDBH:שֵׁשׁ:000001">sesenta</a> los <a name="00400708800026" ll="SDBH:עַתּוּד:000000">machos</a> <a name="00400708800026" ll="SDBH:עַתּוּד:000000">cabríos</a>, y <a name="00400708800036" ll="SDBH:שֵׁשׁ:000001">sesenta</a> los <a name="00400708800030" ll="SDBH:כֶּבֶשׂ:000000">corderos</a> de un <a name="00400708800034" ll="SDBH:שָׁנָה:000000">año</a>. <a name="00400708800038" ll="SDBH:זֶה:000000;SDBH:זֹה:000000">Ésta</a> fue la <a name="00400708800040" ll="SDBH:חֲנֻכָּה:000000">ofrenda</a> <a name="00400708800040" ll="SDBH:חֲנֻכָּה:000000">para</a> <a name="00400708800040" ll="SDBH:חֲנֻכָּה:000000">la</a> <a name="00400708800040" ll="SDBH:חֲנֻכָּה:000000">dedicación</a> del <a name="00400708800044" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, <a name="00400708800046" ll="SDBH:אַחַר:001002">después</a> que fue <a name="00400708800048" ll="SDBH:משׁח:000001">ungido</a>.
    
\v 89 Y cuando <a name="00400708900006">entraba</a> <a name="00400708900008" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00400708900010">en</a> el <a name="00400708900012;00400708900014" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00400708900012;00400708900014" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00400708900012;00400708900014" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>, para <a name="00400708900018" ll="SDBH:דבר:001000">hablar</a> <a name="00400708900020">con</a> Dios, <a name="00400708900024">oía</a> la <a name="00400708900030">voz</a> que <a name="00400708900034">le</a> <a name="00400708900032" ll="SDBH:דבר:001000">hablaba</a> de <a name="00400708900038">encima</a> del <a name="00400708900042" ll="SDBH:כַּפֹּרֶת:000000">propiciatorio</a> <a name="00400708900044">que</a> estaba <a name="00400708900046">sobre</a> el <a name="00400708900048" ll="SDBH:אֲרֹון:000001">arca</a> del <a name="00400708900052" ll="SDBH:עֵדוּת:000001">testimonio</a>, de <a name="00400708900056">entre</a> los <a name="00400708900058" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="00400708900062" ll="SDBH:כְּרוּב:000000">querubines</a>; y <a name="00400708900066" ll="SDBH:דבר:001000">hablaba</a> <a name="00400708900068">con</a> <a name="00400708900068">él</a>.
\c 8
\v 1 <a name="00400800100004" ll="SDBH:דבר:001000">Habló</a> <a name="00400800100006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00400800100008">a</a> <a name="00400800100010" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, <a name="00400800100014" ll="SDBH:אמר:000000">diciendo</a>: 
\v 2 <a name="00400800200002" ll="SDBH:דבר:001000">Habla</a> <a name="00400800200004">a</a> <a name="00400800200006" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00400800200010" ll="SDBH:אמר:000000">dile</a>: Cuando <a name="00400800200016" ll="SDBH:עלה:000034">enciendas</a> las lámparas, las <a name="00400800200036" ll="SDBH:שֶׁבַע:000000">siete</a> lámparas <a name="00400800200034" ll="SDBH:אור:000001">alumbrarán</a> <a name="00400800200012">hacia</a> <a name="00400800200026" ll="SDBH:מוּל:000000">adelante</a> del <a name="00400800200032" ll="SDBH:מְנֹורָה:000000">candelero</a>. 
\v 3 Y <a name="00400800300008" ll="SDBH:אַהֲרֹן:000000">Aarón</a> lo <a name="00400800300004">hizo</a> <a name="00400800300006">así</a>; <a name="00400800300020" ll="SDBH:עלה:000034">encendió</a> <a name="00400800300010">hacia</a> la parte <a name="00400800300012" ll="SDBH:מוּל:000000">anterior</a> del <a name="00400800300018" ll="SDBH:מְנֹורָה:000000">candelero</a> sus lámparas, como <a name="00400800300030" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="00400800300028" ll="SDBH:צוה:000000">mandó</a> a <a name="00400800300034" ll="SDBH:מֹשֶׁה:000000">Moisés</a>. 
\v 4 Y ésta era la <a name="00400800400006" ll="SDBH:מַעֲשֶׂה:000006">hechura</a> del <a name="00400800400010" ll="SDBH:מְנֹורָה:000000">candelero</a>, de <a name="00400800400014" ll="SDBH:זָהָב:000000">oro</a> labrado a <a name="00400800400012" ll="SDBH:מִקְשָׁה:000000">martillo</a>; desde <a name="00400800400026">su</a> <a name="00400800400018" ll="SDBH:יָרֵךְ:000001">pie</a> hasta <a name="00400800400022" ll="SDBH:פֶּרַח:000002" tl="HOTTP488">sus</a> <a name="00400800400022" ll="SDBH:פֶּרַח:000002" tl="HOTTP488">flores</a> era <a name="00400800400024" ll="SDBH:מִקְשָׁה:000000">labrado</a> a martillo; conforme al <a name="00400800400032" ll="SDBH:מַרְאֶה:000000">modelo</a> <a name="00400800400034">que</a> <a name="00400800400038" ll="SDBH:יהוה:000000">Jehová</a> <a name="00400800400036" ll="SDBH:ראה:000003">mostró</a> a <a name="00400800400042" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, <a name="00400800400044">así</a> <a name="00400800400046">hizo</a> el <a name="00400800400052" ll="SDBH:מְנֹורָה:000000">candelero</a>.
    
\v 5 También <a name="00400800500006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00400800500004" ll="SDBH:דבר:001000">habló</a> <a name="00400800500008">a</a> <a name="00400800500010" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, <a name="00400800500014" ll="SDBH:אמר:000000">diciendo</a>: 
\v 6 <a name="00400800600002" ll="SDBH:לקח:000000">Toma</a> a los <a name="00400800600008" ll="SDBH:לֵוִי:000001">levitas</a> de <a name="00400800600012" ll="SDBH:תָּוֶךְ:000000">entre</a> los <a name="00400800600014" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400800600016" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, y <a name="00400800600020" ll="SDBH:טהר:000003">haz</a> <a name="00400800600020" ll="SDBH:טהר:000003">expiación</a> <a name="00400800600022">por</a> <a name="00400800600022">ellos</a>. 
\v 7 <a name="00400800700004">Así</a> <a name="00400800700006">harás</a> para <a name="00400800700012" ll="SDBH:טהר:000003">expiación</a> por ellos: <a name="00400800700014" ll="SDBH:נזה:000001">Rocía</a> <a name="00400800700016">sobre</a> <a name="00400800700016">ellos</a> el <a name="00400800700018" ll="SDBH:מַיִם:000000">agua</a> de la <a name="00400800700020" ll="SDBH:חַטָּאת:000002">expiación</a>, y haz <a name="00400800700024" ll="SDBH:עבר:000001">pasar</a> la <a name="00400800700026" ll="SDBH:תַּעַר:000000">navaja</a> <a name="00400800700028">sobre</a> <a name="00400800700030">todo</a> <a name="00400800700032" ll="SDBH:בָּשָׂר:000000">su</a> <a name="00400800700032" ll="SDBH:בָּשָׂר:000000">cuerpo</a>, y <a name="00400800700036" ll="SDBH:כבס:000000">lavarán</a> <a name="00400800700038" ll="SDBH:בֶּגֶד:000001">sus</a> <a name="00400800700038" ll="SDBH:בֶּגֶד:000001">vestidos</a>, y serán <a name="00400800700042" ll="SDBH:טהר:000005">purificados</a>. 
\v 8 Luego <a name="00400800800004" ll="SDBH:לקח:000000">tomarán</a> un <a name="00400800800006;00400800800008;00400800800010" ll="SDBH:בֵּן:000001;SDBH:בָּקָר:000000;SDBH:פַּר:000000">novillo</a>, con <a name="00400800800014" ll="SDBH:מִנְחָה:000000">su</a> <a name="00400800800014" ll="SDBH:מִנְחָה:000000">ofrenda</a> de <a name="00400800800016" ll="SDBH:סֹלֶת:000000">flor</a> <a name="00400800800016" ll="SDBH:סֹלֶת:000000">de</a> <a name="00400800800016" ll="SDBH:סֹלֶת:000000">harina</a> <a name="00400800800018" ll="SDBH:בלל:000000">amasada</a> con <a name="00400800800024" ll="SDBH:שֶׁמֶן:000000">aceite</a>; y <a name="00400800800036" ll="SDBH:לקח:000000">tomarás</a> <a name="00400800800030" ll="SDBH:שֵׁנִי:000000">otro</a> <a name="00400800800028;00400800800032;00400800800034" ll="SDBH:בֵּן:000001;SDBH:בָּקָר:000000;SDBH:פַּר:000000">novillo</a> para <a name="00400800800040" ll="SDBH:חַטָּאת:000004">expiación</a>. 
\v 9 Y <a name="00400800900004" ll="SDBH:קרב:000001">harás</a> <a name="00400800900004" ll="SDBH:קרב:000001">que</a> los <a name="00400800900010" ll="SDBH:לֵוִי:000001">levitas</a> <a name="00400800900004" ll="SDBH:קרב:000001">se</a> <a name="00400800900004" ll="SDBH:קרב:000001">acerquen</a> <a name="00400800900012;00400800900014">delante</a> del <a name="00400800900016;00400800900018" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00400800900016;00400800900018" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00400800900016;00400800900018" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>, y <a name="00400800900022" ll="SDBH:קהל:000001">reunirás</a> a <a name="00400800900026">toda</a> la <a name="00400800900028" ll="SDBH:עֵדָה:000000">congregación</a> de los <a name="00400800900030" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400800900032" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 10 Y cuando hayas <a name="00400801000004" ll="SDBH:קרב:000001">acercado</a> a los <a name="00400801000010" ll="SDBH:לֵוִי:000001">levitas</a> <a name="00400801000012;00400801000014">delante</a> de <a name="00400801000016" ll="SDBH:יהוה:000000">Jehová</a>, <a name="00400801000020" ll="SDBH:סמך:000000">pondrán</a> los <a name="00400801000022" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801000024" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00400801000028">sus</a> <a name="00400801000028">manos</a> <a name="00400801000030">sobre</a> los <a name="00400801000034" ll="SDBH:לֵוִי:000001">levitas</a>; 
\v 11 y <a name="00400801100004" ll="SDBH:נוף:000002">ofrecerá</a> <a name="00400801100006" ll="SDBH:אַהֲרֹן:000000">Aarón</a> los <a name="00400801100012" ll="SDBH:לֵוִי:000001">levitas</a> <a name="00400801100016;00400801100018">delante</a> de <a name="00400801100020;00400801100042" ll="SDBH:יהוה:000000">Jehová</a> en <a name="00400801100014" ll="SDBH:תְּנוּפָה:000001">ofrenda</a> de los <a name="00400801100026" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801100028" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, y <a name="00400801100032">servirán</a> en el <a name="00400801100040" ll="SDBH:עֲבֹודָה:000000">ministerio</a> de <a name="00400801100020;00400801100042" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 12 Y los <a name="00400801200006" ll="SDBH:לֵוִי:000001">levitas</a> <a name="00400801200008" ll="SDBH:סמך:000000">pondrán</a> <a name="00400801200012">sus</a> <a name="00400801200012">manos</a> <a name="00400801200014">sobre</a> las <a name="00400801200016" ll="SDBH:רֹאשׁ:000000">cabezas</a> de los <a name="00400801200020" ll="SDBH:פַּר:000000">novillos</a>; y <a name="00400801200024">ofrecerás</a> el <a name="00400801200030" ll="SDBH:אֶחָד:000003">uno</a> por <a name="00400801200032" ll="SDBH:חַטָּאת:000004">expiación</a>, y el <a name="00400801200040" ll="SDBH:אֶחָד:000003">otro</a> en <a name="00400801200042" ll="SDBH:עֹלָה:000000">holocausto</a> a <a name="00400801200046" ll="SDBH:יהוה:000000">Jehová</a>, para <a name="00400801200050" ll="SDBH:כפר:000008">hacer</a> <a name="00400801200050" ll="SDBH:כפר:000008">expiación</a> <a name="00400801200052">por</a> los <a name="00400801200056" ll="SDBH:לֵוִי:000001">levitas</a>. 
\v 13 Y <a name="00400801300004">presentarás</a> a los <a name="00400801300010" ll="SDBH:לֵוִי:000001">levitas</a> <a name="00400801300014">delante</a> de <a name="00400801300016" ll="SDBH:אַהֲרֹן:000000">Aarón</a>, y <a name="00400801300020;00400801300022">delante</a> de <a name="00400801300024" ll="SDBH:בֵּן:000000">sus</a> <a name="00400801300024" ll="SDBH:בֵּן:000000">hijos</a>, y <a name="00400801300030">los</a> <a name="00400801300028" ll="SDBH:נוף:000002">ofrecerás</a> en <a name="00400801300032" ll="SDBH:תְּנוּפָה:000001">ofrenda</a> a <a name="00400801300036" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 14 Así <a name="00400801400004" ll="SDBH:בדל:000002">apartarás</a> a los <a name="00400801400010" ll="SDBH:לֵוִי:000001">levitas</a> de <a name="00400801400014" ll="SDBH:תָּוֶךְ:000000">entre</a> los <a name="00400801400016" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801400018" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, y <a name="00400801400022">serán</a> míos los <a name="00400801400028" ll="SDBH:לֵוִי:000001">levitas</a>. 
\v 15 <a name="00400801500004" ll="SDBH:אַחַר:002001">Después</a> de <a name="00400801500006">eso</a> <a name="00400801500008">vendrán</a> los <a name="00400801500012" ll="SDBH:לֵוִי:000001">levitas</a> a <a name="00400801500016" ll="SDBH:עבד:000007;SDBH:עבד:000008">ministrar</a> en el <a name="00400801500020;00400801500022" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00400801500020;00400801500022" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00400801500020;00400801500022" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>; <a name="00400801500026" ll="SDBH:טהר:000003">serán</a> <a name="00400801500026" ll="SDBH:טהר:000003">purificados</a>, y <a name="00400801500034">los</a> <a name="00400801500032" ll="SDBH:נוף:000002">ofrecerás</a> en <a name="00400801500036" ll="SDBH:תְּנוּפָה:000001">ofrenda</a>. 
\v 16 Porque <a name="00400801600004;00400801600006">enteramente</a> me <a name="00400801600004;00400801600006">son</a> <a name="00400801600004;00400801600006">dedicados</a> <a name="00400801600010">a</a> <a name="00400801600010">mí</a> los levitas de <a name="00400801600014" ll="SDBH:תָּוֶךְ:000000">entre</a> los <a name="00400801600016" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801600018" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, <a name="00400801600020">en</a> <a name="00400801600020">lugar</a> <a name="00400801600020">de</a> <a name="00400801600024">todo</a> <a name="00400801600022;00400801600026" ll="SDBH:פִּטְרָה:000000;SDBH:רֶחֶם:000000">primer</a> <a name="00400801600022;00400801600026" ll="SDBH:פִּטְרָה:000000;SDBH:רֶחֶם:000000">nacido</a>; los <a name="00400801600038" ll="SDBH:לקח:000000">he</a> <a name="00400801600038" ll="SDBH:לקח:000000">tomado</a> para mí en lugar de los <a name="00400801600028" ll="SDBH:בְּכֹור:000000">primogénitos</a> de <a name="00400801600030">todos</a> los <a name="00400801600034" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801600036" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 17 Porque mío es <a name="00400801700006">todo</a> <a name="00400801700008" ll="SDBH:בְּכֹור:000000">primogénito</a> de entre los <a name="00400801700012" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801700014" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, así de <a name="00400801700020" ll="SDBH:אָדָם:000000">hombres</a> como de <a name="00400801700028" ll="SDBH:בְּהֵמָה:000001">animales</a>; desde el <a name="00400801700032" ll="SDBH:יֹום:000003">día</a> que yo <a name="00400801700034" ll="SDBH:נכה:000004">herí</a> a <a name="00400801700036">todo</a> <a name="00400801700038" ll="SDBH:בְּכֹור:000000">primogénito</a> en la <a name="00400801700042" ll="SDBH:אֶרֶץ:000000">tierra</a> de <a name="00400801700044" ll="SDBH:מִצְרַיִם:000001">Egipto</a>, los <a name="00400801700046" ll="SDBH:קדשׁ:000001">santifiqué</a> para mí. 
\v 18 Y <a name="00400801800004" ll="SDBH:לקח:000000">he</a> <a name="00400801800004" ll="SDBH:לקח:000000">tomado</a> a los <a name="00400801800010" ll="SDBH:לֵוִי:000001">levitas</a> en <a name="00400801800012">lugar</a> de <a name="00400801800014">todos</a> los <a name="00400801800016" ll="SDBH:בְּכֹור:000000">primogénitos</a> de los <a name="00400801800020" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801800022" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 19 Y yo <a name="00400801900004">he</a> <a name="00400801900004">dado</a> en <a name="00400801900012">don</a> los <a name="00400801900010" ll="SDBH:לֵוִי:000001">levitas</a> a <a name="00400801900016" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y a <a name="00400801900022" ll="SDBH:בֵּן:000000">sus</a> <a name="00400801900022" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801900026" ll="SDBH:תָּוֶךְ:000000">entre</a> los <a name="00400801900028" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801900030" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, para que <a name="00400801900034" ll="SDBH:עבד:000007;SDBH:עבד:000008">ejerzan</a> el <a name="00400801900038" ll="SDBH:עֲבֹודָה:000000">ministerio</a> de los <a name="00400801900040" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801900042" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> en el <a name="00400801900046;00400801900048" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00400801900046;00400801900048" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00400801900046;00400801900048" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>, y <a name="00400801900054" ll="SDBH:כפר:000008">reconcilien</a> <a name="00400801900056">a</a> los <a name="00400801900058" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801900060" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>; para que <a name="00400801900064" ll="SDBH:לֹא:000000">no</a> <a name="00400801900066">haya</a> <a name="00400801900074" ll="SDBH:נֶגֶף:000001">plaga</a> en los <a name="00400801900070" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801900072" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, al <a name="00400801900078" ll="SDBH:נגשׁ:000002">acercarse</a> los <a name="00400801900080" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400801900082" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00400801900084">al</a> <a name="00400801900088" ll="SDBH:קֹדֶשׁ:000002">santuario</a>.
    
\v 20 Y <a name="00400802000006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> y <a name="00400802000010" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00400802000014">toda</a> la <a name="00400802000016" ll="SDBH:עֵדָה:000000">congregación</a> de los <a name="00400802000018" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400802000020" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00400802000004">hicieron</a> con los <a name="00400802000026" ll="SDBH:לֵוִי:000001">levitas</a> conforme a <a name="00400802000030">todas</a> las cosas <a name="00400802000032">que</a> <a name="00400802000034">mandó</a> <a name="00400802000036">Jehová</a> a <a name="00400802000040" ll="SDBH:מֹשֶׁה:000000">Moisés</a> acerca de los <a name="00400802000046" ll="SDBH:לֵוִי:000001">levitas</a>; <a name="00400802000048">así</a> <a name="00400802000050">hicieron</a> con ellos los <a name="00400802000054" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400802000056" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 21 Y los <a name="00400802100008" ll="SDBH:לֵוִי:000001">levitas</a> <a name="00400802100004" ll="SDBH:חטא:001007">se</a> <a name="00400802100004" ll="SDBH:חטא:001007">purificaron</a>, y <a name="00400802100012" ll="SDBH:כבס:000000">lavaron</a> <a name="00400802100014" ll="SDBH:בֶּגֶד:000001">sus</a> <a name="00400802100014" ll="SDBH:בֶּגֶד:000001">vestidos</a>; y <a name="00400802100020" ll="SDBH:אַהֲרֹן:000000">Aarón</a> los ofreció en <a name="00400802100024" ll="SDBH:תְּנוּפָה:000001">ofrenda</a> <a name="00400802100026;00400802100028">delante</a> de <a name="00400802100030" ll="SDBH:יהוה:000000">Jehová</a>, e <a name="00400802100034" ll="SDBH:כפר:000008">hizo</a> <a name="00400802100038" ll="SDBH:אַהֲרֹן:000000">Aarón</a> <a name="00400802100034" ll="SDBH:כפר:000008">expiación</a> <a name="00400802100036">por</a> <a name="00400802100036">ellos</a> para <a name="00400802100042" ll="SDBH:טהר:000003">purificarlos</a>. 
\v 22 <a name="00400802200006;00400802200060">Así</a> <a name="00400802200008">vinieron</a> <a name="00400802200004" ll="SDBH:אַחַר:002001">después</a> los <a name="00400802200012" ll="SDBH:לֵוִי:000001">levitas</a> para <a name="00400802200016" ll="SDBH:עבד:000007;SDBH:עבד:000008">ejercer</a> <a name="00400802200020" ll="SDBH:עֲבֹודָה:000000">su</a> <a name="00400802200020" ll="SDBH:עֲבֹודָה:000000">ministerio</a> en el <a name="00400802200024;00400802200026" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00400802200024;00400802200026" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00400802200024;00400802200026" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a> <a name="00400802200028;00400802200030">delante</a> de <a name="00400802200032" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00400802200036;00400802200038">delante</a> de <a name="00400802200040" ll="SDBH:בֵּן:000000">sus</a> <a name="00400802200040" ll="SDBH:בֵּן:000000">hijos</a>; <a name="00400802200042;00400802200044">de</a> <a name="00400802200042;00400802200044">la</a> <a name="00400802200042;00400802200044">manera</a> <a name="00400802200042;00400802200044">que</a> <a name="00400802200046" ll="SDBH:צוה:000000">mandó</a> <a name="00400802200048" ll="SDBH:יהוה:000000">Jehová</a> a <a name="00400802200052" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00400802200054">acerca</a> de los <a name="00400802200058" ll="SDBH:לֵוִי:000001">levitas</a>, así <a name="00400802200062">hicieron</a> <a name="00400802200064">con</a> <a name="00400802200064">ellos</a>.
    
\v 23 Luego <a name="00400802300004" ll="SDBH:דבר:001000">habló</a> <a name="00400802300006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00400802300008">a</a> <a name="00400802300010" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, <a name="00400802300014" ll="SDBH:אמר:000000">diciendo</a>: 
\v 24 Los <a name="00400802400010" ll="SDBH:לֵוִי:000001">levitas</a> de <a name="00400802400016;00400802400018;00400802400020" ll="SDBH:חָמֵשׁ:000000;SDBH:עֶשֶׂר:000001">veinticinco</a> <a name="00400802400022" ll="SDBH:שָׁנָה:000000">años</a> <a name="00400802400026" ll="SDBH:מַעַל:001003">arriba</a> <a name="00400802400028">entrarán</a> a <a name="00400802400034" ll="SDBH:צָבָא:000000">ejercer</a> su <a name="00400802400032" ll="SDBH:צבא:000000">ministerio</a> en el <a name="00400802400038" ll="SDBH:עֲבֹודָה:000000">servicio</a> del <a name="00400802400040;00400802400042" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00400802400040;00400802400042" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00400802400040;00400802400042" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>. 
\v 25 Pero desde los <a name="00400802500008" ll="SDBH:חָמֵשׁ:000001">cincuenta</a> <a name="00400802500010" ll="SDBH:שָׁנָה:000000">años</a> <a name="00400802500012" ll="SDBH:שׁוב:000013">cesarán</a> de <a name="00400802500016" ll="SDBH:צָבָא:000000">ejercer</a> su <a name="00400802500020" ll="SDBH:עֲבֹודָה:000000">ministerio</a>, y <a name="00400802500024" ll="SDBH:לֹא:000000">nunca</a> <a name="00400802500028">más</a> lo <a name="00400802500026" ll="SDBH:עבד:000007;SDBH:עבד:000008">ejercerán</a>. 
\v 26 <a name="00400802600004" ll="SDBH:שׁרת:000000">Servirán</a> con <a name="00400802600008" ll="SDBH:אָח:000002">sus</a> <a name="00400802600008" ll="SDBH:אָח:000002">hermanos</a> en el <a name="00400802600012;00400802600014" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00400802600012;00400802600014" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00400802600012;00400802600014" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>, para <a name="00400802600018" ll="SDBH:שׁמר:000003">hacer</a> la <a name="00400802600020" ll="SDBH:מִשְׁמֶרֶת:000004">guardia</a>, pero <a name="00400802600026" ll="SDBH:לֹא:000000">no</a> <a name="00400802600028" ll="SDBH:עבד:000007;SDBH:עבד:000008">servirán</a> en el <a name="00400802600024" ll="SDBH:עֲבֹודָה:000000">ministerio</a>. <a name="00400802600030" ll="SDBH:כָּכָה:000000">Así</a> <a name="00400802600032">harás</a> con los <a name="00400802600038" ll="SDBH:לֵוִי:000001">levitas</a> en cuanto a <a name="00400802600042" ll="SDBH:מִשְׁמֶרֶת:000004">su</a> <a name="00400802600042" ll="SDBH:מִשְׁמֶרֶת:000004">ministerio</a>.
\c 9
\v 1 <a name="00400900100004" ll="SDBH:דבר:001000">Habló</a> <a name="00400900100006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00400900100008">a</a> <a name="00400900100010" ll="SDBH:מֹשֶׁה:000000">Moisés</a> en el <a name="00400900100014" ll="SDBH:מִדְבָּר:000000">desierto</a> de <a name="00400900100016" ll="SDBH:סִינַי:000000">Sinaí</a>, en el <a name="00400900100026" ll="SDBH:שֵׁנִי:000000">segundo</a> <a name="00400900100022" ll="SDBH:שָׁנָה:000000">año</a> de su <a name="00400900100030" ll="SDBH:יצא:000000">salida</a> de la <a name="00400900100034" ll="SDBH:אֶרֶץ:000000">tierra</a> de <a name="00400900100036" ll="SDBH:מִצְרַיִם:000001">Egipto</a>, en el <a name="00400900100042" ll="SDBH:חֹדֶשׁ:000000">mes</a> <a name="00400900100046" ll="SDBH:רִאשֹׁון:000000">primero</a>, <a name="00400900100050" ll="SDBH:אמר:000000">diciendo</a>: 
\v 2 Los <a name="00400900200006" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400900200008" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00400900200004">celebrarán</a> la <a name="00400900200014" ll="SDBH:פֶּסַח:000000">pascua</a> a <a name="00400900200018" ll="SDBH:מֹועֵד:000001">su</a> <a name="00400900200018" ll="SDBH:מֹועֵד:000001">tiempo</a>. 
\v 3 El <a name="00400900300004;00400900300006" ll="SDBH:אַרְבַּע:000000;SDBH:עָשָׂר:000000">decimocuarto</a> <a name="00400900300008" ll="SDBH:יֹום:000001">día</a> de este <a name="00400900300014" ll="SDBH:חֹדֶשׁ:000000">mes</a>, <a name="00400900300020">entre</a> las dos <a name="00400900300024" ll="SDBH:עֶרֶב:000000">tardes</a>, la <a name="00400900300026">celebraréis</a> a <a name="00400900300032" ll="SDBH:מֹועֵד:000001">su</a> <a name="00400900300032" ll="SDBH:מֹועֵד:000001">tiempo</a>; conforme a <a name="00400900300036">todos</a> <a name="00400900300038" ll="SDBH:חֻקָּה:000001">sus</a> <a name="00400900300038" ll="SDBH:חֻקָּה:000001">ritos</a> y conforme a <a name="00400900300044">todas</a> <a name="00400900300046" ll="SDBH:מִשְׁפָּט:000006">sus</a> <a name="00400900300046" ll="SDBH:מִשְׁפָּט:000006">leyes</a> la <a name="00400900300048">celebraréis</a>. 
\v 4 Y <a name="00400900400004" ll="SDBH:דבר:001000">habló</a> <a name="00400900400006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00400900400008">a</a> los <a name="00400900400010" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400900400012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> para que <a name="00400900400016">celebrasen</a> la <a name="00400900400020" ll="SDBH:פֶּסַח:000000">pascua</a>. 
\v 5 <a name="00400900500004">Celebraron</a> la <a name="00400900500010" ll="SDBH:פֶּסַח:000000">pascua</a> en el mes <a name="00400900500016" ll="SDBH:רִאשֹׁון:000000">primero</a>, a los <a name="00400900500020;00400900500022" ll="SDBH:אַרְבַּע:000000;SDBH:עָשָׂר:000000">catorce</a> <a name="00400900500024" ll="SDBH:יֹום:000001">días</a> del <a name="00400900500030" ll="SDBH:חֹדֶשׁ:000000">mes</a>, <a name="00400900500032">entre</a> las dos <a name="00400900500036" ll="SDBH:עֶרֶב:000000">tardes</a>, en el <a name="00400900500040" ll="SDBH:מִדְבָּר:000000">desierto</a> de <a name="00400900500042" ll="SDBH:סִינַי:000000">Sinaí</a>; conforme a <a name="00400900500046">todas</a> las cosas <a name="00400900500048">que</a> <a name="00400900500050" ll="SDBH:צוה:000000">mandó</a> <a name="00400900500052" ll="SDBH:יהוה:000000">Jehová</a> a <a name="00400900500056" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, <a name="00400900500058">así</a> <a name="00400900500060">hicieron</a> los <a name="00400900500062" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400900500064" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 6 Pero <a name="00400900600004">hubo</a> algunos <a name="00400900600008">que</a> <a name="00400900600010">estaban</a> <a name="00400900600012" ll="SDBH:טָמֵא:000000">inmundos</a> a causa de <a name="00400900600016" ll="SDBH:נֶפֶשׁ:000005">muerto</a>, y <a name="00400900600022" ll="SDBH:לֹא:000000">no</a> <a name="00400900600024" ll="SDBH:יכל:000000">pudieron</a> <a name="00400900600028">celebrar</a> la <a name="00400900600032" ll="SDBH:פֶּסַח:000000">pascua</a> aquel <a name="00400900600038" ll="SDBH:יֹום:000001">día</a>; y <a name="00400900600046" ll="SDBH:קרב:000000">vinieron</a> <a name="00400900600050">delante</a> de <a name="00400900600052" ll="SDBH:מֹשֶׁה:000000">Moisés</a> y <a name="00400900600058">delante</a> de <a name="00400900600060" ll="SDBH:אַהֲרֹן:000000">Aarón</a> aquel <a name="00400900600066" ll="SDBH:יֹום:000001">día</a>, 
\v 7 y <a name="00400900700014">le</a> <a name="00400900700004" ll="SDBH:אמר:000000">dijeron</a> <a name="00400900700012">aquellos</a> hombres: <a name="00400900700016">Nosotros</a> estamos <a name="00400900700018" ll="SDBH:טָמֵא:000000">inmundos</a> por causa de <a name="00400900700022" ll="SDBH:נֶפֶשׁ:000005">muerto</a>; ¿por qué seremos <a name="00400900700028" ll="SDBH:גרע:000003">impedidos</a> de <a name="00400900700034" ll="SDBH:קרב:000001">ofrecer</a> <a name="00400900700038" ll="SDBH:קָרְבָּן:000000">ofrenda</a> a <a name="00400900700040" ll="SDBH:יהוה:000000">Jehová</a> a <a name="00400900700044" ll="SDBH:מֹועֵד:000001">su</a> <a name="00400900700044" ll="SDBH:מֹועֵד:000001">tiempo</a> entre los <a name="00400900700050" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400900700052" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>? 
\v 8 Y <a name="00400900800008" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00400900800006">les</a> <a name="00400900800004" ll="SDBH:אמר:000000">respondió</a>: <a name="00400900800010">Esperad</a>, y <a name="00400900800014">oiré</a> <a name="00400900800016" ll="SDBH:מָה:000000">lo</a> <a name="00400900800016" ll="SDBH:מָה:000000">que</a> <a name="00400900800018" ll="SDBH:צוה:000000">ordena</a> <a name="00400900800020" ll="SDBH:יהוה:000000">Jehová</a> <a name="00400900800022">acerca</a> <a name="00400900800022">de</a> <a name="00400900800022">vosotros</a>.
    
\v 9 Y <a name="00400900900006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00400900900004" ll="SDBH:דבר:001000">habló</a> <a name="00400900900008">a</a> <a name="00400900900010" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, <a name="00400900900014" ll="SDBH:אמר:000000">diciendo</a>: 
\v 10 <a name="00400901000002" ll="SDBH:דבר:001000">Habla</a> <a name="00400901000004">a</a> los <a name="00400901000006" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400901000008" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, <a name="00400901000010;00400901000012" ll="SDBH:אמר:000000">diciendo</a>: <a name="00400901000014;00400901000016" ll="SDBH:אִישׁ:000000">Cualquiera</a> <a name="00400901000036">de</a> <a name="00400901000036">vosotros</a> <a name="00400901000038" ll="SDBH:אֹו:000001">o</a> de <a name="00400901000042" ll="SDBH:דֹּור:001000">vuestros</a> <a name="00400901000042" ll="SDBH:דֹּור:001000">descendientes</a>, <a name="00400901000018">que</a> <a name="00400901000020">estuviere</a> <a name="00400901000022" ll="SDBH:טָמֵא:000000">inmundo</a> por causa de <a name="00400901000026" ll="SDBH:נֶפֶשׁ:000005">muerto</a> <a name="00400901000028" ll="SDBH:אֹו:000001">o</a> estuviere de <a name="00400901000032" ll="SDBH:דֶּרֶךְ:000003">viaje</a> <a name="00400901000034" ll="SDBH:רָחֹוק:000000">lejos</a>, <a name="00400901000046">celebrará</a> la <a name="00400901000048" ll="SDBH:פֶּסַח:000000">pascua</a> a <a name="00400901000052" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 11 En el <a name="00400901100006" ll="SDBH:חֹדֶשׁ:000000">mes</a> <a name="00400901100010" ll="SDBH:שֵׁנִי:000000">segundo</a>, a los <a name="00400901100014;00400901100016" ll="SDBH:אַרְבַּע:000000;SDBH:עָשָׂר:000000">catorce</a> <a name="00400901100018" ll="SDBH:יֹום:000001">días</a> del mes, <a name="00400901100020">entre</a> las <a name="00400901100024" ll="SDBH:עֶרֶב:000000">dos</a> <a name="00400901100024" ll="SDBH:עֶרֶב:000000">tardes</a>, la <a name="00400901100026">celebrarán</a>; <a name="00400901100030">con</a> <a name="00400901100032" ll="SDBH:מַצָּה:000000">panes</a> <a name="00400901100032" ll="SDBH:מַצָּה:000000">sin</a> <a name="00400901100032" ll="SDBH:מַצָּה:000000">levadura</a> y <a name="00400901100036" ll="SDBH:מָרֹר:000000">hierbas</a> <a name="00400901100036" ll="SDBH:מָרֹר:000000">amargas</a> la <a name="00400901100038" ll="SDBH:אכל:000000">comerán</a>. 
\v 12 <a name="00400901200002" ll="SDBH:לֹא:000000">No</a> <a name="00400901200004" ll="SDBH:שׁאר:000002">dejarán</a> <a name="00400901200006">del</a> animal sacrificado para la <a name="00400901200010" ll="SDBH:בֹּקֶר:000000">mañana</a>, <a name="00400901200016" ll="SDBH:לֹא:000000">ni</a> <a name="00400901200018" ll="SDBH:שׁבר:000000">quebrarán</a> <a name="00400901200014" ll="SDBH:עֶצֶם:000000">hueso</a> de él; conforme a <a name="00400901200024">todos</a> los <a name="00400901200026" ll="SDBH:חֻקָּה:000001">ritos</a> de la <a name="00400901200030" ll="SDBH:פֶּסַח:000000">pascua</a> la <a name="00400901200032">celebrarán</a>. 
\v 13 Mas el <a name="00400901300008">que</a> estuviere <a name="00400901300012" ll="SDBH:טָהֹור:000000">limpio</a>, y <a name="00400901300020" ll="SDBH:לֹא:000000">no</a> <a name="00400901300022">estuviere</a> de <a name="00400901300018" ll="SDBH:דֶּרֶךְ:000003">viaje</a>, si <a name="00400901300026" ll="SDBH:חדל:000002">dejare</a> de <a name="00400901300030">celebrar</a> la <a name="00400901300034" ll="SDBH:פֶּסַח:000000">pascua</a>, la tal <a name="00400901300042" ll="SDBH:נֶפֶשׁ:000004">persona</a> será <a name="00400901300038" ll="SDBH:כרת:000003">cortada</a> de entre <a name="00400901300010">su</a> <a name="00400901300050" ll="SDBH:עַם:000000">pueblo</a>; por cuanto <a name="00400901300058" ll="SDBH:לֹא:000000">no</a> <a name="00400901300060" ll="SDBH:קרב:000001">ofreció</a> a <a name="00400901300046">su</a> <a name="00400901300064" ll="SDBH:מֹועֵד:000001">tiempo</a> la <a name="00400901300054" ll="SDBH:קָרְבָּן:000000">ofrenda</a> de <a name="00400901300056" ll="SDBH:יהוה:000000">Jehová</a>, el tal <a name="00400901300006" ll="SDBH:אִישׁ:000000">hombre</a> <a name="00400901300068" ll="SDBH:נשׂא:000007">llevará</a> <a name="00400901300076">su</a> <a name="00400901300066" ll="SDBH:חֵטְא:000000">pecado</a>. 
\v 14 Y <a name="00400901400004">si</a> <a name="00400901400006" ll="SDBH:גור:000000">morare</a> <a name="00400901400008">con</a> <a name="00400901400008">vosotros</a> <a name="00400901400010" ll="SDBH:גֵּר:000000">extranjero</a>, y <a name="00400901400014">celebrare</a> la <a name="00400901400016" ll="SDBH:פֶּסַח:000000">pascua</a> a <a name="00400901400020" ll="SDBH:יהוה:000000">Jehová</a>, <a name="00400901400022">conforme</a> al <a name="00400901400024" ll="SDBH:חֻקָּה:000001" tl="HOTTP491">rito</a> de la <a name="00400901400028" ll="SDBH:פֶּסַח:000000" tl="HOTTP491">pascua</a> y <a name="00400901400032">conforme</a> a <a name="00400901400034" ll="SDBH:מִשְׁפָּט:000006">sus</a> <a name="00400901400034" ll="SDBH:מִשְׁפָּט:000006">leyes</a> la <a name="00400901400038">celebrará</a>; <a name="00400901400042" ll="SDBH:אֶחָד:000000">un</a> <a name="00400901400042" ll="SDBH:אֶחָד:000000">mismo</a> <a name="00400901400040" ll="SDBH:חֻקָּה:000001">rito</a> <a name="00400901400044">tendréis</a>, tanto el <a name="00400901400054" ll="SDBH:גֵּר:000000">extranjero</a> como el <a name="00400901400060" ll="SDBH:אֶזְרָח:000000">natural</a> de la <a name="00400901400064" ll="SDBH:אֶרֶץ:000000">tierra</a>.
    
\v 15 El <a name="00400901500006" ll="SDBH:יֹום:000003">día</a> que el <a name="00400901500014" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a> fue <a name="00400901500008">erigido</a>, la <a name="00400901500020" ll="SDBH:עָנָן:000000">nube</a> <a name="00400901500016" ll="SDBH:כסה:000000">cubrió</a> el <a name="00400901500026" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a> sobre la <a name="00400901500030;00400901500034" ll="SDBH:אֹהֶל:000002;SDBH:עֵדוּת:000001">tienda</a> <a name="00400901500030;00400901500034" ll="SDBH:אֹהֶל:000002;SDBH:עֵדוּת:000001">del</a> <a name="00400901500030;00400901500034" ll="SDBH:אֹהֶל:000002;SDBH:עֵדוּת:000001">testimonio</a>; y a la <a name="00400901500042" ll="SDBH:עֶרֶב:000000">tarde</a> <a name="00400901500044">había</a> <a name="00400901500046">sobre</a> el <a name="00400901500050" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a> como una <a name="00400901500054" ll="SDBH:מַרְאֶה:000000">apariencia</a> de <a name="00400901500056" ll="SDBH:אֵשׁ:000000">fuego</a>, hasta la <a name="00400901500060" ll="SDBH:בֹּקֶר:000000">mañana</a>. 
\v 16 <a name="00400901600002">Así</a> <a name="00400901600004">era</a> <a name="00400901600006" ll="SDBH:תָּמִיד:000000">continuamente</a>: la <a name="00400901600010" ll="SDBH:עָנָן:000000">nube</a> lo <a name="00400901600012" ll="SDBH:כסה:000000" tl="HOTTP492">cubría</a> de día, y de <a name="00400901600020" ll="SDBH:לַיְלָה:000000">noche</a> la <a name="00400901600016" ll="SDBH:מַרְאֶה:000000">apariencia</a> de <a name="00400901600018" ll="SDBH:אֵשׁ:000000">fuego</a>. 
\v 17 Cuando <a name="00400901700006;00400901700008" ll="SDBH:עלה:000000">se</a> <a name="00400901700006;00400901700008" ll="SDBH:עלה:000000">alzaba</a> la <a name="00400901700012" ll="SDBH:עָנָן:000000">nube</a> del <a name="00400901700020" ll="SDBH:אֹהֶל:000000">tabernáculo</a>, los <a name="00400901700030" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400901700032" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00400901700028" ll="SDBH:נסע:000001">partían</a>; y en el <a name="00400901700038" ll="SDBH:מָקֹום:000000">lugar</a> <a name="00400901700040">donde</a> la <a name="00400901700048" ll="SDBH:עָנָן:000000">nube</a> <a name="00400901700042" ll="SDBH:שׁכן:000000">paraba</a>, <a name="00400901700044" ll="SDBH:שָׁם:000000">allí</a> <a name="00400901700052" ll="SDBH:חנה:000001">acampaban</a> los <a name="00400901700054" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400901700056" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 18 <a name="00400901800002;00400901800016">Al</a> <a name="00400901800004" ll="SDBH:פֶּה:000001">mandato</a> de <a name="00400901800006;00400901800020" ll="SDBH:יהוה:000000">Jehová</a> los <a name="00400901800010" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00400901800012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00400901800008" ll="SDBH:נסע:000001">partían</a>, y al <a name="00400901800018" ll="SDBH:פֶּה:000001">mandato</a> de <a name="00400901800006;00400901800020" ll="SDBH:יהוה:000000">Jehová</a> <a name="00400901800022" ll="SDBH:חנה:000001">acampaban</a>; <a name="00400901800024">todos</a> los <a name="00400901800026" ll="SDBH:יֹום:000003">días</a> <a name="00400901800028">que</a> la <a name="00400901800034" ll="SDBH:עָנָן:000000">nube</a> <a name="00400901800030" ll="SDBH:שׁכן:000000">estaba</a> <a name="00400901800036">sobre</a> el <a name="00400901800040" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a>, <a name="00400901800042" ll="SDBH:חנה:000001">permanecían</a> acampados. 
