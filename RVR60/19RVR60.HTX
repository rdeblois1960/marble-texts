\v 7 <a name="01905000700002">Oye</a>, <a name="01905000700004" ll="SDBH:עַם:000000">pueblo</a> <a name="01905000700004" ll="SDBH:עַם:000000">mío</a>, y <a name="01905000700008" ll="SDBH:דבר:001000">hablaré</a>;Escucha, <a name="01905000700010" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, y <a name="01905000700014" ll="SDBH:עוד:000002">testificaré</a> <a name="01905000700016">contra</a> <a name="01905000700016">ti</a>:<a name="01905000700022">Yo</a> soy <a name="01905000700018" ll="SDBH:אֱלֹהִים:000002">Dios</a>, el <a name="01905000700020" ll="SDBH:אֱלֹהִים:000001">Dios</a> <a name="01905000700020" ll="SDBH:אֱלֹהִים:000001">tuyo</a>.
    
\v 8 <a name="01905000800002" ll="SDBH:לֹא:000000">No</a> te <a name="01905000800008" ll="SDBH:יכח:000003">reprenderé</a> <a name="01905000800004">por</a> <a name="01905000800006" ll="SDBH:זֶבַח:000000">tus</a> <a name="01905000800006" ll="SDBH:זֶבַח:000000">sacrificios</a>,Ni por <a name="01905000800012" ll="SDBH:עֹלָה:000000" il="HK00251;HK00256;LB00253;LB00254;LB00255;NBGlo20140504a;NBGlo20140504b;NBGlo20140504c;Realia:4.2.3">tus</a> <a name="01905000800012" ll="SDBH:עֹלָה:000000" il="HK00251;HK00256;LB00253;LB00254;LB00255;NBGlo20140504a;NBGlo20140504b;NBGlo20140504c;Realia:4.2.3">holocaustos</a>, que están <a name="01905000800018" ll="SDBH:תָּמִיד:000000">continuamente</a> <a name="01905000800016" ll="SDBH:נֶגֶד:000000">delante</a> de mí.
    
\v 9 <a name="01905000900002" ll="SDBH:לֹא:000000">No</a> <a name="01905000900004" ll="SDBH:לקח:000000">tomaré</a> de <a name="01905000900008" ll="SDBH:בַּיִת:000003">tu</a> <a name="01905000900008" ll="SDBH:בַּיִת:000003">casa</a> <a name="01905000900010" ll="SDBH:פַּר:000000" il="Fauna:2.10;LB00022;rs032002108">becerros</a>,Ni machos <a name="01905000900016" ll="SDBH:עַתּוּד:000000" il="BK00016;NBGbs20120208-08;tb030407773">cabríos</a> de <a name="01905000900014" ll="SDBH:מִכְלָא:000000" il="Realia:1.2.1">tus</a> <a name="01905000900014" ll="SDBH:מִכְלָא:000000" il="Realia:1.2.1">apriscos</a>.
    
