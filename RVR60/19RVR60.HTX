\v 3 <a name="01908200300002" ll="SDBH:שׁפט:000003" il="Tel-dan-israelite-gate-im-tor">Defended</a> al <a name="01908200300004" ll="SDBH:דַּל:001000">débil</a> y al <a name="01908200300008" ll="SDBH:יָתֹום:000000">huérfano</a>;Haced <a name="01908200300016" ll="SDBH:צדק:000003">justicia</a> al <a name="01908200300010" ll="SDBH:עָנִי:000000">afligido</a> y al <a name="01908200300014" ll="SDBH:רושׁ:000001">menesteroso</a>.
    
\v 4 <a name="01908200400002" ll="SDBH:פלט:000001">Librad</a> al <a name="01908200400004" ll="SDBH:דַּל:001000">afligido</a> y al <a name="01908200400008" ll="SDBH:אֶבְיֹון:000000">necesitado</a>;<a name="01908200400016" ll="SDBH:נצל:000005">Libradlo</a> de <a name="01908200400012">mano</a> de los <a name="01908200400014" ll="SDBH:רָשָׁע:000000">impíos</a>.
    
\v 5 <a name="01908200500002;01908200500008" ll="SDBH:לֹא:000000">No</a> <a name="01908200500004">saben</a>, no <a name="01908200500010" ll="SDBH:בין:000001">entienden</a>,<a name="01908200500016" ll="SDBH:הלך:000004">Andan</a> en <a name="01908200500014" ll="SDBH:חֲשֵׁכָה:000000">tinieblas</a>;<a name="01908200500018" ll="SDBH:מוט:000000">Tiemblan</a> <a name="01908200500020">todos</a> los <a name="01908200500022" ll="SDBH:מֹוסָד:000000">cimientos</a> de la <a name="01908200500024" ll="SDBH:אֶרֶץ:000000">tierra</a>.
    
\v 6 <a name="01908200600002">Yo</a> <a name="01908200600004" ll="SDBH:אמר:000000">dije</a>: <a name="01908200600008">Vosotros</a> sois <a name="01908200600006" ll="SDBH:אֱלֹהִים:000000">dioses</a>,Y <a name="01908200600016">todos</a> <a name="01908200600016">vosotros</a> <a name="01908200600012" ll="SDBH:בֵּן:000003">hijos</a> del <a name="01908200600014" ll="SDBH:עֶלְיֹון:000000">Altísimo</a>;
    
\v 7 <a name="01908200700002" ll="SDBH:אָכֵן:000001">Pero</a> como <a name="01908200700006" ll="SDBH:אָדָם:000000">hombres</a> <a name="01908200700008" ll="SDBH:מות:000000">moriréis</a>,Y como <a name="01908200700014" ll="SDBH:אֶחָד:000000">cualquiera</a> de los <a name="01908200700018" ll="SDBH:שַׂר:000000">príncipes</a> <a name="01908200700020" ll="SDBH:נפל:000043">caeréis</a>.
    
\v 8 <a name="01908200800002">Levántate</a>, oh <a name="01908200800004" ll="SDBH:אֱלֹהִים:000002">Dios</a>, <a name="01908200800006" ll="SDBH:שׁפט:000000" il="Tel-dan-israelite-gate-im-tor">juzga</a> la <a name="01908200800010" ll="SDBH:אֶרֶץ:000000">tierra</a>;Porque <a name="01908200800014">tú</a> <a name="01908200800016" ll="SDBH:נחל:000000">heredarás</a> <a name="01908200800020">todas</a> las <a name="01908200800024" ll="SDBH:גֹּוי:000000">naciones</a>.
\c 83
\v 2 (1) Oh <a name="01908300200002" ll="SDBH:אֱלֹהִים:000002">Dios</a>, <a name="01908300200004" ll="SDBH:אַל:000000">no</a> guardes <a name="01908300200006" ll="SDBH:דֳּמִי:000000">silencio</a>;No <a name="01908300200012" ll="SDBH:חרשׁ:001001">calles</a>, oh <a name="01908300200020" ll="SDBH:אֵל:000001">Dios</a>, ni te estés <a name="01908300200018" ll="SDBH:שׁקט:000002">quieto</a>.
    
\v 3 (2) <a name="01908300300002">Porque</a> <a name="01908300300004">he</a> <a name="01908300300004">aquí</a> que <a name="01908300300008" ll="SDBH:המה:000000">rugen</a> <a name="01908300300006" ll="SDBH:איב:000001">tus</a> <a name="01908300300006" ll="SDBH:איב:000001">enemigos</a>,Y los <a name="01908300300012" ll="SDBH:שׂנא:000001">que</a> <a name="01908300300012" ll="SDBH:שׂנא:000001">te</a> <a name="01908300300012" ll="SDBH:שׂנא:000001">aborrecen</a> <a name="01908300300014" ll="SDBH:נשׂא:000028">alzan</a> <a name="01908300300016" ll="SDBH:רֹאשׁ:000005">cabeza</a>.
    
\v 4 (3) <a name="01908300400002">Contra</a> <a name="01908300400004" ll="SDBH:עַם:000000">tu</a> <a name="01908300400004" ll="SDBH:עַם:000000">pueblo</a> <a name="01908300400006" ll="SDBH:ערם:001001">han</a> <a name="01908300400006" ll="SDBH:ערם:001001">consultado</a> <a name="01908300400006" ll="SDBH:ערם:001001">astuta</a> y <a name="01908300400008" ll="SDBH:סֹוד:000004">secretamente</a>,Y <a name="01908300400012" ll="SDBH:יעץ:000002">han</a> <a name="01908300400012" ll="SDBH:יעץ:000002">entrado</a> <a name="01908300400012" ll="SDBH:יעץ:000002">en</a> <a name="01908300400012" ll="SDBH:יעץ:000002">consejo</a> <a name="01908300400014">contra</a> <a name="01908300400016" ll="SDBH:צפן:000007">tus</a> <a name="01908300400016" ll="SDBH:צפן:000007">protegidos</a>.
    
\v 5 (4) Han <a name="01908300500002" ll="SDBH:אמר:000000">dicho</a>: Venid, y <a name="01908300500008" ll="SDBH:כחד:000002">destruyámoslos</a> para que no sean <a name="01908300500012" ll="SDBH:גֹּוי:000000">nación</a>,Y <a name="01908300500016" ll="SDBH:לֹא:000000">no</a> haya <a name="01908300500024">más</a> <a name="01908300500018" ll="SDBH:זכר:000000">memoria</a> del <a name="01908300500020" ll="SDBH:שֵׁם:000000">nombre</a> de <a name="01908300500022" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>.
    
