\v 10 (9) <a name="01904101000002" ll="SDBH:גַּם:000000">Aun</a> el <a name="01904101000004">hombre</a> de <a name="01904101000006" ll="SDBH:שָׁלֹום:000001">mi</a> <a name="01904101000006" ll="SDBH:שָׁלֹום:000001">paz</a>, <a name="01904101000008;01904101000012">en</a> <a name="01904101000008;01904101000012">quien</a> <a name="01904101000010" ll="SDBH:בטח:000001">yo</a> <a name="01904101000010" ll="SDBH:בטח:000001">confiaba</a>, el <a name="01904101000014" ll="SDBH:אכל:000000">que</a> de <a name="01904101000016" ll="SDBH:לֶחֶם:000000" il="Tandır_bread;tb061116157">mi</a> <a name="01904101000016" ll="SDBH:לֶחֶם:000000" il="Tandır_bread;tb061116157">pan</a> <a name="01904101000014" ll="SDBH:אכל:000000">comía</a>,<a name="01904101000018;01904101000022" ll="SDBH:גדל:000003;SDBH:עָקֵב:000000" tl="HOTTP2221">Alzó</a> <a name="01904101000020" tl="HOTTP2221">contra</a> <a name="01904101000020" tl="HOTTP2221">mí</a> <a name="01904101000018;01904101000022" ll="SDBH:גדל:000003;SDBH:עָקֵב:000000" tl="HOTTP2221">el</a> <a name="01904101000018;01904101000022" ll="SDBH:גדל:000003;SDBH:עָקֵב:000000" tl="HOTTP2221">calcañar</a>.
    
