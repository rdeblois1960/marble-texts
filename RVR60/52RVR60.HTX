\v 1 <a name="05200100100002" ll="SDBG:Παῦλος:000000">Pablo</a>, <a name="05200100100006" ll="SDBG:Σιλουανός:000000">Silvano</a> <a name="05200100100004">y</a> <a name="05200100100010" ll="SDBG:Τιμόθεος:000000">Timoteo</a>, a la <a name="05200100100014" ll="SDBG:ἐκκλησία:000000">iglesia</a> de los <a name="05200100100016" ll="SDBG:Θεσσαλονικεύς:000000">tesalonicenses</a> <a name="05200100100018" ll="SDBG:ἐν:000006">en</a> <a name="05200100100020" ll="SDBG:θεός:000000">Dios</a> <a name="05200100100022" ll="SDBG:πατήρ:000003">Padre</a> <a name="05200100100008">y</a> en el <a name="05200100100026" ll="SDBG:κύριος:000000">Señor</a> <a name="05200100100028;05200100100030" ll="SDBG:Χριστός:000001;SDBG:Ἰησοῦς:000000">Jesucristo</a>: <a name="05200100100032" ll="SDBG:χάρις:000000">Gracia</a> <a name="05200100100024">y</a> <a name="05200100100038" ll="SDBG:εἰρήνη:000000;SDBG:εἰρήνη:000001">paz</a> sean a vosotros, de Dios nuestro Padre <a name="05200100100036">y</a> del Señor Jesucristo.
    
\v 2 Damos <a name="05200100200008" ll="SDBG:πάντοτε:000000">siempre</a> <a name="05200100200002" ll="SDBG:εὐχαριστέω:000000;SDBG:εὐχαριστέω:000001">gracias</a> a <a name="05200100200006" ll="SDBG:θεός:000000">Dios</a> <a name="05200100200010" ll="SDBG:περί:000001;SDBG:περί:000006">por</a> <a name="05200100200012" ll="SDBG:πᾶς:000000">todos</a> vosotros, <a name="05200100200018" ll="SDBG:ποιέω:000000">haciendo</a> <a name="05200100200016" ll="SDBG:μνεία:000001">memoria</a> de vosotros <a name="05200100200020" ll="SDBG:ἐπί:000008;SDBG:ἐπί:000018">en</a> nuestras <a name="05200100200024" ll="SDBG:προσευχή:000000">oraciones</a>, 
\v 3 <a name="05200100300002" ll="SDBG:μνημονεύω:000003">acordándonos</a> sin cesar <a name="05200100300044" ll="SDBG:ἔμπροσθεν:000000;SDBG:ἔμπροσθεν:000002">delante</a> <a name="05200100300006" ll="SDBG:ὁ:000000">del</a> <a name="05200100300048" ll="SDBG:θεός:000000">Dios</a> <a name="05200100300014">y</a> <a name="05200100300052" ll="SDBG:πατήρ:000003">Padre</a> nuestro de <a name="05200100300010" ll="SDBG:ὁ:000000">la</a> <a name="05200100300008" ll="SDBG:ἔργον:000000">obra</a> de vuestra <a name="05200100300012" ll="SDBG:πίστις:000001;SDBG:πίστις:000003">fe</a>, <a name="05200100300016" ll="SDBG:ὁ:000000">del</a> <a name="05200100300018" ll="SDBG:κόπος:000000">trabajo</a> de vuestro <a name="05200100300022" ll="SDBG:ἀγάπη:000000">amor</a> <a name="05200100300024">y</a> de vuestra <a name="05200100300028" ll="SDBG:ὑπομονή:000000">constancia</a> en <a name="05200100300020" ll="SDBG:ὁ:000000">la</a> <a name="05200100300032" ll="SDBG:ἐλπίς:000000">esperanza</a> en nuestro <a name="05200100300036" ll="SDBG:κύριος:000000">Señor</a> <a name="05200100300040;05200100300042" ll="SDBG:Χριστός:000001;SDBG:Ἰησοῦς:000000">Jesucristo</a>. 
\v 4 Porque <a name="05200100400002" ll="SDBG:οἶδα:000000">conocemos</a>, <a name="05200100400004" ll="SDBG:ἀδελφός:000001">hermanos</a> <a name="05200100400006" ll="SDBG:ἀγαπάω:000000">amados</a> <a name="05200100400008">de</a> <a name="05200100400012" ll="SDBG:θεός:000000">Dios</a>, vuestra <a name="05200100400016" ll="SDBG:ἐκλογή:000000">elección</a>; 
