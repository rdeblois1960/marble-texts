\v 1 Siendo <a name="00601300100004" ll="SDBH:יְהֹושֻׁעַ:000000">Josué</a> ya <a name="00601300100006" ll="SDBH:זקן:000000">viejo</a>, <a name="00601300100008">entrado</a> en <a name="00601300100014" ll="SDBH:יֹום:000004">años</a>, <a name="00601300100020" ll="SDBH:יהוה:000000">Jehová</a> <a name="00601300100022">le</a> <a name="00601300100018" ll="SDBH:אמר:000000">dijo</a>: Tú <a name="00601300100024">eres</a> ya <a name="00601300100026" ll="SDBH:זקן:000000">viejo</a>, de edad <a name="00601300100028;00601300100034" ll="SDBH:יֹום:000004">avanzada</a>, y <a name="00601300100042" ll="SDBH:שׁאר:000000">queda</a> aún <a name="00601300100044;00601300100046" ll="SDBH:מְאֹד:000001">mucha</a> <a name="00601300100040" ll="SDBH:אֶרֶץ:000000">tierra</a> por <a name="00601300100050" ll="SDBH:ירשׁ:000002">poseer</a>. 
\v 2 <a name="00601300200002" ll="SDBH:זֶה:000000;SDBH:זֹה:000000">Ésta</a> es la <a name="00601300200006" ll="SDBH:אֶרֶץ:000000">tierra</a> que <a name="00601300200010" ll="SDBH:שׁאר:000000">queda</a>: <a name="00601300200012">todos</a> los territorios de los <a name="00601300200018" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a>, y <a name="00601300200022">todos</a> los de los <a name="00601300200026" ll="SDBH:גְּשׁוּרִי:000000">gesureos</a>; 
\v 3 <a name="00601300300002">desde</a> <a name="00601300300006" ll="SDBH:שִׁיחֹור:000000">Sihor</a>, <a name="00601300300008">que</a> está <a name="00601300300010">al</a> oriente de <a name="00601300300014" ll="SDBH:מִצְרַיִם:000001">Egipto</a>, hasta el <a name="00601300300020" ll="SDBH:גְּבוּל:000000">límite</a> de <a name="00601300300022" ll="SDBH:עֶקְרֹון:000000">Ecrón</a> al <a name="00601300300024" ll="SDBH:צָפֹון:000001">norte</a>, que <a name="00601300300032" ll="SDBH:חשׁב:000007">se</a> <a name="00601300300032" ll="SDBH:חשׁב:000007">considera</a> de los <a name="00601300300030" ll="SDBH:כְּנַעֲנִי:001000">cananeos</a>; de los <a name="00601300300034" ll="SDBH:חָמֵשׁ:000000">cinco</a> <a name="00601300300036" ll="SDBH:סֶרֶן:001000">príncipes</a> de los <a name="00601300300038" ll="SDBH:פְּלִשְׁתִּי:000000">filisteos</a>, el <a name="00601300300042" ll="SDBH:עַזָּתִי:000000">gazeo</a>, el <a name="00601300300048" ll="SDBH:אַשְׁדֹּודִי:000000">asdodeo</a>, el <a name="00601300300052" ll="SDBH:אֶשְׁקְלֹונִי:000000">ascaloneo</a>, el <a name="00601300300056" ll="SDBH:גִּתִּי:000000">geteo</a> y el <a name="00601300300062" ll="SDBH:עֶקְרֹונִי:000000">ecroneo</a>; también los <a name="00601300300068" ll="SDBH:עַוִּים:001000">aveos</a>; 
\v 4 al <a name="00601300400004" ll="SDBH:תֵּימָן:000000">sur</a> <a name="00601300400006">toda</a> la <a name="00601300400008" ll="SDBH:אֶרֶץ:000000">tierra</a> de los <a name="00601300400012" ll="SDBH:כְּנַעֲנִי:001000">cananeos</a>, y <a name="00601300400016" ll="SDBH:מְעָרָה:000000;SDBH:מְעָרָה:001000" tl="HOTTP764">Mehara</a>, <a name="00601300400018">que</a> es de los <a name="00601300400024" ll="SDBH:צִידֹנִי:000000" il="Bolen00097;HK00372;adr090508677;adr1307138853;pcm02737" tl="HOTTP764">sidonios</a>, hasta <a name="00601300400028" ll="SDBH:אֲפֵק:000001">Afec</a>, hasta los <a name="00601300400032" ll="SDBH:גְּבוּל:000000">límites</a> del <a name="00601300400036" ll="SDBH:אֱמֹרִי:000000">amorreo</a>; 
\v 5 la <a name="00601300500006" ll="SDBH:אֶרֶץ:000000">tierra</a> de los <a name="00601300500010" ll="SDBH:גִּבְלִי:000000">giblitas</a>, y <a name="00601300500014">todo</a> el <a name="00601300500018" ll="SDBH:לְבָנֹון:000000">Líbano</a> hacia <a name="00601300500020" ll="SDBH:מִזְרָח:000001">donde</a> <a name="00601300500020" ll="SDBH:מִזְרָח:000001">sale</a> el <a name="00601300500024" ll="SDBH:שֶׁמֶשׁ:000000">sol</a>, <a name="00601300500026">desde</a> Baal-gad al <a name="00601300500030">pie</a> del <a name="00601300500032" ll="SDBH:הַר:000000">monte</a> <a name="00601300500034" ll="SDBH:חֶרְמֹון:000000">Hermón</a>, <a name="00601300500036" ll="SDBH:עַד:000000">hasta</a> la <a name="00601300500040">entrada</a> de <a name="00601300500042" ll="SDBH:חֲמָת:002000">Hamat</a>; 
\v 6 <a name="00601300600002">todos</a> los que <a name="00601300600004">habitan</a> en las <a name="00601300600008" ll="SDBH:הַר:000001">montañas</a> <a name="00601300600010">desde</a> el <a name="00601300600014" ll="SDBH:לְבָנֹון:000000">Líbano</a> hasta <a name="00601300600018" ll="SDBH:מִשְׂרְפֹות מַיִם:000000" il="adr0807092531" tl="HOTTP765">Misrefotmaim</a>, <a name="00601300600020">todos</a> los <a name="00601300600022" ll="SDBH:צִידֹנִי:000000">sidonios</a>; <a name="00601300600024">yo</a> los <a name="00601300600026" ll="SDBH:ירשׁ:000005">exterminaré</a> delante <a name="00601300600028">de</a> los <a name="00601300600032" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00601300600034" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>; solamente repartirás tú por <a name="00601300600038" ll="SDBH:נפל:000032">suerte</a> el país a los <a name="00601300600042" ll="SDBH:יִשְׂרָאֵל:000000">israelitas</a> por <a name="00601300600046" ll="SDBH:נַחֲלָה:000000">heredad</a>, como te <a name="00601300600052">he</a> <a name="00601300600052">mandado</a>. 
\v 7 <a name="00601300700006" ll="SDBH:חלק:000007">Reparte</a>, pues, <a name="00601300700004">ahora</a> esta <a name="00601300700012" ll="SDBH:אֶרֶץ:000000">tierra</a> en <a name="00601300700020" ll="SDBH:נַחֲלָה:000000">heredad</a> a las <a name="00601300700024" ll="SDBH:תֵּשַׁע:000000" tl="HOTTP766">nueve</a> <a name="00601300700028" ll="SDBH:שֵׁבֶט:000006" tl="HOTTP766">tribus</a>, y a la <a name="00601300700032" ll="SDBH:חֲצִי:000000" tl="HOTTP766">media</a> <a name="00601300700036" ll="SDBH:שֵׁבֶט:000006" tl="HOTTP766">tribu</a> de <a name="00601300700040" ll="SDBH:מְנַשֶּׁה:000000" tl="HOTTP766">Manasés</a>.
    
\v 8 Porque los <a name="00601300800006" ll="SDBH:רְאוּבֵנִי:000000">rubenitas</a> y <a name="00601300800012" ll="SDBH:גָּדִי:000000">gaditas</a> y la otra mitad de Manasés <a name="00601300800014" ll="SDBH:לקח:000000">recibieron</a> ya <a name="00601300800016" ll="SDBH:נַחֲלָה:000000">su</a> <a name="00601300800016" ll="SDBH:נַחֲלָה:000000">heredad</a>, la <a name="00601300800018">cual</a> les <a name="00601300800020">dio</a> <a name="00601300800024" ll="SDBH:מֹשֶׁה:000000">Moisés</a> al <a name="00601300800028" ll="SDBH:עֵבֶר:000001">otro</a> <a name="00601300800028" ll="SDBH:עֵבֶר:000001">lado</a> del <a name="00601300800032" ll="SDBH:יַרְדֵּן:000000">Jordán</a> al <a name="00601300800034" ll="SDBH:מִזְרָח:000000">oriente</a>, <a name="00601300800036;00601300800038">según</a> se la <a name="00601300800040">dio</a> <a name="00601300800044" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00601300800046" ll="SDBH:עֶבֶד:000000">siervo</a> de <a name="00601300800048" ll="SDBH:יהוה:000000">Jehová</a>; 
\v 9 desde <a name="00601300900004" ll="SDBH:עֲרֹועֵר:000000">Aroer</a>, <a name="00601300900006">que</a> está <a name="00601300900008">a</a> la <a name="00601300900010" ll="SDBH:שָׂפָה:000003">orilla</a> del <a name="00601300900012" ll="SDBH:נַחַל:000000">arroyo</a> de <a name="00601300900014" ll="SDBH:אַרְנֹון:000000">Arnón</a>, y la <a name="00601300900020" ll="SDBH:עִיר:000000">ciudad</a> <a name="00601300900022">que</a> está en <a name="00601300900026" ll="SDBH:תָּוֶךְ:000000">medio</a> del <a name="00601300900030" ll="SDBH:נַחַל:000000">valle</a>, y <a name="00601300900034">toda</a> la <a name="00601300900038" ll="SDBH:מִישֹׁור:000001" tl="HOTTP768">llanura</a> de <a name="00601300900040" ll="SDBH:מֵידְבָא:000000" tl="HOTTP768">Medeba</a>, hasta <a name="00601300900044" ll="SDBH:דִּיבֹון:000000">Dibón</a>; 
\v 10 <a name="00601301000004">todas</a> las <a name="00601301000006" ll="SDBH:עִיר:000000">ciudades</a> de <a name="00601301000008" ll="SDBH:סִיחֹון:000000">Sehón</a> <a name="00601301000010" ll="SDBH:מֶלֶךְ:000000">rey</a> de los <a name="00601301000014" ll="SDBH:אֱמֹרִי:000000">amorreos</a>, el <a name="00601301000016">cual</a> <a name="00601301000018" ll="SDBH:מלך:000000">reinó</a> en <a name="00601301000022" ll="SDBH:חֶשְׁבֹּון:001000">Hesbón</a>, hasta los <a name="00601301000026" ll="SDBH:גְּבוּל:000000">límites</a> de los <a name="00601301000028" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00601301000030" ll="SDBH:עַמֹּון:000000">Amón</a>; 
\v 11 y <a name="00601301100006" ll="SDBH:גִּלְעָד:000001">Galaad</a>, y los <a name="00601301100010" ll="SDBH:גְּבוּל:000000">territorios</a> de los <a name="00601301100014" ll="SDBH:גְּשׁוּרִי:000000">gesureos</a> y de los <a name="00601301100020" ll="SDBH:מַעֲכָתִי:000000">maacateos</a>, y <a name="00601301100024">todo</a> el <a name="00601301100026" ll="SDBH:הַר:000000">monte</a> <a name="00601301100028" ll="SDBH:חֶרְמֹון:000000">Hermón</a>, y <a name="00601301100032">toda</a> la tierra de <a name="00601301100036" ll="SDBH:בָּשָׁן:000000">Basán</a> hasta <a name="00601301100040" ll="SDBH:סַלְכָה:000000">Salca</a>; 
\v 12 <a name="00601301200002">todo</a> el <a name="00601301200004" ll="SDBH:מַמְלָכוּת:000001">reino</a> de <a name="00601301200006" ll="SDBH:עֹוג:000000">Og</a> en <a name="00601301200012" ll="SDBH:בָּשָׁן:000000">Basán</a>, el <a name="00601301200014">cual</a> <a name="00601301200016" ll="SDBH:מלך:000000">reinó</a> en <a name="00601301200020" ll="SDBH:עַשְׁתָּרֹת:000000">Astarot</a> y en <a name="00601301200026" ll="SDBH:אֶדְרֶעִי:000000">Edrei</a>, el <a name="00601301200028">cual</a> <a name="00601301200030" ll="SDBH:שׁאר:000000">había</a> <a name="00601301200030" ll="SDBH:שׁאר:000000">quedado</a> del <a name="00601301200034" ll="SDBH:יֶתֶר:000000">resto</a> de los <a name="00601301200038" ll="SDBH:רְפָאִים:001000">refaítas</a>; pues <a name="00601301200044" ll="SDBH:מֹשֶׁה:000000">Moisés</a> los <a name="00601301200042" ll="SDBH:נכה:000003">derrotó</a>, y los <a name="00601301200048" ll="SDBH:ירשׁ:000005">echó</a>. 
\v 13 Mas a los <a name="00601301300016" ll="SDBH:גְּשׁוּרִי:000000">gesureos</a> y a los <a name="00601301300024" ll="SDBH:מַעֲכָתִי:000000">maacateos</a> <a name="00601301300004" ll="SDBH:לֹא:000000">no</a> los <a name="00601301300006" ll="SDBH:ירשׁ:000005">echaron</a> los <a name="00601301300008" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00601301300010" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, sino que <a name="00601301300030" ll="SDBH:גְּשׁוּר:000000">Gesur</a> y <a name="00601301300034" ll="SDBH:מַעֲכָת:000000">Maaca</a> <a name="00601301300028">habitaron</a> entre los <a name="00601301300040" ll="SDBH:יִשְׂרָאֵל:000000">israelitas</a> hasta <a name="00601301300046" ll="SDBH:יֹום:000005">hoy</a>.
    
\v 14 Pero a la <a name="00601301400006" ll="SDBH:שֵׁבֶט:000006">tribu</a> de <a name="00601301400010" ll="SDBH:לֵוִי:000001">Leví</a> <a name="00601301400012" ll="SDBH:לֹא:000000">no</a> <a name="00601301400014">dio</a> <a name="00601301400016" ll="SDBH:נַחֲלָה:000000">heredad</a>; los <a name="00601301400018" ll="SDBH:אִשֶּׁה:000000" tl="HOTTP769">sacrificios</a> de <a name="00601301400020" ll="SDBH:יהוה:000000" tl="HOTTP769">Jehová</a> <a name="00601301400022" ll="SDBH:אֱלֹהִים:000001">Dios</a> de <a name="00601301400024" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> son <a name="00601301400028" ll="SDBH:נַחֲלָה:000000">su</a> <a name="00601301400028" ll="SDBH:נַחֲלָה:000000">heredad</a>, como él les <a name="00601301400034" ll="SDBH:דבר:001000">había</a> <a name="00601301400034" ll="SDBH:דבר:001000">dicho</a>.
    
\v 15 <a name="00601301500004">Dio</a>, pues, <a name="00601301500006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> a la <a name="00601301500010" ll="SDBH:מַטֶּה:000007">tribu</a> de los <a name="00601301500012" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00601301500014" ll="SDBH:רְאוּבֵן:000000">Rubén</a> conforme a <a name="00601301500018" ll="SDBH:מִשְׁפָּחָה:000000">sus</a> <a name="00601301500018" ll="SDBH:מִשְׁפָּחָה:000000">familias</a>. 
\v 16 Y <a name="00601301600004">fue</a> el <a name="00601301600010" ll="SDBH:גְּבוּל:000000">territorio</a> de ellos desde <a name="00601301600014" ll="SDBH:עֲרֹועֵר:000000">Aroer</a>, <a name="00601301600016">que</a> está <a name="00601301600018">a</a> la <a name="00601301600020" ll="SDBH:שָׂפָה:000003">orilla</a> del <a name="00601301600022" ll="SDBH:נַחַל:000000">arroyo</a> de <a name="00601301600024" ll="SDBH:אַרְנֹון:000000">Arnón</a>, y la <a name="00601301600030" ll="SDBH:עִיר:000000">ciudad</a> <a name="00601301600032">que</a> está en <a name="00601301600036" ll="SDBH:תָּוֶךְ:000000">medio</a> del <a name="00601301600040" ll="SDBH:נַחַל:000000">valle</a>, y <a name="00601301600044">toda</a> la <a name="00601301600048" ll="SDBH:מִישֹׁור:000001">llanura</a> <a name="00601301600050">hasta</a> <a name="00601301600052" ll="SDBH:מֵידְבָא:000000">Medeba</a>; 
\v 17 <a name="00601301700002" ll="SDBH:חֶשְׁבֹּון:001000">Hesbón</a>, con <a name="00601301700006">todas</a> <a name="00601301700008" ll="SDBH:עִיר:000000">sus</a> <a name="00601301700008" ll="SDBH:עִיר:000000">ciudades</a> <a name="00601301700010">que</a> están en la <a name="00601301700016" ll="SDBH:מִישֹׁור:000001">llanura</a>; <a name="00601301700018" ll="SDBH:דִּיבֹון:000000">Dibón</a>, Bamot-baal, Bet-baal-meón, 
\v 18 <a name="00601301800004" ll="SDBH:יַהַץ:000000">Jahaza</a>, <a name="00601301800008" ll="SDBH:קְדֵמֹות:000000">Cademot</a>, <a name="00601301800012" ll="SDBH:מֵיפַעַת:000000">Mefaat</a>, 
\v 19 <a name="00601301900004" ll="SDBH:קִרְיָתַיִם:000000">Quiriataim</a>, <a name="00601301900008" ll="SDBH:שִׂבְמָה:000000">Sibma</a>, Zaret-sahar en el <a name="00601301900016" ll="SDBH:הַר:000000">monte</a> del <a name="00601301900020" ll="SDBH:עֵמֶק:000000">valle</a>, 
\v 20 Bet-peor, las <a name="00601302000008" ll="SDBH:אֲשֵׁדָה:000000">laderas</a> de <a name="00601302000012" ll="SDBH:פִּסְגָּה:000000">Pisga</a>, Bet-jesimot, 
\v 21 <a name="00601302100004">todas</a> las <a name="00601302100006" ll="SDBH:עִיר:000000">ciudades</a> de la <a name="00601302100010" ll="SDBH:מִישֹׁור:000001">llanura</a>, y <a name="00601302100014">todo</a> el <a name="00601302100016" ll="SDBH:מַמְלָכוּת:000001">reino</a> de <a name="00601302100018" ll="SDBH:סִיחֹון:000000">Sehón</a> <a name="00601302100020" ll="SDBH:מֶלֶךְ:000000">rey</a> de los <a name="00601302100024" ll="SDBH:אֱמֹרִי:000000">amorreos</a>, <a name="00601302100026">que</a> <a name="00601302100028" ll="SDBH:מלך:000000">reinó</a> en <a name="00601302100032" ll="SDBH:חֶשְׁבֹּון:001000">Hesbón</a>, al <a name="00601302100034">cual</a> <a name="00601302100036" ll="SDBH:נכה:000003">derrotó</a> <a name="00601302100038" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, y a los <a name="00601302100046" ll="SDBH:נָשִׂיא:000001">príncipes</a> de <a name="00601302100048" ll="SDBH:מִדְיָן:001000">Madián</a>, <a name="00601302100052" ll="SDBH:אֱוִי:000000">Evi</a>, <a name="00601302100058" ll="SDBH:רֶקֶם:000001">Requem</a>, <a name="00601302100064" ll="SDBH:צוּר:003000">Zur</a>, <a name="00601302100070" ll="SDBH:חוּר:002002">Hur</a> y <a name="00601302100076" ll="SDBH:רֶבַע:001000">Reba</a>, <a name="00601302100078" ll="SDBH:נָסִיךְ:000002">príncipes</a> de <a name="00601302100080" ll="SDBH:סִיחֹון:000000">Sehón</a> que <a name="00601302100082">habitaban</a> en aquella <a name="00601302100086" ll="SDBH:אֶרֶץ:000000">tierra</a>. 
