\v 30 <a name="00300703000002">Sus</a> <a name="00300703000002">manos</a> <a name="00300703000004">traerán</a> las <a name="00300703000008" ll="SDBH:אִשֶּׁה:000000">ofrendas</a> <a name="00300703000008" ll="SDBH:אִשֶּׁה:000000">que</a> <a name="00300703000008" ll="SDBH:אִשֶּׁה:000000">se</a> <a name="00300703000008" ll="SDBH:אִשֶּׁה:000000">han</a> <a name="00300703000008" ll="SDBH:אִשֶּׁה:000000">de</a> <a name="00300703000008" ll="SDBH:אִשֶּׁה:000000">quemar</a> ante <a name="00300703000010;00300703000044" ll="SDBH:יהוה:000000">Jehová</a>; <a name="00300703000024">traerá</a> la <a name="00300703000016" ll="SDBH:חֵלֶב:000000">grosura</a> <a name="00300703000018">con</a> el <a name="00300703000022" ll="SDBH:חָזֶה:000000">pecho</a>; el <a name="00300703000030" ll="SDBH:חָזֶה:000000">pecho</a> para que sea <a name="00300703000034" ll="SDBH:נוף:000002">mecido</a> como sacrificio <a name="00300703000038" ll="SDBH:תְּנוּפָה:000001">mecido</a> <a name="00300703000042">delante</a> de <a name="00300703000010;00300703000044" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 31 Y la <a name="00300703100014" ll="SDBH:חֵלֶב:000000">grosura</a> la hará <a name="00300703100004" ll="SDBH:קטר:000000">arder</a> el <a name="00300703100008" ll="SDBH:כֹּהֵן:000000">sacerdote</a> en el <a name="00300703100018" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, mas el <a name="00300703100026" ll="SDBH:חָזֶה:000000">pecho</a> <a name="00300703100022">será</a> de <a name="00300703100030" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y de <a name="00300703100036" ll="SDBH:בֵּן:000000">sus</a> <a name="00300703100036" ll="SDBH:בֵּן:000000">hijos</a>. 
\v 32 Y <a name="00300703200012">daréis</a> al <a name="00300703200020" ll="SDBH:כֹּהֵן:000000">sacerdote</a> para ser <a name="00300703200014" ll="SDBH:תְּרוּמָה:000000">elevada</a> <a name="00300703200014" ll="SDBH:תְּרוּמָה:000000">en</a> <a name="00300703200014" ll="SDBH:תְּרוּמָה:000000">ofrenda</a>, la <a name="00300703200006" ll="SDBH:שֹׁוק:000000">espaldilla</a> <a name="00300703200010" ll="SDBH:יָמִין:000000">derecha</a> de <a name="00300703200024;00300703200026" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">vuestros</a> <a name="00300703200024;00300703200026" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">sacrificios</a> <a name="00300703200024;00300703200026" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">de</a> <a name="00300703200024;00300703200026" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">paz</a>. 
\v 33 El que de los <a name="00300703300024" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300703300026" ll="SDBH:אַהֲרֹן:000000">Aarón</a> <a name="00300703300004" ll="SDBH:קרב:000001">ofreciere</a> la <a name="00300703300008" ll="SDBH:דָּם:000000">sangre</a> de los <a name="00300703300012" ll="SDBH:שֶׁלֶם:000000">sacrificios</a> <a name="00300703300012" ll="SDBH:שֶׁלֶם:000000">de</a> <a name="00300703300012" ll="SDBH:שֶׁלֶם:000000">paz</a>, y la <a name="00300703300020" ll="SDBH:חֵלֶב:000000">grosura</a>, <a name="00300703300030">recibirá</a> la <a name="00300703300032" ll="SDBH:שֹׁוק:000000">espaldilla</a> <a name="00300703300036" ll="SDBH:יָמִין:000000">derecha</a> como <a name="00300703300040" ll="SDBH:מָנָה:000000">porción</a> suya. 
\v 34 Porque <a name="00300703400022" ll="SDBH:לקח:000000">he</a> <a name="00300703400022" ll="SDBH:לקח:000000">tomado</a> de los <a name="00300703400034;00300703400036" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">sacrificios</a> <a name="00300703400034;00300703400036" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">de</a> <a name="00300703400034;00300703400036" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">paz</a> de los <a name="00300703400028" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300703400030" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> el <a name="00300703400006" ll="SDBH:חָזֶה:000000">pecho</a> <a name="00300703400010" ll="SDBH:תְּנוּפָה:000001">que</a> <a name="00300703400010" ll="SDBH:תְּנוּפָה:000001">se</a> <a name="00300703400010" ll="SDBH:תְּנוּפָה:000001">mece</a> y la <a name="00300703400016" ll="SDBH:שֹׁוק:000000">espaldilla</a> <a name="00300703400020" ll="SDBH:תְּרוּמָה:000000">elevada</a> <a name="00300703400020" ll="SDBH:תְּרוּמָה:000000">en</a> <a name="00300703400020" ll="SDBH:תְּרוּמָה:000000">ofrenda</a>, y lo <a name="00300703400040">he</a> <a name="00300703400040">dado</a> a <a name="00300703400046" ll="SDBH:אַהֲרֹן:000000">Aarón</a> el <a name="00300703400050" ll="SDBH:כֹּהֵן:000000">sacerdote</a> y a <a name="00300703400056" ll="SDBH:בֵּן:000000">sus</a> <a name="00300703400056" ll="SDBH:בֵּן:000000">hijos</a>, como <a name="00300703400060" ll="SDBH:חֹק:000003">estatuto</a> <a name="00300703400062" ll="SDBH:עֹולָם:000000">perpetuo</a> para los <a name="00300703400068" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300703400070" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>. 
\v 35 <a name="00300703500002" ll="SDBH:זֶה:000000;SDBH:זֹה:000000">Ésta</a> es la <a name="00300703500004" ll="SDBH:מִשְׁחָה:001000">porción</a> de <a name="00300703500006" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y la <a name="00300703500010" ll="SDBH:מִשְׁחָה:001000">porción</a> de <a name="00300703500012" ll="SDBH:בֵּן:000000">sus</a> <a name="00300703500012" ll="SDBH:בֵּן:000000">hijos</a>, de las <a name="00300703500016" ll="SDBH:אִשֶּׁה:000000">ofrendas</a> <a name="00300703500016" ll="SDBH:אִשֶּׁה:000000">encendidas</a> a <a name="00300703500018;00300703500034" ll="SDBH:יהוה:000000">Jehová</a>, desde el <a name="00300703500022" ll="SDBH:יֹום:000003">día</a> que él los <a name="00300703500024" ll="SDBH:קרב:000001">consagró</a> para ser <a name="00300703500030" ll="SDBH:כהן:000000">sacerdotes</a> de <a name="00300703500018;00300703500034" ll="SDBH:יהוה:000000">Jehová</a>, 
\v 36 la <a name="00300703600002">cual</a> <a name="00300703600004" ll="SDBH:צוה:000000">mandó</a> <a name="00300703600006" ll="SDBH:יהוה:000000">Jehová</a> que les <a name="00300703600010">diesen</a>, desde el <a name="00300703600016" ll="SDBH:יֹום:000003">día</a> que él los <a name="00300703600018" ll="SDBH:משׁח:000000">ungió</a> de <a name="00300703600020">entre</a> los <a name="00300703600026" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300703600028" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, como <a name="00300703600030" ll="SDBH:חֻקָּה:000001">estatuto</a> <a name="00300703600032" ll="SDBH:עֹולָם:000000">perpetuo</a> en <a name="00300703600036" ll="SDBH:דֹּור:001000">sus</a> <a name="00300703600036" ll="SDBH:דֹּור:001000">generaciones</a>.
    
\v 37 <a name="00300703700002" ll="SDBH:זֶה:000000;SDBH:זֹה:000000">Ésta</a> es la <a name="00300703700006" ll="SDBH:תֹּורָה:000000">ley</a> del <a name="00300703700012" ll="SDBH:עֹלָה:000000">holocausto</a>, de la <a name="00300703700018" ll="SDBH:מִנְחָה:000000">ofrenda</a>, del <a name="00300703700026" ll="SDBH:חַטָּאת:000004">sacrificio</a> <a name="00300703700026" ll="SDBH:חַטָּאת:000004">por</a> <a name="00300703700026" ll="SDBH:חַטָּאת:000004">el</a> <a name="00300703700026" ll="SDBH:חַטָּאת:000004">pecado</a>, del <a name="00300703700034" ll="SDBH:אָשָׁם:000002">sacrificio</a> <a name="00300703700034" ll="SDBH:אָשָׁם:000002">por</a> <a name="00300703700034" ll="SDBH:אָשָׁם:000002">la</a> <a name="00300703700034" ll="SDBH:אָשָׁם:000002">culpa</a>, de las <a name="00300703700042" ll="SDBH:מִלֻּאִים:000001">consagraciones</a> y del <a name="00300703700048;00300703700052" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">sacrificio</a> <a name="00300703700048;00300703700052" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">de</a> <a name="00300703700048;00300703700052" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">paz</a>, 
\v 38 la <a name="00300703800002">cual</a> <a name="00300703800004" ll="SDBH:צוה:000000;SDBH:צוה:000001">mandó</a> <a name="00300703800006;00300703800040" ll="SDBH:יהוה:000000">Jehová</a> a <a name="00300703800010" ll="SDBH:מֹשֶׁה:000000">Moisés</a> en el <a name="00300703800014" ll="SDBH:הַר:000000">monte</a> de <a name="00300703800016" ll="SDBH:סִינַי:000000">Sinaí</a>, el <a name="00300703800020" ll="SDBH:יֹום:000003">día</a> que <a name="00300703800022">mandó</a> a los <a name="00300703800026" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300703800028" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> que <a name="00300703800032" ll="SDBH:קרב:000001">ofreciesen</a> <a name="00300703800036" ll="SDBH:קָרְבָּן:000000">sus</a> <a name="00300703800036" ll="SDBH:קָרְבָּן:000000">ofrendas</a> a <a name="00300703800006;00300703800040" ll="SDBH:יהוה:000000">Jehová</a>, en el <a name="00300703800044" ll="SDBH:מִדְבָּר:000000">desierto</a> de <a name="00300703800046" ll="SDBH:סִינַי:000000">Sinaí</a>.
\c 8
\v 1 <a name="00300800100004" ll="SDBH:דבר:001000">Habló</a> <a name="00300800100006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00300800100008">a</a> <a name="00300800100010" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, <a name="00300800100014" ll="SDBH:אמר:000000">diciendo</a>: 
\v 2 <a name="00300800200002" ll="SDBH:לקח:000000">Toma</a> a <a name="00300800200006" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y a <a name="00300800200012" ll="SDBH:בֵּן:000000">sus</a> <a name="00300800200012" ll="SDBH:בֵּן:000000">hijos</a> <a name="00300800200004">con</a> él, y las <a name="00300800200022" ll="SDBH:בֶּגֶד:000001">vestiduras</a>, el <a name="00300800200028" ll="SDBH:שֶׁמֶן:000000">aceite</a> de la <a name="00300800200032" ll="SDBH:מִשְׁחָה:000000">unción</a>, el <a name="00300800200038" ll="SDBH:פַּר:000000">becerro</a> de la <a name="00300800200042" ll="SDBH:חַטָּאת:000004">expiación</a>, los <a name="00300800200048" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="00300800200052" ll="SDBH:אַיִל:000000">carneros</a>, y el <a name="00300800200058" ll="SDBH:סַל:000000">canastillo</a> de los <a name="00300800200062" ll="SDBH:מַצָּה:000000">panes</a> <a name="00300800200062" ll="SDBH:מַצָּה:000000">sin</a> <a name="00300800200062" ll="SDBH:מַצָּה:000000">levadura</a>; 
\v 3 y <a name="00300800300012" ll="SDBH:קהל:000000">reúne</a> <a name="00300800300006">toda</a> la <a name="00300800300010" ll="SDBH:עֵדָה:000000">congregación</a> <a name="00300800300014">a</a> la <a name="00300800300016" ll="SDBH:פֶּתַח:000000">puerta</a> del <a name="00300800300018;00300800300020" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00300800300018;00300800300020" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00300800300018;00300800300020" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>. 
\v 4 <a name="00300800400004">Hizo</a>, pues, <a name="00300800400006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300800400008;00300800400010">como</a> <a name="00300800400014" ll="SDBH:יהוה:000000">Jehová</a> <a name="00300800400016">le</a> <a name="00300800400012" ll="SDBH:צוה:000000">mandó</a>, y <a name="00300800400020" ll="SDBH:קהל:000001">se</a> <a name="00300800400020" ll="SDBH:קהל:000001">reunió</a> la <a name="00300800400024" ll="SDBH:עֵדָה:000000">congregación</a> <a name="00300800400026">a</a> la <a name="00300800400028" ll="SDBH:פֶּתַח:000000">puerta</a> del <a name="00300800400030;00300800400032" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00300800400030;00300800400032" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00300800400030;00300800400032" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>.
    
\v 5 Y <a name="00300800500004" ll="SDBH:אמר:000000">dijo</a> <a name="00300800500006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300800500008">a</a> la <a name="00300800500012" ll="SDBH:עֵדָה:000000">congregación</a>: <a name="00300800500018" ll="SDBH:דָּבָר:000000">Esto</a> es lo <a name="00300800500020">que</a> <a name="00300800500024" ll="SDBH:יהוה:000000">Jehová</a> <a name="00300800500022" ll="SDBH:צוה:000000">ha</a> <a name="00300800500022" ll="SDBH:צוה:000000">mandado</a> <a name="00300800500028">hacer</a>. 
\v 6 Entonces <a name="00300800600006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> hizo <a name="00300800600004" ll="SDBH:קרב:000001">acercarse</a> a <a name="00300800600010" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y a <a name="00300800600016" ll="SDBH:בֵּן:000000">sus</a> <a name="00300800600016" ll="SDBH:בֵּן:000000">hijos</a>, y los <a name="00300800600020" ll="SDBH:רחץ:000000">lavó</a> con <a name="00300800600028" ll="SDBH:מַיִם:000000">agua</a>. 
\v 7 Y <a name="00300800700004">puso</a> <a name="00300800700006">sobre</a> <a name="00300800700006">él</a> la <a name="00300800700012" ll="SDBH:כֻּתֹּנֶת:000000">túnica</a>, y le <a name="00300800700016" ll="SDBH:חגר:000002">ciñó</a> con el <a name="00300800700024" ll="SDBH:אַבְנֵט:000000">cinto</a>; le <a name="00300800700028" ll="SDBH:לבשׁ:000002">vistió</a> después el <a name="00300800700036" ll="SDBH:מְעִיל:000000">manto</a>, y <a name="00300800700040">puso</a> <a name="00300800700042">sobre</a> <a name="00300800700042">él</a> el <a name="00300800700048" ll="SDBH:אֵפֹוד:000001">efod</a>, y lo <a name="00300800700052" ll="SDBH:חגר:000002">ciñó</a> con el <a name="00300800700058" ll="SDBH:חֵשֶׁב:000000">cinto</a> del <a name="00300800700062" ll="SDBH:אֵפֹוד:000001">efod</a>, y lo <a name="00300800700066" ll="SDBH:אפד:000000">ajustó</a> con él. 
\v 8 Luego le <a name="00300800800004" ll="SDBH:שׂים:000001">puso</a> <a name="00300800800006">encima</a> el <a name="00300800800012" ll="SDBH:חֹשֶׁן:000000">pectoral</a>, y <a name="00300800800016">puso</a> dentro del mismo los <a name="00300800800028" ll="SDBH:אוּר:000002">Urim</a> y <a name="00300800800036" ll="SDBH:תֻּמִּים:000000">Tumim</a>. 
\v 9 Después <a name="00300800900004" ll="SDBH:שׂים:000001">puso</a> la <a name="00300800900010" ll="SDBH:מִצְנֶפֶת:000000">mitra</a> <a name="00300800900012">sobre</a> <a name="00300800900014" ll="SDBH:רֹאשׁ:000000">su</a> <a name="00300800900014" ll="SDBH:רֹאשׁ:000000">cabeza</a>, y <a name="00300800900020">sobre</a> la <a name="00300800900024" ll="SDBH:מִצְנֶפֶת:000000">mitra</a>, en <a name="00300800900028" ll="SDBH:מוּל:000000">frente</a>, puso la <a name="00300800900034" ll="SDBH:צִיץ:000001">lámina</a> de <a name="00300800900038" ll="SDBH:זָהָב:000000">oro</a>, la <a name="00300800900040" ll="SDBH:נֵזֶר:000003">diadema</a> <a name="00300800900044" ll="SDBH:קֹדֶשׁ:000001">santa</a>, como <a name="00300800900052" ll="SDBH:יהוה:000000">Jehová</a> <a name="00300800900050" ll="SDBH:צוה:000000">había</a> <a name="00300800900050" ll="SDBH:צוה:000000">mandado</a> a <a name="00300800900056" ll="SDBH:מֹשֶׁה:000000">Moisés</a>.
    
\v 10 Y <a name="00300801000004" ll="SDBH:לקח:000000">tomó</a> <a name="00300801000006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> el <a name="00300801000010" ll="SDBH:שֶׁמֶן:000000">aceite</a> de la <a name="00300801000014" ll="SDBH:מִשְׁחָה:000000">unción</a> y <a name="00300801000018" ll="SDBH:משׁח:000000">ungió</a> el <a name="00300801000024" ll="SDBH:מִשְׁכָּן:000002">tabernáculo</a> y <a name="00300801000030">todas</a> las cosas <a name="00300801000032">que</a> estaban en él, y las <a name="00300801000038" ll="SDBH:קדשׁ:000004">santificó</a>. 
\v 11 Y <a name="00300801100004" ll="SDBH:נזה:000001">roció</a> <a name="00300801100006">de</a> <a name="00300801100006">él</a> <a name="00300801100008">sobre</a> el <a name="00300801100012" ll="SDBH:מִזְבֵּחַ:000000">altar</a> <a name="00300801100014" ll="SDBH:שֶׁבַע:000000">siete</a> <a name="00300801100016">veces</a>, y <a name="00300801100020" ll="SDBH:משׁח:000000">ungió</a> el <a name="00300801100026" ll="SDBH:מִזְבֵּחַ:000000">altar</a> y <a name="00300801100032">todos</a> <a name="00300801100034" ll="SDBH:כְּלִי:000000">sus</a> <a name="00300801100034" ll="SDBH:כְּלִי:000000">utensilios</a>, y la <a name="00300801100042" ll="SDBH:כִּיֹּור:000001">fuente</a> y su base, para <a name="00300801100052" ll="SDBH:קדשׁ:000004">santificarlos</a>. 
\v 12 Y <a name="00300801200004" ll="SDBH:יצק:000002">derramó</a> del <a name="00300801200008" ll="SDBH:שֶׁמֶן:000000">aceite</a> de la <a name="00300801200012" ll="SDBH:מִשְׁחָה:000000">unción</a> <a name="00300801200014">sobre</a> la <a name="00300801200016" ll="SDBH:רֹאשׁ:000000">cabeza</a> de <a name="00300801200018" ll="SDBH:אַהֲרֹן:000000">Aarón</a>, y lo <a name="00300801200022" ll="SDBH:משׁח:000000">ungió</a> para <a name="00300801200028" ll="SDBH:קדשׁ:000004">santificarlo</a>. 
\v 13 Después <a name="00300801300006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> hizo <a name="00300801300004" ll="SDBH:קרב:000001">acercarse</a> los <a name="00300801300010" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300801300012" ll="SDBH:אַהֲרֹן:000000">Aarón</a>, y les <a name="00300801300016" ll="SDBH:לבשׁ:000002">vistió</a> las <a name="00300801300018" ll="SDBH:כֻּתֹּנֶת:000000">túnicas</a>, les <a name="00300801300022" ll="SDBH:חגר:000002">ciñó</a> con <a name="00300801300026" ll="SDBH:אַבְנֵט:000000">cintos</a>, y les <a name="00300801300030" ll="SDBH:חבשׁ:000004">ajustó</a> las <a name="00300801300034" ll="SDBH:מִגְבָּעָה:000000">tiaras</a>, como <a name="00300801300042" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="00300801300040" ll="SDBH:צוה:000000">había</a> <a name="00300801300040" ll="SDBH:צוה:000000">mandado</a> a <a name="00300801300046" ll="SDBH:מֹשֶׁה:000000">Moisés</a>.
    
\v 14 Luego hizo <a name="00300801400004" ll="SDBH:נגשׁ:000004">traer</a> el <a name="00300801400008" ll="SDBH:פַּר:000000">becerro</a> de la <a name="00300801400012" ll="SDBH:חַטָּאת:000004">expiación</a>, y <a name="00300801400018" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00300801400022" ll="SDBH:בֵּן:000000">sus</a> <a name="00300801400022" ll="SDBH:בֵּן:000000">hijos</a> <a name="00300801400016" ll="SDBH:סמך:000000">pusieron</a> <a name="00300801400026">sus</a> <a name="00300801400026">manos</a> <a name="00300801400028">sobre</a> la <a name="00300801400030" ll="SDBH:רֹאשׁ:000000">cabeza</a> del <a name="00300801400032" ll="SDBH:פַּר:000000">becerro</a> de la <a name="00300801400036" ll="SDBH:חַטָּאת:000004">expiación</a>, 
\v 15 y lo <a name="00300801500004" ll="SDBH:שׁחט:000000">degolló</a>; y <a name="00300801500010" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300801500008" ll="SDBH:לקח:000000">tomó</a> la <a name="00300801500016" ll="SDBH:דָּם:000000" il="Blood" tl="HOTTP370">sangre</a>, y <a name="00300801500020">puso</a> con <a name="00300801500034" ll="SDBH:אֶצְבַּע:000000">su</a> <a name="00300801500034" ll="SDBH:אֶצְבַּע:000000">dedo</a> <a name="00300801500022">sobre</a> los <a name="00300801500024" ll="SDBH:קֶרֶן:000003">cuernos</a> del <a name="00300801500028" ll="SDBH:מִזְבֵּחַ:000000">altar</a> <a name="00300801500030" ll="SDBH:סָבִיב:000000">alrededor</a>, y <a name="00300801500038" ll="SDBH:חטא:001008">purificó</a> el <a name="00300801500044" ll="SDBH:מִזְבֵּחַ:000000">altar</a>; y <a name="00300801500054" ll="SDBH:יצק:000002">echó</a> la demás <a name="00300801500052" ll="SDBH:דָּם:000000">sangre</a> <a name="00300801500056">al</a> <a name="00300801500058" ll="SDBH:יְסֹוד:000000">pie</a> del <a name="00300801500062" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, y lo <a name="00300801500066" ll="SDBH:קדשׁ:000004">santificó</a> para <a name="00300801500070" ll="SDBH:כפר:000008">reconciliar</a> <a name="00300801500072">sobre</a> <a name="00300801500072">él</a>. 
\v 16 Después <a name="00300801600004" ll="SDBH:לקח:000000">tomó</a> <a name="00300801600008">toda</a> la <a name="00300801600012" ll="SDBH:חֵלֶב:000000">grosura</a> <a name="00300801600014">que</a> estaba <a name="00300801600016">sobre</a> los <a name="00300801600020" ll="SDBH:קֶרֶב:000000">intestinos</a>, y la <a name="00300801600026" ll="SDBH:יֹתֶרֶת:000000">grosura</a> del hígado, y los <a name="00300801600036" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="00300801600040" ll="SDBH:כִּלְיָה:000000">riñones</a>, y la <a name="00300801600046" ll="SDBH:חֵלֶב:000000">grosura</a> de ellos, y lo hizo <a name="00300801600050" ll="SDBH:קטר:000000">arder</a> <a name="00300801600052" ll="SDBH:מֹשֶׁה:000000">Moisés</a> sobre el <a name="00300801600056" ll="SDBH:מִזְבֵּחַ:000000">altar</a>. 
\v 17 Mas el <a name="00300801700008" ll="SDBH:פַּר:000000">becerro</a>, <a name="00300801700014" ll="SDBH:עֹור:000000">su</a> <a name="00300801700014" ll="SDBH:עֹור:000000">piel</a>, <a name="00300801700020" ll="SDBH:בָּשָׂר:000000">su</a> <a name="00300801700020" ll="SDBH:בָּשָׂר:000000">carne</a> y su estiércol, lo <a name="00300801700028" ll="SDBH:שׂרף:000001">quemó</a> al <a name="00300801700034" ll="SDBH:אֵשׁ:000000">fuego</a> <a name="00300801700038" ll="SDBH:חוּץ:000000">fuera</a> del <a name="00300801700044" ll="SDBH:מַחֲנֶה:000000">campamento</a>, como <a name="00300801700052" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="00300801700050" ll="SDBH:צוה:000000">había</a> <a name="00300801700050" ll="SDBH:צוה:000000">mandado</a> a <a name="00300801700056" ll="SDBH:מֹשֶׁה:000000">Moisés</a>.
    
\v 18 Después hizo que <a name="00300801800004" ll="SDBH:קרב:000001">trajeran</a> el <a name="00300801800008" ll="SDBH:אַיִל:000000">carnero</a> del <a name="00300801800012" ll="SDBH:עֹלָה:000000">holocausto</a>, y <a name="00300801800018" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00300801800022" ll="SDBH:בֵּן:000000">sus</a> <a name="00300801800022" ll="SDBH:בֵּן:000000">hijos</a> <a name="00300801800016" ll="SDBH:סמך:000000">pusieron</a> <a name="00300801800026">sus</a> <a name="00300801800026">manos</a> <a name="00300801800028">sobre</a> la <a name="00300801800030" ll="SDBH:רֹאשׁ:000000">cabeza</a> del <a name="00300801800034" ll="SDBH:אַיִל:000000">carnero</a>; 
\v 19 y lo <a name="00300801900004" ll="SDBH:שׁחט:000000">degolló</a>; y <a name="00300801900008" ll="SDBH:זרק:000000">roció</a> <a name="00300801900010" ll="SDBH:מֹשֶׁה:000000">Moisés</a> la <a name="00300801900016" ll="SDBH:דָּם:000000">sangre</a> <a name="00300801900018">sobre</a> el <a name="00300801900022" ll="SDBH:מִזְבֵּחַ:000000">altar</a> <a name="00300801900024" ll="SDBH:סָבִיב:000000">alrededor</a>, 
\v 20 y <a name="00300802000014" ll="SDBH:נֵתַח:000000">cortó</a> el <a name="00300802000008" ll="SDBH:אַיִל:000000">carnero</a> en <a name="00300802000034" ll="SDBH:נֵתַח:000000">trozos</a>; y <a name="00300802000020" ll="SDBH:מֹשֶׁה:000000">Moisés</a> hizo <a name="00300802000018" ll="SDBH:קטר:000000">arder</a> la <a name="00300802000026" ll="SDBH:רֹאשׁ:000000">cabeza</a>, y los <a name="00300802000010" ll="SDBH:נתח:000000">trozos</a>, y la <a name="00300802000042" ll="SDBH:פֶּדֶר:000000">grosura</a>. 
\v 21 <a name="00300802100018" ll="SDBH:רחץ:000000">Lavó</a> luego con <a name="00300802100024" ll="SDBH:מַיִם:000000">agua</a> los <a name="00300802100008" ll="SDBH:קֶרֶב:000000">intestinos</a> y las <a name="00300802100016" ll="SDBH:כְּרָעַיִם:000000">piernas</a>, y <a name="00300802100028" ll="SDBH:קטר:000000">quemó</a> <a name="00300802100030" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300802100034">todo</a> el <a name="00300802100038" ll="SDBH:אַיִל:000000">carnero</a> sobre el <a name="00300802100042" ll="SDBH:מִזְבֵּחַ:000000">altar</a>; <a name="00300802100044" ll="SDBH:עֹלָה:000000">holocausto</a> de <a name="00300802100050" ll="SDBH:רֵיחַ:000000">olor</a> <a name="00300802100052" ll="SDBH:נִיחֹוחַ:000000">grato</a>, <a name="00300802100054" ll="SDBH:אִשֶּׁה:000000">ofrenda</a> <a name="00300802100054" ll="SDBH:אִשֶּׁה:000000">encendida</a> para <a name="00300802100060;00300802100068" ll="SDBH:יהוה:000000">Jehová</a>, como <a name="00300802100060;00300802100068" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="00300802100066" ll="SDBH:צוה:000000">había</a> <a name="00300802100066" ll="SDBH:צוה:000000">mandado</a> a <a name="00300802100072" ll="SDBH:מֹשֶׁה:000000">Moisés</a>.
    
\v 22 Después hizo que <a name="00300802200004" ll="SDBH:קרב:000001">trajeran</a> el <a name="00300802200014" ll="SDBH:שֵׁנִי:000000">otro</a> <a name="00300802200010" ll="SDBH:אַיִל:000000">carnero</a>, el <a name="00300802200016" ll="SDBH:אַיִל:000000">carnero</a> de las <a name="00300802200020" ll="SDBH:מִלֻּאִים:000001">consagraciones</a>, y <a name="00300802200026" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00300802200030" ll="SDBH:בֵּן:000000">sus</a> <a name="00300802200030" ll="SDBH:בֵּן:000000">hijos</a> <a name="00300802200024" ll="SDBH:סמך:000000">pusieron</a> <a name="00300802200034">sus</a> <a name="00300802200034">manos</a> <a name="00300802200036">sobre</a> la <a name="00300802200038" ll="SDBH:רֹאשׁ:000000">cabeza</a> del <a name="00300802200042" ll="SDBH:אַיִל:000000">carnero</a>. 
\v 23 Y lo <a name="00300802300004" ll="SDBH:שׁחט:000000">degolló</a>; y <a name="00300802300008" ll="SDBH:לקח:000000">tomó</a> <a name="00300802300010" ll="SDBH:מֹשֶׁה:000000">Moisés</a> de la <a name="00300802300014" ll="SDBH:דָּם:000000">sangre</a>, y la <a name="00300802300018">puso</a> <a name="00300802300020">sobre</a> el <a name="00300802300022" ll="SDBH:תְּנוּךְ:000000">lóbulo</a> de la <a name="00300802300024" ll="SDBH:אֹזֶן:000000">oreja</a> <a name="00300802300030" ll="SDBH:יְמָנִי:000000">derecha</a> de <a name="00300802300026" ll="SDBH:אַהֲרֹן:000000">Aarón</a>, <a name="00300802300034">sobre</a> el dedo <a name="00300802300036" ll="SDBH:בֹּהֶן:000000">pulgar</a> de <a name="00300802300038">su</a> <a name="00300802300038">mano</a> <a name="00300802300042" ll="SDBH:יְמָנִי:000000">derecha</a>, y <a name="00300802300046">sobre</a> el dedo <a name="00300802300048" ll="SDBH:בֹּהֶן:000000">pulgar</a> de <a name="00300802300050" ll="SDBH:רֶגֶל:000000">su</a> <a name="00300802300050" ll="SDBH:רֶגֶל:000000">pie</a> <a name="00300802300054" ll="SDBH:יְמָנִי:000000">derecho</a>. 
\v 24 Hizo <a name="00300802400004" ll="SDBH:קרב:000001">acercarse</a> luego los <a name="00300802400008" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300802400010" ll="SDBH:אַהֲרֹן:000000">Aarón</a>, y <a name="00300802400014">puso</a> <a name="00300802400016" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300802400018">de</a> la <a name="00300802400022" ll="SDBH:דָּם:000000">sangre</a> <a name="00300802400024">sobre</a> el <a name="00300802400026" ll="SDBH:תְּנוּךְ:000000">lóbulo</a> de <a name="00300802400028" ll="SDBH:אֹזֶן:000000">sus</a> <a name="00300802400028" ll="SDBH:אֹזֶן:000000">orejas</a> <a name="00300802400032" ll="SDBH:יְמָנִי:000000">derechas</a>, <a name="00300802400036">sobre</a> los <a name="00300802400038" ll="SDBH:בֹּהֶן:000000">pulgares</a> de <a name="00300802400040">sus</a> <a name="00300802400040">manos</a> <a name="00300802400044" ll="SDBH:יְמָנִי:000000">derechas</a>, y <a name="00300802400048">sobre</a> los <a name="00300802400050" ll="SDBH:בֹּהֶן:000000">pulgares</a> de <a name="00300802400052" ll="SDBH:רֶגֶל:000000">sus</a> <a name="00300802400052" ll="SDBH:רֶגֶל:000000">pies</a> <a name="00300802400056" ll="SDBH:יְמָנִי:000000">derechos</a>; y <a name="00300802400060" ll="SDBH:זרק:000000">roció</a> <a name="00300802400062" ll="SDBH:מֹשֶׁה:000000">Moisés</a> la <a name="00300802400068" ll="SDBH:דָּם:000000">sangre</a> <a name="00300802400070">sobre</a> el <a name="00300802400074" ll="SDBH:מִזְבֵּחַ:000000">altar</a> <a name="00300802400076" ll="SDBH:סָבִיב:000000">alrededor</a>.
    
\v 25 Después <a name="00300802500004" ll="SDBH:לקח:000000">tomó</a> la <a name="00300802500010" ll="SDBH:חֵלֶב:000000">grosura</a>, la <a name="00300802500018" ll="SDBH:אַלְיָה:000000">cola</a>, <a name="00300802500024">toda</a> la <a name="00300802500028" ll="SDBH:חֵלֶב:000000">grosura</a> <a name="00300802500030">que</a> estaba <a name="00300802500032">sobre</a> los <a name="00300802500036" ll="SDBH:קֶרֶב:000000">intestinos</a>, la <a name="00300802500042" ll="SDBH:יֹתֶרֶת:000000">grosura</a> del hígado, los <a name="00300802500052" ll="SDBH:שְׁנַיִם:000000">dos</a> <a name="00300802500056" ll="SDBH:כִּלְיָה:000000">riñones</a> y la <a name="00300802500062" ll="SDBH:חֵלֶב:000000">grosura</a> de ellos, y la <a name="00300802500068" ll="SDBH:שֹׁוק:000000">espaldilla</a> <a name="00300802500072" ll="SDBH:יָמִין:000000">derecha</a>. 
\v 26 Y del <a name="00300802600006" ll="SDBH:סַל:000000">canastillo</a> de los <a name="00300802600010" ll="SDBH:מַצָּה:000000">panes</a> <a name="00300802600010" ll="SDBH:מַצָּה:000000">sin</a> <a name="00300802600010" ll="SDBH:מַצָּה:000000">levadura</a>, <a name="00300802600012">que</a> estaba <a name="00300802600014;00300802600016">delante</a> de <a name="00300802600018" ll="SDBH:יהוה:000000">Jehová</a>, <a name="00300802600020" ll="SDBH:לקח:000000">tomó</a> <a name="00300802600026" ll="SDBH:אֶחָד:000000">una</a> <a name="00300802600022" ll="SDBH:חַלָּה:000000">torta</a> <a name="00300802600024" ll="SDBH:מַצָּה:000000">sin</a> <a name="00300802600024" ll="SDBH:מַצָּה:000000">levadura</a>, y <a name="00300802600036" ll="SDBH:אֶחָד:000000">una</a> <a name="00300802600030" ll="SDBH:חַלָּה:000000">torta</a> de <a name="00300802600032" ll="SDBH:לֶחֶם:000000">pan</a> de <a name="00300802600034" ll="SDBH:שֶׁמֶן:000000">aceite</a>, y <a name="00300802600042" ll="SDBH:אֶחָד:000000">una</a> <a name="00300802600040" ll="SDBH:רָקִיק:000000">hojaldre</a>, y las <a name="00300802600046" ll="SDBH:שׂים:000001">puso</a> <a name="00300802600048">con</a> la <a name="00300802600052" ll="SDBH:חֵלֶב:000000">grosura</a> y <a name="00300802600056">con</a> la <a name="00300802600058" ll="SDBH:שֹׁוק:000000">espaldilla</a> <a name="00300802600062" ll="SDBH:יָמִין:000000">derecha</a>. 
\v 27 Y lo <a name="00300802700004">puso</a> <a name="00300802700010">todo</a> <a name="00300802700012">en</a> las <a name="00300802700014" ll="SDBH:כַּף:000000">manos</a> de <a name="00300802700016" ll="SDBH:אַהֲרֹן:000000">Aarón</a>, y <a name="00300802700020">en</a> las <a name="00300802700022" ll="SDBH:כַּף:000000">manos</a> de <a name="00300802700024" ll="SDBH:בֵּן:000000">sus</a> <a name="00300802700024" ll="SDBH:בֵּן:000000">hijos</a>, e hizo mecerlo como ofrenda <a name="00300802700032" ll="SDBH:תְּנוּפָה:000001">mecida</a> <a name="00300802700036">delante</a> de <a name="00300802700038" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 28 Después <a name="00300802800004" ll="SDBH:לקח:000000">tomó</a> aquellas cosas <a name="00300802800006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> de las <a name="00300802800014" ll="SDBH:כַּף:000000">manos</a> de <a name="00300802800032">ellos</a>, y las <a name="00300802800018" ll="SDBH:קטר:000000">hizo</a> <a name="00300802800018" ll="SDBH:קטר:000000">arder</a> en el <a name="00300802800022" ll="SDBH:מִזְבֵּחַ:000000">altar</a> <a name="00300802800012">sobre</a> el <a name="00300802800028" ll="SDBH:עֹלָה:000000">holocausto</a>; eran las <a name="00300802800030" ll="SDBH:מִלֻּאִים:000001">consagraciones</a> en <a name="00300802800036" ll="SDBH:רֵיחַ:000000">olor</a> <a name="00300802800038" ll="SDBH:נִיחֹוחַ:000000">grato</a>, <a name="00300802800040" ll="SDBH:אִשֶּׁה:000000">ofrenda</a> <a name="00300802800040" ll="SDBH:אִשֶּׁה:000000">encendida</a> a <a name="00300802800046" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 29 Y <a name="00300802900004" ll="SDBH:לקח:000000">tomó</a> <a name="00300802900006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> el <a name="00300802900012" ll="SDBH:חָזֶה:000000">pecho</a>, y lo meció, ofrenda <a name="00300802900018" ll="SDBH:תְּנוּפָה:000001">mecida</a> <a name="00300802900022">delante</a> de <a name="00300802900024;00300802900050" ll="SDBH:יהוה:000000">Jehová</a>; del <a name="00300802900028" ll="SDBH:אַיִל:000000">carnero</a> de las <a name="00300802900032" ll="SDBH:מִלֻּאִים:000001">consagraciones</a>, aquélla <a name="00300802900038">fue</a> la <a name="00300802900042" ll="SDBH:מָנָה:000000">parte</a> de <a name="00300802900036" ll="SDBH:מֹשֶׁה:000000">Moisés</a>, como <a name="00300802900024;00300802900050" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="00300802900048" ll="SDBH:צוה:000000">había</a> <a name="00300802900048" ll="SDBH:צוה:000000">mandado</a> a <a name="00300802900054" ll="SDBH:מֹשֶׁה:000000">Moisés</a>.
    
\v 30 Luego <a name="00300803000004" ll="SDBH:לקח:000000">tomó</a> <a name="00300803000006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300803000008">del</a> <a name="00300803000010" ll="SDBH:שֶׁמֶן:000000">aceite</a> de la <a name="00300803000014" ll="SDBH:מִשְׁחָה:000000">unción</a>, y <a name="00300803000018">de</a> la <a name="00300803000022" ll="SDBH:דָּם:000000">sangre</a> <a name="00300803000024">que</a> estaba <a name="00300803000026">sobre</a> el <a name="00300803000030" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, y <a name="00300803000034" ll="SDBH:נזה:000001">roció</a> <a name="00300803000036">sobre</a> <a name="00300803000038" ll="SDBH:אַהֲרֹן:000000">Aarón</a>, y <a name="00300803000040">sobre</a> <a name="00300803000042" ll="SDBH:בֶּגֶד:000001" il="NBGmk20140519a;NBGmk20140519c;NBGmk20140519d" tl="HOTTP371">sus</a> <a name="00300803000042" ll="SDBH:בֶּגֶד:000001" il="NBGmk20140519a;NBGmk20140519c;NBGmk20140519d" tl="HOTTP371">vestiduras</a>, <a name="00300803000046">sobre</a> <a name="00300803000048" ll="SDBH:בֵּן:000000">sus</a> <a name="00300803000048" ll="SDBH:בֵּן:000000">hijos</a>, y <a name="00300803000052">sobre</a> las <a name="00300803000054" ll="SDBH:בֶּגֶד:000001">vestiduras</a> de <a name="00300803000056" ll="SDBH:בֵּן:000000">sus</a> <a name="00300803000056" ll="SDBH:בֵּן:000000">hijos</a> <a name="00300803000058">con</a> <a name="00300803000058">él</a>; y <a name="00300803000062" ll="SDBH:קדשׁ:000004">santificó</a> a <a name="00300803000066" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00300803000070" ll="SDBH:בֶּגֶד:000001" il="NBGmk20140519a;NBGmk20140519c;NBGmk20140519d" tl="HOTTP372">sus</a> <a name="00300803000070" ll="SDBH:בֶּגֶד:000001" il="NBGmk20140519a;NBGmk20140519c;NBGmk20140519d" tl="HOTTP372">vestiduras</a>, y a <a name="00300803000076" ll="SDBH:בֵּן:000000">sus</a> <a name="00300803000076" ll="SDBH:בֵּן:000000">hijos</a> y las <a name="00300803000082" ll="SDBH:בֶּגֶד:000001">vestiduras</a> de <a name="00300803000084" ll="SDBH:בֵּן:000000">sus</a> <a name="00300803000084" ll="SDBH:בֵּן:000000">hijos</a> <a name="00300803000086">con</a> <a name="00300803000086">él</a>.
    
\v 31 Y <a name="00300803100004" ll="SDBH:אמר:000000">dijo</a> <a name="00300803100006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300803100008">a</a> <a name="00300803100010" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00300803100014">a</a> <a name="00300803100016" ll="SDBH:בֵּן:000000">sus</a> <a name="00300803100016" ll="SDBH:בֵּן:000000">hijos</a>: <a name="00300803100018" ll="SDBH:בשׁל:000001">Hervid</a> la <a name="00300803100024" ll="SDBH:בָּשָׂר:000000">carne</a> a la <a name="00300803100026" ll="SDBH:פֶּתַח:000000">puerta</a> del <a name="00300803100028;00300803100030" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00300803100028;00300803100030" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00300803100028;00300803100030" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>; y <a name="00300803100036" ll="SDBH:אכל:000000">comedla</a> <a name="00300803100034" ll="SDBH:שָׁם:000000">allí</a> con el <a name="00300803100046" ll="SDBH:לֶחֶם:000000">pan</a> <a name="00300803100048">que</a> está en el <a name="00300803100052" ll="SDBH:סַל:000000">canastillo</a> de las <a name="00300803100056" ll="SDBH:מִלֻּאִים:000001">consagraciones</a>, según yo <a name="00300803100062" ll="SDBH:צוה:000000;SDBH:צוה:000001" tl="HOTTP373">he</a> <a name="00300803100062" ll="SDBH:צוה:000000;SDBH:צוה:000001" tl="HOTTP373">mandado</a>, <a name="00300803100066" ll="SDBH:אמר:000000">diciendo</a>: <a name="00300803100068" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00300803100072" ll="SDBH:בֵּן:000000">sus</a> <a name="00300803100072" ll="SDBH:בֵּן:000000">hijos</a> la <a name="00300803100074" ll="SDBH:אכל:000000">comerán</a>. 
\v 32 Y lo que <a name="00300803200006" ll="SDBH:יתר:000002">sobre</a> de la <a name="00300803200012" ll="SDBH:בָּשָׂר:000000">carne</a> y del <a name="00300803200020" ll="SDBH:לֶחֶם:000000">pan</a>, lo <a name="00300803200028" ll="SDBH:שׂרף:000001">quemaréis</a> al <a name="00300803200026" ll="SDBH:אֵשׁ:000000">fuego</a>. 
\v 33 De la <a name="00300803300006" ll="SDBH:פֶּתַח:000000">puerta</a> del <a name="00300803300008;00300803300010" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00300803300008;00300803300010" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00300803300008;00300803300010" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a> <a name="00300803300012" ll="SDBH:לֹא:000000">no</a> <a name="00300803300014" ll="SDBH:יצא:000000">saldréis</a> en <a name="00300803300016" ll="SDBH:שֶׁבַע:000000">siete</a> <a name="00300803300018" ll="SDBH:יֹום:000003">días</a>, <a name="00300803300020" ll="SDBH:עַד:000001">hasta</a> el <a name="00300803300022" ll="SDBH:יֹום:000003">día</a> que <a name="00300803300024" ll="SDBH:מלא:000005">se</a> <a name="00300803300024" ll="SDBH:מלא:000005">cumplan</a> los <a name="00300803300026" ll="SDBH:יֹום:000003">días</a> de <a name="00300803300028" ll="SDBH:מִלֻּאִים:000001">vuestras</a> <a name="00300803300028" ll="SDBH:מִלֻּאִים:000001">consagraciones</a>; <a name="00300803300030">porque</a> por <a name="00300803300032" ll="SDBH:שֶׁבַע:000000">siete</a> <a name="00300803300034" ll="SDBH:יֹום:000001">días</a> <a name="00300803300036;00300803300038;00300803300040" ll="SDBH:מלא:000018">seréis</a> <a name="00300803300036;00300803300038;00300803300040" ll="SDBH:מלא:000018">consagrados</a>. 
\v 34 De la manera <a name="00300803400004">que</a> <a name="00300803400012" ll="SDBH:יֹום:000001">hoy</a> <a name="00300803400006">se</a> <a name="00300803400006">ha</a> <a name="00300803400006">hecho</a>, <a name="00300803400018" ll="SDBH:צוה:000000">mandó</a> <a name="00300803400024">hacer</a> <a name="00300803400020" ll="SDBH:יהוה:000000">Jehová</a> para <a name="00300803400028;00300803400030" ll="SDBH:כפר:000008">expiaros</a>. 
\v 35 A la <a name="00300803500004" ll="SDBH:פֶּתַח:000000">puerta</a>, pues, del <a name="00300803500006;00300803500008" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00300803500006;00300803500008" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00300803500006;00300803500008" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a> <a name="00300803500010">estaréis</a> <a name="00300803500012" ll="SDBH:יֹומָם:000000">día</a> y <a name="00300803500016" ll="SDBH:לַיְלָה:000000">noche</a> por <a name="00300803500018" ll="SDBH:שֶׁבַע:000000">siete</a> <a name="00300803500020" ll="SDBH:יֹום:000001">días</a>, y <a name="00300803500024" ll="SDBH:שׁמר:000003">guardaréis</a> la <a name="00300803500028" ll="SDBH:מִשְׁמֶרֶת:000004">ordenanza</a> delante de <a name="00300803500030" ll="SDBH:יהוה:000000">Jehová</a>, para que <a name="00300803500034" ll="SDBH:לֹא:000000">no</a> <a name="00300803500036" ll="SDBH:מות:000000">muráis</a>; <a name="00300803500038">porque</a> <a name="00300803500040">así</a> <a name="00300803500042" ll="SDBH:צוה:000005">me</a> <a name="00300803500042" ll="SDBH:צוה:000005">ha</a> <a name="00300803500042" ll="SDBH:צוה:000005">sido</a> <a name="00300803500042" ll="SDBH:צוה:000005">mandado</a>. 
\v 36 Y <a name="00300803600006" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y <a name="00300803600010" ll="SDBH:בֵּן:000000">sus</a> <a name="00300803600010" ll="SDBH:בֵּן:000000">hijos</a> <a name="00300803600004">hicieron</a> <a name="00300803600014">todas</a> las <a name="00300803600018" ll="SDBH:דָּבָר:000000">cosas</a> <a name="00300803600020">que</a> <a name="00300803600022" ll="SDBH:צוה:000000">mandó</a> <a name="00300803600024" ll="SDBH:יהוה:000000">Jehová</a> <a name="00300803600026;00300803600028">por</a> <a name="00300803600026;00300803600028">medio</a> de <a name="00300803600030" ll="SDBH:מֹשֶׁה:000000">Moisés</a>.
\c 9
\v 1 En el <a name="00300900100010" ll="SDBH:יֹום:000001">día</a> <a name="00300900100014" ll="SDBH:שְׁמִינִי:000000">octavo</a>, <a name="00300900100018" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300900100016" ll="SDBH:קרא:000003">llamó</a> a <a name="00300900100022" ll="SDBH:אַהֲרֹן:000000">Aarón</a> y a <a name="00300900100028" ll="SDBH:בֵּן:000000">sus</a> <a name="00300900100028" ll="SDBH:בֵּן:000000">hijos</a>, y a los <a name="00300900100034" ll="SDBH:זָקֵן:000003">ancianos</a> de <a name="00300900100036" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>; 
\v 2 y <a name="00300900200004" ll="SDBH:אמר:000000">dijo</a> <a name="00300900200006">a</a> <a name="00300900200008" ll="SDBH:אַהֲרֹן:000000">Aarón</a>: <a name="00300900200010" ll="SDBH:לקח:000000">Toma</a> <a name="00300900200014;00300900200016;00300900200018" ll="SDBH:בֵּן:000001;SDBH:בָּקָר:000000;SDBH:עֵגֶל:000000">de</a> <a name="00300900200014;00300900200016;00300900200018" ll="SDBH:בֵּן:000001;SDBH:בָּקָר:000000;SDBH:עֵגֶל:000000">la</a> <a name="00300900200014;00300900200016;00300900200018" ll="SDBH:בֵּן:000001;SDBH:בָּקָר:000000;SDBH:עֵגֶל:000000">vacada</a> <a name="00300900200014;00300900200016;00300900200018" ll="SDBH:בֵּן:000001;SDBH:בָּקָר:000000;SDBH:עֵגֶל:000000">un</a> <a name="00300900200014;00300900200016;00300900200018" ll="SDBH:בֵּן:000001;SDBH:בָּקָר:000000;SDBH:עֵגֶל:000000">becerro</a> para <a name="00300900200022" ll="SDBH:חַטָּאת:000004">expiación</a>, y un <a name="00300900200026" ll="SDBH:אַיִל:000000">carnero</a> para <a name="00300900200030" ll="SDBH:עֹלָה:000000">holocausto</a>, <a name="00300900200032" ll="SDBH:תָּמִים:000003">sin</a> <a name="00300900200032" ll="SDBH:תָּמִים:000003">defecto</a>, y <a name="00300900200036" ll="SDBH:קרב:000001">ofrécelos</a> <a name="00300900200040">delante</a> de <a name="00300900200042" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 3 Y <a name="00300900300004">a</a> los <a name="00300900300006" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300900300008" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00300900300010" ll="SDBH:דבר:001000">hablarás</a> <a name="00300900300014" ll="SDBH:אמר:000000">diciendo</a>: <a name="00300900300016" ll="SDBH:לקח:000000">Tomad</a> un <a name="00300900300018;00300900300020" ll="SDBH:עֵז:000000;SDBH:שָׂעִיר:000000">macho</a> <a name="00300900300018;00300900300020" ll="SDBH:עֵז:000000;SDBH:שָׂעִיר:000000">cabrío</a> para <a name="00300900300024" ll="SDBH:חַטָּאת:000004">expiación</a>, y un <a name="00300900300028" ll="SDBH:עֵגֶל:000000">becerro</a> y un <a name="00300900300032" ll="SDBH:כֶּבֶשׂ:000000">cordero</a> de un <a name="00300900300036" ll="SDBH:שָׁנָה:000000">año</a>, <a name="00300900300038" ll="SDBH:תָּמִים:000003">sin</a> <a name="00300900300038" ll="SDBH:תָּמִים:000003">defecto</a>, para <a name="00300900300042" ll="SDBH:עֹלָה:000000">holocausto</a>. 
\v 4 Asimismo un <a name="00300900400004" ll="SDBH:שֹׁור:000000">buey</a> y un <a name="00300900400008" ll="SDBH:אַיִל:000000">carnero</a> para <a name="00300900400012" ll="SDBH:שֶׁלֶם:000000">sacrificio</a> <a name="00300900400012" ll="SDBH:שֶׁלֶם:000000">de</a> <a name="00300900400012" ll="SDBH:שֶׁלֶם:000000">paz</a>, que <a name="00300900400016" ll="SDBH:זבח:000001">inmoléis</a> <a name="00300900400020">delante</a> de <a name="00300900400022;00300900400042" ll="SDBH:יהוה:000000">Jehová</a>, y una <a name="00300900400026" ll="SDBH:מִנְחָה:000000">ofrenda</a> <a name="00300900400028" ll="SDBH:בלל:000000">amasada</a> con <a name="00300900400034" ll="SDBH:שֶׁמֶן:000000">aceite</a>; porque <a name="00300900400022;00300900400042" ll="SDBH:יהוה:000000">Jehová</a> <a name="00300900400044" ll="SDBH:ראה:000001">se</a> <a name="00300900400044" ll="SDBH:ראה:000001">aparecerá</a> <a name="00300900400040" ll="SDBH:יֹום:000005">hoy</a> <a name="00300900400046">a</a> <a name="00300900400046">vosotros</a>. 
\v 5 Y <a name="00300900500004" ll="SDBH:לקח:000000">llevaron</a> lo <a name="00300900500008">que</a> <a name="00300900500010" ll="SDBH:צוה:000001">mandó</a> <a name="00300900500012" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300900500016" ll="SDBH:פָּנֶה:000000">delante</a> del <a name="00300900500018;00300900500020" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00300900500018;00300900500020" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00300900500018;00300900500020" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>, y <a name="00300900500024" ll="SDBH:קרב:000000">vino</a> <a name="00300900500026">toda</a> la <a name="00300900500030" ll="SDBH:עֵדָה:000000">congregación</a> y <a name="00300900500034">se</a> <a name="00300900500034">puso</a> <a name="00300900500036;00300900500038">delante</a> de <a name="00300900500040" ll="SDBH:יהוה:000000">Jehová</a>. 
\v 6 Entonces <a name="00300900600006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300900600004" ll="SDBH:אמר:000000">dijo</a>: <a name="00300900600012" ll="SDBH:דָּבָר:000000">Esto</a> es lo <a name="00300900600014">que</a> <a name="00300900600016" ll="SDBH:צוה:000000">mandó</a> <a name="00300900600018;00300900600030" ll="SDBH:יהוה:000000">Jehová</a>; <a name="00300900600020">hacedlo</a>, y la <a name="00300900600028" ll="SDBH:כָּבֹוד:000004">gloria</a> de <a name="00300900600018;00300900600030" ll="SDBH:יהוה:000000">Jehová</a> se <a name="00300900600026">os</a> <a name="00300900600024" ll="SDBH:ראה:000001">aparecerá</a>. 
\v 7 Y <a name="00300900700004" ll="SDBH:אמר:000000">dijo</a> <a name="00300900700006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00300900700008">a</a> <a name="00300900700010" ll="SDBH:אַהֲרֹן:000000">Aarón</a>: <a name="00300900700012" ll="SDBH:קרב:000000">Acércate</a> <a name="00300900700014">al</a> <a name="00300900700018" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, y <a name="00300900700022">haz</a> <a name="00300900700026" ll="SDBH:חַטָּאת:000004">tu</a> <a name="00300900700026" ll="SDBH:חַטָּאת:000004">expiación</a> y <a name="00300900700032" ll="SDBH:עֹלָה:000000">tu</a> <a name="00300900700032" ll="SDBH:עֹלָה:000000">holocausto</a>, y <a name="00300900700036" ll="SDBH:כפר:000008">haz</a> <a name="00300900700036" ll="SDBH:כפר:000008">la</a> <a name="00300900700036" ll="SDBH:כפר:000008">reconciliación</a> <a name="00300900700038" ll="SDBH:בַּעַד:000002">por</a> <a name="00300900700038" ll="SDBH:בַּעַד:000002">ti</a> y <a name="00300900700042" ll="SDBH:בַּעַד:000002" tl="HOTTP374">por</a> el <a name="00300900700046" ll="SDBH:עַם:000000" tl="HOTTP374">pueblo</a>; <a name="00300900700050">haz</a> también la <a name="00300900700054" ll="SDBH:קָרְבָּן:000000">ofrenda</a> del <a name="00300900700058">pueblo</a>, y <a name="00300900700062" ll="SDBH:כפר:000008">haz</a> <a name="00300900700062" ll="SDBH:כפר:000008">la</a> <a name="00300900700062" ll="SDBH:כפר:000008">reconciliación</a> <a name="00300900700064" ll="SDBH:בַּעַד:000002">por</a> <a name="00300900700064" ll="SDBH:בַּעַד:000002">ellos</a>, <a name="00300900700066;00300900700068">como</a> <a name="00300900700070" ll="SDBH:צוה:000000">ha</a> <a name="00300900700070" ll="SDBH:צוה:000000">mandado</a> <a name="00300900700072" ll="SDBH:יהוה:000000">Jehová</a>.
    
\v 8 Entonces <a name="00300900800004" ll="SDBH:קרב:000000">se</a> <a name="00300900800004" ll="SDBH:קרב:000000">acercó</a> <a name="00300900800006" ll="SDBH:אַהֲרֹן:000000">Aarón</a> <a name="00300900800008">al</a> <a name="00300900800012" ll="SDBH:מִזְבֵּחַ:000000">altar</a> y <a name="00300900800016" ll="SDBH:שׁחט:000000">degolló</a> el <a name="00300900800020" ll="SDBH:עֵגֶל:000000">becerro</a> de la <a name="00300900800024" ll="SDBH:חַטָּאת:000004">expiación</a> <a name="00300900800026">que</a> era por él. 
\v 9 Y los <a name="00300900900006" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300900900008" ll="SDBH:אַהֲרֹן:000000">Aarón</a> <a name="00300900900016">le</a> <a name="00300900900004" ll="SDBH:קרב:000001">trajeron</a> la <a name="00300900900014" ll="SDBH:דָּם:000000">sangre</a>; y él <a name="00300900900020" ll="SDBH:טבל:000000">mojó</a> <a name="00300900900022" ll="SDBH:אֶצְבַּע:000000">su</a> <a name="00300900900022" ll="SDBH:אֶצְבַּע:000000">dedo</a> en la <a name="00300900900028" ll="SDBH:דָּם:000000">sangre</a>, y <a name="00300900900032">puso</a> de ella <a name="00300900900034">sobre</a> los <a name="00300900900036" ll="SDBH:קֶרֶן:000003">cuernos</a> del <a name="00300900900040" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, y <a name="00300900900050" ll="SDBH:יצק:000002">derramó</a> el resto de la <a name="00300900900048" ll="SDBH:דָּם:000000">sangre</a> <a name="00300900900052">al</a> <a name="00300900900054" ll="SDBH:יְסֹוד:000000">pie</a> del <a name="00300900900058" ll="SDBH:מִזְבֵּחַ:000000">altar</a>. 
\v 10 E hizo <a name="00300901000038" ll="SDBH:קטר:000000">arder</a> sobre el <a name="00300901000042" ll="SDBH:מִזְבֵּחַ:000000">altar</a> la <a name="00300901000008" ll="SDBH:חֵלֶב:000000">grosura</a> con los <a name="00300901000016" ll="SDBH:כִּלְיָה:000000">riñones</a> y la <a name="00300901000024" ll="SDBH:יֹתֶרֶת:000000">grosura</a> <a name="00300901000026">del</a> hígado <a name="00300901000032">de</a> la <a name="00300901000036" ll="SDBH:חַטָּאת:000004">expiación</a>, como <a name="00300901000050" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="00300901000048" ll="SDBH:צוה:000000">había</a> <a name="00300901000048" ll="SDBH:צוה:000000">mandado</a> a <a name="00300901000054" ll="SDBH:מֹשֶׁה:000000">Moisés</a>. 
\v 11 Mas la <a name="00300901100008" ll="SDBH:בָּשָׂר:000000">carne</a> y la <a name="00300901100016" ll="SDBH:עֹור:000000">piel</a> las <a name="00300901100018" ll="SDBH:שׂרף:000001">quemó</a> al <a name="00300901100024" ll="SDBH:אֵשׁ:000000">fuego</a> <a name="00300901100028" ll="SDBH:חוּץ:000000">fuera</a> del <a name="00300901100034" ll="SDBH:מַחֲנֶה:000000">campamento</a>.
    
\v 12 <a name="00300901200004" ll="SDBH:שׁחט:000000">Degolló</a> asimismo el <a name="00300901200010" ll="SDBH:עֹלָה:000000">holocausto</a>, y los <a name="00300901200016" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300901200018" ll="SDBH:אַהֲרֹן:000000">Aarón</a> <a name="00300901200020">le</a> <a name="00300901200014" ll="SDBH:מצא:000014">presentaron</a> la <a name="00300901200026" ll="SDBH:דָּם:000000">sangre</a>, la cual <a name="00300901200030" ll="SDBH:זרק:000000">roció</a> él <a name="00300901200038" ll="SDBH:סָבִיב:000000">alrededor</a> <a name="00300901200032">sobre</a> el <a name="00300901200036" ll="SDBH:מִזְבֵּחַ:000000">altar</a>. 
\v 13 Después <a name="00300901300012">le</a> <a name="00300901300010" ll="SDBH:מצא:000014">presentaron</a> el <a name="00300901300008" ll="SDBH:עֹלָה:000000">holocausto</a> pieza por pieza, y la <a name="00300901300024" ll="SDBH:רֹאשׁ:000000">cabeza</a>; y lo hizo <a name="00300901300028" ll="SDBH:קטר:000000">quemar</a> <a name="00300901300030">sobre</a> el <a name="00300901300034" ll="SDBH:מִזְבֵּחַ:000000">altar</a>. 
\v 14 Luego <a name="00300901400004" ll="SDBH:רחץ:000000">lavó</a> los <a name="00300901400010" ll="SDBH:קֶרֶב:000000">intestinos</a> y las <a name="00300901400018" ll="SDBH:כְּרָעַיִם:000000">piernas</a>, y los <a name="00300901400022" ll="SDBH:קטר:000000">quemó</a> <a name="00300901400024">sobre</a> el <a name="00300901400028" ll="SDBH:עֹלָה:000000">holocausto</a> en el <a name="00300901400032" ll="SDBH:מִזְבֵּחַ:000000">altar</a>.
    
\v 15 <a name="00300901500004" ll="SDBH:קרב:000001">Ofreció</a> también la <a name="00300901500008" ll="SDBH:קָרְבָּן:000000">ofrenda</a> del <a name="00300901500012" ll="SDBH:עַם:000000">pueblo</a>, y <a name="00300901500016" ll="SDBH:לקח:000000">tomó</a> el macho <a name="00300901500020" ll="SDBH:שָׂעִיר:000000">cabrío</a> <a name="00300901500026">que</a> era para la <a name="00300901500024" ll="SDBH:חַטָּאת:000004">expiación</a> del <a name="00300901500032">pueblo</a>, y lo <a name="00300901500036" ll="SDBH:שׁחט:000000">degolló</a>, y lo ofreció por el <a name="00300901500040" ll="SDBH:חטא:001009">pecado</a> como el <a name="00300901500046" ll="SDBH:רִאשֹׁון:000000">primero</a>. 
\v 16 Y <a name="00300901600004" ll="SDBH:קרב:000001">ofreció</a> el <a name="00300901600010" ll="SDBH:עֹלָה:000000">holocausto</a>, e <a name="00300901600014">hizo</a> según el <a name="00300901600020" ll="SDBH:מִשְׁפָּט:000006">rito</a>. 
\v 17 <a name="00300901700004" ll="SDBH:קרב:000001">Ofreció</a> asimismo la <a name="00300901700010" ll="SDBH:מִנְחָה:000000">ofrenda</a>, y <a name="00300901700014" ll="SDBH:מלא:000002">llenó</a> <a name="00300901700018">de</a> <a name="00300901700018">ella</a> <a name="00300901700016" ll="SDBH:כַּף:000000">su</a> <a name="00300901700016" ll="SDBH:כַּף:000000">mano</a>, y la hizo <a name="00300901700022" ll="SDBH:קטר:000000">quemar</a> <a name="00300901700024">sobre</a> el <a name="00300901700028" ll="SDBH:מִזְבֵּחַ:000000">altar</a>, <a name="00300901700030">además</a> del <a name="00300901700036" ll="SDBH:עֹלָה:000000">holocausto</a> de la <a name="00300901700040" ll="SDBH:בֹּקֶר:000000">mañana</a>.
    
\v 18 <a name="00300901800004" ll="SDBH:שׁחט:000000">Degolló</a> también el <a name="00300901800010" ll="SDBH:שֹׁור:000000">buey</a> y el <a name="00300901800018" ll="SDBH:אַיִל:000000">carnero</a> en <a name="00300901800020;00300901800024" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">sacrificio</a> <a name="00300901800020;00300901800024" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">de</a> <a name="00300901800020;00300901800024" ll="SDBH:זֶבַח:000000;SDBH:שֶׁלֶם:000000">paz</a>, <a name="00300901800026">que</a> era del <a name="00300901800032" ll="SDBH:עַם:000000">pueblo</a>; y los <a name="00300901800038" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00300901800040" ll="SDBH:אַהֲרֹן:000000">Aarón</a> <a name="00300901800048">le</a> <a name="00300901800036" ll="SDBH:מצא:000014">presentaron</a> la <a name="00300901800046" ll="SDBH:דָּם:000000">sangre</a>, la cual <a name="00300901800052" ll="SDBH:זרק:000000">roció</a> él <a name="00300901800054">sobre</a> el <a name="00300901800058" ll="SDBH:מִזְבֵּחַ:000000">altar</a> <a name="00300901800060" ll="SDBH:סָבִיב:000000">alrededor</a>; 
\v 19 y las <a name="00300901900008" ll="SDBH:חֵלֶב:000000">grosuras</a> <a name="00300901900010">del</a> <a name="00300901900014" ll="SDBH:שֹׁור:000000">buey</a> y <a name="00300901900018">del</a> <a name="00300901900022" ll="SDBH:אַיִל:000000">carnero</a>, la <a name="00300901900026" ll="SDBH:אַלְיָה:000000">cola</a>, la grosura que cubre los intestinos, los <a name="00300901900038" ll="SDBH:כִּלְיָה:000000" tl="HOTTP375">riñones</a>, y la <a name="00300901900042" ll="SDBH:יֹתֶרֶת:000000">grosura</a> del hígado; 
\v 20 y <a name="00300902000004" ll="SDBH:שׂים:000001" tl="HOTTP376">pusieron</a> las <a name="00300902000010" ll="SDBH:חֵלֶב:000000">grosuras</a> <a name="00300902000012">sobre</a> los <a name="00300902000016" ll="SDBH:חָזֶה:000000">pechos</a>, y él las <a name="00300902000020" ll="SDBH:קטר:000000">quemó</a> sobre el <a name="00300902000028" ll="SDBH:מִזְבֵּחַ:000000">altar</a>. 
\v 21 Pero los <a name="00300902100008" ll="SDBH:חָזֶה:000000">pechos</a>, con la <a name="00300902100014" ll="SDBH:שֹׁוק:000000">espaldilla</a> <a name="00300902100018" ll="SDBH:יָמִין:000000">derecha</a>, los meció <a name="00300902100022" ll="SDBH:אַהֲרֹן:000000">Aarón</a> como ofrenda <a name="00300902100024" ll="SDBH:תְּנוּפָה:000001">mecida</a> <a name="00300902100028">delante</a> de <a name="00300902100030" ll="SDBH:יהוה:000000">Jehová</a>, como <a name="00300902100030" ll="SDBH:יהוה:000000">Jehová</a> lo <a name="00300902100036" ll="SDBH:צוה:000001" tl="HOTTP377">había</a> <a name="00300902100036" ll="SDBH:צוה:000001" tl="HOTTP377">mandado</a> a <a name="00300902100038" ll="SDBH:מֹשֶׁה:000000" il="TEN COMMANDMENTS" tl="HOTTP377">Moisés</a>.
    
\v 22 Después <a name="00300902200004" ll="SDBH:נשׂא:000043">alzó</a> <a name="00300902200006" ll="SDBH:אַהֲרֹן:000000">Aarón</a> <a name="00300902200012">sus</a> <a name="00300902200012">manos</a> <a name="00300902200014">hacia</a> el <a name="00300902200018" ll="SDBH:עַם:000000">pueblo</a> y <a name="00300902200022" ll="SDBH:ברך:001001">lo</a> <a name="00300902200022" ll="SDBH:ברך:001001">bendijo</a>; y después de <a name="00300902200030">hacer</a> la <a name="00300902200034" ll="SDBH:חַטָּאת:000004">expiación</a>, el <a name="00300902200040" ll="SDBH:עֹלָה:000000">holocausto</a> y el <a name="00300902200046" ll="SDBH:שֶׁלֶם:000000">sacrificio</a> <a name="00300902200046" ll="SDBH:שֶׁלֶם:000000">de</a> <a name="00300902200046" ll="SDBH:שֶׁלֶם:000000">paz</a>, <a name="00300902200026" ll="SDBH:ירד:000001">descendió</a>. 
\v 23 Y <a name="00300902300004">entraron</a> <a name="00300902300006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> y <a name="00300902300010" ll="SDBH:אַהֲרֹן:000000">Aarón</a> <a name="00300902300012">en</a> el <a name="00300902300014;00300902300016" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">tabernáculo</a> <a name="00300902300014;00300902300016" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">de</a> <a name="00300902300014;00300902300016" ll="SDBH:אֹהֶל:000001;SDBH:מֹועֵד:000005">reunión</a>, y <a name="00300902300020" ll="SDBH:יצא:000000">salieron</a> y <a name="00300902300024" ll="SDBH:ברך:001001">bendijeron</a> al <a name="00300902300030" ll="SDBH:עַם:000000">pueblo</a>; y la <a name="00300902300036" ll="SDBH:כָּבֹוד:000004">gloria</a> de <a name="00300902300038" ll="SDBH:יהוה:000000">Jehová</a> <a name="00300902300034" ll="SDBH:ראה:000001">se</a> <a name="00300902300034" ll="SDBH:ראה:000001">apareció</a> <a name="00300902300040">a</a> <a name="00300902300042">todo</a> el <a name="00300902300046">pueblo</a>. 
