\v 3 <a name="00302500300002" ll="SDBH:שֵׁשׁ:000000">Seis</a> <a name="00302500300004" ll="SDBH:שָׁנָה:000000">años</a> <a name="00302500300006" ll="SDBH:זרע:000000">sembrarás</a> <a name="00302500300008" ll="SDBH:שָׂדֶה:000001">tu</a> <a name="00302500300008" ll="SDBH:שָׂדֶה:000001">tierra</a>, y <a name="00302500300012" ll="SDBH:שֵׁשׁ:000000">seis</a> <a name="00302500300014" ll="SDBH:שָׁנָה:000000">años</a> <a name="00302500300016" ll="SDBH:זמר:001000">podarás</a> <a name="00302500300018" ll="SDBH:כֶּרֶם:000000">tu</a> <a name="00302500300018" ll="SDBH:כֶּרֶם:000000">viña</a> y <a name="00302500300022" ll="SDBH:אסף:000001">recogerás</a> <a name="00302500300026" ll="SDBH:תְּבוּאָה:000000">sus</a> <a name="00302500300026" ll="SDBH:תְּבוּאָה:000000">frutos</a>. 
\v 4 Pero el <a name="00302500400012" ll="SDBH:שְׁבִיעִי:000000">séptimo</a> <a name="00302500400008" ll="SDBH:שָׁנָה:000000">año</a> la <a name="00302500400024" ll="SDBH:אֶרֶץ:000000">tierra</a> <a name="00302500400014;00302500400016;00302500400018" ll="SDBH:שַׁבָּת:000000;SDBH:שַׁבָּתֹון:000000">tendrá</a> <a name="00302500400014;00302500400016;00302500400018" ll="SDBH:שַׁבָּת:000000;SDBH:שַׁבָּתֹון:000000">descanso</a>, <a name="00302500400026" ll="SDBH:שַׁבָּת:000000">reposo</a> para <a name="00302500400030" ll="SDBH:יהוה:000000">Jehová</a>; <a name="00302500400034" ll="SDBH:לֹא:000000">no</a> <a name="00302500400036" ll="SDBH:זרע:000000">sembrarás</a> <a name="00302500400032" ll="SDBH:שָׂדֶה:000001">tu</a> <a name="00302500400032" ll="SDBH:שָׂדֶה:000001">tierra</a>, <a name="00302500400042" ll="SDBH:לֹא:000000">ni</a> <a name="00302500400044" ll="SDBH:זמר:001000">podarás</a> <a name="00302500400040" ll="SDBH:כֶּרֶם:000000">tu</a> <a name="00302500400040" ll="SDBH:כֶּרֶם:000000">viña</a>. 
