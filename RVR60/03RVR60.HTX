\v 19 Y <a name="00302401900004" ll="SDBH:אִישׁ:000000">el</a> que <a name="00302401900008">causare</a> <a name="00302401900010" ll="SDBH:מוּם:000000">lesión</a> en <a name="00302401900014" ll="SDBH:עָמִית:000000">su</a> <a name="00302401900014" ll="SDBH:עָמִית:000000">prójimo</a>, según <a name="00302401900020">hizo</a>, <a name="00302401900022">así</a> le sea <a name="00302401900024">hecho</a>: 
\v 20 <a name="00302402000002" ll="SDBH:שֶׁבֶר:000000">rotura</a> <a name="00302402000004">por</a> <a name="00302402000006" ll="SDBH:שֶׁבֶר:000000">rotura</a>, <a name="00302402000008" ll="SDBH:עַיִן:000000">ojo</a> <a name="00302402000010">por</a> <a name="00302402000012" ll="SDBH:עַיִן:000000">ojo</a>, <a name="00302402000014" ll="SDBH:שֵׁן:000000">diente</a> <a name="00302402000016">por</a> <a name="00302402000018" ll="SDBH:שֵׁן:000000">diente</a>; según la <a name="00302402000026" ll="SDBH:מוּם:000000">lesión</a> que haya <a name="00302402000024">hecho</a> a otro, <a name="00302402000034">tal</a> <a name="00302402000036">se</a> <a name="00302402000036">hará</a> a él. 
\v 21 El que <a name="00302402100004" ll="SDBH:נכה:000004">hiere</a> algún <a name="00302402100006" ll="SDBH:בְּהֵמָה:000001">animal</a> ha de <a name="00302402100008" ll="SDBH:שׁלם:000006">restituirlo</a>; mas el que <a name="00302402100012" ll="SDBH:נכה:000004">hiere</a> de muerte a un <a name="00302402100014" ll="SDBH:אָדָם:000000">hombre</a>, que <a name="00302402100016" ll="SDBH:מות:000001">muera</a>. 
\v 22 <a name="00302402200004" ll="SDBH:אֶחָד:000000">Un</a> mismo <a name="00302402200002" ll="SDBH:מִשְׁפָּט:000006">estatuto</a> <a name="00302402200006;00302402200008">tendréis</a> para el <a name="00302402200014" ll="SDBH:גֵּר:000000">extranjero</a>, como para el <a name="00302402200020" ll="SDBH:אֶזְרָח:000000">natural</a>; <a name="00302402200024">porque</a> <a name="00302402200026">yo</a> soy <a name="00302402200028" ll="SDBH:יהוה:000000">Jehová</a> <a name="00302402200030" ll="SDBH:אֱלֹהִים:000001">vuestro</a> <a name="00302402200030" ll="SDBH:אֱלֹהִים:000001">Dios</a>. 
\v 23 Y <a name="00302402300004" ll="SDBH:דבר:001000">habló</a> <a name="00302402300006" ll="SDBH:מֹשֶׁה:000000">Moisés</a> <a name="00302402300008">a</a> los <a name="00302402300010" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00302402300012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>, y <a name="00302402300016" ll="SDBH:יצא:000001">ellos</a> <a name="00302402300016" ll="SDBH:יצא:000001">sacaron</a> del <a name="00302402300034" ll="SDBH:מַחֲנֶה:000000">campamento</a> al <a name="00302402300022" ll="SDBH:קלל:000005">blasfemo</a> y lo <a name="00302402300038;00302402300042" ll="SDBH:אֶבֶן:000001;SDBH:רגם:000000">apedrearon</a>. Y los <a name="00302402300046" ll="SDBH:בֵּן:000000">hijos</a> de <a name="00302402300048" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> <a name="00302402300050">hicieron</a> según <a name="00302402300058" ll="SDBH:יהוה:000000">Jehová</a> <a name="00302402300056" ll="SDBH:צוה:000000">había</a> <a name="00302402300056" ll="SDBH:צוה:000000">mandado</a> a <a name="00302402300062" ll="SDBH:מֹשֶׁה:000000">Moisés</a>.
\c 25
\v 1 <a name="00302500100006" ll="SDBH:יהוה:000000">Jehová</a> <a name="00302500100004" ll="SDBH:דבר:001000">habló</a> <a name="00302500100008">a</a> <a name="00302500100010" ll="SDBH:מֹשֶׁה:000000">Moisés</a> en el <a name="00302500100014" ll="SDBH:הַר:000000">monte</a> de <a name="00302500100016" ll="SDBH:סִינַי:000000">Sinaí</a>, <a name="00302500100020" ll="SDBH:אמר:000000">diciendo</a>: 
