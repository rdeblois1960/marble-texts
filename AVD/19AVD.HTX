\v 18 (17) <a name="01900901800004" ll="SDBH:רָשָׁע:000000:Guilty">اَلْأَشْرَارُ</a> <a name="01900901800002" ll="SDBH:שׁוב:000023:Move">يَرْجِعُونَ</a> إِلَى <a name="01900901800008" ll="SDBH:שְׁאֹול:000000:Universe">ٱلْهَاوِيَةِ</a>، كُلُّ <a name="01900901800012" ll="SDBH:גֹּוי:000000:Groups">ٱلْأُمَمِ</a> ٱلنَّاسِينَ <a name="01900901800016" ll="SDBH:אֱלֹהִים:000002:Deities">ٱللّٰهَ</a>.
\v 19 (18) لأَنَّهُ <a name="01900901900004" ll="SDBH:לֹא:000000:Negators">لاَ</a> <a name="01900901900010" ll="SDBH:שׁכח:000000:Non-Know">يُنْسَى</a> <a name="01900901900012" ll="SDBH:אֶבְיֹון:000000:Poor">ٱلْمِسْكِينُ</a> إِلَى ٱلْأَبَدِ. <a name="01900901900014" ll="SDBH:תִּקְוָה:001000:Confident">رَجَاءُ</a> <a name="01900901900016" ll="SDBH:עָנָו:000000:Insignificant;SDBH:עָנִי:000000:Classes|Unsafe|Poor">ٱلْبَائِسِينَ</a> لاَ <a name="01900901900020" ll="SDBH:אבד:000006:Non-Happen">يَخِيبُ</a> إِلَى <a name="01900901900024" ll="SDBH:עַד:001000:Time">ٱلدَّهْرِ</a>.
\v 20 (19) <a name="01900902000002">قُمْ</a> يَا <a name="01900902000004" ll="SDBH:יהוה:000000:Names of Deities">رَبُّ</a>. <a name="01900902000006" ll="SDBH:אַל:000000:">لاَ</a> <a name="01900902000008" ll="SDBH:עזז:000001:Strong">يَعْتَزَّ</a> <a name="01900902000010" ll="SDBH:אֱנֹושׁ:000000:People">ٱلْإِنْسَانُ</a>. <a name="01900902000012" ll="SDBH:שׁפט:000000:Judge" il="Tel-dan-israelite-gate-im-tor">لِتُحَاكَمِ</a> <a name="01900902000014" ll="SDBH:גֹּוי:000000:Groups">ٱلْأُمَمُ</a> <a name="01900902000018">قُدَّامَكَ</a>.
\v 21 (20) يَا <a name="01900902100004" ll="SDBH:יהוה:000000:Names of Deities">رَبُّ</a> <a name="01900902100002" ll="SDBH:שׁית:000005:Happen">ٱجْعَلْ</a> عَلَيْهِمْ <a name="01900902100006" ll="SDBH:מֹורָא:000000:Afraid">رُعْبًا</a> <a name="01900902100010">لِيَعْلَمَ</a> <a name="01900902100012" ll="SDBH:גֹּוי:000000:Groups">ٱلْأُمَمُ</a> أَنَّهُمْ <a name="01900902100014" ll="SDBH:אֱנֹושׁ:000000:People">بَشَرٌ</a>. <a name="01900902100018">سِلاَهْ</a>.
\c 10
\v 1 يَا <a name="01901000100004">رَبُّ</a>، لِمَاذَا <a name="01901000100006">تَقِفُ</a> <a name="01901000100010" ll="SDBH:רָחֹוק:000000:Space">بَعِيدًا</a>؟ لِمَاذَا <a name="01901000100012" ll="SDBH:עלם:000001:Hide">تَخْتَفِي</a> فِي <a name="01901000100016" ll="SDBH:עֵת:000000:Time">أَزْمِنَةِ</a> ٱلضِّيقِ؟
\v 2 فِي <a name="01901000200004" ll="SDBH:גַּאֲוָה:000001:Great">كِبْرِيَاءِ</a> <a name="01901000200006" ll="SDBH:רָשָׁע:000000:Guilty">ٱلشِّرِّيرِ</a> <a name="01901000200008" ll="SDBH:דלק:000004:Unsafe">يَحْتَرِقُ</a> <a name="01901000200010" ll="SDBH:עָנִי:000000:Classes|Unsafe|Poor">ٱلْمِسْكِينُ</a>. <a name="01901000200012" ll="SDBH:תפשׂ:000001:Control">يُؤْخَذُونَ</a> <a name="01901000200016" ll="SDBH:מְזִמָּה:000000:Think">بِالْمُؤَامَرَةِ</a> ٱلَّتِي <a name="01901000200020" ll="SDBH:חשׁב:000001:Think">فَكَّرُوا</a> بِهَا.
\v 3 لأَنَّ <a name="01901000300006" ll="SDBH:רָשָׁע:000000:Guilty">ٱلشِّرِّيرَ</a> <a name="01901000300004" ll="SDBH:הלל:001000:Praise">يَفْتَخِرُ</a> <a name="01901000300010" ll="SDBH:תַּאֲוָה:000001:Wish">بِشَهَوَاتِ</a> <a name="01901000300012" ll="SDBH:נֶפֶשׁ:000001:Parts: Creatures">نَفْسِهِ</a>، <a name="01901000300016" ll="SDBH:בצע:000005:Wish">وَٱلْخَاطِفُ</a> يُجَدِّفُ. <a name="01901000300020" ll="SDBH:נאץ:000000:Unwilling|Despise" tl="HOTTP2100">يُهِينُ</a> <a name="01901000300022" ll="SDBH:יהוה:000000:Names of Deities">ٱلرَّبَّ</a>.
\v 4 <a name="01901000400002" ll="SDBH:רָשָׁע:000000:Guilty">ٱلشِّرِّيرُ</a> حَسَبَ <a name="01901000400006" ll="SDBH:גֹּבַהּ:000002:Great">تَشَامُخِ</a> أَنْفِهِ يَقُولُ: <a name="01901000400010" ll="SDBH:בַּל:000000:Negators">لاَ</a> <a name="01901000400012" ll="SDBH:דרשׁ:000004:Bear">يُطَالِبُ</a>. كُلُّ <a name="01901000400020" ll="SDBH:מְזִמָּה:000000:Think">أَفْكَارِهِ</a> أَنَّهُ <a name="01901000400014">لاَ</a> <a name="01901000400016" ll="SDBH:אֱלֹהִים:000002:Deities">إِلهَ</a>.
\v 5 <a name="01901000500002" ll="SDBH:חיל:001000:Endure">تَثْبُتُ</a> <a name="01901000500004" ll="SDBH:דֶּרֶךְ:000007:Act">سُبْلُهُ</a> فِي كُلِّ <a name="01901000500012" ll="SDBH:עֵת:000001:Time">حِينٍ</a>. <a name="01901000500014" ll="SDBH:מָרֹום:000005:Space" tl="HOTTP2101">عَالِيَةٌ</a> <a name="01901000500016" ll="SDBH:מִשְׁפָּט:000006:Laws">أَحْكَامُكَ</a> فَوْقَهُ. كُلُّ أَعْدَائِهِ <a name="01901000500026" ll="SDBH:פוח:000002:Speak">يَنْفُثُ</a> فِيهِمْ.
\v 6 <a name="01901000600002" ll="SDBH:אמר:000000:Speak">قَالَ</a> فِي <a name="01901000600006" ll="SDBH:לֵב:000001:Think">قَلْبِهِ</a>: <a name="01901000600008" ll="SDBH:בַּל:000000:Negators">لاَ</a> <a name="01901000600010" ll="SDBH:מוט:000002:Afraid">أَتَزَعْزَعُ</a>. مِنْ <a name="01901000600014" ll="SDBH:דֹּור:001000:Groups">دَوْرٍ</a> إِلَى دَوْرٍ <a name="01901000600022" ll="SDBH:לֹא:000000:Negators" tl="HOTTP2102">بِلاَ</a> <a name="01901000600026" ll="SDBH:רַע:000004:Unsafe" tl="HOTTP2102">سُوءٍ</a>.
\v 7 <a name="01901000700004" ll="SDBH:פֶּה:000000:Parts: Creatures">فَمُهُ</a> <a name="01901000700006" ll="SDBH:מלא:000008:Engage">مَمْلُوءٌ</a> <a name="01901000700002" ll="SDBH:אָלָה:000001:Curse" tl="HOTTP2102">لَعْنَةً</a> <a name="01901000700010" ll="SDBH:מִרְמָה:000000:Wrong">وَغِشًّا</a> <a name="01901000700014" ll="SDBH:תֹּךְ:000000:Wrong">وَظُلْمًا</a>. تَحْتَ <a name="01901000700018" ll="SDBH:לָשֹׁון:000000:Parts: Creatures">لِسَانِهِ</a> مَشَقَّةٌ <a name="01901000700024" ll="SDBH:אָוֶן:000000:Wrong">وَإِثْمٌ</a>.
\v 8 <a name="01901000800002" ll="SDBH:ישׁב:000000:Space|Stance">يَجْلِسُ</a> فِي مَكْمَنِ <a name="01901000800008" ll="SDBH:חָצֵר:000000:Towns" tl="HOTTP2103">ٱلدِّيَارِ</a>، فِي <a name="01901000800014" ll="SDBH:מִסְתָּר:000000:Space">ٱلْمُخْتَفَيَاتِ</a> <a name="01901000800016" ll="SDBH:הרג:000000:Dead">يَقْتُلُ</a> <a name="01901000800018" ll="SDBH:נָקִי:000000:Innocent">ٱلْبَرِيَّ</a>. <a name="01901000800020" ll="SDBH:עַיִן:000000:Parts: Creatures">عَيْنَاهُ</a> <a name="01901000800026" ll="SDBH:צפן:000000:Hide" tl="HOTTP2104">تُرَاقِبَانِ</a> <a name="01901000800024" ll="SDBH:חֵלְכָה:000000:Engage">ٱلْمِسْكِينَ</a>.
\v 9 <a name="01901000900002" ll="SDBH:ארב:000000:Hide">يَكْمُنُ</a> فِي <a name="01901000900008" ll="SDBH:מִסְתָּר:000000:Space">ٱلْمُخْتَفَى</a> <a name="01901000900012" ll="SDBH:אַרְיֵה:000000:" il="LB00047;HK00046;Fauna:2.24a;Fauna:2.24b;Fauna:2.24c">كَأَسَدٍ</a> فِي <a name="01901000900016" ll="SDBH:סֹךְ:000000:Tents">عِرِّيسِهِ</a>. <a name="01901000900018" ll="SDBH:ארב:000000:Hide">يَكْمُنُ</a> <a name="01901000900022" ll="SDBH:חטף:000000:Hold">لِيَخْطَفَ</a> <a name="01901000900024" ll="SDBH:עָנִי:000000:Classes|Unsafe|Poor">ٱلْمِسْكِينَ</a>. <a name="01901000900026" ll="SDBH:חטף:000000:Hold">يَخْطَفُ</a> <a name="01901000900028" ll="SDBH:עָנִי:000000:Classes|Unsafe|Poor">ٱلْمِسْكِينَ</a> <a name="01901000900032" ll="SDBH:משׁך:000002:Move">بِجَذْبِهِ</a> فِي <a name="01901000900036" ll="SDBH:רֶשֶׁת:000000:Traps" il="LB00210;LB00209;Realia:1.3.1.1c;Realia:1.4.2;mat00691">شَبَكَتِهِ</a>،
\v 10 <a name="01901001000002" ll="SDBH:דכה:000001:Distress" tl="HOTTP2105">فَتَنْسَحِقُ</a> <a name="01901001000006" ll="SDBH:שׁחח:000005:Stance">وَتَنْحَنِي</a> <a name="01901001000010" ll="SDBH:נפל:000036:Stance">وَتَسْقُطُ</a> ٱلْمَسَاكِينُ <a name="01901001000014" ll="SDBH:עָצוּם:000002:Strong">بِبَرَاثِنِهِ</a>.
\v 11 <a name="01901001100002" ll="SDBH:אמר:000000:Speak">قَالَ</a> فِي <a name="01901001100006" ll="SDBH:לֵב:000001:Think">قَلْبِهِ</a>: إِنَّ <a name="01901001100010" ll="SDBH:אֵל:000001:Deities">ٱللّٰهَ</a> قَدْ <a name="01901001100008" ll="SDBH:שׁכח:000000:Non-Know">نَسِيَ</a>. <a name="01901001100012" ll="SDBH:סתר:000004:Non-Engage">حَجَبَ</a> <a name="01901001100014" ll="SDBH:פָּנֶה:000000:">وَجْهَهُ</a>. <a name="01901001100016" ll="SDBH:בַּל:000000:Negators">لاَ</a> <a name="01901001100018" ll="SDBH:ראה:000000:See">يَرَى</a> إِلَى <a name="01901001100022" ll="SDBH:נֵצַח:000000:Time">ٱلْأَبَدِ</a>.
\v 12 <a name="01901001200002">قُمْ</a> يَا <a name="01901001200004" ll="SDBH:יהוה:000000:Names of Deities">رَبُّ</a>. يَا <a name="01901001200006" ll="SDBH:אֵל:000001:Deities">اَللّٰهُ</a> <a name="01901001200008" ll="SDBH:נשׂא:000045:Engage">ٱرْفَعْ</a> <a name="01901001200010">يَدَكَ</a>. لاَ <a name="01901001200014" ll="SDBH:שׁכח:000000:Non-Know">تَنْسَ</a> <a name="01901001200016" ll="SDBH:עָנָו:000000:Insignificant">ٱلْمَسَاكِينَ</a>.
\v 13 <a name="01901001300004" ll="SDBH:מָה:000000:Object Referents">لِمَاذَا</a> <a name="01901001300006" ll="SDBH:נאץ:000000:Unwilling|Despise">أَهَانَ</a> <a name="01901001300008" ll="SDBH:רָשָׁע:000000:Guilty">ٱلشِّرِّيرُ</a> <a name="01901001300010" ll="SDBH:אֱלֹהִים:000002:Deities">ٱللّٰهَ</a>؟ لِمَاذَا <a name="01901001300012" ll="SDBH:אמר:000000:Speak">قَالَ</a> فِي <a name="01901001300016" ll="SDBH:לֵב:000001:Think">قَلْبِهِ</a>: <a name="01901001300018" ll="SDBH:לֹא:000000:Negators">لاَ</a> <a name="01901001300020" ll="SDBH:דרשׁ:000004:Bear">تُطَالِبُ</a>؟
\v 14 قَدْ <a name="01901001400002" ll="SDBH:ראה:000000:See">رَأَيْتَ</a>. لأَنَّكَ <a name="01901001400014" ll="SDBH:נבט:000000:See">تُبْصِرُ</a> ٱلْمَشَقَّةَ <a name="01901001400012" ll="SDBH:כַּעַס:000000:Angry">وَٱلْغَمَّ</a> <a name="01901001400018">لِتُجَازِيَ</a> <a name="01901001400022">بِيَدِكَ</a>. إِلَيْكَ <a name="01901001400026" ll="SDBH:עזב:000004:Confident">يُسَلِّمُ</a> <a name="01901001400028" ll="SDBH:חֵלְכָה:000000:Engage">ٱلْمِسْكِينُ</a> أَمْرَهُ. أَنْتَ <a name="01901001400034">صِرْتَ</a> <a name="01901001400036" ll="SDBH:עזר:000001:Help">مُعِينَ</a> <a name="01901001400030" ll="SDBH:יָתֹום:000000:Classes">ٱلْيَتِيمِ</a>.
\v 15 <a name="01901001500002" ll="SDBH:שׁבר:000000:Impact">اِحْطِمْ</a> <a name="01901001500004" ll="SDBH:זְרֹועַ:000000:Parts: Creatures">ذِرَاعَ</a> <a name="01901001500006" ll="SDBH:רָשָׁע:000000:Guilty">ٱلْفَاجِرِ</a>. <a name="01901001500010" ll="SDBH:רַע:000001:Evil">وَٱلشِّرِّيرُ</a> تَطْلُبُ <a name="01901001500014" ll="SDBH:רֶשַׁע:000001:Wrong">شَرَّهُ</a> <a name="01901001500016" ll="SDBH:בַּל:000000:Negators">وَلاَ</a> <a name="01901001500018" ll="SDBH:מצא:000011:Know">تَجِدُهُ</a>.
\v 16 <a name="01901001600002" ll="SDBH:יהוה:000000:Names of Deities">ٱلرَّبُّ</a> <a name="01901001600004" ll="SDBH:מֶלֶךְ:000000:Leaders">مَلِكٌ</a> إِلَى <a name="01901001600006" ll="SDBH:עֹולָם:000000:Time">ٱلدَّهْرِ</a> <a name="01901001600010" ll="SDBH:עַד:001000:Time">وَٱلْأَبَدِ</a>. <a name="01901001600012" ll="SDBH:אבד:000004:Non-Exist|Dead">بَادَتِ</a> <a name="01901001600014" ll="SDBH:גֹּוי:000000:Groups">ٱلْأُمَمُ</a> مِنْ <a name="01901001600018" ll="SDBH:אֶרֶץ:000000:Land">أَرْضِهِ</a>.
\v 17 <a name="01901001700002" ll="SDBH:תַּאֲוָה:000001:Wish">تَأَوُّهَ</a> <a name="01901001700004" ll="SDBH:עָנָו:000000:Insignificant">ٱلْوُدَعَاءِ</a> قَدْ سَمِعْتَ يَا <a name="01901001700008" ll="SDBH:יהוה:000000:Names of Deities">رَبُّ</a>. <a name="01901001700010" ll="SDBH:כון:000005:Confident">تُثَبِّتُ</a> <a name="01901001700012" ll="SDBH:לֵב:000001:Think">قُلُوبَهُمْ</a>. <a name="01901001700014" ll="SDBH:קשׁב:000000:Hear">تُمِيلُ</a> <a name="01901001700016" ll="SDBH:אֹזֶן:000000:Parts: Creatures">أُذُنَكَ</a>
