\v 10 لِذلِكَ <a name="01907301000004" ll="SDBH:שׁוב:000005:Associate">يَرْجعُ</a> <a name="01907301000008" tl="HOTTP2377">شَعْبُهُ</a> إِلَى <a name="01907301000010" tl="HOTTP2378">هُنَا</a>، <a name="01907301000014" ll="SDBH:מַיִם:000000:Liquids" tl="HOTTP2379">وَكَمِيَاهٍ</a> مُرْوِيَةٍ <a name="01907301000018" ll="SDBH:מצה:000000:Detach" tl="HOTTP2379">يُمْتَصُّونَ</a> مِنْهُمْ.
\v 11 <a name="01907301100004" ll="SDBH:אמר:000000:Speak">وَقَالُوا</a>: كَيْفَ <a name="01907301100008">يَعْلَمُ</a> <a name="01907301100010" ll="SDBH:אֵל:000001:Deities">ٱللّٰهُ</a>؟ <a name="01907301100014">وَهَلْ</a> عِنْدَ <a name="01907301100020" ll="SDBH:עֶלְיֹון:000000:High">ٱلْعَلِيِّ</a> <a name="01907301100016" ll="SDBH:דֵּעָה:000000:Know">مَعْرِفَةٌ</a>؟
\v 12 هُوَذَا <a name="01907301200004" ll="SDBH:אֵלֶּה:000000:Identifiers">هٰؤُلاَءِ</a> هُمُ <a name="01907301200006" ll="SDBH:רָשָׁע:000000:Guilty">ٱلْأَشْرَارُ</a>، وَمُسْتَرِيحِينَ إِلَى <a name="01907301200012" ll="SDBH:עֹולָם:000000:Time" tl="HOTTP2380">ٱلدَّهْرِ</a> <a name="01907301200014" ll="SDBH:שׂגה:000002:Possess">يُكْثِرُونَ</a> <a name="01907301200016" ll="SDBH:חַיִל:000001:Possess">ثَرْوَةً</a>.
\v 13 حَقًّا قَدْ <a name="01907301300006" ll="SDBH:זכה:000002:Innocent">زَكَّيْتُ</a> <a name="01907301300008" ll="SDBH:לֵבָב:000001:Think">قَلْبِي</a> <a name="01907301300004" ll="SDBH:רִיק:000001:Fail">بَاطِلاً</a> <a name="01907301300012" ll="SDBH:רחץ:000000:Clean" il="Realia:5.16">وَغَسَلْتُ</a> <a name="01907301300016" ll="SDBH:נִקָּיֹון:000001:Innocent">بِالنَّقَاوَةِ</a> <a name="01907301300018" ll="SDBH:כַּף:000008:Innocent">يَدَيَّ</a>.
\v 14 وَكُنْتُ <a name="01907301400006" ll="SDBH:נגע:000001:Impact">مُصَابًا</a> <a name="01907301400012" ll="SDBH:יֹום:000007:Time">ٱلْيَوْمَ</a> كُلَّهُ، <a name="01907301400016" ll="SDBH:תֹּוכַחַת:000001:Chastise">وَتَأَدَّبْتُ</a> كُلَّ <a name="01907301400022" ll="SDBH:בֹּקֶר:000000:Time">صَبَاحٍ</a>.
\v 15 لَوْ <a name="01907301500004" ll="SDBH:אמר:000000:Speak">قُلْتُ</a> <a name="01907301500006" ll="SDBH:ספר:000006:Speak">أُحَدِّثُ</a> هكَذَا، <a name="01907301500016" ll="SDBH:בגד:000000:Unfaithful">لَغَدَرْتُ</a> <a name="01907301500012" ll="SDBH:דֹּור:001001:Groups">بِجِيلِ</a> بَنِيكَ.
\v 16 فَلَمَّا <a name="01907301600004" ll="SDBH:חשׁב:000006:Think">قَصَدْتُ</a> <a name="01907301600008">مَعْرِفَةَ</a> هٰذَا، إِذَا هُوَ تَعَبٌ فِي <a name="01907301600020" ll="SDBH:עַיִן:000000:Parts: Creatures">عَيْنَيَّ</a>.
\v 17 حَتَّى <a name="01907301700004">دَخَلْتُ</a> <a name="01907301700008" ll="SDBH:מִקְדָּשׁ:000001:Buildings" tl="HOTTP2382" il="HK00245;NBGlo20140708c;NBGlo20140825;NBGlo20140606;NBGlo20140606a;NBGlo20140606b;jlm_soltemp_d01;jlm_soltemp_m06;jlm_soltemp_pillars_m01">مَقَادِسَ</a> <a name="01907301700010" ll="SDBH:אֵל:000001:Deities" tl="HOTTP2382">ٱللّٰهِ</a>، <a name="01907301700012" ll="SDBH:בין:000001:Know">وَٱنْتَبَهْتُ</a> إِلَى <a name="01907301700016" ll="SDBH:אַחֲרִית:000001:Orientation">آخِرَتِهِمْ</a>.
\v 18 حَقًّا فِي مَزَالِقَ <a name="01907301800008" ll="SDBH:שׁית:000001:Space">جَعَلْتَهُمْ</a>. <a name="01907301800012" ll="SDBH:נפל:000039:Stance">أَسْقَطْتَهُمْ</a> إِلَى ٱلْبَوَارِ.
\v 19 <a name="01907301900002" ll="SDBH:אֵיךְ:000000:Event Referents: Modification">كَيْفَ</a> <a name="01907301900004">صَارُوا</a> <a name="01907301900008" ll="SDBH:שַׁמָּה:000000:Empty|Landscapes">لِلْخَرَابِ</a> <a name="01907301900012" ll="SDBH:רֶגַע:000000:Time">بَغْتَةً</a>! <a name="01907301900014" ll="SDBH:סוף:000000:Non-Exist">ٱضْمَحَلُّوا</a>، <a name="01907301900016" ll="SDBH:תמם:000002:Dead">فَنُوا</a> مِنَ <a name="01907301900020" ll="SDBH:בַּלָּהָה:000000:Afraid">ٱلدَّوَاهِي</a>.
\v 20 <a name="01907302000004" ll="SDBH:חֲלֹום:000000:Vision" il="tb112004059w;Bolen00401;AT Image00693;Blake001;Blake002">كَحُلْمٍ</a> عِنْدَ ٱلتَّيَقُّظِ يَا <a name="01907302000010" ll="SDBH:אֲדֹנָי:000000:Titles" tl="HOTTP2383">رَبُّ</a>، عِنْدَ <a name="01907302000014" ll="SDBH:עור:000000:Awake">ٱلتَّيَقُّظِ</a> <a name="01907302000018" ll="SDBH:בזה:000000:Despise|Revile">تَحْتَقِرُ</a> <a name="01907302000016" ll="SDBH:צֶלֶם:000001:Compare">خَيَالَهُمْ</a>.
\v 21 لأَنَّهُ <a name="01907302100004" ll="SDBH:חמץ:000001:Angry">تَمَرْمَرَ</a> <a name="01907302100006" ll="SDBH:לֵבָב:000001:Think">قَلْبِي</a>، <a name="01907302100012" ll="SDBH:שׁנן:000001:Distress">وَٱنْتَخَسْتُ</a> فِي <a name="01907302100010" ll="SDBH:כִּלְיָה:000001:Self">كُلْيَتَيَّ</a>.
\v 22 <a name="01907302200004">وَأَنَا</a> <a name="01907302200006" ll="SDBH:בַּעַר:000000:Foolish">بَلِيدٌ</a> وَلاَ <a name="01907302200012">أَعْرِفُ</a>. <a name="01907302200016">صِرْتُ</a> <a name="01907302200014" ll="SDBH:בְּהֵמָה:000000:Animals">كَبَهِيمٍ</a> <a name="01907302200018">عِنْدَكَ</a>.
\v 23 وَلكِنِّي <a name="01907302300006" ll="SDBH:תָּמִיד:000000:Lasting">دَائِمًا</a> مَعَكَ. <a name="01907302300010" ll="SDBH:אחז:000002:Hold">أَمْسَكْتَ</a> <a name="01907302300014">بِيَدِي</a> <a name="01907302300016" ll="SDBH:יָמִין:000000:Orientation">ٱلْيُمْنَى</a>.
\v 24 بِرَأْيِكَ تَهْدِينِي، <a name="01907302400010" ll="SDBH:אַחַר:002001:Event Referents: Time">وَبَعْدُ</a> إِلَى <a name="01907302400012" ll="SDBH:כָּבֹוד:000000:Great">مَجْدٍ</a> <a name="01907302400014" ll="SDBH:לקח:000000:Hold">تَأْخُذُنِي</a>.
\v 25 مَنْ لِي فِي <a name="01907302500010" ll="SDBH:שָׁמַיִם:000001:Universe">ٱلسَّمَاءِ</a>؟ وَمَعَكَ <a name="01907302500016" ll="SDBH:לֹא:000000:Negators">لاَ</a> <a name="01907302500018" ll="SDBH:חפץ:000000:Love">أُرِيدُ</a> شَيْئًا فِي <a name="01907302500024" ll="SDBH:אֶרֶץ:000000:Land">ٱلْأَرْضِ</a>.
\v 26 قَدْ <a name="01907302600002" ll="SDBH:כלה:000013:Weak">فَنِيَ</a> <a name="01907302600004" ll="SDBH:שְׁאֵר:000000:Parts: Creatures">لَحْمِي</a> <a name="01907302600008" ll="SDBH:לֵבָב:000001:Think">وَقَلْبِي</a>. <a name="01907302600010" ll="SDBH:צוּר:000002:Deities" tl="HOTTP2384">صَخْرَةُ</a> <a name="01907302600012" ll="SDBH:לֵבָב:000001:Think" tl="HOTTP2384">قَلْبِي</a> <a name="01907302600016" ll="SDBH:חֵלֶק:000002:Associate">وَنَصِيبِي</a> <a name="01907302600018" ll="SDBH:אֱלֹהִים:000002:Deities">ٱللّٰهُ</a> إِلَى <a name="01907302600022" ll="SDBH:עֹולָם:000000:Time">ٱلدَّهْرِ</a>.
\v 27 لأَنَّهُ هُوَذَا ٱلْبُعَدَاءُ عَنْكَ <a name="01907302700008" ll="SDBH:אבד:000004:Non-Exist|Dead">يَبِيدُونَ</a>. <a name="01907302700010" ll="SDBH:צמת:000002:Non-Exist">تُهْلِكُ</a> <a name="01907302700012">كُلَّ</a> مَنْ <a name="01907302700014" ll="SDBH:זנה:000002:Unfaithful">يَزْنِي</a> عَنْكَ.
\v 28 <a name="01907302800004">أَمَّا</a> أَنَا <a name="01907302800006" ll="SDBH:קִרְבָה:000000:Associate">فَالاقْتِرَابُ</a> إِلَى <a name="01907302800008" ll="SDBH:אֱלֹהִים:000002:Deities">ٱللّٰهِ</a> <a name="01907302800012" ll="SDBH:טֹוב:000002:Right">حَسَنٌ</a> لِي. <a name="01907302800014" ll="SDBH:שׁית:000013:Confident">جَعَلْتُ</a> <a name="01907302800018" ll="SDBH:אֲדֹנָי:000000:Titles">بِالسَّيِّدِ</a> ٱلرَّبِّ <a name="01907302800022" ll="SDBH:מַחְסֶה:000001:Confident">مَلْجَإِي</a>، <a name="01907302800026" ll="SDBH:ספר:000006:Speak" tl="HOTTP2385">لأُخْبِرَ</a> بِكُلِّ <a name="01907302800030" ll="SDBH:מְלָאכָה:000000:Serve|Wise" tl="HOTTP2385">صَنَائِعِكَ</a>.
\c 74
\v 1 لِمَاذَا <a name="01907400100012" ll="SDBH:זנח:000001:Unwilling">رَفَضْتَنَا</a> يَا <a name="01907400100010" ll="SDBH:אֱלֹהִים:000002:Deities">اَللّٰهُ</a> إِلَى <a name="01907400100016" ll="SDBH:נֵצַח:000000:Time">ٱلْأَبَدِ</a>؟ لِمَاذَا <a name="01907400100018" ll="SDBH:עשׁן:000003:Intense">يُدَخِّنُ</a> غَضَبُكَ عَلَى <a name="01907400100024" ll="SDBH:צֹאן:000000:Domestic Animals" il="LB00012;LB00017;LB00015;LB00014;NBGpb20140401b;NBGbs20120208-08;NBGbs20120710-139;tb111706002;Bolen00156;Bolen00369;AT Image00088;AT Image00159;AT Image00247;AT Image00556;Fauna0012">غَنَمِ</a> <a name="01907400100026" ll="SDBH:מַרְעִית:000001:Care">مَرْعَاكَ</a>؟
\v 2 <a name="01907400200002" ll="SDBH:זכר:000000:Know">ٱذْكُرْ</a> <a name="01907400200004" ll="SDBH:עֵדָה:000000:Groups">جَمَاعَتَكَ</a> ٱلَّتِي <a name="01907400200006" ll="SDBH:קנה:000000:Possess">ٱقْتَنَيْتَهَا</a> مُنْذُ <a name="01907400200008" ll="SDBH:קֶדֶם:000002:Time">ٱلْقِدَمِ</a>، <a name="01907400200010" ll="SDBH:גאל:000003:Safe" tl="HOTTP2386">وَفَدَيْتَهَا</a> <a name="01907400200012" ll="SDBH:שֵׁבֶט:000006:Groups" tl="HOTTP2386">سِبْطَ</a> مِيرَاثِكَ، <a name="01907400200016" ll="SDBH:הַר:000000:Landforms">جَبَلَ</a> <a name="01907400200018" ll="SDBH:צִיֹּון:000000:Names of Landforms|Names of Locations" il="tb060716478;Bolen00132">صِهْيَوْنَ</a> هٰذَا ٱلَّذِي <a name="01907400200022" ll="SDBH:שׁכן:000000:Dwell">سَكَنْتَ</a> فِيهِ.
\v 3 <a name="01907400300002" ll="SDBH:רום:000028:Engage" tl="HOTTP2387">ٱرْفَعْ</a> <a name="01907400300004" tl="HOTTP2387">خَطَوَاتِكَ</a> إِلَى ٱلْخِرَبِ <a name="01907400300010" ll="SDBH:נֵצַח:000000:Time">ٱلْأَبَدِيَّةِ</a>. ٱلْكُلَّ قَدْ حَطَّمَ ٱلْعَدُوُّ فِي <a name="01907400300022" ll="SDBH:קֹדֶשׁ:000002:Buildings">ٱلْمَقْدِسِ</a>.
\v 4 قَدْ <a name="01907400400002" ll="SDBH:שׁאג:000000:Sounds">زَمْجَرَ</a> مُقَاوِمُوكَ فِي <a name="01907400400008">وَسَطِ</a> <a name="01907400400010" ll="SDBH:מֹועֵד:000000:Space">مَعْهَدِكَ</a>، <a name="01907400400012" ll="SDBH:שׂים:000001:Space">جَعَلُوا</a> <a name="01907400400014" ll="SDBH:אֹות:000000:Banners" il="Realia:2.17a">آيَاتِهِمْ</a> <a name="01907400400016" ll="SDBH:אֹות:000000:Banners" il="Realia:2.17a">آيَاتٍ</a>.
\v 5 <a name="01907400500002" tl="HOTTP2388">يَبَانُ</a> كَأَنَّهُ <a name="01907400500006" tl="HOTTP2389">رَافِعُ</a> <a name="01907400500018" ll="SDBH:קַרְדֹּם:000000:Tools" il="NBGpb20140206a;Adze-01-024b;Adze_15-236;SAMA_(5);Bolen00099;Hoe-01-021;HoeBlade-01-022;Bolen00164;Bolen00224;Realia0015;Adze">فُؤُوسٍ</a> عَلَى <a name="01907400500016" ll="SDBH:עֵץ:000000:Trees">ٱلْأَشْجَارِ</a> ٱلْمُشْتَبِكَةِ.
\v 6 وَٱلْآنَ <a name="01907400600010" ll="SDBH:פִּתּוּחַ:000000:Shape" tl="HOTTP2391">مَنْقُوشَاتِهِ</a> <a name="01907400600012">مَعًا</a> <a name="01907400600016" ll="SDBH:כַּשִּׁיל:000000:Tools" il="Adze-01-024b;Bolen00099">بِالْفُؤُوسِ</a> <a name="01907400600020" ll="SDBH:הלם:000000:Impact;SDBH:כֵּילָף:000000:Tools">وَٱلْمَعَاوِلِ</a> <a name="01907400600022" il="Bolen00123">يَكْسِرُونَ</a>.
\v 7 <a name="01907400700002" ll="SDBH:שׁלח:000018:Burn">أَطْلَقُوا</a> <a name="01907400700008" ll="SDBH:אֵשׁ:000000:Fire" il="Fire02">ٱلنَّارَ</a> فِي <a name="01907400700010" ll="SDBH:מִקְדָּשׁ:000001:Buildings" il="HK00245;NBGlo20140708c;NBGlo20140825;NBGlo20140606;NBGlo20140606a;NBGlo20140606b;jlm_soltemp_d01;jlm_soltemp_m06;jlm_soltemp_pillars_m01">مَقْدِسِكَ</a>. <a name="01907400700018" ll="SDBH:חלל:000002:Common">دنَّسُوا</a> <a name="01907400700016" ll="SDBH:אֶרֶץ:000000:Land">لِلأَرْضِ</a> <a name="01907400700020" ll="SDBH:מִשְׁכָּן:000000:Dwell">مَسْكَنَ</a> <a name="01907400700022" ll="SDBH:שֵׁם:000003:Self">ٱسْمِكَ</a>.
\v 8 <a name="01907400800002" ll="SDBH:אמר:000000:Speak">قَالُوا</a> فِي <a name="01907400800006" ll="SDBH:לֵב:000001:Think">قُلُوبِهِمْ</a>: لِنُفْنِينَّهُمْ مَعًا!. <a name="01907400800012" ll="SDBH:שׂרף:000000:Burn" il="Fire02">أَحْرَقُوا</a> كُلَّ <a name="01907400800016" ll="SDBH:מֹועֵד:000000:Space">مَعَاهِدِ</a> <a name="01907400800018" ll="SDBH:אֵל:000001:Deities">ٱللّٰهِ</a> فِي <a name="01907400800024" ll="SDBH:אֶרֶץ:000000:Land">ٱلْأَرْضِ</a>.
\v 9 <a name="01907400900002" ll="SDBH:אֹות:000000:Banners;SDBH:אֹות:000001:Know" tl="HOTTP2392" il="Realia:2.17a;Bolen00045">آيَاتِنَا</a> <a name="01907400900004" ll="SDBH:לֹא:000000:Negators">لاَ</a> <a name="01907400900006" ll="SDBH:ראה:000000:See">نَرَى</a>. لاَ <a name="01907400900012" ll="SDBH:נָבִיא:000000:Prophets">نَبِيَّ</a> <a name="01907400900010">بَعْدُ</a>، وَلاَ بَيْنَنَا مَنْ <a name="01907400900020">يَعْرِفُ</a> حَتَّى مَتَى.
