\v 2 مِنْ <a name="01901700200006">قُدَّامِكَ</a> <a name="01901700200010" ll="SDBH:יצא:000025:Happen">يَخْرُجُ</a> <a name="01901700200008" ll="SDBH:מִשְׁפָּט:000001:Think">قَضَائِي</a>. <a name="01901700200012" ll="SDBH:עַיִן:000000:Parts: Creatures">عَيْنَاكَ</a> <a name="01901700200014" ll="SDBH:חזה:000001:See">تَنْظُرَانِ</a> <a name="01901700200016" ll="SDBH:מֵישָׁרִים:000000:Right">ٱلْمُسْتَقِيمَاتِ</a>.
\v 3 <a name="01901700300002" ll="SDBH:בחן:000000:Test">جَرَّبْتَ</a> <a name="01901700300004" ll="SDBH:לֵב:000001:Think">قَلْبِي</a>. <a name="01901700300006" ll="SDBH:פקד:000000:See">تَعَهَّدْتَهُ</a> <a name="01901700300008" ll="SDBH:לַיְלָה:000000:Time">لَيْلاً</a>. <a name="01901700300010" ll="SDBH:צרף:000000:Test">مَحَّصْتَنِي</a>. <a name="01901700300012" ll="SDBH:בַּל:000000:Negators">لاَ</a> <a name="01901700300014" ll="SDBH:מצא:000011:Know">تَجِدُ</a> فِيَّ <a name="01901700300016" ll="SDBH:זמם:000000:Think" tl="HOTTP2119">ذُمُومًا</a>. <a name="01901700300018" ll="SDBH:בַּל:000000:Negators">لاَ</a> <a name="01901700300020">يَتَعَدَّى</a> <a name="01901700300022" ll="SDBH:פֶּה:000000:Parts: Creatures">فَمِي</a>.
\v 4 مِنْ جِهَةِ <a name="01901700400004" ll="SDBH:פְּעֻלָּה:000001:Act">أَعْمَالِ</a> <a name="01901700400006" ll="SDBH:אָדָם:000000:People">ٱلنَّاسِ</a> <a name="01901700400010" ll="SDBH:דָּבָר:000000:Speak">فَبِكَلاَمِ</a> <a name="01901700400012" ll="SDBH:שָׂפָה:000001:Speak">شَفَتَيْكَ</a> أَنَا <a name="01901700400016" ll="SDBH:שׁמר:000000:See">تَحَفَّظْتُ</a> مِنْ <a name="01901700400018" ll="SDBH:אֹרַח:000002:Act">طُرُقِ</a> <a name="01901700400020" ll="SDBH:פָּרִיץ:000001:People">ٱلْمُعْتَنِفِ</a>.
\v 5 <a name="01901700500002" ll="SDBH:תמך:000003:Engage">تَمَسَّكَتْ</a> <a name="01901700500004" ll="SDBH:אָשֻׁר:000000:Act">خُطُوَاتِي</a> <a name="01901700500008" ll="SDBH:מַעְגָּל:000002:Act">بِآثارِكَ</a> <a name="01901700500010" ll="SDBH:בַּל:000000:Negators">فَمَا</a> <a name="01901700500012" ll="SDBH:מוט:000000:Shake">زَلَّتْ</a> <a name="01901700500014">قَدَمَايَ</a>.
\v 6 أَنَا <a name="01901700600004" ll="SDBH:קרא:000000:Shout">دَعَوْتُكَ</a> لأَنَّكَ <a name="01901700600008" ll="SDBH:ענה:000000:Speak">تَسْتَجِيبُ</a> لِي يَا <a name="01901700600010" ll="SDBH:אֵל:000001:Deities">اَللّٰهُ</a>. <a name="01901700600012" ll="SDBH:נטה:000003:Hear">أَمِلْ</a> <a name="01901700600014" ll="SDBH:אֹזֶן:000000:Parts: Creatures">أُذُنَيْكَ</a> إِلَيَّ. <a name="01901700600018">ٱسْمَعْ</a> <a name="01901700600020" ll="SDBH:אִמְרָה:000000:Speak">كَلاَمِي</a>.
\v 7 مَيِّزْ <a name="01901700700004" ll="SDBH:חֶסֶד:000000:Faithful">مَرَاحِمَكَ</a>، يَا <a name="01901700700006" ll="SDBH:ישׁע:000001:Safe">مُخَلِّصَ</a> <a name="01901700700008" ll="SDBH:חסה:000001:Confident">ٱلْمُتَّكِلِينَ</a> عَلَيْكَ، <a name="01901700700016" ll="SDBH:יָמִין:000000:Orientation">بِيَمِينِكَ</a> مِنَ <a name="01901700700012">ٱلْمُقَاوِمِينَ</a>.
\v 8 <a name="01901700800002" ll="SDBH:שׁמר:000007:Safe">ٱحْفَظْنِي</a> مِثْلَ <a name="01901700800006" ll="SDBH:אִישֹׁון:000000:Parts: Creatures">حَدَقَةِ</a> <a name="01901700800010" ll="SDBH:עַיִן:000000:Parts: Creatures">ٱلْعَيْنِ</a>. <a name="01901700800014" ll="SDBH:צֵל:000001:Safe">بِظِلِّ</a> <a name="01901700800016" ll="SDBH:כָּנָף:000000:Parts: Creatures">جَنَاحَيْكَ</a> <a name="01901700800018" ll="SDBH:סתר:000002:Hide">ٱسْتُرْنِي</a>
\v 9 مِنْ <a name="01901700900004">وَجْهِ</a> <a name="01901700900006" ll="SDBH:רָשָׁע:000000:Guilty">ٱلْأَشْرَارِ</a> <a name="01901700900008" ll="SDBH:זוּ:000000:Linkers: Description">ٱلَّذِينَ</a> <a name="01901700900010" ll="SDBH:שׁדד:000000:Impact">يُخْرِبُونَنِي</a>، أَعْدَائِي <a name="01901700900016" ll="SDBH:נֶפֶשׁ:000001:Parts: Creatures">بِالنَّفْسِ</a> ٱلَّذِينَ <a name="01901700900018" ll="SDBH:נקף:001000:Space">يَكْتَنِفُونَنِي</a>.
\v 10 قَلْبَهُمُ <a name="01901701000002" ll="SDBH:חֵלֶב:000000:Parts: Creatures">ٱلسَّمِينَ</a> قَدْ <a name="01901701000004" ll="SDBH:סגר:000001:Close">أَغْلَقُوا</a>. <a name="01901701000006" ll="SDBH:פֶּה:000000:Parts: Creatures">بِأَفْوَاهِهِمْ</a> قَدْ <a name="01901701000008" ll="SDBH:דבר:001000:Speak">تَكَلَّمُوا</a> <a name="01901701000012" ll="SDBH:גֵּאוּת:000001:Great">بِالْكِبْرِيَاءِ</a>.
\v 11 فِي خُطُوَاتِنَا ٱلْآنَ قَدْ <a name="01901701100006" tl="HOTTP2120">أَحَاطُوا</a> بِنَا. <a name="01901701100012" ll="SDBH:שׁית:000014:Think">نَصَبُوا</a> <a name="01901701100010" ll="SDBH:עַיִן:000000:Parts: Creatures">أَعْيُنَهُمْ</a> <a name="01901701100016" ll="SDBH:נטה:000007:Stance">لِيُزْلِقُونَا</a> إِلَى <a name="01901701100022" ll="SDBH:אֶרֶץ:000000:Land">ٱلْأَرْضِ</a>.
\v 12 مَثَلُهُ <a name="01901701200002" ll="SDBH:דִּמְיֹון:000000:Compare">مَثَلُ</a> <a name="01901701200006" ll="SDBH:אַרְיֵה:000000:" il="LB00047;HK00046;Fauna:2.24a;Fauna:2.24b;Fauna:2.24c">ٱلْأَسَدِ</a> <a name="01901701200008" ll="SDBH:כסף:000000:Wish">ٱلْقَرِمِ</a> إِلَى <a name="01901701200012" ll="SDBH:טרף:000000:Impact|Dead">ٱلْافْتِرَاسِ</a>، <a name="01901701200018" ll="SDBH:כְּפִיר:000000:Wild Animals" il="LB00047;HK00046;Fauna:2.24a;Fauna:2.24b;Fauna:2.24c">وَكَالشِّبْلِ</a> <a name="01901701200020">ٱلْكَامِنِ</a> فِي <a name="01901701200024" ll="SDBH:מִסְתָּר:000000:Space">عِرِّيسِهِ</a>.
\v 13 <a name="01901701300002">قُمْ</a> يَا <a name="01901701300004" ll="SDBH:יהוה:000000:Names of Deities">رَبُّ</a>. <a name="01901701300006" ll="SDBH:קדם:000000:Space">تَقَدَّمْهُ</a>. <a name="01901701300010" ll="SDBH:כרע:000001:Stance">اِصْرَعْهُ</a>. <a name="01901701300012" ll="SDBH:פלט:000001:Safe">نَجِّ</a> <a name="01901701300014" ll="SDBH:נֶפֶשׁ:000001:Parts: Creatures">نَفْسِي</a> مِنَ <a name="01901701300018" ll="SDBH:רָשָׁע:000000:Guilty">ٱلشِّرِّيرِ</a> <a name="01901701300020" ll="SDBH:חֶרֶב:000001:Weapons" il="LB00202;NBGmk20130711a;NBGmk20130711b;NBGmk20130711c;StraightSword-02-003;adr1305240787;tb061616732;Bolen00139">بِسَيْفِكَ</a>،
\v 14 مِنَ ٱلنَّاسِ <a name="01901701400006">بِيَدِكَ</a> يَا <a name="01901701400008" ll="SDBH:יהוה:000000:Names of Deities">رَبُّ</a>، مِنْ أَهْلِ ٱلدُّنْيَا. <a name="01901701400018" ll="SDBH:חֵלֶק:000004:Engage">نَصِيبُهُمْ</a> فِي حَيَاتِهِمْ. <a name="01901701400028" ll="SDBH:צפן:000007:Love">بِذَخَائِرِكَ</a> <a name="01901701400034" ll="SDBH:מלא:000002:Contain">تَمْلأُ</a> <a name="01901701400036" ll="SDBH:בֶּטֶן:000000:Parts: Creatures">بُطُونَهُمْ</a>. <a name="01901701400038" ll="SDBH:שׂבע:000000:Contain">يَشْبَعُونَ</a> أَوْلاَدًا وَيَتْرُكُونَ <a name="01901701400046" ll="SDBH:יֶתֶר:000007:Possess">فُضَالَتَهُمْ</a> <a name="01901701400050" ll="SDBH:עֹולֵל:000000:People|Kinship">لأَطْفَالِهِمْ</a>.
\v 15 أَمَّا أَنَا <a name="01901701500006" ll="SDBH:צֶדֶק:000001:Right">فَبِالْبِرِّ</a> <a name="01901701500008" ll="SDBH:חזה:000001:See">أَنْظُرُ</a> <a name="01901701500010" ll="SDBH:פָּנֶה:000000:">وَجْهَكَ</a>. <a name="01901701500012" ll="SDBH:שׂבע:000002:Engage">أَشْبَعُ</a> إِذَا ٱسْتَيْقَظْتُ <a name="01901701500018" ll="SDBH:תְּמוּנָה:000000:Compare">بِشَبَهِكَ</a>.
\c 18
\v 2 (1) <a name="01901800200006" ll="SDBH:רחם:000000:Love" tl="HOTTP2122">أُحِبُّكَ</a> يَا <a name="01901800200008" ll="SDBH:יהוה:000000:Names of Deities" tl="HOTTP2122">رَبُّ</a>، يَا <a name="01901800200010" ll="SDBH:חֵזֶק:000000:Strong" tl="HOTTP2122">قُوَّتِي</a>.
\v 3 (2) <a name="01901800300002" ll="SDBH:יהוה:000000:Names of Deities">ٱلرَّبُّ</a> <a name="01901800300004" ll="SDBH:סֶלַע:000000:Stone">صَخْرَتِي</a> وَحِصْنِي <a name="01901800300012" ll="SDBH:פלט:000001:Safe">وَمُنْقِذِي</a>. <a name="01901800300014" ll="SDBH:אֵל:000001:Deities">إِلهِي</a> <a name="01901800300016" ll="SDBH:צוּר:000002:Deities">صَخْرَتِي</a> بِهِ <a name="01901800300018" ll="SDBH:חסה:000000:Hide">أَحْتَمِي</a>. <a name="01901800300022" ll="SDBH:מָגֵן:000000:Weapons" il="NBGmk20131008">تُرْسِي</a> <a name="01901800300026" ll="SDBH:קֶרֶן:000000:Parts: Animals" il="BK00179;Realia:7.3.1c;NBGmk20140407c;adr1305240961;Ram">وَقَرْنُ</a> <a name="01901800300028" ll="SDBH:יֵשַׁע:000000:Safe">خَلاَصِي</a> <a name="01901800300030" ll="SDBH:מִשְׂגָּב:000001:Titles">وَمَلْجَإِي</a>.
\v 4 (3) <a name="01901800400004" ll="SDBH:קרא:000000:Shout">أَدْعُو</a> <a name="01901800400006" ll="SDBH:יהוה:000000:Names of Deities">ٱلرَّبَّ</a> <a name="01901800400002" ll="SDBH:הלל:001001:Great">ٱلْحَمِيدَ</a>، فَأَتَخَلَّصُ مِنْ أَعْدَائِي.
\v 5 (4) <a name="01901800500002" ll="SDBH:אפף:000000:Move">اِكْتَنَفَتْنِي</a> <a name="01901800500004" ll="SDBH:חֶבֶל:000001:Traps" tl="HOTTP2123">حِبَالُ</a> <a name="01901800500006" ll="SDBH:מָוֶת:000000:Dead" tl="HOTTP2123">ٱلْمَوْتِ</a>، <a name="01901800500010" ll="SDBH:נַחַל:000000:Landforms|Waterbodies" il="NBGbs20090428-1_39;tb110603119">وَسُيُولُ</a> <a name="01901800500012" ll="SDBH:בְּלִיַּעַל:000001:Dead">ٱلْهَلاَكِ</a> <a name="01901800500014" ll="SDBH:בעת:000000:Distress">أَفْزَعَتْنِي</a>.
\v 6 (5) <a name="01901800600002" ll="SDBH:חֶבֶל:000001:Traps">حِبَالُ</a> <a name="01901800600004" ll="SDBH:שְׁאֹול:000000:Universe">ٱلْهَاوِيَةِ</a> <a name="01901800600006">حَاقَتْ</a> بِي. <a name="01901800600010" ll="SDBH:מֹוקֵשׁ:000000:Traps">أَشْرَاكُ</a> <a name="01901800600012" ll="SDBH:מָוֶת:000000:Dead">ٱلْمَوْتِ</a> <a name="01901800600008" ll="SDBH:קדם:000002:Meet">ٱنْتَشَبَتْ</a> بِي.
\v 7 (6) فِي <a name="01901800700006" ll="SDBH:צַר:000002:Unsafe">ضِيقِي</a> <a name="01901800700010" ll="SDBH:קרא:000000:Shout">دَعَوْتُ</a> <a name="01901800700012" ll="SDBH:יהוה:000000:Names of Deities">ٱلرَّبَّ</a>، وَإِلَى <a name="01901800700018" ll="SDBH:אֱלֹהִים:000001:Deities">إِلهِي</a> <a name="01901800700020" ll="SDBH:שׁוע:000000:Shout">صَرَخْتُ</a>، <a name="01901800700022">فَسَمِعَ</a> مِنْ <a name="01901800700026" ll="SDBH:הֵיכָל:000000:Buildings">هَيْكَلِهِ</a> <a name="01901800700028">صَوْتِي</a>، <a name="01901800700032" ll="SDBH:שַׁוְעָה:000000:Shout">وَصُرَاخِي</a> <a name="01901800700036">قُدَّامَهُ</a> <a name="01901800700038">دَخَلَ</a> <a name="01901800700042" ll="SDBH:אֹזֶן:000000:Parts: Creatures">أُذُنَيْهِ</a>.
\v 8 (7) <a name="01901800800004" ll="SDBH:געשׁ:000000:Shake">فَارْتَجَّتِ</a> <a name="01901800800012" ll="SDBH:אֶרֶץ:000000:Land">ٱلْأَرْضُ</a> <a name="01901800800008" ll="SDBH:רעשׁ:000000:Shake" il="Bolen00504">وَٱرْتَعَشَتْ</a>، <a name="01901800800016" ll="SDBH:מֹוסָד:000000:Parts: Constructions">أُسُسُ</a> <a name="01901800800018" ll="SDBH:הַר:000000:Landforms">ٱلْجِبَالِ</a> <a name="01901800800020" ll="SDBH:רגז:000000:Shake" il="Bolen00504">ٱرْتَعَدَتْ</a> <a name="01901800800024" ll="SDBH:געשׁ:000000:Shake">وَٱرْتَجَّتْ</a> لأَنَّهُ <a name="01901800800028" ll="SDBH:חרה:000002:Angry">غَضِبَ</a>.
\v 9 (8) <a name="01901800900002" ll="SDBH:עלה:000000:Move">صَعِدَ</a> <a name="01901800900004" ll="SDBH:עָשָׁן:000000:Smoke" il="Smoke">دُخَانٌ</a> مِنْ أَنْفِهِ، <a name="01901800900012" ll="SDBH:אֵשׁ:000000:Fire" il="Fire02">وَنَارٌ</a> مِنْ <a name="01901800900016" ll="SDBH:פֶּה:000000:Parts: Creatures">فَمِهِ</a> <a name="01901800900018" ll="SDBH:אכל:000002:Non-Exist|Dead">أَكَلَتْ</a>. <a name="01901800900020" ll="SDBH:גֶּחָל:000000:Wood" il="Burningcoal">جَمْرٌ</a> <a name="01901800900022" ll="SDBH:בער:000000:Burn">ٱشْتَعَلَتْ</a> مِنْهُ.
