\v 12 <a name="02200601200002" ll="SDBH:לֹא:000000:Negators">فَلَمْ</a> <a name="02200601200004">أَشْعُرْ</a> إِلاَّ وَقَدْ <a name="02200601200008" ll="SDBH:שׂים:000001:Space">جَعَلَتْنِي</a> <a name="02200601200006" ll="SDBH:נֶפֶשׁ:000001:Parts: Creatures">نَفْسِي</a> بَيْنَ <a name="02200601200010" ll="SDBH:מֶרְכָּבָה:000000:Vehicles" tl="HOTTP2935" il="Realia:2.15a;NBGmk20140410">مَرْكَبَاتِ</a> <a name="02200601200012" ll="SDBH:עַם:000000:Groups" tl="HOTTP2935">قَوْمِ</a> <a name="02200601200014" ll="SDBH:נָדִיב:000001:Leaders|Classes" tl="HOTTP2935">شَرِيفٍ</a>.
\c 7
\v 1 (13) <a name="02200700100002" ll="SDBH:שׁוב:000023:Move">اِرْجِعِي</a>، <a name="02200700100004" ll="SDBH:שׁוב:000023:Move">ٱرْجِعِي</a> يَا شُولَمِّيثُ. <a name="02200700100010" ll="SDBH:שׁוב:000023:Move">ٱرْجِعِي</a>، <a name="02200700100012" ll="SDBH:שׁוב:000023:Move">ٱرْجِعِي</a> <a name="02200700100016" ll="SDBH:חזה:000001:See">فَنَنْظُرَ</a> إِلَيْكِ. مَاذَا <a name="02200700100022" ll="SDBH:חזה:000001:See">تَرَوْنَ</a> فِي شُولَمِّيثَ، مِثْلَ <a name="02200700100032" ll="SDBH:מְחֹלָה:000000:Turn" il="Bolen00382">رَقْصِ</a> صَفَّيْنِ؟
\v 2 (1) مَا <a name="02200700200004" ll="SDBH:יפה:000000:Good">أَجْمَلَ</a> <a name="02200700200006">رِجْلَيْكِ</a> <a name="02200700200012" ll="SDBH:נַעַל:000000:Clothing" il="LB00161;LB00160;NBGmk20140519e;Realia0140">بِالنَّعْلَيْنِ</a> يَا <a name="02200700200014" ll="SDBH:בַּת:000000:Kinship">بِنْتَ</a> <a name="02200700200016" ll="SDBH:נָדִיב:000001:Leaders|Classes">ٱلْكَرِيمِ</a>! دَوَائِرُ <a name="02200700200020" ll="SDBH:יָרֵךְ:000000:Parts: Creatures">فَخْذَيْكِ</a> مِثْلُ <a name="02200700200024" ll="SDBH:חֲלִי:000000:Jewellery" il="NBG-sieraden;Realia0162">ٱلْحَلِيِّ</a>، <a name="02200700200026" ll="SDBH:מַעֲשֶׂה:000003:Artifacts">صَنْعَةِ</a> <a name="02200700200028">يَدَيْ</a> <a name="02200700200030" ll="SDBH:אָמָּן:000000:Professions">صَنَّاعٍ</a>.
\v 3 (2) سُرَّتُكِ <a name="02200700300004" ll="SDBH:אַגָּן:000000:Containers" il="NBGmk20140119b;Bolen00356">كَأْسٌ</a> <a name="02200700300008" ll="SDBH:סַהַר:000000:Shape">مُدَوَّرَةٌ</a>، لاَ <a name="02200700300012" ll="SDBH:חסר:000000:Non-Space">يُعْوِزُهَا</a> <a name="02200700300016" ll="SDBH:מֶזֶג:000000:Liquids|Wine">شَرَابٌ</a> مَمْزُوجٌ. <a name="02200700300018" ll="SDBH:בֶּטֶן:000000:Parts: Creatures">بَطْنُكِ</a> <a name="02200700300020" ll="SDBH:עֲרֵמָה:000000:Quantity|Shape">صُبْرَةُ</a> <a name="02200700300022" ll="SDBH:חִטָּה:000000:Crops" il="Flora:3.1.4a;Flora:3.1.4b;PikiWiki_Israel_7817;Bolen00493">حِنْطَةٍ</a> مُسَيَّجَةٌ <a name="02200700300030" ll="SDBH:שׁוּשַׁן:000000:Plants" il="Flora:6.1.2a;Flora:6.1.2b">بِالسَّوْسَنِ</a>.
\v 4 (3) <a name="02200700400004" ll="SDBH:שַׁד:000000:Parts: Creatures">ثَدْيَاكِ</a> كَخَشْفَتَيْنِ، <a name="02200700400012" ll="SDBH:תֹּואֲמִם:000000:People">تَوْأَمَيْ</a> <a name="02200700400014" ll="SDBH:צְבִיָּה:000000:Wild Animals" il="Fauna:2.15a;Fauna:2.15b;Fauna0010">ظَبْيَةٍ</a>.
\v 5 (4) <a name="02200700500002" ll="SDBH:צַוָּאר:000000:Parts: Creatures">عُنُقُكِ</a> <a name="02200700500006" ll="SDBH:מִגְדָּל:000000:Constructions|Fortifications" il="NBGpb20140401a">كَبُرْجٍ</a> مِنْ <a name="02200700500010" ll="SDBH:שֵׁן:000003:Substances">عَاجٍ</a>. <a name="02200700500012" ll="SDBH:עַיִן:000000:Parts: Creatures">عَيْنَاكِ</a> <a name="02200700500014" ll="SDBH:בְּרֵכָה:000000:Waterbodies">كَالْبِرَكِ</a> فِي حَشْبُونَ عِنْدَ <a name="02200700500022" ll="SDBH:שַׁעַר:000000:Parts: Constructions" il="Realia:3.13.3.5b;Tel_Arad;CityGate-03-042c;Bolen00142;Bolen00143">بَابِ</a> بَثِّ رَبِّيمَ. أَنْفُكِ <a name="02200700500030" ll="SDBH:מִגְדָּל:000000:Constructions|Fortifications" il="NBGpb20140401a">كَبُرْجِ</a> <a name="02200700500034" ll="SDBH:לְבָנֹון:000000:Names of Landforms" il="Lebanon_mountains">لُبْنَانَ</a> <a name="02200700500036" ll="SDBH:צפה:000000:See">ٱلنَّاظِرِ</a> <a name="02200700500038" ll="SDBH:פָּנֶה:000000:">تُجَاهَ</a> <a name="02200700500040" ll="SDBH:דַּמֶּשֶׂק:000000:Names of Locations" il="HK00375;tb110406671;mat07576;mat01135">دِمَشْقَ</a>.
\v 6 (5) <a name="02200700600002" ll="SDBH:רֹאשׁ:000000:Parts: Creatures">رَأْسُكِ</a> عَلَيْكِ مِثْلُ ٱلْكَرْمَلِ، <a name="02200700600014" ll="SDBH:דַּלָּה:000001:Parts: People">وَشَعْرُ</a> رَأْسِكِ <a name="02200700600022" ll="SDBH:אַרְגָּמָן:000000:Cloth" il="jc0106151288;Bolen00602">كَأُرْجُوَانٍ</a>. <a name="02200700600024" ll="SDBH:מֶלֶךְ:000000:Leaders" tl="HOTTP2936">مَلِكٌ</a> قَدْ <a name="02200700600026" ll="SDBH:אסר:000005:Control">أُسِرَ</a> <a name="02200700600032" ll="SDBH:רַהַט:000001:Parts: Creatures">بِالْخُصَلِ</a>.
\v 7 (6) مَا <a name="02200700700004" ll="SDBH:יפה:000000:Good">أَجْمَلَكِ</a> وَمَا <a name="02200700700010" ll="SDBH:נעם:000000:Good">أَحْلاَكِ</a> أَيَّتُهَا ٱلْحَبِيبَةُ <a name="02200700700018" ll="SDBH:תַּעֲנוּג:000000:Rejoice" tl="HOTTP2938">بِاللَّذَّاتِ</a>!
\v 8 (7) <a name="02200700800004" ll="SDBH:קֹומָה:000000:Dimension">قَامَتُكِ</a> هٰذِهِ <a name="02200700800006" ll="SDBH:דמה:001000:Compare">شَبِيهَةٌ</a> <a name="02200700800010" ll="SDBH:תָּמָר:000000:Trees" il="Flora:2.5a;Flora:2.5b;NBGbs20120714-652;NBGbs20120205-81;tb051106839;tb032306453">بِالنَّخْلَةِ</a>، <a name="02200700800014" ll="SDBH:שַׁד:000000:Parts: Creatures">وَثَدْيَاكِ</a> <a name="02200700800018" ll="SDBH:אֶשְׁכֹּול:000000:Parts: Plants" il="grapes1662, rp;grapes1686, rp">بِالْعَنَاقِيدِ</a>.
\v 9 (8) <a name="02200700900002" ll="SDBH:אמר:000000:Speak">قُلْتُ</a>: إِنِّي <a name="02200700900004" ll="SDBH:עלה:000004:Move">أَصْعَدُ</a> إِلَى <a name="02200700900008" ll="SDBH:תָּמָר:000000:Trees" il="Flora:2.5a;Flora:2.5b;NBGbs20120714-652;NBGbs20120205-81;tb051106839;tb032306453">ٱلنَّخْلَةِ</a> <a name="02200700900010" ll="SDBH:אחז:000002:Hold">وَأُمْسِكُ</a> <a name="02200700900014" ll="SDBH:סַנְסִנָּה:000000:Parts: Plants">بِعُذُوقِهَا</a>. وَتَكُونُ <a name="02200700900022" ll="SDBH:שַׁד:000000:Parts: Creatures">ثَدْيَاكِ</a> <a name="02200700900026" ll="SDBH:אֶשְׁכֹּול:000000:Parts: Plants" il="grapes1662, rp;grapes1686, rp">كَعَنَاقِيدِ</a> <a name="02200700900030" ll="SDBH:גֶּפֶן:000001:Plants" il="HK00112;LB00107;HK00110;HK00109;Flora:2.12a;Flora:2.12b;vineyard1679, rp;tb061504248">ٱلْكَرْمِ</a>، <a name="02200700900034" ll="SDBH:רֵיחַ:000000:Smell">وَرَائِحَةُ</a> أَنْفِكِ <a name="02200700900042" ll="SDBH:תַּפּוּחַ:000000:Trees|Fruits" il="Flora:2.2;Apple-tree;appletree">كَالتُّفَّاحِ</a>،
\v 10 (9) <a name="02200701000004" ll="SDBH:חֵךְ:000002:Speak;SDBH:חֵךְ:000005:Impact">وَحَنَكُكِ</a> <a name="02200701000012" ll="SDBH:טֹוב:000000:Good">كَأَجْوَدِ</a> <a name="02200701000008" ll="SDBH:יַיִן:000000:Liquids|Wine|Crops" il="LB00107;HK00108;LB00103;Flora:2.12a;vineyard1679, rp">ٱلْخَمْرِ</a>. <a name="02200701000018" ll="SDBH:דֹּוד:000000:Love" tl="HOTTP2939">لِحَبِيبِي</a> <a name="02200701000022" ll="SDBH:מֵישָׁרִים:000000:Right">ٱلسَّائِغَةُ</a> ٱلْمُرَقْرِقَةُ <a name="02200701000024" ll="SDBH:דבב:000000:Flow" tl="HOTTP2940">ٱلسَّائِحَةُ</a> عَلَى <a name="02200701000026" ll="SDBH:שָׂפָה:000000:" tl="HOTTP2940">شِفَاهِ</a> <a name="02200701000028" ll="SDBH:יָשֵׁן:000000:Asleep" tl="HOTTP2940">ٱلنَّائِمِينَ</a>.
\v 11 (10) أَنَا <a name="02200701100006" ll="SDBH:דֹּוד:000000:Love">لِحَبِيبِي</a>، وَإِلَيَّ <a name="02200701100012" ll="SDBH:תְּשׁוּקָה:000000:Wish">ٱشْتِيَاقُهُ</a>.
\v 12 (11) تَعَالَ يَا <a name="02200701200004" ll="SDBH:דֹּוד:000000:Love">حَبِيبِي</a> <a name="02200701200006" ll="SDBH:יצא:000000:Move">لِنَخْرُجْ</a> إِلَى <a name="02200701200010" ll="SDBH:שָׂדֶה:000000:Landscapes">ٱلْحَقْلِ</a>، <a name="02200701200012" ll="SDBH:לין:000000:Space">وَلْنَبِتْ</a> فِي ٱلْقُرَى.
\v 13 (12) <a name="02200701300002" ll="SDBH:שׁכם:000000:Stance|Time">لِنُبَكِّرَنَّ</a> إِلَى <a name="02200701300008" ll="SDBH:כֶּרֶם:000000:Landscapes" il="HK00112;LB00107;Flora:2.12a;grapes1662, rp;grapes1686, rp;tb061504248;Bolen00212;Bolen00213;Bolen00214;Rit00100">ٱلْكُرُومِ</a>، <a name="02200701300010" ll="SDBH:ראה:000000:See">لِنَنْظُرَ</a>: هَلْ <a name="02200701300014" ll="SDBH:פרח:000000:Stage">أَزْهَرَ</a> <a name="02200701300018" ll="SDBH:גֶּפֶן:000001:Plants" il="HK00112;LB00107;HK00110;HK00109;Flora:2.12a;Flora:2.12b;vineyard1679, rp;tb061504248">ٱلْكَرْمُ</a>؟ هَلْ <a name="02200701300020" ll="SDBH:פתח:000000:Open">تَفَتَّحَ</a> <a name="02200701300024" ll="SDBH:סְמָדַר:000000:Stage">ٱلْقُعَالُ</a>؟ هَلْ نَوَّرَ <a name="02200701300030" ll="SDBH:רִמֹּון:000000:Trees|Fruits" il="LB00086;HK00089;Flora:2.10a;Flora:2.10b;NBGbs20110223-026;NBGbs20130730-202">ٱلرُّمَّانُ</a>؟ هُنَالِكَ <a name="02200701300034">أُعْطِيكَ</a> <a name="02200701300038" ll="SDBH:דֹּוד:000001:Love">حُبِّي</a>.
\v 14 (13) اَللُّفَّاحُ <a name="02200701400006">يَفُوحُ</a> <a name="02200701400008" ll="SDBH:רֵיחַ:000000:Smell">رَائِحَةً</a>، وَعِنْدَ <a name="02200701400014" ll="SDBH:פֶּתַח:000000:Parts: Constructions" il="HK00239;Realia:3.1.2a;Realia:3.1.2b;mat01418">أَبْوَابِنَا</a> كُلُّ <a name="02200701400018" ll="SDBH:מֶגֶד:000000:Good">ٱلنَّفَائِسِ</a> مِنْ <a name="02200701400020" ll="SDBH:חָדָשׁ:000000:Young">جَدِيدَةٍ</a> <a name="02200701400024" ll="SDBH:יָשָׁן:000000:Time">وَقَدِيمَةٍ</a>، <a name="02200701400028" ll="SDBH:צפן:000002:Space">ذَخَرْتُهَا</a> لَكَ يَا <a name="02200701400026" ll="SDBH:דֹּוד:000000:Love">حَبِيبِي</a>.
\c 8
\v 1 <a name="02200800100004">لَيْتَكَ</a> <a name="02200800100008" ll="SDBH:אָח:000000:Kinship">كَأَخٍ</a> لِي <a name="02200800100012" ll="SDBH:ינק:000000:Ingest" il="Motherhood1912;Bolen00195">ٱلرَّاضِعِ</a> <a name="02200800100014" ll="SDBH:שַׁד:000000:Parts: Creatures" il="Bolen00195">ثَدْيَيْ</a> <a name="02200800100016" ll="SDBH:אֵם:000000:Kinship" il="Motherhood1912">أُمِّي</a>، <a name="02200800100018" ll="SDBH:מצא:000012:Meet">فَأَجِدَكَ</a> فِي <a name="02200800100024" ll="SDBH:חוּץ:000000:Event Referents: Location" il="HK00169;Realia:3.13.2;tb052001215">ٱلْخَارِجِ</a> <a name="02200800100026" ll="SDBH:נשׁק:000000:Kiss">وَأُقَبِّلَكَ</a> وَلاَ <a name="02200800100032" ll="SDBH:בוז:000000:Despise|Revile">يُخْزُونَنِي</a>.
\v 2 <a name="02200800200002" tl="HOTTP2941">وَأَقُودُكَ</a> <a name="02200800200004">وَأَدْخُلُ</a> بِكَ <a name="02200800200008" ll="SDBH:בַּיִת:000000:Buildings" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20140917;NBGmk20120826;NBGmk20120826a">بَيْتَ</a> <a name="02200800200010" ll="SDBH:אֵם:000000:Kinship" il="Motherhood1912">أُمِّي</a>، وَهِيَ <a name="02200800200012" ll="SDBH:למד:000001:Know" tl="HOTTP2942">تُعَلِّمُنِي</a>، <a name="02200800200014" ll="SDBH:שׁקה:000002:Ingest">فَأَسْقِيكَ</a> مِنَ <a name="02200800200018" ll="SDBH:יַיִן:000000:Liquids|Wine|Crops" il="LB00107;HK00108;LB00103;Flora:2.12a;vineyard1679, rp">ٱلْخَمْرِ</a> <a name="02200800200022" ll="SDBH:רֶקַח:000000:Spices">ٱلْمَمْزُوجَةِ</a> مِنْ <a name="02200800200026" ll="SDBH:עָסִיס:000000:Liquids|Wine|Crops">سُلاَفِ</a> <a name="02200800200028" ll="SDBH:רִמֹּון:000000:Trees|Fruits" il="LB00086;HK00089;Flora:2.10a;Flora:2.10b;NBGbs20110223-026;NBGbs20130730-202">رُمَّانِي</a>.
\v 3 <a name="02200800300002" ll="SDBH:שְׂמֹאל:000000:Orientation">شِمَالُهُ</a> <a name="02200800300004">تَحْتَ</a> <a name="02200800300006" ll="SDBH:רֹאשׁ:000000:Parts: Creatures">رَأْسِي</a>، <a name="02200800300010" ll="SDBH:יָמִין:000000:Orientation">وَيَمِينُهُ</a> <a name="02200800300012" ll="SDBH:חבק:000000:Hold">تُعَانِقُنِي</a>.
