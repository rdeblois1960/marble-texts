\v 1 هَا أَنْتِ <a name="02200400100004" ll="SDBH:יָפֶה:000000:Good">جَمِيلَةٌ</a> يَا <a name="02200400100006" ll="SDBH:רַעְיָה:000000:Friends">حَبِيبَتِي</a>، هَا أَنْتِ <a name="02200400100010" ll="SDBH:יָפֶה:000000:Good">جَمِيلَةٌ</a>! <a name="02200400100012" ll="SDBH:עַיִן:000000:Parts: Creatures">عَيْنَاكِ</a> <a name="02200400100014" ll="SDBH:יֹונָה:000000:Birds" il="HK00064;LB00068;BK00066;BK00067;Fauna:3.7a;Fauna:3.7b;Rock_pigeons;Columba_livia">حَمَامَتَانِ</a> مِنْ <a name="02200400100018" ll="SDBH:בַּעַד:000001:Position">تَحْتِ</a> <a name="02200400100022" ll="SDBH:צַמָּה:000000:Clothing" il="Veil_Tuareg;Veil-Sinai;Veil-pink;Veil;Realia0139">نَقَابِكِ</a>. <a name="02200400100024" ll="SDBH:שֵׂעָר:000000:Parts: Creatures">شَعْرُكِ</a> كَقَطِيعِ <a name="02200400100032" ll="SDBH:עֵז:000000:Domestic Animals" il="BK00016;NBGbs20120208-08;tb030407773;Goats-Goat_family;Fauna0011;Fauna0012">مِعْزٍ</a> <a name="02200400100036" ll="SDBH:גלשׁ:000000:Move">رَابِضٍ</a> عَلَى <a name="02200400100040" ll="SDBH:הַר:000001:Landforms" tl="HOTTP2922">جَبَلِ</a> <a name="02200400100042" ll="SDBH:גִּלְעָד:000001:Names of Locations" tl="HOTTP2922" il="tb031315454;tb060403135">جِلْعَادَ</a>.
\v 2 <a name="02200400200002" ll="SDBH:שֵׁן:000000:Parts: Creatures">أَسْنَانُكِ</a> كَقَطِيعِ <a name="02200400200010" ll="SDBH:קצב:000001:Detach">ٱلْجَزَائِزِ</a> <a name="02200400200014" ll="SDBH:עלה:000004:Move">ٱلصَّادِرَةِ</a> مِنَ <a name="02200400200020" ll="SDBH:רַחְצָה:000000:Clean">ٱلْغَسْلِ</a>، ٱللَّوَاتِي <a name="02200400200024">كُلُّ</a> وَٱحِدَةٍ <a name="02200400200026" ll="SDBH:תאם:000000:Birth">مُتْئِمٌ</a>، <a name="02200400200032">وَلَيْسَ</a> فِيهِنَّ <a name="02200400200030" ll="SDBH:שַׁכּוּל:000000:Dissociate">عَقِيمٌ</a>.
\v 3 <a name="02200400300010" ll="SDBH:שָׂפָה:000000:">شَفَتَاكِ</a> <a name="02200400300004" ll="SDBH:חוּט:000000:Cords">كَسِلْكَةٍ</a> مِنَ <a name="02200400300008" ll="SDBH:שָׁנִי:000000:Color">ٱلْقِرْمِزِ</a>، <a name="02200400300014" ll="SDBH:מִדְבָּר:001000:Parts: People">وَفَمُكِ</a> <a name="02200400300016" ll="SDBH:נָאוֶה:000000:Good">حُلْوٌ</a>. <a name="02200400300026" ll="SDBH:רַקָּה:000000:Parts: People">خَدُّكِ</a> <a name="02200400300020" ll="SDBH:פֶּלַח:000000:Parts: Food">كَفِلْقَةِ</a> <a name="02200400300024" ll="SDBH:רִמֹּון:000000:Trees|Fruits" il="LB00086;HK00089;Flora:2.10a;Flora:2.10b;NBGbs20110223-026;NBGbs20130730-202">رُمَّانَةٍ</a> <a name="02200400300030" ll="SDBH:בַּעַד:000001:Position">تَحْتَ</a> <a name="02200400300034" ll="SDBH:צַמָּה:000000:Clothing" il="Veil_Tuareg;Veil-Sinai;Veil-pink;Veil;Realia0139">نَقَابِكِ</a>.
\v 4 <a name="02200400400008" ll="SDBH:צַוָּאר:000000:Parts: Creatures">عُنُقُكِ</a> <a name="02200400400004" ll="SDBH:מִגְדָּל:000000:Constructions|Fortifications" il="NBGpb20140401a">كَبُرْجِ</a> <a name="02200400400006" ll="SDBH:דָּוִד:000000:Names of People">دَاوُدَ</a> <a name="02200400400010" ll="SDBH:בנה:000000:Exist">ٱلْمَبْنِيِّ</a> <a name="02200400400014" ll="SDBH:תַּלְפִּיֹּות:000000:Arrange;SDBH:תַּלְפִּיֹּות:001000:Weapons">لِلأَسْلِحَةِ</a>. أَلْفُ <a name="02200400400020" ll="SDBH:מָגֵן:000000:Weapons" il="NBGmk20131008">مِجَنٍّ</a> <a name="02200400400022" ll="SDBH:תלה:000000:Hang">عُلِّقَ</a> عَلَيْهِ، كُلُّهَا <a name="02200400400028" ll="SDBH:שֶׁלֶט:000000:Weapons" il="NBGmk20131008">أَتْرَاسُ</a> <a name="02200400400032" ll="SDBH:גִּבֹּור:000000:People">ٱلْجَبَابِرَةِ</a>.
\v 5 <a name="02200400500004" ll="SDBH:שַׁד:000000:Parts: Creatures">ثَدْيَاكِ</a> <a name="02200400500008" ll="SDBH:שְׁנַיִם:000000:Quantity|Frequency">كَخِشْفَتَيْ</a> <a name="02200400500014" ll="SDBH:צְבִיָּה:000000:Wild Animals" il="Fauna:2.15a;Fauna:2.15b;Fauna0010">ظَبْيَةٍ</a>، <a name="02200400500012" ll="SDBH:תֹּואֲמִם:000000:People">تَوْأَمَيْنِ</a> يَرْعَيَانِ بَيْنَ <a name="02200400500024" ll="SDBH:שׁוּשַׁן:000000:Plants" tl="HOTTP2923" il="Flora:6.1.2a;Flora:6.1.2b">ٱلسَّوْسَنِ</a>.
\v 6 إِلَى أَنْ <a name="02200400600006" ll="SDBH:פוח:000004:Time">يَفِيحَ</a> <a name="02200400600010" ll="SDBH:יֹום:000000:Time|Shine">ٱلنَّهَارُ</a> <a name="02200400600014" ll="SDBH:נוס:000000:Flee">وَتَنْهَزِمَ</a> ٱلظِّلاَلُ، أَذْهَبُ إِلَى <a name="02200400600026" ll="SDBH:הַר:000000:Landforms">جَبَلِ</a> <a name="02200400600030" ll="SDBH:מֹר:000000:Spices" il="Flora:4.1.8a;Flora:4.1.8b;Flora:4.1.8c">ٱلْمُرِّ</a> وَإِلَى <a name="02200400600036" ll="SDBH:גִּבְעָה:000000:Landforms">تَلِّ</a> ٱللُّبَانِ.
\v 7 <a name="02200400700002">كُلُّكِ</a> <a name="02200400700004" ll="SDBH:יָפֶה:000000:Good">جَمِيلٌ</a> يَا <a name="02200400700006" ll="SDBH:רַעְיָה:000000:Friends">حَبِيبَتِي</a> <a name="02200400700012">لَيْسَ</a> فِيكِ <a name="02200400700010" ll="SDBH:מוּם:000000:Impact">عَيْبَةٌ</a>.
\v 8 <a name="02200400800016">هَلُمِّي</a> مَعِي مِنْ <a name="02200400800006" ll="SDBH:לְבָנֹון:000000:Names of Landforms" il="Lebanon_mountains">لُبْنَانَ</a> يَا <a name="02200400800008" ll="SDBH:כַּלָּה:000001:Kinship" il="ef0078;Realia0139">عَرُوسُ</a>، مَعِي مِنْ <a name="02200400800014" ll="SDBH:לְבָנֹון:000000:Names of Landforms" il="Lebanon_mountains">لُبْنَانَ</a>! ٱنْظُرِي مِنْ <a name="02200400800022" ll="SDBH:רֹאשׁ:000011:Orientation">رَأْسِ</a> أَمَانَةَ، مِنْ <a name="02200400800028" ll="SDBH:רֹאשׁ:000011:Orientation">رَأْسِ</a> <a name="02200400800030" ll="SDBH:שְׂנִיר:000000:Names of Landforms" il="adr090512334;tbs98139804">شَنِيرَ</a> <a name="02200400800034" ll="SDBH:חֶרְמֹון:000000:Names of Landforms" il="adr090512334;tbs98139804">وَحَرْمُونَ</a>، مِنْ <a name="02200400800038" ll="SDBH:מְעֹנָה:000000:Dwell">خُدُورِ</a> <a name="02200400800040" ll="SDBH:אֲרִי:000000:Wild Animals" il="LB00047;HK00046;Fauna:2.24a;Fauna:2.24b;Fauna:2.24c">ٱلْأُسُودِ</a>، مِنْ جِبَالِ <a name="02200400800046" ll="SDBH:נָמֵר:000000:Wild Animals" il="Fauna:2.23a;Fauna:2.23b">ٱلنُّمُورِ</a>.
\v 9 قَدْ <a name="02200400900002" ll="SDBH:לבב:000001:Love">سَبَيْتِ</a> قَلْبِي يَا <a name="02200400900004" ll="SDBH:אָחֹות:000002:Titles">أُخْتِي</a> <a name="02200400900006" ll="SDBH:כַּלָּה:000001:Kinship" il="ef0078;Realia0139">ٱلْعَرُوسُ</a>. قَدْ <a name="02200400900008" ll="SDBH:לבב:000001:Love">سَبَيْتِ</a> قَلْبِي <a name="02200400900012" ll="SDBH:אֶחָד:000000:Quantity|Frequency">بِإِحْدَى</a> <a name="02200400900020" ll="SDBH:עַיִן:000000:Parts: Creatures">عَيْنَيْكِ</a>، <a name="02200400900026" ll="SDBH:עֲנָק:000000:Jewellery" il="Pearl;Bolen00162;Bolen00307;Bolen00492;Realia0165">بِقَلاَدَةٍ</a> <a name="02200400900016" ll="SDBH:אֶחָד:000000:Quantity|Frequency">وَٱحِدَةٍ</a> مِنْ عُنُقِكِ.
\v 10 مَا <a name="02200401000004" ll="SDBH:יפה:000003:Well">أَحْسَنَ</a> <a name="02200401000006" ll="SDBH:דֹּוד:000001:Love" tl="HOTTP2925">حُبَّكِ</a> يَا <a name="02200401000008" ll="SDBH:אָחֹות:000002:Titles">أُخْتِي</a> <a name="02200401000010" ll="SDBH:כַּלָּה:000001:Kinship" il="ef0078;Realia0139">ٱلْعَرُوسُ</a>! كَمْ <a name="02200401000016" ll="SDBH:דֹּוד:000001:Love">مَحَبَّتُكِ</a> <a name="02200401000014" ll="SDBH:טוב:000000:Good">أَطْيَبُ</a> مِنَ <a name="02200401000020" ll="SDBH:יַיִן:000000:Liquids|Wine|Crops" il="LB00107;HK00108;LB00103;Flora:2.12a;vineyard1679, rp">ٱلْخَمْرِ</a>! وَكَمْ <a name="02200401000024" ll="SDBH:רֵיחַ:000000:Smell">رَائِحَةُ</a> <a name="02200401000026" ll="SDBH:שֶׁמֶן:000000:Liquids" il="NBGbs20100214-0211;il_oil_press_d01;Italian_olive_oil_2007">أَدْهَانِكِ</a> أَطْيَبُ مِنْ كُلِّ <a name="02200401000032" ll="SDBH:בֹּשֶׂם:000001:Spices" il="Flora:4.2.4;Bolen00098;tb091403025">ٱلْأَطْيَابِ</a>!
\v 11 <a name="02200401100006" ll="SDBH:שָׂפָה:000000:">شَفَتَاكِ</a> يَا <a name="02200401100008" ll="SDBH:כַּלָּה:000001:Kinship" il="ef0078;Realia0139">عَرُوسُ</a> <a name="02200401100004" ll="SDBH:נטף:000000:Flow">تَقْطُرَانِ</a> <a name="02200401100002" ll="SDBH:נֹפֶת:000000:Food">شَهْدًا</a>. <a name="02200401100016">تَحْتَ</a> <a name="02200401100018" ll="SDBH:לָשֹׁון:000000:Parts: Creatures">لِسَانِكِ</a> <a name="02200401100010" ll="SDBH:דְּבַשׁ:000000:Dairy" il="Honeycomb1">عَسَلٌ</a> <a name="02200401100014" ll="SDBH:חָלָב:000000:Liquids|Dairy" il="Milk">وَلَبَنٌ</a>، <a name="02200401100022" ll="SDBH:רֵיחַ:000000:Smell">وَرَائِحَةُ</a> <a name="02200401100024" ll="SDBH:שַׂלְמָה:000000:Clothing">ثِيَابِكِ</a> <a name="02200401100028" ll="SDBH:רֵיחַ:000000:Smell">كَرَائِحَةِ</a> <a name="02200401100030" ll="SDBH:לְבָנֹון:000000:Names of Landforms" il="Lebanon_mountains">لُبْنَانَ</a>.
\v 12 <a name="02200401200006" ll="SDBH:אָחֹות:000002:Titles">أُخْتِي</a> <a name="02200401200008" ll="SDBH:כַּלָּה:000001:Kinship" il="ef0078;Realia0139">ٱلْعَرُوسُ</a> <a name="02200401200002" ll="SDBH:גַּן:000000:Landscapes">جَنَّةٌ</a> <a name="02200401200004" ll="SDBH:נעל:000000:Close" il="Realia:3.1.2.1a">مُغْلَقَةٌ</a>، <a name="02200401200010" ll="SDBH:גַּל:000002:Waterbodies" tl="HOTTP2926">عَيْنٌ</a> <a name="02200401200012" ll="SDBH:נעל:000000:Close">مُقْفَلَةٌ</a>، <a name="02200401200014" ll="SDBH:מַעְיָן:000000:Waterbodies">يَنْبُوعٌ</a> <a name="02200401200016" ll="SDBH:חתם:000001:Close">مَخْتُومٌ</a>.
\v 13 أَغْرَاسُكِ <a name="02200401300004" ll="SDBH:פַּרְדֵּס:000000:Landscapes">فِرْدَوْسُ</a> <a name="02200401300006" ll="SDBH:רִמֹּון:000000:Trees|Fruits" il="LB00086;HK00089;Flora:2.10a;Flora:2.10b;NBGbs20110223-026;NBGbs20130730-202">رُمَّانٍ</a> مَعَ <a name="02200401300010" ll="SDBH:פְּרִי:000000:Fruits" il="LB00086;HK00089;Flora:2.10a;Flora:2.10b;Flora:2.11a;NBGbs20110223-026;NBGbs20130730-202">أَثْمَارٍ</a> <a name="02200401300012" ll="SDBH:מֶגֶד:000000:Good">نَفِيسَةٍ</a>، <a name="02200401300014" ll="SDBH:כֹּפֶר:002000:Plants" tl="HOTTP2928" il="Flora:4.1.7a;Flora:4.1.7b">فَاغِيَةٍ</a> <a name="02200401300018" ll="SDBH:נֵרְדְּ:000000:Spices" tl="HOTTP2928" il="HK00117;Flora:4.1.9;Flacon_with_nards_perfume">وَنَارِدِينٍ</a>.
\v 14 <a name="02200401400002" ll="SDBH:נֵרְדְּ:000000:Spices" il="HK00117;Flora:4.1.9;Flacon_with_nards_perfume">نَارِدِينٍ</a> <a name="02200401400006" ll="SDBH:כַּרְכֹּם:000000:Plants" il="Flora:4.2.6">وَكُرْكُمٍ</a>. قَصَبِ <a name="02200401400008" ll="SDBH:קָנֶה:000001:Plants|Spices" il="Flora:4.1.2">ٱلذَّرِيرَةِ</a> <a name="02200401400012" ll="SDBH:קִנָּמֹון:000000:Spices" il="Flora:4.1.4a;Flora:4.1.4b;Flora:4.1.4c;Cinnamomum_verum_spices">وَقِرْفَةٍ</a>، مَعَ كُلِّ <a name="02200401400018" ll="SDBH:עֵץ:000000:Trees">عُودِ</a> ٱللُّبَانِ. <a name="02200401400022" ll="SDBH:מֹר:000000:Spices" il="Flora:4.1.8a;Flora:4.1.8b;Flora:4.1.8c">مُرٌّ</a> <a name="02200401400026" ll="SDBH:אֲהָלֹות:000000:Trees" il="Flora:4.1.1b;Flora:4.1.1c">وَعُودٌ</a> مَعَ كُلِّ <a name="02200401400032" ll="SDBH:רֹאשׁ:000008:Good">أَنْفَسِ</a> <a name="02200401400034" ll="SDBH:בֹּשֶׂם:000001:Spices" il="Flora:4.2.4;Bolen00098;tb091403025">ٱلْأَطْيَابِ</a>.
\v 15 <a name="02200401500002" ll="SDBH:מַעְיָן:000000:Waterbodies">يَنْبُوعُ</a> <a name="02200401500004" ll="SDBH:גַּן:000000:Landscapes" tl="HOTTP2929">جَنَّاتٍ</a>، <a name="02200401500006" ll="SDBH:בְּאֵר:000000:Waterbodies" il="NBGbs20110227-52;tb051407434">بِئْرُ</a> <a name="02200401500008" ll="SDBH:מַיִם:000000:Liquids" il="Water_drop_001">مِيَاهٍ</a> <a name="02200401500010" ll="SDBH:חַי:000002:Stage">حَيَّةٍ</a>، <a name="02200401500014" ll="SDBH:נזל:000001:Waterbodies">وَسُيُولٌ</a> مِنْ <a name="02200401500018" ll="SDBH:לְבָנֹון:000000:Names of Landforms" il="Lebanon_mountains">لُبْنَانَ</a>.
\v 16 <a name="02200401600002" ll="SDBH:עור:000002:Act">اِسْتَيْقِظِي</a> يَا رِيحَ <a name="02200401600004" ll="SDBH:צָפֹון:000002:Air">ٱلشَّمَالِ</a>، <a name="02200401600008">وَتَعَالَيْ</a> يَا رِيحَ <a name="02200401600010" ll="SDBH:תֵּימָן:000001:Blow">ٱلْجَنُوبِ</a>! <a name="02200401600012" ll="SDBH:פוח:000000:Blow">هَبِّي</a> عَلَى <a name="02200401600014" ll="SDBH:גַּן:000000:Landscapes">جَنَّتِي</a> <a name="02200401600016" ll="SDBH:נזל:000000:Flow">فَتَقْطُرَ</a> <a name="02200401600018" ll="SDBH:בֹּשֶׂם:000001:Spices" il="Flora:4.2.4;Bolen00098;tb091403025">أَطْيَابُهَا</a>. <a name="02200401600020">لِيَأْتِ</a> <a name="02200401600022" ll="SDBH:דֹּוד:000000:Love">حَبِيبِي</a> إِلَى <a name="02200401600026" ll="SDBH:גַּן:000000:Landscapes">جَنَّتِهِ</a> <a name="02200401600030" ll="SDBH:אכל:000000:Ingest">وَيَأْكُلْ</a> <a name="02200401600032" ll="SDBH:פְּרִי:000000:Fruits" il="LB00086;HK00089;Flora:2.10a;Flora:2.10b;Flora:2.11a;NBGbs20110223-026;NBGbs20130730-202">ثَمَرَهُ</a> <a name="02200401600034" ll="SDBH:מֶגֶד:000000:Good">ٱلنَّفِيسَ</a>.
\c 5
\v 1 قَدْ <a name="02200500100002">دَخَلْتُ</a> <a name="02200500100006" ll="SDBH:גַּן:000000:Landscapes">جَنَّتِي</a> يَا <a name="02200500100008" ll="SDBH:אָחֹות:000002:Titles">أُخْتِي</a> <a name="02200500100010" ll="SDBH:כַּלָּה:000001:Kinship" il="ef0078;Realia0139">ٱلْعَرُوسُ</a>. <a name="02200500100012" ll="SDBH:ארה:000000:Detach">قَطَفْتُ</a> <a name="02200500100014" ll="SDBH:מֹר:000000:Spices" il="Flora:4.1.8a;Flora:4.1.8b;Flora:4.1.8c">مُرِّي</a> مَعَ طِيبِي. <a name="02200500100020" ll="SDBH:אכל:000000:Ingest">أَكَلْتُ</a> <a name="02200500100022" ll="SDBH:יַעַר:000000:Food" il="Honeycomb1">شَهْدِي</a> مَعَ <a name="02200500100026" ll="SDBH:דְּבַשׁ:000000:Dairy" il="Honeycomb1">عَسَلِي</a>. <a name="02200500100028" ll="SDBH:שׁתה:000000:Ingest">شَرِبْتُ</a> <a name="02200500100030" ll="SDBH:יַיִן:000000:Liquids|Wine|Crops" il="LB00107;HK00108;LB00103;Flora:2.12a;vineyard1679, rp">خَمْرِي</a> مَعَ <a name="02200500100034" ll="SDBH:חָלָב:000000:Liquids|Dairy" il="Milk">لَبَنِي</a>. <a name="02200500100036" ll="SDBH:אכל:000000:Ingest">كُلُوا</a> أَيُّهَا ٱلْأَصْحَابُ. <a name="02200500100040" ll="SDBH:שׁתה:000000:Ingest">ٱشْرَبُوا</a> <a name="02200500100044" ll="SDBH:שׁכר:000000:Ingest">وَٱسْكَرُوا</a> أَيُّهَا <a name="02200500100046" ll="SDBH:דֹּוד:000000:Love;SDBH:דֹּוד:000001:Love">ٱلْأَحِبَّاءُ</a>.
\v 2 أَنَا <a name="02200500200004" ll="SDBH:יָשֵׁן:000000:Asleep">نَائِمَةٌ</a> <a name="02200500200008" ll="SDBH:לֵב:000001:Think">وَقَلْبِي</a> <a name="02200500200010" ll="SDBH:עור:000000:Awake">مُسْتَيْقِظٌ</a>. <a name="02200500200012">صَوْتُ</a> <a name="02200500200014" ll="SDBH:דֹּוד:000000:Love">حَبِيبِي</a> <a name="02200500200016" ll="SDBH:דפק:000000:Impact">قَارِعًا</a>: <a name="02200500200018" ll="SDBH:פתח:000002:Open">اِفْتَحِي</a> لِي يَا <a name="02200500200022" ll="SDBH:אָחֹות:000002:Titles">أُخْتِي</a>، يَا <a name="02200500200024" ll="SDBH:רַעְיָה:000000:Friends">حَبِيبَتِي</a>، يَا <a name="02200500200026" ll="SDBH:יֹונָה:000000:Birds" il="HK00064;LB00068;BK00066;BK00067;Fauna:3.7a;Fauna:3.7b;Rock_pigeons;Columba_livia">حَمَامَتِي</a>، يَا <a name="02200500200028" ll="SDBH:תָּם:000001:Good">كَامِلَتِي</a>! لأَنَّ <a name="02200500200032" ll="SDBH:רֹאשׁ:000000:Parts: Creatures">رَأْسِي</a> <a name="02200500200034" ll="SDBH:מלא:000000:Contain">ٱمْتَلأَ</a> مِنَ <a name="02200500200036" ll="SDBH:טַל:000000:Rain" il="Dew;Bolen00154">ٱلطَّلِّ</a>، <a name="02200500200038" ll="SDBH:קְוֻצֹּות:000000:Parts: Creatures">وَقُصَصِي</a> مِنْ <a name="02200500200040" ll="SDBH:רָסִיס:000000:Wet">نُدَى</a> <a name="02200500200042" ll="SDBH:לַיְלָה:000000:Time">ٱللَّيْلِ</a>.
\v 3 قَدْ <a name="02200500300002" ll="SDBH:פשׁט:000002:Undress">خَلَعْتُ</a> <a name="02200500300006" ll="SDBH:כֻּתֹּנֶת:000002:Clothing" il="NBGmk20140519a">ثَوْبِي</a>، <a name="02200500300008" ll="SDBH:אֵיכָכָה:000000:Event Referents: Modification">فَكَيْفَ</a> <a name="02200500300010">أَلْبَسُهُ</a>؟ قَدْ <a name="02200500300012" ll="SDBH:רחץ:000000:Clean">غَسَلْتُ</a> <a name="02200500300016" ll="SDBH:רֶגֶל:000000:Parts: Creatures">رِجْلَيَّ</a>، <a name="02200500300018" ll="SDBH:אֵיכָכָה:000000:Event Referents: Modification">فَكَيْفَ</a> <a name="02200500300020" ll="SDBH:טנף:000000:Unclean">أُوَسِّخُهُمَا</a>؟
\v 4 <a name="02200500400002" ll="SDBH:דֹּוד:000000:Love">حَبِيبِي</a> <a name="02200500400004" ll="SDBH:שׁלח:000015:Stance">مَدَّ</a> <a name="02200500400006">يَدَهُ</a> <a name="02200500400008">مِنَ</a> <a name="02200500400012" ll="SDBH:חֹר:000000:Parts: Scenery">ٱلْكَوَّةِ</a>، <a name="02200500400018" ll="SDBH:המה:000000:Sounds">فَأَنَّتْ</a> عَلَيْهِ <a name="02200500400016" ll="SDBH:מֵעֶה:000000:Parts: People">أَحْشَائِي</a>.
\v 5 <a name="02200500500002">قُمْتُ</a> <a name="02200500500008" ll="SDBH:פתח:000002:Open">لأَفْتَحَ</a> <a name="02200500500012" ll="SDBH:דֹּוד:000000:Love">لِحَبِيبِي</a> <a name="02200500500016">وَيَدَايَ</a> <a name="02200500500018" ll="SDBH:נטף:000000:Flow">تَقْطُرَانِ</a> <a name="02200500500020" ll="SDBH:מֹר:000000:Spices" il="Flora:4.1.8a;Flora:4.1.8b;Flora:4.1.8c">مُرًّا</a>، <a name="02200500500024" ll="SDBH:אֶצְבַּע:000000:Parts: Creatures">وَأَصَابِعِي</a> <a name="02200500500026" ll="SDBH:מֹר:000000:Spices" il="Flora:4.1.8a;Flora:4.1.8b;Flora:4.1.8c">مُرٌّ</a> <a name="02200500500028">قَاطِرٌ</a> عَلَى <a name="02200500500032" ll="SDBH:כַּף:000011:Shape">مَقْبَضِ</a> <a name="02200500500036" ll="SDBH:מַנְעוּל:000000:Parts: Constructions" il="Realia:3.1.2.1a">ٱلْقُفْلِ</a>.
\v 6 <a name="02200500600002" ll="SDBH:פתח:000002:Open">فَتَحْتُ</a> <a name="02200500600008" ll="SDBH:דֹּוד:000000:Love">لِحَبِيبِي</a>، لكِنَّ <a name="02200500600012" ll="SDBH:דֹּוד:000000:Love">حَبِيبِي</a> <a name="02200500600014" ll="SDBH:חמק:000000:Stance">تَحَوَّلَ</a> <a name="02200500600016" ll="SDBH:עבר:000000:Move">وَعَبَرَ</a>. <a name="02200500600018" ll="SDBH:נֶפֶשׁ:000001:Parts: Creatures">نَفْسِي</a> <a name="02200500600020" ll="SDBH:יצא:000003:Afraid">خَرَجَتْ</a> عِنْدَمَا <a name="02200500600024" ll="SDBH:דבר:001000:Speak" tl="HOTTP2930">أَدْبَرَ</a>. <a name="02200500600026" ll="SDBH:בקשׁ:000000:Search">طَلَبْتُهُ</a> فَمَا <a name="02200500600032" ll="SDBH:מצא:000000:Search">وَجَدْتُهُ</a>. <a name="02200500600034" ll="SDBH:קרא:000003:Move">دَعَوْتُهُ</a> فَمَا <a name="02200500600040" ll="SDBH:ענה:000000:Speak">أَجَابَنِي</a>.
