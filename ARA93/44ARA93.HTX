\v 17 <a name="04400701700002" ll="SDBG:καθώς:000003">Como</a>, <a name="04400701700004">porém</a>, se <a name="04400701700006" ll="SDBG:ἐγγίζω:000001">aproximasse</a> o <a name="04400701700010" ll="SDBG:χρόνος:000001">tempo</a> da <a name="04400701700014" ll="SDBG:ἐπαγγελία:000000">promessa</a> <a name="04400701700016" ll="SDBG:ὅς:000000">que</a> <a name="04400701700022" ll="SDBG:θεός:000000">Deus</a> jurou a <a name="04400701700026" ll="SDBG:Ἀβραάμ:000000" il="Three_Patriarchs_in_Paradise;adr1305165446">Abraão</a>, o <a name="04400701700032" ll="SDBG:λαός:000001">povo</a> <a name="04400701700028" ll="SDBG:αὐξάνω:000000">cresceu</a> <a name="04400701700034" ll="SDBG:καί:000000">e</a> se <a name="04400701700036" ll="SDBG:πληθύνω:000000">multiplicou</a> <a name="04400701700038" ll="SDBG:ἐν:000000">no</a> <a name="04400701700040" ll="SDBG:Αἴγυπτος:000000" il="NBGbs20140204-40">Egito</a>, 
\v 18 até <a name="04400701800002" ll="SDBG:ἄχρι(ς):000000">que</a> se <a name="04400701800006" ll="SDBG:ἀνίστημι:000005">levantou</a> ali <a name="04400701800010" ll="SDBG:ἕτερος:000001">outro</a> <a name="04400701800008" ll="SDBG:βασιλεύς:000000">rei</a>, <a name="04400701800004" ll="SDBG:ὅς:000000">que</a> <a name="04400701800018" ll="SDBG:οὐ:000000">não</a> conhecia a <a name="04400701800024" ll="SDBG:Ἰωσήφ:000000">José</a>. 
\v 19 Este outro <a name="04400701900002" ll="SDBG:οὗτος:000000">rei</a> tratou com <a name="04400701900004" ll="SDBG:κατασοφίζομαι:000000">astúcia</a> a nossa <a name="04400701900008" ll="SDBG:γένος:000001">raça</a> e <a name="04400701900012" ll="SDBG:κακόω:000000">torturou</a> os nossos <a name="04400701900016" ll="SDBG:πατήρ:000002">pais</a>, a ponto de forçá-los <a name="04400701900022" ll="SDBG:ποιέω:000002">a</a> <a name="04400701900028" ll="SDBG:ἔκθετος:000000">enjeitar</a> seus <a name="04400701900026" ll="SDBG:βρέφος:000000" il="Children_of_Kabul,_Afghanistan">filhos</a>, para que <a name="04400701900036" ll="SDBG:μή:000000">não</a> sobrevivessem. 
\v 20 <a name="04400702000002" ll="SDBG:ἐν:000019">Por</a> <a name="04400702000004" ll="SDBG:ὅς:000000">esse</a> <a name="04400702000006" ll="SDBG:καιρός:000000">tempo</a>, <a name="04400702000008" ll="SDBG:γεννάω:000001">nasceu</a> Moisés, que era formoso aos olhos <a name="04400702000016" ll="SDBG:ἀστεῖος:000000">de</a> <a name="04400702000020" ll="SDBG:θεός:000000">Deus</a>. <a name="04400702000022" ll="SDBG:ὅς:000000">Por</a> <a name="04400702000028" ll="SDBG:τρεῖς:000000">três</a> meses, foi ele <a name="04400702000024" ll="SDBG:ἀνατρέφω:000000">mantido</a> <a name="04400702000030" ll="SDBG:ἐν:000000">na</a> <a name="04400702000034" ll="SDBG:οἶκος:000001">casa</a> de seu <a name="04400702000038" ll="SDBG:πατήρ:000000;SDBG:πατήρ:000001">pai</a>; 
\v 21 quando foi <a name="04400702100002" ll="SDBG:ἐκτίθημι:000000">exposto</a>, a <a name="04400702100014" ll="SDBG:θυγάτηρ:000000">filha</a> de <a name="04400702100016" ll="SDBG:Φαραώ:000000">Faraó</a> <a name="04400702100006" ll="SDBG:αὐτός:001000">o</a> <a name="04400702100008" ll="SDBG:ἀναιρέω:000002">recolheu</a> <a name="04400702100018" ll="SDBG:καί:000001">e</a> <a name="04400702100010;04400702100020" ll="SDBG:αὐτός:001000;SDBG:ἀνατρέφω:000000">criou</a> <a name="04400702100026">como</a> seu <a name="04400702100024" ll="SDBG:ἑαυτοῦ:000000">próprio</a> <a name="04400702100028" ll="SDBG:υἱός:000000">filho</a>. 
\v 22 <a name="04400702200002;04400702200018" ll="SDBG:δέ:000000">E</a> Moisés foi <a name="04400702200004" ll="SDBG:παιδεύω:000000">educado</a> em <a name="04400702200010" ll="SDBG:πᾶς:000000">toda</a> a <a name="04400702200012" ll="SDBG:σοφία:000001">ciência</a> dos <a name="04400702200014" ll="SDBG:Αἰγύπτιος:000000" il="NBGbs20140204-40">egípcios</a> <a name="04400702200022;04400702200026" ll="SDBG:καί:000000;SDBG:ἐν:000010">e</a> era <a name="04400702200020" ll="SDBG:δυνατός:000002">poderoso</a> <a name="04400702200008" ll="SDBG:ἐν:000010">em</a> <a name="04400702200024" ll="SDBG:λόγος:000000">palavras</a> e <a name="04400702200028" ll="SDBG:ἔργον:000000">obras</a>.
    
\v 23 <a name="04400702300002;04400702300008" ll="SDBG:αὐτός:001000;SDBG:ὡς:000003">Quando</a> <a name="04400702300006" ll="SDBG:πληρόω:000008">completou</a> quarenta anos, <a name="04400702300014" ll="SDBG:ἀναβαίνω:000004">veio</a>-lhe a ideia <a name="04400702300016;04400702300020;04400702300022" ll="SDBG:αὐτός:001000;SDBG:καρδία:000000">de</a> <a name="04400702300024" ll="SDBG:ἐπισκέπτομαι:000001">visitar</a> seus <a name="04400702300028" ll="SDBG:ἀδελφός:000003">irmãos</a>, os <a name="04400702300034" ll="SDBG:υἱός:000008">filhos</a> de <a name="04400702300036" ll="SDBG:Ἰσραήλ:000001">Israel</a>. 
\v 24 Vendo um <a name="04400702400006" ll="SDBG:τὶς:000000">homem</a> tratado <a name="04400702400008" ll="SDBG:ἀδικέω:000002">injustamente</a>, tomou-lhe a <a name="04400702400010" ll="SDBG:ἀμύνομαι:000000">defesa</a> <a name="04400702400002" ll="SDBG:καί:000001">e</a> <a name="04400702400014;04400702400016" ll="SDBG:ποιέω:000000;SDBG:ἐκδίκησις:000000;SDBG:ἐκδίκησις:000001">vingou</a> o <a name="04400702400020" ll="SDBG:καταπονέω:000000">oprimido</a>, <a name="04400702400022" ll="SDBG:πατάσσω:000001">matando</a> o <a name="04400702400026" ll="SDBG:Αἰγύπτιος:001000" il="NBGbs20140204-40">egípcio</a>. 
\v 25 <a name="04400702500004">Ora</a>, Moisés <a name="04400702500002" ll="SDBG:νομίζω:000000">cuidava</a> que seus <a name="04400702500010" ll="SDBG:ἀδελφός:000002">irmãos</a> <a name="04400702500006" ll="SDBG:συνίημι:000000">entenderiam</a> <a name="04400702500014" ll="SDBG:ὅτι:000000">que</a> <a name="04400702500018" ll="SDBG:θεός:000000">Deus</a> <a name="04400702500012" ll="SDBG:αὐτός:001000">os</a> queria <a name="04400702500026;04400702500028" ll="SDBG:δίδωμι:000000;SDBG:σωτηρία:000000">salvar</a> por <a name="04400702500020" ll="SDBG:διά:000001">intermédio</a> <a name="04400702500024" ll="SDBG:αὐτός:001000">dele</a>; eles, <a name="04400702500034" ll="SDBG:δέ:000002">porém</a>, <a name="04400702500036" ll="SDBG:οὐ:000000">não</a> <a name="04400702500038" ll="SDBG:συνίημι:000000">compreenderam</a>. 
\v 26 No <a name="04400702600008" ll="SDBG:ἡμέρα:000001">dia</a> seguinte, aproximou-se de <a name="04400702600012" ll="SDBG:αὐτός:001000">uns</a> que <a name="04400702600014" ll="SDBG:μάχομαι:000000">brigavam</a> <a name="04400702600016" ll="SDBG:καί:000001">e</a> procurou <a name="04400702600020" ll="SDBG:αὐτός:001000">reconduzi</a>-los <a name="04400702600022" ll="SDBG:εἰς:000010">à</a> <a name="04400702600024" ll="SDBG:εἰρήνη:000000">paz</a>, dizendo: <a name="04400702600028" ll="SDBG:ἀνήρ:000000">Homens</a>, vós sois <a name="04400702600030" ll="SDBG:ἀδελφός:000002">irmãos</a>; por <a name="04400702600034" ll="SDBG:ἱνατί:000000">que</a> vos <a name="04400702600036" ll="SDBG:ἀδικέω:000000">ofendeis</a> uns aos <a name="04400702600038" ll="SDBG:ἀλλήλων:000000">outros</a>? 
\v 27 <a name="04400702700004" ll="SDBG:δέ:000002">Mas</a> o que <a name="04400702700006" ll="SDBG:ἀδικέω:000000">agredia</a> o <a name="04400702700010" ll="SDBG:πλησίον:000001">próximo</a> <a name="04400702700014" ll="SDBG:αὐτός:001000">o</a> repeliu, dizendo: <a name="04400702700018" ll="SDBG:τίς:000000">Quem</a> te <a name="04400702700022" ll="SDBG:καθίστημι:000000">constituiu</a> <a name="04400702700024" ll="SDBG:ἄρχων:000000" il="tb011506651;tb041705529;tbs91069704">autoridade</a> <a name="04400702700026" ll="SDBG:καί:000000">e</a> <a name="04400702700028" ll="SDBG:δικαστής:000000">juiz</a> <a name="04400702700030" ll="SDBG:ἐπί:000012">sobre</a> nós? 
\v 28 <a name="04400702800002;04400702800008" ll="SDBG:μή:000001;SDBG:σύ:000000">Acaso</a>, <a name="04400702800010" ll="SDBG:θέλω:000000;SDBG:θέλω:000002">queres</a> <a name="04400702800004" ll="SDBG:ἀναιρέω:000000">matar</a>-me, <a name="04400702800012" ll="SDBG:ὅς:000000">como</a> <a name="04400702800016" ll="SDBG:ἀναιρέω:000000">fizeste</a> ontem ao <a name="04400702800022" ll="SDBG:Αἰγύπτιος:001000" il="NBGbs20140204-40">egípcio</a>? 
\v 29 <a name="04400702900008" ll="SDBG:ἐν:000016">A</a> estas <a name="04400702900012" ll="SDBG:λόγος:000000">palavras</a> Moisés <a name="04400702900002" ll="SDBG:φεύγω:000000">fugiu</a> <a name="04400702900016">e</a> <a name="04400702900018" ll="SDBG:γίνομαι:000002">tornou</a>-se <a name="04400702900020" ll="SDBG:πάροικος:000000">peregrino</a> <a name="04400702900022" ll="SDBG:ἐν:000000">na</a> <a name="04400702900024" ll="SDBG:γῆ:000003">terra</a> de Midiã, <a name="04400702900028" ll="SDBG:οὗ:000000">onde</a> lhe <a name="04400702900030" ll="SDBG:γεννάω:000000">nasceram</a> <a name="04400702900034" ll="SDBG:δύο:000000">dois</a> <a name="04400702900032" ll="SDBG:υἱός:000000">filhos</a>.
    
\v 30 <a name="04400703000004" ll="SDBG:πληρόω:000008">Decorridos</a> quarenta <a name="04400703000006" ll="SDBG:ἔτος:000000;SDBG:ἔτος:000001">anos</a>, <a name="04400703000012" ll="SDBG:αὐτός:001000">apareceu</a>-lhe, <a name="04400703000014" ll="SDBG:ἐν:000000">no</a> <a name="04400703000018" ll="SDBG:ἔρημος:000001" il="PikiWiki_Israel_32657;RS26447_ISR91T-418;tb113006604;ws092715042">deserto</a> do <a name="04400703000022" ll="SDBG:ὄρος:000000" il="20100409_korinthos03;jo-kafrein_f01;tb011017754;tb031315454">monte</a> <a name="04400703000024" ll="SDBG:Σινᾶ:000000" il="Sinai-191;Sinai-3;Sinai190">Sinai</a>, um <a name="04400703000026" ll="SDBG:ἄγγελος:000001" il="Kneeling angel;tb031115118">anjo</a>, por <a name="04400703000028" ll="SDBG:ἐν:000007">entre</a> as <a name="04400703000030" ll="SDBG:φλόξ:000000">chamas</a> de uma sarça que <a name="04400703000032" ll="SDBG:πῦρ:000000" il="Fire02">ardia</a>. 
\v 31 Moisés, <a name="04400703100004">porém</a>, diante daquela <a name="04400703100014" ll="SDBG:ὅραμα:000000">visão</a>, ficou <a name="04400703100010" ll="SDBG:θαυμάζω:000000">maravilhado</a> <a name="04400703100018" ll="SDBG:δέ:000001">e</a>, <a name="04400703100016" ll="SDBG:προσέρχομαι:000000">aproximando</a>-se para <a name="04400703100022" ll="SDBG:κατανοέω:000000;SDBG:κατανοέω:000002">observar</a>, <a name="04400703100020" ll="SDBG:αὐτός:001000">ouviu</a>-se a <a name="04400703100026" ll="SDBG:φωνή:000001">voz</a> do <a name="04400703100028" ll="SDBG:κύριος:000000">Senhor</a>: 
\v 32 Eu <a name="04400703200002" ll="SDBG:ἐγώ:000000">sou</a> o <a name="04400703200006" ll="SDBG:θεός:000000">Deus</a> dos teus <a name="04400703200010" ll="SDBG:πατήρ:000002">pais</a>, o <a name="04400703200016" ll="SDBG:θεός:000000">Deus</a> de <a name="04400703200018" ll="SDBG:Ἀβραάμ:000000" il="Three_Patriarchs_in_Paradise;adr1305165446">Abraão</a>, de <a name="04400703200022" ll="SDBG:Ἰσαάκ:000000">Isaque</a> <a name="04400703200020" ll="SDBG:καί:000000">e</a> de <a name="04400703200026" ll="SDBG:Ἰακώβ:000000">Jacó</a>. Moisés, tremendo de <a name="04400703200028;04400703200032" ll="SDBG:γίνομαι:000002;SDBG:ἔντρομος:000000">medo</a>, <a name="04400703200036" ll="SDBG:οὐ:000000">não</a> <a name="04400703200038" ll="SDBG:τολμάω:000000">ousava</a> <a name="04400703200040" ll="SDBG:κατανοέω:000003">contemplá</a>-la. 
\v 33 <a name="04400703300006" ll="SDBG:αὐτός:001000">Disse</a>-lhe o <a name="04400703300010" ll="SDBG:κύριος:000000">Senhor</a>: <a name="04400703300012" ll="SDBG:λύω:000000">Tira</a> a <a name="04400703300016" ll="SDBG:ὑπόδημα:000000" il="LB00161;NBGmk20140519e">sandália</a> dos <a name="04400703300020" ll="SDBG:πούς:000000">pés</a>, <a name="04400703300026" ll="SDBG:γάρ:000000">porque</a> o <a name="04400703300028" ll="SDBG:τόπος:000000">lugar</a> em <a name="04400703300032" ll="SDBG:ὅς:000000">que</a> <a name="04400703300034" ll="SDBG:ἵστημι:000006">estás</a> é <a name="04400703300036" ll="SDBG:γῆ:000001">terra</a> <a name="04400703300038" ll="SDBG:ἅγιος:000000">santa</a>. 
\v 34 Vi, com efeito, o <a name="04400703400008" ll="SDBG:κάκωσις:000000">sofrimento</a> do meu <a name="04400703400006;04400703400012" ll="SDBG:λαός:000001;SDBG:ὁ:000000">povo</a> <a name="04400703400018" ll="SDBG:ἐν:000000">no</a> <a name="04400703400020" ll="SDBG:Αἴγυπτος:000000" il="NBGbs20140204-40">Egito</a>, <a name="04400703400030" ll="SDBG:ἀκούω:000000">ouvi</a> o <a name="04400703400028" ll="SDBG:αὐτός:001000">seu</a> <a name="04400703400026" ll="SDBG:στεναγμός:000000">gemido</a> <a name="04400703400022" ll="SDBG:καί:000000">e</a> <a name="04400703400034" ll="SDBG:καταβαίνω:000000">desci</a> para <a name="04400703400036" ll="SDBG:ἐξαιρέω:000001">libertá</a>-lo. <a name="04400703400044" ll="SDBG:δεῦρο:000000">Vem</a> <a name="04400703400042" ll="SDBG:νῦν:000000">agora</a>, <a name="04400703400032">e</a> eu te <a name="04400703400046" ll="SDBG:ἀποστέλλω:000000">enviarei</a> <a name="04400703400050" ll="SDBG:εἰς:000000">ao</a> <a name="04400703400052" ll="SDBG:Αἴγυπτος:000000" il="NBGbs20140204-40">Egito</a>.
    
\v 35 A este Moisés, a <a name="04400703500008;04400703500014" ll="SDBG:τίς:000000;SDBG:ὅς:000000">quem</a> negaram <a name="04400703500010" ll="SDBG:ἀρνέομαι:000004">reconhecer</a>, dizendo: Quem te <a name="04400703500018" ll="SDBG:καθίστημι:000000">constituiu</a> <a name="04400703500020" ll="SDBG:ἄρχων:000000" il="tb011506651;tb041705529;tbs91069704">autoridade</a> <a name="04400703500022" ll="SDBG:καί:000000">e</a> <a name="04400703500024" ll="SDBG:δικαστής:000000">juiz</a>? A este <a name="04400703500040" ll="SDBG:ἀποστέλλω:000000">enviou</a> <a name="04400703500030" ll="SDBG:θεός:000000">Deus</a> como <a name="04400703500034" ll="SDBG:ἄρχων:000000" il="tb011506651;tb041705529;tbs91069704">chefe</a> <a name="04400703500032" ll="SDBG:καί:000005">e</a> <a name="04400703500038" ll="SDBG:λυτρωτής:000000">libertador</a>, <a name="04400703500054" ll="SDBG:ἐν:000000">com</a> a <a name="04400703500044" ll="SDBG:χείρ:000005">assistência</a> do <a name="04400703500046" ll="SDBG:ἄγγελος:000001" il="Kneeling angel;tb031115118">anjo</a> <a name="04400703500004" ll="SDBG:ὁ:000000">que</a> <a name="04400703500052" ll="SDBG:αὐτός:001000">lhe</a> apareceu na sarça. 
\v 36 <a name="04400703600002" ll="SDBG:οὗτος:000000">Este</a> <a name="04400703600006" ll="SDBG:αὐτός:001000">os</a> <a name="04400703600004" ll="SDBG:ἐξάγω:000000">tirou</a>, <a name="04400703600008" ll="SDBG:ποιέω:000000">fazendo</a> <a name="04400703600010" ll="SDBG:τέρας:000000">prodígios</a> <a name="04400703600012" ll="SDBG:καί:000000">e</a> <a name="04400703600014" ll="SDBG:σημεῖον:000000">sinais</a> <a name="04400703600016" ll="SDBG:ἐν:000000">na</a> <a name="04400703600018" ll="SDBG:γῆ:000003">terra</a> do <a name="04400703600020" ll="SDBG:Αἴγυπτος:000000" il="NBGbs20140204-40">Egito</a>, assim <a name="04400703600022" ll="SDBG:καί:000001">como</a> <a name="04400703600024" ll="SDBG:ἐν:000003">no</a> <a name="04400703600028" ll="SDBG:θάλασσα:000001">mar</a> <a name="04400703600026" ll="SDBG:ἐρυθρός:000000" il="NBGbs20120204-08;P2060003;aqaba P1050440;tb052505130">Vermelho</a> <a name="04400703600030" ll="SDBG:καί:000001">e</a> <a name="04400703600032" ll="SDBG:ἐν:000000">no</a> <a name="04400703600036" ll="SDBG:ἔρημος:000001" il="PikiWiki_Israel_32657;RS26447_ISR91T-418;tb113006604;ws092715042">deserto</a>, durante quarenta <a name="04400703600038" ll="SDBG:ἔτος:000000;SDBG:ἔτος:000001">anos</a>. 
\v 37 Foi Moisés <a name="04400703700002;04400703700006" ll="SDBG:οὗτος:000000;SDBG:ὁ:000000">quem</a> disse aos <a name="04400703700016" ll="SDBG:υἱός:000008">filhos</a> de <a name="04400703700018" ll="SDBG:Ἰσραήλ:000001">Israel</a>: <a name="04400703700028" ll="SDBG:θεός:000000">Deus</a> vos <a name="04400703700024" ll="SDBG:ἀνίστημι:000000">suscitará</a> <a name="04400703700030" ll="SDBG:ἐκ:000007">dentre</a> vossos <a name="04400703700034" ll="SDBG:ἀδελφός:000002">irmãos</a> um <a name="04400703700020" ll="SDBG:προφήτης:000000">profeta</a> <a name="04400703700038" ll="SDBG:ὡς:000000">semelhante</a> a mim. 
\v 38 É <a name="04400703800002" ll="SDBG:οὗτος:000000">este</a> Moisés quem <a name="04400703800008" ll="SDBG:γίνομαι:000007">esteve</a> <a name="04400703800010" ll="SDBG:ἐν:000000">na</a> <a name="04400703800014" ll="SDBG:ἐκκλησία:000002">congregação</a> <a name="04400703800016" ll="SDBG:ἐν:000000">no</a> <a name="04400703800020" ll="SDBG:ἔρημος:000001" il="PikiWiki_Israel_32657;RS26447_ISR91T-418;tb113006604;ws092715042">deserto</a>, <a name="04400703800022" ll="SDBG:μετά:000000">com</a> o <a name="04400703800026" ll="SDBG:ἄγγελος:000001" il="Kneeling angel;tb031115118">anjo</a> <a name="04400703800006" ll="SDBG:ὁ:000000">que</a> <a name="04400703800032" ll="SDBG:αὐτός:001000">lhe</a> <a name="04400703800030" ll="SDBG:λαλέω:000000">falava</a> <a name="04400703800034" ll="SDBG:ἐν:000003">no</a> <a name="04400703800038" ll="SDBG:ὄρος:000000" il="20100409_korinthos03;jo-kafrein_f01;tb011017754;tb031315454">monte</a> <a name="04400703800040" ll="SDBG:Σινᾶ:000000" il="Sinai-191;Sinai-3;Sinai190">Sinai</a> <a name="04400703800042" ll="SDBG:καί:000000">e</a> com os nossos <a name="04400703800046" ll="SDBG:πατήρ:000002">pais</a>; o <a name="04400703800050" ll="SDBG:ὅς:000000">qual</a> <a name="04400703800052" ll="SDBG:δέχομαι:000000">recebeu</a> <a name="04400703800054" ll="SDBG:λόγιον:000000">palavras</a> <a name="04400703800056" ll="SDBG:ζάω:000000">vivas</a> <a name="04400703800058" ll="SDBG:δίδωμι:000000">para</a> no-las transmitir. 
\v 39 A <a name="04400703900002" ll="SDBG:ὅς:000000">quem</a> nossos <a name="04400703900014" ll="SDBG:πατήρ:000002">pais</a> <a name="04400703900004" ll="SDBG:οὐ:000000">não</a> <a name="04400703900006" ll="SDBG:θέλω:000002">quiseram</a> <a name="04400703900008;04400703900010" ll="SDBG:γίνομαι:000002;SDBG:ὑπήκοος:000000">obedecer</a>; <a name="04400703900018" ll="SDBG:ἀλλά:000000">antes</a>, o repeliram <a name="04400703900022" ll="SDBG:καί:000002">e</a>, no seu <a name="04400703900030" ll="SDBG:καρδία:000000">coração</a>, <a name="04400703900024" ll="SDBG:στρέφω:000006">voltaram</a> <a name="04400703900034" ll="SDBG:εἰς:000000">para</a> o <a name="04400703900036" ll="SDBG:Αἴγυπτος:000000" il="NBGbs20140204-40">Egito</a>, 
\v 40 dizendo a <a name="04400704000006" ll="SDBG:Ἀαρών:000000" il="tb053008887">Arão</a>: <a name="04400704000008" ll="SDBG:ποιέω:000004">Faze</a>-nos <a name="04400704000012" ll="SDBG:θεός:000001">deuses</a> <a name="04400704000014" ll="SDBG:ὅς:000000">que</a> vão adiante <a name="04400704000016" ll="SDBG:προπορεύομαι:000001">de</a> nós; <a name="04400704000022" ll="SDBG:γάρ:000000">porque</a>, quanto a <a name="04400704000026" ll="SDBG:οὗτος:000000">este</a> Moisés, <a name="04400704000028" ll="SDBG:ὅς:000000">que</a> nos <a name="04400704000030" ll="SDBG:ἐξάγω:000000">tirou</a> <a name="04400704000034" ll="SDBG:ἐκ:000000">da</a> <a name="04400704000036" ll="SDBG:γῆ:000003">terra</a> do <a name="04400704000038" ll="SDBG:Αἴγυπτος:000000" il="NBGbs20140204-40">Egito</a>, <a name="04400704000040" ll="SDBG:οὐ:000000">não</a> sabemos o <a name="04400704000044" ll="SDBG:τίς:000000">que</a> <a name="04400704000048" ll="SDBG:αὐτός:001000">lhe</a> <a name="04400704000046" ll="SDBG:γίνομαι:000003">aconteceu</a>. 
\v 41 <a name="04400704100006;04400704100012" ll="SDBG:ἐκεῖνος:000000;SDBG:ἐν:000019">Naqueles</a> <a name="04400704100010" ll="SDBG:ἡμέρα:000002">dias</a>, fizeram um <a name="04400704100004" ll="SDBG:μοσχοποιέω:000000" il="tb032014107">bezerro</a> <a name="04400704100002">e</a> <a name="04400704100016" ll="SDBG:ἀνάγω:000001">ofereceram</a> <a name="04400704100018" ll="SDBG:θυσία:000000" il="LB00253">sacrifício</a> ao <a name="04400704100022" ll="SDBG:εἴδωλον:000000" il="Realia:4.6b;adr1305227007">ídolo</a>, <a name="04400704100026" ll="SDBG:εὐφραίνω:000001;SDBG:εὐφραίνω:000002">alegrando</a>-se <a name="04400704100028" ll="SDBG:ἐν:000010">com</a> as <a name="04400704100032" ll="SDBG:ἔργον:000002">obras</a> das suas <a name="04400704100036" ll="SDBG:χείρ:000000">mãos</a>. 
\v 42 <a name="04400704200004">Mas</a> <a name="04400704200008" ll="SDBG:θεός:000000">Deus</a> se <a name="04400704200002" ll="SDBG:στρέφω:000004">afastou</a> <a name="04400704200010">e</a> <a name="04400704200014" ll="SDBG:αὐτός:001000">os</a> <a name="04400704200012" ll="SDBG:παραδίδωμι:000001">entregou</a> ao <a name="04400704200016" ll="SDBG:λατρεύω:000000">culto</a> da <a name="04400704200020" ll="SDBG:στρατιά:000001">milícia</a> <a name="04400704200024" ll="SDBG:οὐρανός:000000;SDBG:οὐρανός:000005">celestial</a>, <a name="04400704200026" ll="SDBG:καθώς:000002">como</a> está <a name="04400704200028" ll="SDBG:γράφω:000001">escrito</a> <a name="04400704200030" ll="SDBG:ἐν:000020">no</a> <a name="04400704200032" ll="SDBG:βίβλος:000001" il="HK00153">Livro</a> dos <a name="04400704200036" ll="SDBG:προφήτης:000000">Profetas</a>:Ó <a name="04400704200060" ll="SDBG:οἶκος:000004">casa</a> de <a name="04400704200062" ll="SDBG:Ἰσραήλ:000001">Israel</a>, <a name="04400704200038" ll="SDBG:μή:000001">porventura</a>, me <a name="04400704200046" ll="SDBG:προσφέρω:000002">oferecestes</a> <a name="04400704200040" ll="SDBG:σφάγιον:000000" il="LB00253">vítimas</a> <a name="04400704200042" ll="SDBG:καί:000000">e</a> <a name="04400704200044" ll="SDBG:θυσία:000000" il="LB00253">sacrifícios</a> <a name="04400704200054" ll="SDBG:ἐν:000000">no</a> <a name="04400704200058" ll="SDBG:ἔρημος:000001" il="PikiWiki_Israel_32657;RS26447_ISR91T-418;tb113006604;ws092715042">deserto</a>, pelo espaço de quarenta <a name="04400704200050" ll="SDBG:ἔτος:000000;SDBG:ἔτος:000001">anos</a>,
    
\v 43 <a name="04400704300002">e</a>, acaso, não <a name="04400704300004" ll="SDBG:ἀναλαμβάνω:000000">levantastes</a> o <a name="04400704300008" ll="SDBG:σκηνή:000000">tabernáculo</a> de Moloque <a name="04400704300014" ll="SDBG:καί:000000">e</a> a <a name="04400704300018" ll="SDBG:ἄστρον:000000;SDBG:ἄστρον:000001">estrela</a> do <a name="04400704300022" ll="SDBG:θεός:000001">deus</a> Renfã, <a name="04400704300030" ll="SDBG:τύπος:000001">figuras</a> <a name="04400704300032" ll="SDBG:ὅς:000000">que</a> <a name="04400704300034" ll="SDBG:ποιέω:000004">fizestes</a> para <a name="04400704300038" ll="SDBG:αὐτός:001000">as</a> <a name="04400704300036" ll="SDBG:προσκυνέω:000000">adorar</a>? Por <a name="04400704300040">isso</a>, vos desterrarei <a name="04400704300042" ll="SDBG:μετοικίζω:000000">para</a> <a name="04400704300046" ll="SDBG:ἐπέκεινα:000000">além</a> da <a name="04400704300048" ll="SDBG:Βαβυλών:000000" il="HK00224;Ishtar_gate;jc1229149058;mat07384;mat13229;tb112004093-2">Babilônia</a>.
    
\v 44 <a name="04400704400038" ll="SDBG:αὐτός:001000">O</a> <a name="04400704400004" ll="SDBG:σκηνή:000001">tabernáculo</a> do <a name="04400704400008" ll="SDBG:μαρτύριον:000001">Testemunho</a> estava <a name="04400704400018" ll="SDBG:ἐν:000000">entre</a> nossos <a name="04400704400014" ll="SDBG:πατήρ:000002">pais</a> no <a name="04400704400022" ll="SDBG:ἔρημος:000001" il="PikiWiki_Israel_32657;RS26447_ISR91T-418;tb113006604;ws092715042">deserto</a>, <a name="04400704400024" ll="SDBG:καθώς:000002">como</a> <a name="04400704400026" ll="SDBG:διατάσσω:000000">determinara</a> aquele que <a name="04400704400030" ll="SDBG:λαλέω:000000">disse</a> a Moisés que o <a name="04400704400036" ll="SDBG:ποιέω:000004">fizesse</a> <a name="04400704400040" ll="SDBG:κατά:000010">segundo</a> o <a name="04400704400044" ll="SDBG:τύπος:000002">modelo</a> <a name="04400704400046" ll="SDBG:ὅς:000000">que</a> tinha <a name="04400704400048" ll="SDBG:ὁράω:000000">visto</a>. 
\v 45 O <a name="04400704500002" ll="SDBG:ὅς:000000">qual</a> <a name="04400704500004" ll="SDBG:καί:000002">também</a> nossos <a name="04400704500012" ll="SDBG:πατήρ:000002">pais</a>, <a name="04400704500016" ll="SDBG:μετά:000000">com</a> <a name="04400704500018" ll="SDBG:Ἰησοῦς:000004">Josué</a>, tendo-o <a name="04400704500008" ll="SDBG:διαδέχομαι:000000">recebido</a>, o <a name="04400704500006" ll="SDBG:εἰσάγω:000001">levaram</a>, <a name="04400704500020" ll="SDBG:ἐν:000018">quando</a> tomaram <a name="04400704500024" ll="SDBG:κατάσχεσις:000001">posse</a> das <a name="04400704500028" ll="SDBG:ἔθνος:000000">nações</a> <a name="04400704500030" ll="SDBG:ὅς:000000">que</a> <a name="04400704500036" ll="SDBG:θεός:000000">Deus</a> <a name="04400704500032" ll="SDBG:ἐξωθέω:000000">expulsou</a> <a name="04400704500038" ll="SDBG:ἀπό:000000;SDBG:ἀπό:000002">da</a> <a name="04400704500040" ll="SDBG:πρόσωπον:000005">presença</a> <a name="04400704500044" ll="SDBG:πατήρ:000002">deles</a>, <a name="04400704500048" ll="SDBG:ἕως:000000">até</a> aos <a name="04400704500052" ll="SDBG:ἡμέρα:000002">dias</a> de Davi. 
\v 46 <a name="04400704600002" ll="SDBG:ὅς:000000">Este</a> <a name="04400704600004" ll="SDBG:εὑρίσκω:000003">achou</a> <a name="04400704600006" ll="SDBG:χάρις:000003">graça</a> diante <a name="04400704600008" ll="SDBG:ἐνώπιον:000000">de</a> <a name="04400704600012" ll="SDBG:θεός:000000">Deus</a> <a name="04400704600014" ll="SDBG:καί:000002">e</a> lhe <a name="04400704600016" ll="SDBG:αἰτέω:000000">suplicou</a> a faculdade de <a name="04400704600018" ll="SDBG:εὑρίσκω:000002">prover</a> <a name="04400704600020" ll="SDBG:σκήνωμα:000000;SDBG:σκήνωμα:000001">morada</a> para o Deus de <a name="04400704600026" ll="SDBG:Ἰακώβ:000000">Jacó</a>. 
