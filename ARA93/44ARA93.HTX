\v 1 <a name="04400500100004" ll="SDBG:δέ:000002">Entretanto</a>, <a name="04400500100006">certo</a> <a name="04400500100002" ll="SDBG:ἀνήρ:000000">homem</a>, <a name="04400500100010" ll="SDBG:ὄνομα:000000">chamado</a> Ananias, <a name="04400500100012" ll="SDBG:σύν:000000">com</a> sua <a name="04400500100018" ll="SDBG:γυνή:000001">mulher</a> Safira, <a name="04400500100022" ll="SDBG:πωλέω:000000">vendeu</a> uma <a name="04400500100024" ll="SDBG:κτῆμα:000000">propriedade</a>, 
\v 2 <a name="04400500200002" ll="SDBG:καί:000001">mas</a>, em acordo <a name="04400500200014" ll="SDBG:καί:000002">com</a> sua <a name="04400500200018" ll="SDBG:γυνή:000001">mulher</a>, reteve parte <a name="04400500200006" ll="SDBG:ἀπό:000003">do</a> <a name="04400500200010" ll="SDBG:τιμή:000002">preço</a> <a name="04400500200020" ll="SDBG:καί:000001">e</a>, <a name="04400500200022" ll="SDBG:φέρω:000001">levando</a> o <a name="04400500200024;04400500200026" ll="SDBG:μέρος:000000">restante</a>, <a name="04400500200038" ll="SDBG:τίθημι:000012">depositou</a>-o <a name="04400500200028" ll="SDBG:παρά:000000">aos</a> <a name="04400500200032" ll="SDBG:πούς:000003">pés</a> dos <a name="04400500200036" ll="SDBG:ἀπόστολος:000000">apóstolos</a>.
    
\v 3 <a name="04400500300004" ll="SDBG:δέ:000002">Então</a>, disse <a name="04400500300008" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>: Ananias, por que <a name="04400500300016" ll="SDBG:πληρόω:000007">encheu</a> <a name="04400500300020" ll="SDBG:Σατανᾶς:000000;SDBG:Σατανᾶς:000001">Satanás</a> teu <a name="04400500300024" ll="SDBG:καρδία:000006">coração</a>, para que <a name="04400500300028" ll="SDBG:ψεύδομαι:000000">mentisses</a> ao <a name="04400500300034" ll="SDBG:πνεῦμα:000000">Espírito</a> <a name="04400500300038" ll="SDBG:ἅγιος:000000">Santo</a>, reservando parte <a name="04400500300044" ll="SDBG:ἀπό:000003">do</a> <a name="04400500300048" ll="SDBG:τιμή:000002">valor</a> do <a name="04400500300052" ll="SDBG:χωρίον:000000">campo</a>? 
\v 4 <a name="04400500400004" ll="SDBG:μένω:000002">Conservando</a>-o, porventura, <a name="04400500400002;04400500400044" ll="SDBG:οὐ:000000;SDBG:οὐχί:000001">não</a> seria <a name="04400500400008" ll="SDBG:μένω:000002">teu</a>? <a name="04400500400010" ll="SDBG:καί:000001">E</a>, <a name="04400500400012" ll="SDBG:πιπράσκω:000000">vendido</a>, não <a name="04400500400022" ll="SDBG:ὑπάρχω:000000">estaria</a> <a name="04400500400014" ll="SDBG:ἐν:000004">em</a> <a name="04400500400018" ll="SDBG:σός:000000">teu</a> <a name="04400500400020" ll="SDBG:ἐξουσία:000004">poder</a>? <a name="04400500400024" ll="SDBG:τίς:000001">Como</a>, <a name="04400500400026" ll="SDBG:ὅτι:000000">pois</a>, <a name="04400500400028" ll="SDBG:τίθημι:000011">assentaste</a> <a name="04400500400030">no</a> <a name="04400500400034" ll="SDBG:καρδία:000009">coração</a> este <a name="04400500400040" ll="SDBG:πρᾶγμα:000000">desígnio</a>? Não <a name="04400500400046" ll="SDBG:ψεύδομαι:000000">mentiste</a> aos <a name="04400500400048" ll="SDBG:ἄνθρωπος:000000">homens</a>, <a name="04400500400050" ll="SDBG:ἀλλά:000000">mas</a> a <a name="04400500400054" ll="SDBG:θεός:000000">Deus</a>. 
\v 5 <a name="04400500500002" ll="SDBG:ἀκούω:000000">Ouvindo</a> estas <a name="04400500500012" ll="SDBG:λόγος:000000">palavras</a>, Ananias <a name="04400500500016" ll="SDBG:πίπτω:000001">caiu</a> e <a name="04400500500018" ll="SDBG:ἐκψύχω:000000">expirou</a>, <a name="04400500500022" ll="SDBG:γίνομαι:000003">sobrevindo</a> <a name="04400500500026" ll="SDBG:μέγας:000001">grande</a> <a name="04400500500024" ll="SDBG:φόβος:000000">temor</a> <a name="04400500500028" ll="SDBG:ἐπί:000007">a</a> <a name="04400500500030" ll="SDBG:πᾶς:000000">todos</a> os <a name="04400500500034" ll="SDBG:ἀκούω:000000">ouvintes</a>. 
\v 6 <a name="04400500600002" ll="SDBG:ἀνίστημι:000000">Levantando</a>-se os <a name="04400500600008" ll="SDBG:νέος:000002">moços</a>, <a name="04400500600010" ll="SDBG:συστέλλω:000001;SDBG:συστέλλω:000002">cobriram</a>-lhe o corpo <a name="04400500600014" ll="SDBG:καί:000001">e</a>, <a name="04400500600016" ll="SDBG:ἐκφέρω:000000">levando</a>-o, o <a name="04400500600018" ll="SDBG:θάπτω:000000">sepultaram</a>. 
\v 7 <a name="04400500700002;04400500700006" ll="SDBG:γίνομαι:000009;SDBG:ὡς:000008">Quase</a> <a name="04400500700010" ll="SDBG:τρεῖς:000000">três</a> <a name="04400500700008" ll="SDBG:ὥρα:000002">horas</a> <a name="04400500700012;04400500700014" ll="SDBG:διάστημα:000000;SDBG:καί:000001">depois</a>, <a name="04400500700030" ll="SDBG:εἰσέρχομαι:000000">entrou</a> a <a name="04400500700018" ll="SDBG:γυνή:000001">mulher</a> de <a name="04400500700020" ll="SDBG:αὐτός:001000">Ananias</a>, <a name="04400500700022" ll="SDBG:μή:000000">não</a> sabendo o que <a name="04400500700028" ll="SDBG:γίνομαι:000003">ocorrera</a>. 
\v 8 <a name="04400500800004" ll="SDBG:δέ:000001">Então</a>, <a name="04400500800010" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>, dirigindo-se a <a name="04400500800008" ll="SDBG:αὐτός:001000">ela</a>, <a name="04400500800002" ll="SDBG:ἀποκρίνομαι:000001">perguntou</a>-lhe: <a name="04400500800016" ll="SDBG:εἰ:000002">Dize</a>-me, <a name="04400500800024" ll="SDBG:ἀποδίδωμι:000005">vendestes</a> por <a name="04400500800018" ll="SDBG:τοσοῦτος:000001">tanto</a> aquela <a name="04400500800022" ll="SDBG:χωρίον:000000">terra</a>? Ela <a name="04400500800028" ll="SDBG:δέ:000001">respondeu</a>: <a name="04400500800032" ll="SDBG:ναί:000000">Sim</a>, por <a name="04400500800034" ll="SDBG:τοσοῦτος:000001">tanto</a>. 
\v 9 <a name="04400500900008" ll="SDBG:πρός:000004">Tornou</a>-lhe <a name="04400500900006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>: Por <a name="04400500900012;04400500900014" ll="SDBG:τίς:000001;SDBG:ὅτι:000000">que</a> entrastes em <a name="04400500900016" ll="SDBG:συμφωνέω:000000">acordo</a> para <a name="04400500900020" ll="SDBG:πειράζω:000000">tentar</a> o <a name="04400500900024" ll="SDBG:πνεῦμα:000000">Espírito</a> do <a name="04400500900026" ll="SDBG:κύριος:000000">Senhor</a>? Eis aí <a name="04400500900044" ll="SDBG:ἐπί:000001">à</a> <a name="04400500900048" ll="SDBG:θύρα:000000;SDBG:θύρα:000001" il="Realia:3.1.2a;Realia:3.1.2b">porta</a> os <a name="04400500900032" ll="SDBG:πούς:000000">pés</a> dos que <a name="04400500900036" ll="SDBG:θάπτω:000000">sepultaram</a> o teu <a name="04400500900040" ll="SDBG:ἀνήρ:000002">marido</a>, <a name="04400500900050" ll="SDBG:καί:000001">e</a> eles também te <a name="04400500900052" ll="SDBG:ἐκφέρω:000000">levarão</a>. 
\v 10 No mesmo <a name="04400501000004;04400501000006" ll="SDBG:δέ:000001;SDBG:παραχρῆμα:000000">instante</a>, <a name="04400501000002" ll="SDBG:πίπτω:000001">caiu</a> ela aos <a name="04400501000012" ll="SDBG:πούς:000000">pés</a> de <a name="04400501000014" ll="SDBG:αὐτός:001000">Pedro</a> <a name="04400501000016" ll="SDBG:καί:000000">e</a> <a name="04400501000018" ll="SDBG:ἐκψύχω:000000">expirou</a>. <a name="04400501000020" ll="SDBG:εἰσέρχομαι:000000">Entrando</a> os <a name="04400501000026" ll="SDBG:νεανίσκος:000000">moços</a>, <a name="04400501000028" ll="SDBG:εὑρίσκω:000000">acharam</a>-na <a name="04400501000032" ll="SDBG:νεκρός:000000">morta</a> <a name="04400501000034" ll="SDBG:καί:000001">e</a>, <a name="04400501000036" ll="SDBG:ἐκφέρω:000000">levando</a>-a, <a name="04400501000038" ll="SDBG:θάπτω:000000">sepultaram</a>-na <a name="04400501000008" ll="SDBG:πρός:000002">junto</a> <a name="04400501000046" ll="SDBG:αὐτός:001000">do</a> <a name="04400501000044" ll="SDBG:ἀνήρ:000002">marido</a>. 
\v 11 <a name="04400501100002;04400501100018" ll="SDBG:καί:000000;SDBG:καί:000001">E</a> <a name="04400501100004" ll="SDBG:γίνομαι:000003">sobreveio</a> <a name="04400501100008" ll="SDBG:μέγας:000001">grande</a> <a name="04400501100006" ll="SDBG:φόβος:000000">temor</a> <a name="04400501100010" ll="SDBG:ἐπί:000007">a</a> <a name="04400501100012" ll="SDBG:ὅλος:000000">toda</a> a <a name="04400501100016" ll="SDBG:ἐκκλησία:000000">igreja</a> e <a name="04400501100020" ll="SDBG:ἐπί:000007">a</a> todos <a name="04400501100022" ll="SDBG:πᾶς:000000">quantos</a> <a name="04400501100026" ll="SDBG:ἀκούω:000002">ouviram</a> a notícia destes acontecimentos.
    
\v 12 <a name="04400501200022" ll="SDBG:πολύς:000000">Muitos</a> <a name="04400501200016" ll="SDBG:σημεῖον:000000">sinais</a> <a name="04400501200018;04400501200030" ll="SDBG:καί:000000">e</a> <a name="04400501200020" ll="SDBG:τέρας:000000">prodígios</a> eram <a name="04400501200014" ll="SDBG:γίνομαι:000003">feitos</a> <a name="04400501200024" ll="SDBG:ἐν:000001">entre</a> o <a name="04400501200028" ll="SDBG:λαός:000002">povo</a> <a name="04400501200002" ll="SDBG:διά:000001">pelas</a> <a name="04400501200008" ll="SDBG:χείρ:000000;SDBG:χείρ:000002">mãos</a> dos <a name="04400501200012" ll="SDBG:ἀπόστολος:000000">apóstolos</a>. E costumavam <a name="04400501200036" ll="SDBG:ἅπας:000000">todos</a> reunir-se, de comum <a name="04400501200034" ll="SDBG:ὁμοθυμαδόν:000000">acordo</a>, <a name="04400501200038" ll="SDBG:ἐν:000000">no</a> <a name="04400501200042" ll="SDBG:στοά:000000" il="LB00248;jlm_herod_tm_nw_m01">Pórtico</a> de <a name="04400501200044" ll="SDBG:Σολομών:000000">Salomão</a>. 
\v 13 <a name="04400501300004" ll="SDBG:δέ:000002">Mas</a>, dos restantes, <a name="04400501300008" ll="SDBG:οὐδείς:000000">ninguém</a> <a name="04400501300010" ll="SDBG:τολμάω:000000">ousava</a> <a name="04400501300012" ll="SDBG:κολλάω:000000">ajuntar</a>-se a <a name="04400501300014" ll="SDBG:αὐτός:001000">eles</a>; <a name="04400501300016" ll="SDBG:ἀλλά:000000">porém</a> o <a name="04400501300024" ll="SDBG:λαός:000002">povo</a> <a name="04400501300020" ll="SDBG:αὐτός:001000">lhes</a> tributava grande <a name="04400501300018" ll="SDBG:μεγαλύνω:000002">admiração</a>. 
\v 14 <a name="04400501400004">E</a> <a name="04400501400006" ll="SDBG:προστίθημι:000000">crescia</a> mais e <a name="04400501400002" ll="SDBG:μᾶλλον:000000">mais</a> a <a name="04400501400014" ll="SDBG:πλῆθος:000000">multidão</a> de <a name="04400501400008" ll="SDBG:πιστεύω:000002">crentes</a>, <a name="04400501400018" ll="SDBG:καί:000005;SDBG:τέ:000003">tanto</a> <a name="04400501400016" ll="SDBG:ἀνήρ:000000">homens</a> <a name="04400501400020" ll="SDBG:καί:000005">como</a> <a name="04400501400022" ll="SDBG:γυνή:000000">mulheres</a>, agregados ao <a name="04400501400012" ll="SDBG:κύριος:000000">Senhor</a>, 
\v 15 a ponto <a name="04400501500002" ll="SDBG:ὥστε:000000">de</a> <a name="04400501500012" ll="SDBG:ἐκφέρω:000000">levarem</a> os <a name="04400501500016" ll="SDBG:ἀσθενής:000002">enfermos</a> até pelas ruas <a name="04400501500004">e</a> os <a name="04400501500020" ll="SDBG:τίθημι:000000">colocarem</a> <a name="04400501500022" ll="SDBG:ἐπί:000000">sobre</a> leitos <a name="04400501500018" ll="SDBG:καί:000000">e</a> macas, para <a name="04400501500030" ll="SDBG:ἵνα:000000">que</a>, ao <a name="04400501500032" ll="SDBG:ἔρχομαι:000001">passar</a> <a name="04400501500034" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>, ao <a name="04400501500036" ll="SDBG:κἄν:000000">menos</a> a sua <a name="04400501500040" ll="SDBG:σκιά:000001">sombra</a> se <a name="04400501500042" ll="SDBG:ἐπισκιάζω:000000">projetasse</a> <a name="04400501500044" ll="SDBG:τὶς:000000">nalguns</a> <a name="04400501500046" ll="SDBG:αὐτός:001000">deles</a>. 
\v 16 <a name="04400501600002" ll="SDBG:συνέρχομαι:000000">Afluía</a> <a name="04400501600006" ll="SDBG:καί:000002">também</a> <a name="04400501600010" ll="SDBG:πλῆθος:000001">muita</a> gente das <a name="04400501600016" ll="SDBG:πόλις:000000">cidades</a> <a name="04400501600014" ll="SDBG:πέριξ:000000">vizinhas</a> a Jerusalém, <a name="04400501600020" ll="SDBG:φέρω:000001">levando</a> <a name="04400501600022" ll="SDBG:ἀσθενής:000002">doentes</a> <a name="04400501600024" ll="SDBG:καί:000000">e</a> <a name="04400501600026" ll="SDBG:ὀχλέω:000000">atormentados</a> <a name="04400501600028" ll="SDBG:ὑπό:000002">de</a> <a name="04400501600030" ll="SDBG:πνεῦμα:000008">espíritos</a> <a name="04400501600032" ll="SDBG:ἀκάθαρτος:000000">imundos</a>, e <a name="04400501600034;04400501600038" ll="SDBG:ἅπας:000000;SDBG:ὅστις:000000">todos</a> eram <a name="04400501600036" ll="SDBG:θεραπεύω:000000">curados</a>.
    
\v 17 <a name="04400501700002" ll="SDBG:ἀνίστημι:000000">Levantando</a>-se, <a name="04400501700004" ll="SDBG:δέ:000002">porém</a>, o sumo <a name="04400501700008" ll="SDBG:ἀρχιερεύς:000001" il="LB00266;NBGlo20141009c;Realia:4.5.4">sacerdote</a> <a name="04400501700010" ll="SDBG:καί:000000">e</a> todos <a name="04400501700012" ll="SDBG:πᾶς:000000">os</a> que estavam <a name="04400501700016" ll="SDBG:σύν:000000">com</a> <a name="04400501700018" ll="SDBG:αὐτός:001000">ele</a>, <a name="04400501700006" ll="SDBG:ὁ:000000">isto</a> é, a <a name="04400501700024" ll="SDBG:αἵρεσις:000000">seita</a> dos <a name="04400501700028" ll="SDBG:Σαδδουκαῖος:000000">saduceus</a>, tomaram-se de <a name="04400501700032" ll="SDBG:ζῆλος:000001">inveja</a>, 
\v 18 <a name="04400501800004;04400501800010" ll="SDBG:ἐπιβάλλω:000006;SDBG:ἐπί:000007">prenderam</a> os <a name="04400501800014" ll="SDBG:ἀπόστολος:000000">apóstolos</a> <a name="04400501800002">e</a> <a name="04400501800020" ll="SDBG:αὐτός:001000">os</a> <a name="04400501800018" ll="SDBG:τίθημι:000000">recolheram</a> <a name="04400501800022" ll="SDBG:ἐν:000005">à</a> <a name="04400501800024" ll="SDBG:τήρησις:000000;SDBG:τήρησις:000001">prisão</a> <a name="04400501800026" ll="SDBG:δημόσιος:000000">pública</a>. 
