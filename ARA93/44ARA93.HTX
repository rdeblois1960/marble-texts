\v 12 <a name="04402501200002" ll="SDBG:τότε:000000">Então</a>, <a name="04402501200006" ll="SDBG:Φῆστος:000000">Festo</a>, tendo <a name="04402501200008" ll="SDBG:συλλαλέω:000000">falado</a> <a name="04402501200010" ll="SDBG:μετά:000000">com</a> o <a name="04402501200014" ll="SDBG:συμβούλιον:000001">conselho</a>, <a name="04402501200016" ll="SDBG:ἀποκρίνομαι:000000">respondeu</a>: <a name="04402501200022" ll="SDBG:ἐπί:000003">Para</a> <a name="04402501200018" ll="SDBG:Καῖσαρ:000000" il="tb060408002">César</a> apelaste, para <a name="04402501200024" ll="SDBG:Καῖσαρ:000000" il="tb060408002">César</a> <a name="04402501200026" ll="SDBG:πορεύομαι:000000">irás</a>.
    
\v 13 <a name="04402501300006" ll="SDBG:διαγίνομαι:000000">Passados</a> <a name="04402501300008" ll="SDBG:τὶς:000000">alguns</a> <a name="04402501300002" ll="SDBG:ἡμέρα:000002">dias</a>, o <a name="04402501300014" ll="SDBG:βασιλεύς:000000">rei</a> <a name="04402501300010" ll="SDBG:Ἀγρίππας:000000" il="adr0807088321;tb031114354">Agripa</a> <a name="04402501300016" ll="SDBG:καί:000000">e</a> <a name="04402501300018" ll="SDBG:Βερνίκη:000000">Berenice</a> <a name="04402501300020" ll="SDBG:καταντάω:000000">chegaram</a> <a name="04402501300022" ll="SDBG:εἰς:000000">a</a> <a name="04402501300024" ll="SDBG:Καισάρεια:000000" il="Caesarea aquaduct;Caesarea-prison;il_caesarae_theatre_f01;il_caesarea_herod_palace_d03;il_caesarea_swimmingpool_f01;il_caesarea_theatre_d01">Cesareia</a> a fim de <a name="04402501300026" ll="SDBG:ἀσπάζομαι:000000" il="CN01609">saudar</a> a <a name="04402501300030" ll="SDBG:Φῆστος:000000">Festo</a>. 
\v 14 <a name="04402501400004" ll="SDBG:δέ:000001">Como</a> se <a name="04402501400010" ll="SDBG:διατρίβω:000000">demorassem</a> <a name="04402501400012" ll="SDBG:ἐκεῖ:000000">ali</a> alguns <a name="04402501400008" ll="SDBG:ἡμέρα:000002">dias</a>, <a name="04402501400016" ll="SDBG:Φῆστος:000000">Festo</a> expôs ao <a name="04402501400020" ll="SDBG:βασιλεύς:000000">rei</a> o <a name="04402501400026" ll="SDBG:κατά:000012">caso</a> de <a name="04402501400030" ll="SDBG:Παῦλος:000000" il="Saint_Paul">Paulo</a>, <a name="04402501400032;04402501400042" ll="SDBG:λέγω:000000;SDBG:ὑπό:000002">dizendo</a>: <a name="04402501400044" ll="SDBG:Φῆλιξ:000000">Félix</a> <a name="04402501400040" ll="SDBG:καταλείπω:000001">deixou</a> aqui <a name="04402501400046" ll="SDBG:δέσμιος:000000">preso</a> <a name="04402501400036">certo</a> <a name="04402501400034" ll="SDBG:ἀνήρ:000000">homem</a>, 
