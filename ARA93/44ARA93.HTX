\v 31 A <a name="04400903100008" ll="SDBG:ἐκκλησία:000001">igreja</a>, na <a name="04400903100004;04400903100006" ll="SDBG:μέν:000000">verdade</a>, <a name="04400903100026" ll="SDBG:ἔχω:000005">tinha</a> <a name="04400903100028" ll="SDBG:εἰρήνη:000000">paz</a> <a name="04400903100010" ll="SDBG:κατά:000004">por</a> <a name="04400903100012" ll="SDBG:ὅλος:000000">toda</a> a <a name="04400903100016" ll="SDBG:Ἰουδαία:000000">Judeia</a>, <a name="04400903100020" ll="SDBG:Γαλιλαία:000000" il="tb052200102;tb110306599">Galileia</a> <a name="04400903100018" ll="SDBG:καί:000000">e</a> <a name="04400903100024" ll="SDBG:Σαμάρεια:000000" il="tb070507860">Samaria</a>, <a name="04400903100030" ll="SDBG:οἰκοδομέω:000001">edificando</a>-se <a name="04400903100022" ll="SDBG:καί:000000">e</a> <a name="04400903100034" ll="SDBG:πορεύομαι:000003">caminhando</a> no <a name="04400903100038" ll="SDBG:φόβος:000002">temor</a> do <a name="04400903100042" ll="SDBG:κύριος:000000">Senhor</a>, <a name="04400903100032" ll="SDBG:καί:000000">e</a>, no <a name="04400903100048" ll="SDBG:παράκλησις:000000">conforto</a> do <a name="04400903100054" ll="SDBG:πνεῦμα:000000">Espírito</a> <a name="04400903100052" ll="SDBG:ἅγιος:000000">Santo</a>, crescia em <a name="04400903100056" ll="SDBG:πληθύνω:000000">número</a>.
    
\v 32 <a name="04400903200002;04400903200008" ll="SDBG:γίνομαι:000003;SDBG:διέρχομαι:000001">Passando</a> <a name="04400903200006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> <a name="04400903200010" ll="SDBG:διά:000006">por</a> toda <a name="04400903200012" ll="SDBG:πᾶς:000000">parte</a>, <a name="04400903200014" ll="SDBG:κατέρχομαι:000000">desceu</a> <a name="04400903200016" ll="SDBG:καί:000002">também</a> <a name="04400903200018" ll="SDBG:πρός:000000">aos</a> <a name="04400903200022" ll="SDBG:ἅγιος:000002">santos</a> <a name="04400903200020" ll="SDBG:ὁ:000000">que</a> <a name="04400903200026" ll="SDBG:κατοικέω:000000">habitavam</a> em <a name="04400903200028" ll="SDBG:Λύδδα:000000" il="Bolen00092;tb011107437">Lida</a>. 
\v 33 <a name="04400903300002" ll="SDBG:εὑρίσκω:000000">Encontrou</a> <a name="04400903300006" ll="SDBG:ἐκεῖ:000000">ali</a> <a name="04400903300010">certo</a> <a name="04400903300008" ll="SDBG:ἄνθρωπος:000001">homem</a>, <a name="04400903300012" ll="SDBG:ὄνομα:000000">chamado</a> <a name="04400903300014" ll="SDBG:Αἰνέας:000000">Eneias</a>, que <a name="04400903300022" ll="SDBG:κατάκειμαι:000000">havia</a> <a name="04400903300020" ll="SDBG:ὀκτώ:000000">oito</a> <a name="04400903300018" ll="SDBG:ἔτος:000000">anos</a> <a name="04400903300024" ll="SDBG:ἐπί:000000">jazia</a> de <a name="04400903300016" ll="SDBG:ἐκ:000011">cama</a>, <a name="04400903300028" ll="SDBG:ὅς:000000">pois</a> era <a name="04400903300032" ll="SDBG:παραλύω:000000" il="JESUS & PARALYTIC;LB00305;LB00306;LB00307">paralítico</a>. 
\v 34 <a name="04400903400006" ll="SDBG:αὐτός:001000">Disse</a>-lhe <a name="04400903400010" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>: <a name="04400903400012" ll="SDBG:Αἰνέας:000000">Eneias</a>, <a name="04400903400018" ll="SDBG:Ἰησοῦς:000000">Jesus</a> <a name="04400903400020" ll="SDBG:Χριστός:000001">Cristo</a> te <a name="04400903400014" ll="SDBG:ἰάομαι:000000">cura</a>! <a name="04400903400022" ll="SDBG:ἀνίστημι:000003">Levanta</a>-te <a name="04400903400002" ll="SDBG:καί:000001">e</a> arruma o <a name="04400903400028" ll="SDBG:σεαυτοῦ:000000">teu</a> leito. <a name="04400903400024" ll="SDBG:καί:000001">Ele</a>, <a name="04400903400032" ll="SDBG:εὐθέως:000000">imediatamente</a>, se <a name="04400903400034" ll="SDBG:ἀνίστημι:000003">levantou</a>. 
\v 35 <a name="04400903500006" ll="SDBG:αὐτός:001000">Viram</a>-no <a name="04400903500008" ll="SDBG:πᾶς:000000">todos</a> os <a name="04400903500012" ll="SDBG:κατοικέω:000000">habitantes</a> de <a name="04400903500014" ll="SDBG:Λύδδα:000000" il="Bolen00092;tb011107437">Lida</a> <a name="04400903500002" ll="SDBG:καί:000001">e</a> Sarona, os <a name="04400903500022" ll="SDBG:ὅστις:000000">quais</a> se <a name="04400903500024" ll="SDBG:ἐπιστρέφω:000001">converteram</a> <a name="04400903500026" ll="SDBG:ἐπί:000004">ao</a> <a name="04400903500030" ll="SDBG:κύριος:000000">Senhor</a>.
    
\v 36 Havia <a name="04400903600002" ll="SDBG:ἐν:000000">em</a> <a name="04400903600004" ll="SDBG:Ἰόππη:000000" il="HK00370;Jaffa">Jope</a> <a name="04400903600008">uma</a> <a name="04400903600012" ll="SDBG:μαθήτρια:000000">discípula</a> por <a name="04400903600014" ll="SDBG:ὄνομα:000000">nome</a> <a name="04400903600016" ll="SDBG:Ταβιθά:000000">Tabita</a>, nome este <a name="04400903600018" ll="SDBG:ὅς:000000">que</a>, <a name="04400903600020" ll="SDBG:διερμηνεύω:000000">traduzido</a>, quer <a name="04400903600022" ll="SDBG:λέγω:000003">dizer</a> <a name="04400903600024" ll="SDBG:Δορκάς:000000">Dorcas</a>; era <a name="04400903600026" ll="SDBG:οὗτος:000000">ela</a> <a name="04400903600030" ll="SDBG:πλήρης:000002">notável</a> pelas <a name="04400903600034" ll="SDBG:ἀγαθός:000000">boas</a> <a name="04400903600032" ll="SDBG:ἔργον:000000">obras</a> <a name="04400903600036" ll="SDBG:καί:000000">e</a> <a name="04400903600038" ll="SDBG:ἐλεημοσύνη:000000">esmolas</a> <a name="04400903600040" ll="SDBG:ὅς:000000">que</a> <a name="04400903600042" ll="SDBG:ποιέω:000000">fazia</a>. 
\v 37 <a name="04400903700004">Ora</a>, <a name="04400903700002" ll="SDBG:γίνομαι:000003">aconteceu</a>, <a name="04400903700006;04400903700012" ll="SDBG:ἐκεῖνος:000000;SDBG:ἐν:000019">naqueles</a> <a name="04400903700010" ll="SDBG:ἡμέρα:000002">dias</a>, que ela <a name="04400903700014" ll="SDBG:ἀσθενέω:000001">adoeceu</a> e veio a morrer; <a name="04400903700022" ll="SDBG:δέ:000001">e</a>, depois <a name="04400903700016" ll="SDBG:αὐτός:001000">de</a> a <a name="04400903700020" ll="SDBG:λούω:000000">lavarem</a>, <a name="04400903700024" ll="SDBG:τίθημι:000000">puseram</a>-na <a name="04400903700028" ll="SDBG:ἐν:000000">no</a> <a name="04400903700030" ll="SDBG:ὑπερῷον:000000" il="LB00235;Realia:3.1.6.4">cenáculo</a>. 
\v 38 <a name="04400903800004">Como</a> <a name="04400903800008" ll="SDBG:Λύδδα:000000" il="Bolen00092;tb011107437">Lida</a> era <a name="04400903800002" ll="SDBG:ἐγγύς:000000">perto</a> de <a name="04400903800012" ll="SDBG:Ἰόππη:000000" il="HK00370;Jaffa">Jope</a>, <a name="04400903800018" ll="SDBG:ἀκούω:000002">ouvindo</a> os <a name="04400903800016" ll="SDBG:μαθητής:000000">discípulos</a> <a name="04400903800020" ll="SDBG:ὅτι:000000">que</a> <a name="04400903800022" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> estava <a name="04400903800026;04400903800028" ll="SDBG:αὐτός:001000;SDBG:ἐν:000000">ali</a>, enviaram-lhe <a name="04400903800032" ll="SDBG:δύο:000000">dois</a> <a name="04400903800034" ll="SDBG:ἀνήρ:000000">homens</a> que lhe <a name="04400903800040" ll="SDBG:παρακαλέω:000000">pedissem</a>: <a name="04400903800042" ll="SDBG:μή:000002">Não</a> <a name="04400903800044" ll="SDBG:ὀκνέω:000000">demores</a> em <a name="04400903800046" ll="SDBG:διέρχομαι:000000">vir</a> <a name="04400903800048" ll="SDBG:ἕως:000002">ter</a> conosco. 
\v 39 <a name="04400903900006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> <a name="04400903900002" ll="SDBG:ἀνίστημι:000003">atendeu</a> <a name="04400903900024" ll="SDBG:καί:000001">e</a> foi <a name="04400903900008" ll="SDBG:συνέρχομαι:000001">com</a> <a name="04400903900010" ll="SDBG:αὐτός:001000">eles</a>. Tendo <a name="04400903900014" ll="SDBG:παραγίνομαι:000000">chegado</a>, <a name="04400903900016" ll="SDBG:ἀνάγω:000000">conduziram</a>-no <a name="04400903900018" ll="SDBG:εἰς:000001">para</a> o <a name="04400903900022" ll="SDBG:ὑπερῷον:000000" il="LB00235;Realia:3.1.6.4">cenáculo</a>; <a name="04400903900038" ll="SDBG:καί:000000">e</a> <a name="04400903900030" ll="SDBG:πᾶς:000000">todas</a> as viúvas <a name="04400903900028" ll="SDBG:αὐτός:001000">o</a> <a name="04400903900026" ll="SDBG:παρίστημι:000000">cercaram</a>, <a name="04400903900036" ll="SDBG:κλαίω:000000" il="HK00290">chorando</a> <a name="04400903900044" ll="SDBG:καί:000000">e</a> <a name="04400903900040" ll="SDBG:ἐπιδείκνυμι:000000">mostrando</a>-lhe <a name="04400903900042" ll="SDBG:χιτών:000000" il="NBGmk20140407a;Realia:6.3">túnicas</a> e <a name="04400903900046" ll="SDBG:ἱμάτιον:000001" il="Realia:6.2">vestidos</a> <a name="04400903900048" ll="SDBG:ὅσος:000000">que</a> <a name="04400903900060" ll="SDBG:Δορκάς:000000">Dorcas</a> <a name="04400903900050" ll="SDBG:ποιέω:000000">fizera</a> enquanto estava <a name="04400903900052" ll="SDBG:μετά:000000">com</a> <a name="04400903900054" ll="SDBG:αὐτός:001000">elas</a>. 
\v 40 <a name="04400904000004" ll="SDBG:δέ:000001">Mas</a> <a name="04400904000012" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>, tendo feito <a name="04400904000002" ll="SDBG:ἐκβάλλω:000002">sair</a> a <a name="04400904000006;04400904000008" ll="SDBG:πᾶς:000000;SDBG:ἔξω:000001">todos</a>, <a name="04400904000016" ll="SDBG:τίθημι:000013">pondo</a>-se de <a name="04400904000020" ll="SDBG:γόνυ:000004" il="Kneeling angel">joelhos</a>, <a name="04400904000022" ll="SDBG:προσεύχομαι:000000" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20121121;NBGmk20140526">orou</a>; <a name="04400904000014" ll="SDBG:καί:000001">e</a>, <a name="04400904000026" ll="SDBG:ἐπιστρέφω:000000">voltando</a>-se <a name="04400904000028" ll="SDBG:πρός:000000">para</a> o <a name="04400904000032" ll="SDBG:σῶμα:000000">corpo</a>, disse: <a name="04400904000036" ll="SDBG:Ταβιθά:000000">Tabita</a>, <a name="04400904000038" ll="SDBG:ἀνίστημι:000003">levanta</a>-te! Ela <a name="04400904000044" ll="SDBG:ἀνοίγω:000000">abriu</a> os <a name="04400904000048" ll="SDBG:ὀφθαλμός:000000">olhos</a> <a name="04400904000024" ll="SDBG:καί:000001">e</a>, vendo a <a name="04400904000058" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>, <a name="04400904000060" ll="SDBG:ἀνακαθίζω:000000">sentou</a>-se. 
\v 41 Ele, <a name="04400904100002" ll="SDBG:δίδωμι:000003">dando</a>-lhe a <a name="04400904100008" ll="SDBG:χείρ:000000">mão</a>, <a name="04400904100010" ll="SDBG:ἀνίστημι:000000">levantou</a>-a; <a name="04400904100004" ll="SDBG:δέ:000001">e</a>, <a name="04400904100014" ll="SDBG:φωνέω:000000">chamando</a> os <a name="04400904100020" ll="SDBG:ἅγιος:000002">santos</a>, <a name="04400904100022" ll="SDBG:καί:000000">especialmente</a> as viúvas, <a name="04400904100028" ll="SDBG:παρίστημι:000000">apresentou</a>-a <a name="04400904100032" ll="SDBG:ζάω:000001">viva</a>. 
\v 42 <a name="04400904200004">Isto</a> se <a name="04400904200006" ll="SDBG:γίνομαι:000002">tornou</a> <a name="04400904200002" ll="SDBG:γνωστός:000000;SDBG:γνωστός:000002">conhecido</a> <a name="04400904200008" ll="SDBG:κατά:000002">por</a> <a name="04400904200010" ll="SDBG:ὅλος:000000">toda</a> <a name="04400904200014" ll="SDBG:Ἰόππη:000000" il="HK00370;Jaffa">Jope</a>, <a name="04400904200016">e</a> <a name="04400904200020" ll="SDBG:πολύς:000000">muitos</a> <a name="04400904200018" ll="SDBG:πιστεύω:000001">creram</a> <a name="04400904200022">no</a> <a name="04400904200026" ll="SDBG:κύριος:000000">Senhor</a>. 
\v 43 Pedro <a name="04400904300002;04400904300010" ll="SDBG:γίνομαι:000003;SDBG:μένω:000000">ficou</a> <a name="04400904300012" ll="SDBG:ἐν:000000">em</a> <a name="04400904300014" ll="SDBG:Ἰόππη:000000" il="HK00370;Jaffa">Jope</a> <a name="04400904300008" ll="SDBG:ἱκανός:000004">muitos</a> <a name="04400904300006" ll="SDBG:ἡμέρα:000001">dias</a>, em casa <a name="04400904300016" ll="SDBG:παρά:000008">de</a> <a name="04400904300018">um</a> <a name="04400904300022" ll="SDBG:βυρσεύς:000000" il="Realia:1.13;mat00991">curtidor</a> chamado <a name="04400904300020" ll="SDBG:Σίμων:000005">Simão</a>.
\c 10
\v 1 Morava <a name="04401000100008" ll="SDBG:ἐν:000000">em</a> <a name="04401000100010" ll="SDBG:Καισάρεια:000000" il="Caesarea aquaduct;Caesarea-prison;il_caesarae_theatre_f01;il_caesarea_herod_palace_d03;il_caesarea_swimmingpool_f01;il_caesarea_theatre_d01">Cesareia</a> <a name="04401000100006">um</a> <a name="04401000100002" ll="SDBG:ἀνήρ:000000">homem</a> de <a name="04401000100012" ll="SDBG:ὄνομα:000000">nome</a> <a name="04401000100014" ll="SDBG:Κορνήλιος:000000">Cornélio</a>, <a name="04401000100016" ll="SDBG:ἑκατοντάρχης:000000" il="Centurion2">centurião</a> <a name="04401000100018" ll="SDBG:ἐκ:000007">da</a> <a name="04401000100020" ll="SDBG:σπεῖρα:000000" il="tb032014723">coorte</a> <a name="04401000100024" ll="SDBG:καλέω:000000">chamada</a> <a name="04401000100026" ll="SDBG:Ἰταλικός:000000">Italiana</a>, 
\v 2 <a name="04401000200002" ll="SDBG:εὐσεβής:000000">piedoso</a> <a name="04401000200004" ll="SDBG:καί:000000">e</a> <a name="04401000200006" ll="SDBG:φοβέω:000002">temente</a> a <a name="04401000200010" ll="SDBG:θεός:000000">Deus</a> <a name="04401000200012" ll="SDBG:σύν:000000">com</a> <a name="04401000200014" ll="SDBG:πᾶς:000000">toda</a> a sua <a name="04401000200018" ll="SDBG:οἶκος:000001">casa</a> <a name="04401000200032" ll="SDBG:καί:000000">e</a> que <a name="04401000200022" ll="SDBG:ποιέω:000000">fazia</a> <a name="04401000200026" ll="SDBG:πολύς:000000">muitas</a> <a name="04401000200024" ll="SDBG:ἐλεημοσύνη:000000">esmolas</a> ao <a name="04401000200030" ll="SDBG:λαός:000003">povo</a> e, de contínuo, <a name="04401000200034" ll="SDBG:δέομαι:000000">orava</a> a <a name="04401000200038" ll="SDBG:θεός:000000">Deus</a>. 
\v 3 Esse homem observou <a name="04401000300008" ll="SDBG:φανερῶς:000001">claramente</a> <a name="04401000300004" ll="SDBG:ἐν:000004">durante</a> uma <a name="04401000300006" ll="SDBG:ὅραμα:000001">visão</a>, <a name="04401000300010" ll="SDBG:ὡσεί:000001">cerca</a> da <a name="04401000300014" ll="SDBG:ὥρα:000002">hora</a> nona do <a name="04401000300020" ll="SDBG:ἡμέρα:000001">dia</a>, um <a name="04401000300022" ll="SDBG:ἄγγελος:000001" il="Kneeling angel;tb031115118">anjo</a> de <a name="04401000300026" ll="SDBG:θεός:000000">Deus</a> que se <a name="04401000300028" ll="SDBG:εἰσέρχομαι:000000">aproximou</a> <a name="04401000300030;04401000300032" ll="SDBG:αὐτός:001000;SDBG:πρός:000000">dele</a> <a name="04401000300034" ll="SDBG:καί:000001">e</a> <a name="04401000300038" ll="SDBG:αὐτός:001000">lhe</a> disse: 
\v 4 Cornélio! Este, fixando <a name="04401000400008" ll="SDBG:αὐτός:001000">nele</a> os <a name="04401000400006" ll="SDBG:ἀτενίζω:000000">olhos</a> <a name="04401000400004;04401000400010" ll="SDBG:δέ:000001;SDBG:καί:000001">e</a> <a name="04401000400014" ll="SDBG:γίνομαι:000002">possuído</a> de <a name="04401000400012" ll="SDBG:ἔμφοβος:000000">temor</a>, perguntou: <a name="04401000400018" ll="SDBG:τίς:000000">Que</a> é, <a name="04401000400022" ll="SDBG:κύριος:000000;SDBG:κύριος:000003">Senhor</a>? <a name="04401000400036" ll="SDBG:καί:000000">E</a> o anjo <a name="04401000400028" ll="SDBG:αὐτός:001000">lhe</a> disse: As tuas <a name="04401000400032" ll="SDBG:προσευχή:000000" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20121121;NBGmk20140526">orações</a> e as tuas <a name="04401000400040" ll="SDBG:ἐλεημοσύνη:000000">esmolas</a> <a name="04401000400044" ll="SDBG:ἀναβαίνω:000000">subiram</a> <a name="04401000400046">para</a> <a name="04401000400048" ll="SDBG:μνημόσυνον:000000">memória</a> diante de <a name="04401000400054" ll="SDBG:θεός:000000">Deus</a>. 
\v 5 <a name="04401000500004" ll="SDBG:νῦν:000000">Agora</a>, <a name="04401000500006" ll="SDBG:πέμπω:000000">envia</a> <a name="04401000500008" ll="SDBG:ἀνήρ:000000">mensageiros</a> <a name="04401000500010" ll="SDBG:εἰς:000000">a</a> <a name="04401000500012" ll="SDBG:Ἰόππη:000000" il="HK00370;Jaffa">Jope</a> <a name="04401000500002">e</a> manda <a name="04401000500016" ll="SDBG:μεταπέμπω:000000">chamar</a> <a name="04401000500018" ll="SDBG:Σίμων:000000">Simão</a>, <a name="04401000500022" ll="SDBG:ὅς:000000">que</a> tem por sobrenome <a name="04401000500026" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>. 
\v 6 <a name="04401000600002" ll="SDBG:οὗτος:000000">Ele</a> está <a name="04401000600004" ll="SDBG:ξενίζω:000000">hospedado</a> <a name="04401000600006" ll="SDBG:παρά:000008">com</a> <a name="04401000600010" ll="SDBG:Σίμων:000005">Simão</a>, <a name="04401000600012" ll="SDBG:βυρσεύς:000000" il="Realia:1.13;mat00991">curtidor</a>, <a name="04401000600014" ll="SDBG:ὅς:000000">cuja</a> <a name="04401000600018" ll="SDBG:οἰκία:000000" il="HK00232;LB00231;LB00235;NBGbs20100214-204;NBGbs20110223-127;Realia:3.1;il_capernaum_house_d01">residência</a> está situada <a name="04401000600020" ll="SDBG:παρά:000000">à</a> <a name="04401000600022" ll="SDBG:θάλασσα:000001">beira</a>-mar. 
\v 7 Logo <a name="04401000700002" ll="SDBG:ὡς:000003">que</a> se <a name="04401000700006" ll="SDBG:ἀπέρχομαι:000000">retirou</a> o <a name="04401000700010" ll="SDBG:ἄγγελος:000001" il="Kneeling angel;tb031115118">anjo</a> <a name="04401000700008" ll="SDBG:ὁ:000000">que</a> lhe <a name="04401000700014" ll="SDBG:λαλέω:000000">falava</a>, <a name="04401000700018" ll="SDBG:φωνέω:000000">chamou</a> <a name="04401000700020" ll="SDBG:δύο:000000">dois</a> dos seus <a name="04401000700024" ll="SDBG:οἰκέτης:000000">domésticos</a> <a name="04401000700026" ll="SDBG:καί:000000">e</a> um <a name="04401000700028" ll="SDBG:στρατιώτης:000000">soldado</a> <a name="04401000700030" ll="SDBG:εὐσεβής:000000">piedoso</a> dos que estavam a <a name="04401000700016" ll="SDBG:αὐτός:001000">seu</a> <a name="04401000700034" ll="SDBG:προσκαρτερέω:000002">serviço</a> 
\v 8 <a name="04401000800002" ll="SDBG:καί:000001">e</a>, havendo-lhes <a name="04401000800004;04401000800008" ll="SDBG:αὐτός:001000;SDBG:ἐξηγέομαι:000000">contado</a> <a name="04401000800006" ll="SDBG:ἅπας:000000">tudo</a>, <a name="04401000800010" ll="SDBG:ἀποστέλλω:000000">enviou</a>-os <a name="04401000800014" ll="SDBG:εἰς:000000">a</a> <a name="04401000800018" ll="SDBG:Ἰόππη:000000" il="HK00370;Jaffa">Jope</a>.
    
\v 9 No dia <a name="04401000900006" ll="SDBG:ἐπαύριον:000000">seguinte</a>, <a name="04401000900008" ll="SDBG:ὁδοιπορέω:000000">indo</a> <a name="04401000900010" ll="SDBG:ἐκεῖνος:000000">eles</a> de caminho <a name="04401000900012" ll="SDBG:καί:000000">e</a> <a name="04401000900018" ll="SDBG:ἐγγίζω:000000">estando</a> já perto da <a name="04401000900016" ll="SDBG:πόλις:000000">cidade</a>, <a name="04401000900020" ll="SDBG:ἀναβαίνω:000000">subiu</a> <a name="04401000900022" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> <a name="04401000900024" ll="SDBG:ἐπί:000005">ao</a> <a name="04401000900028" ll="SDBG:δῶμα:000000" il="LB00234;Realia:3.1">eirado</a>, por <a name="04401000900032" ll="SDBG:περί:000002">volta</a> da <a name="04401000900034" ll="SDBG:ὥρα:000002">hora</a> <a name="04401000900036" ll="SDBG:ἕκτος:000000">sexta</a>, a fim de <a name="04401000900030" ll="SDBG:προσεύχομαι:000000" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20121121;NBGmk20140526">orar</a>. 
\v 10 Estando <a name="04401001000002" ll="SDBG:γίνομαι:000002">com</a> <a name="04401001000006" ll="SDBG:πρόσπεινος:000000">fome</a>, <a name="04401001000010" ll="SDBG:θέλω:000002">quis</a> <a name="04401001000012" ll="SDBG:γεύομαι:000001">comer</a>; <a name="04401001000004" ll="SDBG:δέ:000001">mas</a>, enquanto lhe <a name="04401001000014" ll="SDBG:παρασκευάζω:000000">preparavam</a> a comida, <a name="04401001000018" ll="SDBG:αὐτός:001000">sobreveio</a>-lhe <a name="04401001000022" ll="SDBG:ἐπί:000007">um</a> <a name="04401001000026" ll="SDBG:ἔκστασις:000001">êxtase</a>; 
\v 11 <a name="04401001100002" ll="SDBG:καί:000001">então</a>, <a name="04401001100004" ll="SDBG:θεωρέω:000000">viu</a> o <a name="04401001100008" ll="SDBG:οὐρανός:000000;SDBG:οὐρανός:000001">céu</a> <a name="04401001100010" ll="SDBG:ἀνοίγω:000000">aberto</a> <a name="04401001100012" ll="SDBG:καί:000001">e</a> <a name="04401001100014" ll="SDBG:καταβαίνω:000000">descendo</a> <a name="04401001100018">um</a> <a name="04401001100016" ll="SDBG:σκεῦος:000000;SDBG:σκεῦος:000001">objeto</a> <a name="04401001100020" ll="SDBG:ὡς:000000">como</a> se fosse um <a name="04401001100024" ll="SDBG:μέγας:000002">grande</a> <a name="04401001100022" ll="SDBG:ὀθόνη:000000" il="Bolen00103">lençol</a>, o qual era <a name="04401001100030" ll="SDBG:καθίημι:000000">baixado</a> <a name="04401001100032" ll="SDBG:ἐπί:000005">à</a> <a name="04401001100036" ll="SDBG:γῆ:000000">terra</a> pelas <a name="04401001100026" ll="SDBG:τέσσαρες:000000">quatro</a> <a name="04401001100028" ll="SDBG:ἀρχή:000007">pontas</a>, 
\v 12 <a name="04401001200006" ll="SDBG:ὑπάρχω:000000">contendo</a> toda <a name="04401001200008" ll="SDBG:πᾶς:000004">sorte</a> de <a name="04401001200012" ll="SDBG:τετράπους:000000">quadrúpedes</a>, <a name="04401001200016" ll="SDBG:ἑρπετόν:000000" il="Agama_Female;Agama_lizard;ArbeLevy-Vipera_palaestinae_4;BK00056;Fauna:4.3;Fauna:4.6a;Fauna:4.6c;Fauna:4.7b;Fauna:4.9a;Fauna:4.9b">répteis</a> da <a name="04401001200020" ll="SDBG:γῆ:000000">terra</a> <a name="04401001200014" ll="SDBG:καί:000000">e</a> aves do <a name="04401001200028" ll="SDBG:οὐρανός:000004">céu</a>. 
\v 13 <a name="04401001300002;04401001300018" ll="SDBG:καί:000001">E</a> ouviu-se uma <a name="04401001300006" ll="SDBG:φωνή:000001">voz</a> que se <a name="04401001300004" ll="SDBG:γίνομαι:000003">dirigia</a> <a name="04401001300008" ll="SDBG:πρός:000000">a</a> <a name="04401001300010" ll="SDBG:αὐτός:001000">ele</a>: <a name="04401001300012" ll="SDBG:ἀνίστημι:000003">Levanta</a>-te, <a name="04401001300014" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>! <a name="04401001300016" ll="SDBG:θύω:000001">Mata</a> e come. 
\v 14 <a name="04401001400004" ll="SDBG:δέ:000002">Mas</a> <a name="04401001400006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> replicou: De modo <a name="04401001400010" ll="SDBG:μηδαμῶς:000000">nenhum</a>, <a name="04401001400012" ll="SDBG:κύριος:000000">Senhor</a>! <a name="04401001400014" ll="SDBG:ὅτι:000001">Porque</a> <a name="04401001400016" ll="SDBG:οὐδέποτε:000000">jamais</a> comi coisa <a name="04401001400020" ll="SDBG:πᾶς:000001">alguma</a> <a name="04401001400022" ll="SDBG:κοινός:000001">comum</a> e <a name="04401001400026" ll="SDBG:ἀκάθαρτος:000000">imunda</a>. 
\v 15 Segunda <a name="04401001500010" ll="SDBG:δεύτερος:000000">vez</a>, a <a name="04401001500004" ll="SDBG:φωνή:000001">voz</a> <a name="04401001500012;04401001500014" ll="SDBG:αὐτός:001000;SDBG:πρός:000004">lhe</a> <a name="04401001500006;04401001500008" ll="SDBG:πάλιν:000000;SDBG:ἐκ:000010">falou</a>: Ao <a name="04401001500016" ll="SDBG:ὅς:000000">que</a> <a name="04401001500020" ll="SDBG:θεός:000000">Deus</a> <a name="04401001500022" ll="SDBG:καθαρίζω:000001">purificou</a> <a name="04401001500026" ll="SDBG:μή:000000">não</a> consideres <a name="04401001500024;04401001500028" ll="SDBG:κοινόω:000001;SDBG:σύ:000000">comum</a>. 
\v 16 <a name="04401001600006" ll="SDBG:γίνομαι:000003">Sucedeu</a> isto por três <a name="04401001600008" ll="SDBG:ἐπί:000017">vezes</a>, <a name="04401001600012" ll="SDBG:καί:000001">e</a>, logo, aquele <a name="04401001600020" ll="SDBG:σκεῦος:000000;SDBG:σκεῦος:000001">objeto</a> foi <a name="04401001600016" ll="SDBG:ἀναλαμβάνω:000000">recolhido</a> <a name="04401001600022" ll="SDBG:εἰς:000001">ao</a> <a name="04401001600026" ll="SDBG:οὐρανός:000000;SDBG:οὐρανός:000001">céu</a>.
    
\v 17 <a name="04401001700002" ll="SDBG:ὡς:000004">Enquanto</a> <a name="04401001700014" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> estava <a name="04401001700010" ll="SDBG:διαπορέω:000000">perplexo</a> <a name="04401001700016" ll="SDBG:τίς:000000">sobre</a> <a name="04401001700018" ll="SDBG:ἄν:000001">qual</a> seria o significado da <a name="04401001700024;04401001700026" ll="SDBG:ὅραμα:000001;SDBG:ὅς:000000">visão</a>, eis que os <a name="04401001700034" ll="SDBG:ἀνήρ:000000">homens</a> <a name="04401001700038" ll="SDBG:ἀποστέλλω:000000">enviados</a> da parte de <a name="04401001700044" ll="SDBG:Κορνήλιος:000000">Cornélio</a>, tendo <a name="04401001700046" ll="SDBG:διερωτάω:000000">perguntado</a> pela <a name="04401001700050" ll="SDBG:οἰκία:000000" il="HK00232;LB00231;LB00235;NBGbs20100214-204;NBGbs20110223-127;Realia:3.1;il_capernaum_house_d01">casa</a> de <a name="04401001700054" ll="SDBG:Σίμων:000005">Simão</a>, <a name="04401001700056" ll="SDBG:ἐφίστημι:000000">pararam</a> junto <a name="04401001700058" ll="SDBG:ἐπί:000001">à</a> <a name="04401001700062" ll="SDBG:πυλών:000000">porta</a>; 
\v 18 <a name="04401001800002" ll="SDBG:καί:000001">e</a>, <a name="04401001800004" ll="SDBG:φωνέω:000001">chamando</a>, <a name="04401001800006" ll="SDBG:πυνθάνομαι:000000">indagavam</a> <a name="04401001800008" ll="SDBG:εἰ:000002">se</a> <a name="04401001800020" ll="SDBG:ξενίζω:000000">estava</a> <a name="04401001800018" ll="SDBG:ἐνθάδε:000000">ali</a> hospedado <a name="04401001800010" ll="SDBG:Σίμων:000000">Simão</a>, <a name="04401001800012" ll="SDBG:ὁ:000000">por</a> sobrenome <a name="04401001800016" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>. 
\v 19 <a name="04401001900004">Enquanto</a> meditava <a name="04401001900006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> <a name="04401001900010" ll="SDBG:περί:000001">acerca</a> da <a name="04401001900014" ll="SDBG:ὅραμα:000001">visão</a>, <a name="04401001900018" ll="SDBG:αὐτός:001000">disse</a>-lhe o <a name="04401001900022" ll="SDBG:πνεῦμα:000000">Espírito</a>: Estão aí dois <a name="04401001900026" ll="SDBG:ἀνήρ:000000">homens</a> que te <a name="04401001900030" ll="SDBG:ζητέω:000000">procuram</a>; 
\v 20 <a name="04401002000004" ll="SDBG:ἀνίστημι:000003">levanta</a>-te, <a name="04401002000002" ll="SDBG:ἀλλά:000002;SDBG:ἀλλά:000003">pois</a>, <a name="04401002000006" ll="SDBG:καταβαίνω:000000">desce</a> <a name="04401002000008" ll="SDBG:καί:000001">e</a> <a name="04401002000010" ll="SDBG:πορεύομαι:000000">vai</a> <a name="04401002000012" ll="SDBG:σύν:000000">com</a> <a name="04401002000014" ll="SDBG:αὐτός:001000">eles</a>, <a name="04401002000016" ll="SDBG:μηδείς:000000">nada</a> <a name="04401002000018" ll="SDBG:διακρίνω:000003">duvidando</a>; porque <a name="04401002000022" ll="SDBG:ἐγώ:000000">eu</a> <a name="04401002000026" ll="SDBG:αὐτός:001000">os</a> <a name="04401002000024" ll="SDBG:ἀποστέλλω:000000">enviei</a>. 
\v 21 <a name="04401002100004" ll="SDBG:δέ:000001">E</a>, <a name="04401002100002" ll="SDBG:καταβαίνω:000000">descendo</a> <a name="04401002100006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> para <a name="04401002100008" ll="SDBG:πρός:000004">junto</a> dos <a name="04401002100012" ll="SDBG:ἀνήρ:000000">homens</a>, disse: Aqui <a name="04401002100018" ll="SDBG:ἐγώ:000000">me</a> <a name="04401002100020">tendes</a>; sou eu <a name="04401002100026" ll="SDBG:τίς:000000">a</a> <a name="04401002100022" ll="SDBG:ὅς:000000">quem</a> <a name="04401002100024" ll="SDBG:ζητέω:000000">buscais</a>? A <a name="04401002100030;04401002100032;04401002100034" ll="SDBG:αἰτία:000000;SDBG:διά:000005;SDBG:ὅς:000000">que</a> <a name="04401002100036" ll="SDBG:πάρειμι:000000">viestes</a>? 
\v 22 <a name="04401002200004" ll="SDBG:δέ:000001">Então</a>, disseram: O <a name="04401002200010" ll="SDBG:ἑκατοντάρχης:000000" il="Centurion2">centurião</a> <a name="04401002200008" ll="SDBG:Κορνήλιος:000000">Cornélio</a>, <a name="04401002200012" ll="SDBG:ἀνήρ:000000">homem</a> <a name="04401002200014" ll="SDBG:δίκαιος:000000">reto</a> <a name="04401002200016" ll="SDBG:καί:000000">e</a> <a name="04401002200018" ll="SDBG:φοβέω:000002">temente</a> a <a name="04401002200022" ll="SDBG:θεός:000000">Deus</a> <a name="04401002200026" ll="SDBG:τέ:000000">e</a> tendo bom <a name="04401002200024" ll="SDBG:μαρτυρέω:000001">testemunho</a> <a name="04401002200028" ll="SDBG:ὑπό:000002">de</a> <a name="04401002200030" ll="SDBG:ὅλος:000000">toda</a> a <a name="04401002200034" ll="SDBG:ἔθνος:000000">nação</a> <a name="04401002200038" ll="SDBG:Ἰουδαῖος:001000">judaica</a>, foi <a name="04401002200040" ll="SDBG:χρηματίζω:000000">instruído</a> <a name="04401002200042" ll="SDBG:ὑπό:000002">por</a> um <a name="04401002200046" ll="SDBG:ἅγιος:000000">santo</a> <a name="04401002200044" ll="SDBG:ἄγγελος:000001" il="Kneeling angel;tb031115118">anjo</a> para <a name="04401002200048" ll="SDBG:μεταπέμπω:000000">chamar</a>-te <a name="04401002200052" ll="SDBG:εἰς:000000">a</a> sua <a name="04401002200056" ll="SDBG:οἶκος:000000;SDBG:οἶκος:000006" il="LB00231;NBGbs20100214-204;Realia:3.1;il_capernaum_house_d01">casa</a> <a name="04401002200060" ll="SDBG:καί:000001">e</a> <a name="04401002200062" ll="SDBG:ἀκούω:000000">ouvir</a> <a name="04401002200066" ll="SDBG:παρά:000003">as</a> tuas <a name="04401002200064" ll="SDBG:ῥῆμα:000001">palavras</a>.
    
\v 23 Pedro, <a name="04401002300004" ll="SDBG:οὖν:000000">pois</a>, convidando-os a <a name="04401002300006" ll="SDBG:αὐτός:001000">entrar</a>, <a name="04401002300008" ll="SDBG:ξενίζω:000000">hospedou</a>-os. No dia <a name="04401002300014" ll="SDBG:ἐπαύριον:000000">seguinte</a>, levantou-se e <a name="04401002300018" ll="SDBG:ἐξέρχομαι:000000">partiu</a> <a name="04401002300020" ll="SDBG:σύν:000000">com</a> <a name="04401002300022" ll="SDBG:αὐτός:001000">eles</a>; <a name="04401002300024" ll="SDBG:καί:000002">também</a> <a name="04401002300026" ll="SDBG:τὶς:000000">alguns</a> <a name="04401002300030" ll="SDBG:ἀδελφός:000001">irmãos</a> dos que <a name="04401002300034" ll="SDBG:ἀπό:000002">habitavam</a> em <a name="04401002300036" ll="SDBG:Ἰόππη:000000" il="HK00370;Jaffa">Jope</a> <a name="04401002300038" ll="SDBG:συνέρχομαι:000001">foram</a> em sua <a name="04401002300040" ll="SDBG:αὐτός:001000">companhia</a>. 
\v 24 No dia <a name="04401002400006" ll="SDBG:ἐπαύριον:000000">imediato</a>, <a name="04401002400008" ll="SDBG:εἰσέρχομαι:000000">entrou</a> <a name="04401002400010" ll="SDBG:εἰς:000001">em</a> <a name="04401002400014" ll="SDBG:Καισάρεια:000000" il="Caesarea aquaduct;Caesarea-prison;il_caesarae_theatre_f01;il_caesarea_herod_palace_d03;il_caesarea_swimmingpool_f01;il_caesarea_theatre_d01">Cesareia</a>. <a name="04401002400020" ll="SDBG:Κορνήλιος:000000">Cornélio</a> estava esperando <a name="04401002400024" ll="SDBG:προσδοκάω:000001">por</a> <a name="04401002400026" ll="SDBG:αὐτός:001000">eles</a>, tendo <a name="04401002400028" ll="SDBG:συγκαλέω:000000">reunido</a> seus <a name="04401002400032" ll="SDBG:συγγενής:000000">parentes</a> <a name="04401002400036" ll="SDBG:καί:000000">e</a> <a name="04401002400042" ll="SDBG:φίλος:000000">amigos</a> <a name="04401002400040" ll="SDBG:ἀναγκαῖος:000001">íntimos</a>. 
\v 25 Aconteceu <a name="04401002500006" ll="SDBG:γίνομαι:000003">que</a>, <a name="04401002500010" ll="SDBG:εἰσέρχομαι:000000">indo</a> <a name="04401002500014" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> a entrar, <a name="04401002500018" ll="SDBG:αὐτός:001000">lhe</a> <a name="04401002500016" ll="SDBG:συναντάω:000000">saiu</a> <a name="04401002500022" ll="SDBG:Κορνήλιος:000000">Cornélio</a> ao encontro e, <a name="04401002500024" ll="SDBG:πίπτω:000002">prostrando</a>-se-lhe <a name="04401002500026" ll="SDBG:ἐπί:000001">aos</a> <a name="04401002500030" ll="SDBG:πούς:000000">pés</a>, o <a name="04401002500032" ll="SDBG:προσκυνέω:000001">adorou</a>. 
\v 26 <a name="04401002600004" ll="SDBG:δέ:000002">Mas</a> <a name="04401002600006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> <a name="04401002600010" ll="SDBG:αὐτός:001000">o</a> <a name="04401002600008" ll="SDBG:ἐγείρω:000000">levantou</a>, <a name="04401002600012" ll="SDBG:λέγω:000000">dizendo</a>: <a name="04401002600014" ll="SDBG:ἀνίστημι:000003">Ergue</a>-te, que <a name="04401002600020" ll="SDBG:αὐτός:001001">eu</a> também <a name="04401002600024">sou</a> <a name="04401002600022" ll="SDBG:ἄνθρωπος:000000">homem</a>. 
\v 27 <a name="04401002700004" ll="SDBG:συνομιλέω:000000">Falando</a> com <a name="04401002700006" ll="SDBG:αὐτός:001000">ele</a>, <a name="04401002700008" ll="SDBG:εἰσέρχομαι:000000">entrou</a>, <a name="04401002700012" ll="SDBG:εὑρίσκω:000000">encontrando</a> <a name="04401002700016" ll="SDBG:πολύς:000000">muitos</a> reunidos <a name="04401002700014" ll="SDBG:συνέρχομαι:000000">ali</a>, 
\v 28 a quem se <a name="04401002800006;04401002800008" ll="SDBG:αὐτός:001000;SDBG:πρός:000004">dirigiu</a>, <a name="04401002800002" ll="SDBG:φημί:000000">dizendo</a>: Vós bem <a name="04401002800012" ll="SDBG:ἐπίσταμαι:000000">sabeis</a> <a name="04401002800014" ll="SDBG:ὡς:000001">que</a> é <a name="04401002800016" ll="SDBG:ἀθέμιτος:000000">proibido</a> a <a name="04401002800020" ll="SDBG:ἀνήρ:000001">um</a> <a name="04401002800022" ll="SDBG:Ἰουδαῖος:000000">judeu</a> <a name="04401002800024" ll="SDBG:κολλάω:000000">ajuntar</a>-se ou <a name="04401002800026" ll="SDBG:ἤ:000000">mesmo</a> <a name="04401002800028" ll="SDBG:προσέρχομαι:000001">aproximar</a>-se a alguém de outra <a name="04401002800030" ll="SDBG:ἀλλόφυλος:000000">raça</a>; mas <a name="04401002800036" ll="SDBG:θεός:000000">Deus</a> me demonstrou que a <a name="04401002800040" ll="SDBG:μηδείς:000000">nenhum</a> <a name="04401002800050" ll="SDBG:ἄνθρωπος:000000">homem</a> <a name="04401002800048" ll="SDBG:λέγω:000002">considerasse</a> <a name="04401002800042" ll="SDBG:κοινός:000001">comum</a> <a name="04401002800044" ll="SDBG:ἤ:000000">ou</a> <a name="04401002800046" ll="SDBG:ἀκάθαρτος:000000">imundo</a>; 
\v 29 por <a name="04401002900002;04401002900004" ll="SDBG:διό:000000">isso</a>, uma vez <a name="04401002900010" ll="SDBG:μεταπέμπω:000000">chamado</a>, <a name="04401002900008" ll="SDBG:ἔρχομαι:000001">vim</a> sem vacilar. <a name="04401002900012" ll="SDBG:πυνθάνομαι:000000">Pergunto</a>, <a name="04401002900014" ll="SDBG:οὖν:000000">pois</a>: por <a name="04401002900016" ll="SDBG:τίς:000000">que</a> <a name="04401002900018" ll="SDBG:λόγος:000006">razão</a> me mandastes <a name="04401002900020" ll="SDBG:μεταπέμπω:000000">chamar</a>? 
\v 30 <a name="04401003000008" ll="SDBG:φημί:000000">Respondeu</a>-lhe <a name="04401003000006" ll="SDBG:Κορνήλιος:000000">Cornélio</a>: <a name="04401003000010">Faz</a>, hoje, <a name="04401003000012" ll="SDBG:τέταρτος:000000">quatro</a> <a name="04401003000014" ll="SDBG:ἡμέρα:000001">dias</a> que, por <a name="04401003000016" ll="SDBG:μέχρι:000000">volta</a> desta <a name="04401003000022" ll="SDBG:ὥρα:000002">hora</a>, estava eu observando <a name="04401003000032" ll="SDBG:ἐν:000000">em</a> minha <a name="04401003000036" ll="SDBG:οἶκος:000000;SDBG:οἶκος:000006" il="LB00231;NBGbs20100214-204;Realia:3.1;il_capernaum_house_d01">casa</a> a hora nona de <a name="04401003000030" ll="SDBG:προσεύχομαι:000000" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20121121;NBGmk20140526">oração</a>, <a name="04401003000002" ll="SDBG:καί:000001">e</a> eis que se <a name="04401003000046" ll="SDBG:ἵστημι:000008">apresentou</a> <a name="04401003000048" ll="SDBG:ἐνώπιον:000000">diante</a> de mim um <a name="04401003000044" ll="SDBG:ἀνήρ:000000">varão</a> <a name="04401003000052" ll="SDBG:ἐν:000004">de</a> <a name="04401003000054" ll="SDBG:ἐσθής:000000">vestes</a> <a name="04401003000056" ll="SDBG:λαμπρός:000000">resplandecentes</a> 
\v 31 <a name="04401003100002" ll="SDBG:καί:000001">e</a> <a name="04401003100004" ll="SDBG:φημί:000000">disse</a>: <a name="04401003100006" ll="SDBG:Κορνήλιος:000000">Cornélio</a>, a tua <a name="04401003100014" ll="SDBG:προσευχή:000000" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20121121;NBGmk20140526">oração</a> foi <a name="04401003100008" ll="SDBG:εἰσακούω:000000">ouvida</a>, <a name="04401003100016" ll="SDBG:καί:000000">e</a> as tuas <a name="04401003100020" ll="SDBG:ἐλεημοσύνη:000000">esmolas</a>, lembradas na <a name="04401003100026" ll="SDBG:ἐνώπιον:000001">presença</a> de <a name="04401003100030" ll="SDBG:θεός:000000">Deus</a>. 
\v 32 <a name="04401003200002" ll="SDBG:πέμπω:000002">Manda</a>, <a name="04401003200004" ll="SDBG:οὖν:000000">pois</a>, alguém <a name="04401003200006" ll="SDBG:εἰς:000000">a</a> <a name="04401003200008" ll="SDBG:Ἰόππη:000000" il="HK00370;Jaffa">Jope</a> a <a name="04401003200012" ll="SDBG:μετακαλέω:000000">chamar</a> <a name="04401003200014" ll="SDBG:Σίμων:000000;SDBG:Σίμων:000005">Simão</a>, <a name="04401003200016" ll="SDBG:ὅς:000000">por</a> sobrenome <a name="04401003200020" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>; <a name="04401003200024" ll="SDBG:ξενίζω:000000">acha</a>-se <a name="04401003200022" ll="SDBG:οὗτος:000000">este</a> hospedado <a name="04401003200026" ll="SDBG:ἐν:000000">em</a> <a name="04401003200028" ll="SDBG:οἰκία:000000" il="HK00232;LB00231;LB00235;NBGbs20100214-204;NBGbs20110223-127;Realia:3.1;il_capernaum_house_d01">casa</a> de <a name="04401003200030" ll="SDBG:Σίμων:000000">Simão</a>, <a name="04401003200032" ll="SDBG:βυρσεύς:000000" il="Realia:1.13;mat00991">curtidor</a>, <a name="04401003200034" ll="SDBG:παρά:000000">à</a> <a name="04401003200036" ll="SDBG:θάλασσα:000001">beira</a>-mar. 
\v 33 <a name="04401003300004" ll="SDBG:οὖν:000000">Portanto</a>, sem <a name="04401003300002" ll="SDBG:ἐξαυτῆς:000000">demora</a>, mandei <a name="04401003300006" ll="SDBG:πέμπω:000002">chamar</a>-te, <a name="04401003300014" ll="SDBG:τέ:000001">e</a> <a name="04401003300018" ll="SDBG:ποιέω:000005">fizeste</a> <a name="04401003300016" ll="SDBG:καλῶς:000000">bem</a> em <a name="04401003300020" ll="SDBG:παραγίνομαι:000000">vir</a>. <a name="04401003300022" ll="SDBG:νῦν:000000">Agora</a>, <a name="04401003300024" ll="SDBG:οὖν:000000">pois</a>, <a name="04401003300036" ll="SDBG:πάρειμι:000000">estamos</a> <a name="04401003300026" ll="SDBG:πᾶς:000000">todos</a> aqui, na <a name="04401003300030" ll="SDBG:ἐνώπιον:000001">presença</a> de <a name="04401003300034" ll="SDBG:θεός:000000">Deus</a>, prontos para <a name="04401003300038" ll="SDBG:ἀκούω:000000">ouvir</a> <a name="04401003300040" ll="SDBG:πᾶς:000000">tudo</a> o que te foi <a name="04401003300044" ll="SDBG:προστάσσω:000000">ordenado</a> da <a name="04401003300048" ll="SDBG:ὑπό:000002">parte</a> do <a name="04401003300052" ll="SDBG:κύριος:000000">Senhor</a>. 
\v 34 <a name="04401003400004" ll="SDBG:δέ:000001">Então</a>, <a name="04401003400002;04401003400010" ll="SDBG:στόμα:000001;SDBG:ἀνοίγω:000003">falou</a> <a name="04401003400006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>, dizendo: <a name="04401003400018" ll="SDBG:καταλαμβάνω:000004">Reconheço</a>, <a name="04401003400014">por</a> <a name="04401003400016" ll="SDBG:ἀλήθεια:000001">verdade</a>, <a name="04401003400020" ll="SDBG:ὅτι:000000">que</a> <a name="04401003400030" ll="SDBG:θεός:000000">Deus</a> <a name="04401003400022" ll="SDBG:οὐ:000000">não</a> faz acepção de pessoas; 
\v 35 pelo <a name="04401003500002" ll="SDBG:ἀλλά:000000">contrário</a>, <a name="04401003500004" ll="SDBG:ἐν:000000">em</a> <a name="04401003500006" ll="SDBG:πᾶς:000000">qualquer</a> <a name="04401003500008" ll="SDBG:ἔθνος:000000">nação</a>, aquele que <a name="04401003500014" ll="SDBG:αὐτός:001000">o</a> <a name="04401003500012" ll="SDBG:φοβέω:000001;SDBG:φοβέω:000002">teme</a> <a name="04401003500016" ll="SDBG:καί:000000">e</a> <a name="04401003500018" ll="SDBG:ἐργάζομαι:000002">faz</a> o que é <a name="04401003500020" ll="SDBG:δικαιοσύνη:000000">justo</a> <a name="04401003500024" ll="SDBG:αὐτός:001000">lhe</a> é <a name="04401003500022" ll="SDBG:δεκτός:000000">aceitável</a>. 
\v 36 Esta é a <a name="04401003600004" ll="SDBG:λόγος:000000">palavra</a> <a name="04401003600006" ll="SDBG:ὅς:000000">que</a> Deus <a name="04401003600008" ll="SDBG:ἀποστέλλω:000001">enviou</a> aos <a name="04401003600012" ll="SDBG:υἱός:000008">filhos</a> de <a name="04401003600014" ll="SDBG:Ἰσραήλ:000001">Israel</a>, anunciando-lhes o <a name="04401003600016" ll="SDBG:εὐαγγελίζω:000000">evangelho</a> da <a name="04401003600018" ll="SDBG:εἰρήνη:000000;SDBG:εἰρήνη:000001">paz</a>, por <a name="04401003600020" ll="SDBG:διά:000000">meio</a> de <a name="04401003600022" ll="SDBG:Ἰησοῦς:000000">Jesus</a> <a name="04401003600024" ll="SDBG:Χριστός:000001">Cristo</a>. <a name="04401003600026" ll="SDBG:οὗτος:000000">Este</a> é o <a name="04401003600032" ll="SDBG:κύριος:000000">Senhor</a> de <a name="04401003600030" ll="SDBG:πᾶς:000000">todos</a>. 
\v 37 Vós conheceis a <a name="04401003700010" ll="SDBG:ῥῆμα:000002">palavra</a> que se <a name="04401003700008" ll="SDBG:γίνομαι:000003">divulgou</a> <a name="04401003700012" ll="SDBG:κατά:000002">por</a> <a name="04401003700014" ll="SDBG:ὅλος:000000">toda</a> a <a name="04401003700018" ll="SDBG:Ἰουδαία:000001">Judeia</a>, tendo começado <a name="04401003700022" ll="SDBG:ἀπό:000002">desde</a> a <a name="04401003700026" ll="SDBG:Γαλιλαία:000000" il="tb052200102;tb110306599">Galileia</a>, <a name="04401003700028" ll="SDBG:μετά:000006">depois</a> do <a name="04401003700032" ll="SDBG:βάπτισμα:000000" il="Bolen00066">batismo</a> <a name="04401003700034" ll="SDBG:ὅς:000000">que</a> <a name="04401003700038" ll="SDBG:Ἰωάννης:000000">João</a> <a name="04401003700036" ll="SDBG:κηρύσσω:000002">pregou</a>, 
\v 38 <a name="04401003800010" ll="SDBG:ὡς:000001">como</a> <a name="04401003800018" ll="SDBG:θεός:000000">Deus</a> <a name="04401003800012" ll="SDBG:χρίω:000000" il="Anoint">ungiu</a> a <a name="04401003800002" ll="SDBG:Ἰησοῦς:000000">Jesus</a> <a name="04401003800006" ll="SDBG:ἀπό:000002">de</a> Nazaré com o <a name="04401003800020" ll="SDBG:πνεῦμα:000000">Espírito</a> <a name="04401003800022" ll="SDBG:ἅγιος:000000">Santo</a> <a name="04401003800024" ll="SDBG:καί:000000">e</a> com <a name="04401003800026" ll="SDBG:δύναμις:000001">poder</a>, o <a name="04401003800014;04401003800028" ll="SDBG:αὐτός:001000;SDBG:ὅς:000000">qual</a> andou por toda <a name="04401003800030" ll="SDBG:διέρχομαι:000001">parte</a>, fazendo o <a name="04401003800032" ll="SDBG:εὐεργετέω:000000">bem</a> <a name="04401003800034" ll="SDBG:καί:000000">e</a> <a name="04401003800036" ll="SDBG:ἰάομαι:000000">curando</a> a <a name="04401003800038" ll="SDBG:πᾶς:000000">todos</a> os <a name="04401003800042" ll="SDBG:καταδυναστεύω:000000">oprimidos</a> <a name="04401003800044" ll="SDBG:ὑπό:000002">do</a> <a name="04401003800048" ll="SDBG:διάβολος:000000">diabo</a>, <a name="04401003800050" ll="SDBG:ὅτι:000001">porque</a> <a name="04401003800054" ll="SDBG:θεός:000000">Deus</a> era <a name="04401003800058" ll="SDBG:μετά:000009">com</a> <a name="04401003800060" ll="SDBG:αὐτός:001000">ele</a>; 
\v 39 <a name="04401003900002" ll="SDBG:καί:000002">e</a> nós somos <a name="04401003900006" ll="SDBG:μάρτυς:000000">testemunhas</a> de <a name="04401003900008" ll="SDBG:πᾶς:000000">tudo</a> o <a name="04401003900010" ll="SDBG:ὅς:000000">que</a> ele <a name="04401003900012" ll="SDBG:ποιέω:000001">fez</a> <a name="04401003900014;04401003900016" ll="SDBG:καί:000005;SDBG:τέ:000002;SDBG:ἐν:000000">na</a> <a name="04401003900020" ll="SDBG:χώρα:000001">terra</a> dos <a name="04401003900024" ll="SDBG:Ἰουδαῖος:001000">judeus</a> <a name="04401003900026" ll="SDBG:καί:000005">e</a> <a name="04401003900028" ll="SDBG:ἐν:000000">em</a> Jerusalém; ao <a name="04401003900032" ll="SDBG:ὅς:000000">qual</a> também tiraram a <a name="04401003900036" ll="SDBG:ἀναιρέω:000000">vida</a>, <a name="04401003900038" ll="SDBG:κρεμάννυμι:000001">pendurando</a>-o <a name="04401003900040">no</a> <a name="04401003900042" ll="SDBG:ξύλον:000005">madeiro</a>. 
\v 40 A este <a name="04401004000008" ll="SDBG:ἐγείρω:000004">ressuscitou</a> <a name="04401004000006" ll="SDBG:θεός:000000">Deus</a> no <a name="04401004000014" ll="SDBG:τρίτος:000000">terceiro</a> <a name="04401004000016" ll="SDBG:ἡμέρα:000001">dia</a> <a name="04401004000018" ll="SDBG:καί:000001">e</a> <a name="04401004000026" ll="SDBG:γίνομαι:000002">concedeu</a> que <a name="04401004000020" ll="SDBG:δίδωμι:000002">fosse</a> <a name="04401004000024" ll="SDBG:ἐμφανής:000000">manifesto</a>, 
