\id ACT
\c 1
\v 1 <a name="04400100100010" ll="SDBG:ποιέω:000000">Escrevi</a> o <a name="04400100100004;04400100100006" ll="SDBG:μέν:000001;SDBG:πρῶτος:000000">primeiro</a> <a name="04400100100008" ll="SDBG:λόγος:000003">livro</a>, <a name="04400100100016" ll="SDBG:ὦ:000000">ó</a> <a name="04400100100018" ll="SDBG:Θεόφιλος:000000">Teófilo</a>, relatando todas as <a name="04400100100012;04400100100014" ll="SDBG:περί:000001;SDBG:πᾶς:000000">coisas</a> que <a name="04400100100026" ll="SDBG:Ἰησοῦς:000000">Jesus</a> <a name="04400100100030" ll="SDBG:καί:000005;SDBG:τέ:000003">começou</a> a <a name="04400100100028" ll="SDBG:ποιέω:000000">fazer</a> <a name="04400100100032" ll="SDBG:καί:000005">e</a> a <a name="04400100100034" ll="SDBG:διδάσκω:000000">ensinar</a> 
\v 2 <a name="04400100200002" ll="SDBG:ἄχρι(ς):000000">até</a> ao <a name="04400100200006" ll="SDBG:ἡμέρα:000001">dia</a> em <a name="04400100200004" ll="SDBG:ὅς:000000">que</a>, depois de haver dado <a name="04400100200008" ll="SDBG:ἐντέλλομαι:000000">mandamentos</a> por <a name="04400100200014" ll="SDBG:διά:000000">intermédio</a> do <a name="04400100200016" ll="SDBG:πνεῦμα:000000">Espírito</a> <a name="04400100200018" ll="SDBG:ἅγιος:000000">Santo</a> aos <a name="04400100200012" ll="SDBG:ἀπόστολος:000000">apóstolos</a> <a name="04400100200020" ll="SDBG:ὅς:000000">que</a> escolhera, foi elevado às <a name="04400100200024" ll="SDBG:ἀναλαμβάνω:000000">alturas</a>. 
\v 3 A <a name="04400100300002" ll="SDBG:ὅς:000000">estes</a> <a name="04400100300004" ll="SDBG:καί:000002">também</a>, depois <a name="04400100300012;04400100300018" ll="SDBG:αὐτός:001000;SDBG:μετά:000006">de</a> ter <a name="04400100300016" ll="SDBG:πάσχω:000000">padecido</a>, <a name="04400100300008" ll="SDBG:ἑαυτοῦ:000000">se</a> <a name="04400100300006" ll="SDBG:παρίστημι:000000">apresentou</a> <a name="04400100300010" ll="SDBG:ζάω:000001">vivo</a>, <a name="04400100300020" ll="SDBG:ἐν:000015">com</a> <a name="04400100300022" ll="SDBG:πολύς:000000">muitas</a> provas <a name="04400100300024" ll="SDBG:τεκμήριον:000000">incontestáveis</a>, <a name="04400100300032" ll="SDBG:ὀπτάνομαι:000000">aparecendo</a>-lhes <a name="04400100300026" ll="SDBG:διά:000008">durante</a> quarenta <a name="04400100300028" ll="SDBG:ἡμέρα:000007">dias</a> <a name="04400100300036" ll="SDBG:καί:000002">e</a> <a name="04400100300038" ll="SDBG:λέγω:000000">falando</a> das coisas <a name="04400100300042" ll="SDBG:περί:000001">concernentes</a> ao <a name="04400100300046" ll="SDBG:βασιλεία:000000">reino</a> de <a name="04400100300050" ll="SDBG:θεός:000000">Deus</a>. 
\v 4 <a name="04400100400002" ll="SDBG:καί:000001">E</a>, comendo com <a name="04400100400008" ll="SDBG:αὐτός:001000">eles</a>, <a name="04400100400006" ll="SDBG:παραγγέλλω:000000">determinou</a>-lhes que <a name="04400100400014" ll="SDBG:μή:000000">não</a> se <a name="04400100400016" ll="SDBG:χωρίζω:000002">ausentassem</a> <a name="04400100400010" ll="SDBG:ἀπό:000002">de</a> <a name="04400100400012" ll="SDBG:Ἱεροσόλυμα:000000" il="HK00236;NBGbs20090227-1-3;NBGbs20090227-1_14;Satellite Bible Atlas00025;Temple_Mount;jlm_herod_d01;jlm_topography_d04;tb010907368">Jerusalém</a>, <a name="04400100400018" ll="SDBG:ἀλλά:000000">mas</a> que <a name="04400100400020" ll="SDBG:περιμένω:000000">esperassem</a> a <a name="04400100400024" ll="SDBG:ἐπαγγελία:000000">promessa</a> do <a name="04400100400028" ll="SDBG:πατήρ:000003">Pai</a>, a <a name="04400100400030" ll="SDBG:ὅς:000000">qual</a>, disse ele, de mim <a name="04400100400032" ll="SDBG:ἀκούω:000000">ouvistes</a>. 
\v 5 <a name="04400100500002" ll="SDBG:ὅτι:000001">Porque</a> <a name="04400100500004" ll="SDBG:Ἰωάννης:000000">João</a>, na <a name="04400100500006" ll="SDBG:μέν:000003">verdade</a>, <a name="04400100500008" ll="SDBG:βαπτίζω:000001" il="Bolen00066">batizou</a> com <a name="04400100500010" ll="SDBG:ὕδωρ:000000">água</a>, <a name="04400100500014" ll="SDBG:δέ:000004">mas</a> vós sereis <a name="04400100500020" ll="SDBG:βαπτίζω:000001" il="Bolen00066">batizados</a> com1 o <a name="04400100500018" ll="SDBG:πνεῦμα:000000">Espírito</a> <a name="04400100500022" ll="SDBG:ἅγιος:000000">Santo</a>, <a name="04400100500024" ll="SDBG:οὐ:000000">não</a> <a name="04400100500028" ll="SDBG:πολύς:000000">muito</a> <a name="04400100500026" ll="SDBG:μετά:000006">depois</a> destes <a name="04400100500032" ll="SDBG:ἡμέρα:000001">dias</a>.
    
\v 6 <a name="04400100600004" ll="SDBG:μέν:000000">Então</a>, os <a name="04400100600006">que</a> estavam <a name="04400100600008" ll="SDBG:συνέρχομαι:000000">reunidos</a> <a name="04400100600012" ll="SDBG:αὐτός:001000">lhe</a> <a name="04400100600014" ll="SDBG:λέγω:000000">perguntaram</a>: <a name="04400100600016;04400100600018" ll="SDBG:εἰ:000000;SDBG:κύριος:000000">Senhor</a>, será este o <a name="04400100600024" ll="SDBG:χρόνος:000000">tempo</a> <a name="04400100600020" ll="SDBG:ἐν:000019">em</a> que restaures o <a name="04400100600032" ll="SDBG:βασιλεία:000000">reino</a> a <a name="04400100600036" ll="SDBG:Ἰσραήλ:000001">Israel</a>? 
\v 7 <a name="04400100700006" ll="SDBG:πρός:000004">Respondeu</a>-lhes: <a name="04400100700010" ll="SDBG:οὐ:000000">Não</a> vos compete <a name="04400100700016" ll="SDBG:γινώσκω:000000">conhecer</a> <a name="04400100700018" ll="SDBG:χρόνος:000001">tempos</a> <a name="04400100700020" ll="SDBG:ἤ:000000">ou</a> <a name="04400100700022" ll="SDBG:καιρός:000000">épocas</a> <a name="04400100700024" ll="SDBG:ὅς:000000">que</a> o <a name="04400100700028" ll="SDBG:πατήρ:000003">Pai</a> <a name="04400100700030" ll="SDBG:τίθημι:000005">reservou</a> <a name="04400100700032" ll="SDBG:ἐν:000010">pela</a> sua <a name="04400100700036" ll="SDBG:ἴδιος:000000">exclusiva</a> <a name="04400100700038" ll="SDBG:ἐξουσία:000000">autoridade</a>; 
\v 8 <a name="04400100800002" ll="SDBG:ἀλλά:000000">mas</a> <a name="04400100800004" ll="SDBG:λαμβάνω:000007">recebereis</a> <a name="04400100800006" ll="SDBG:δύναμις:000001">poder</a>, ao <a name="04400100800008" ll="SDBG:ἐπέρχομαι:000002">descer</a> <a name="04400100800016" ll="SDBG:ἐπί:000007">sobre</a> vós o <a name="04400100800014" ll="SDBG:πνεῦμα:000000">Espírito</a> <a name="04400100800012" ll="SDBG:ἅγιος:000000">Santo</a>, <a name="04400100800020">e</a> sereis minhas <a name="04400100800026" ll="SDBG:μάρτυς:000000">testemunhas</a> <a name="04400100800030" ll="SDBG:καί:000005;SDBG:τέ:000002">tanto</a> <a name="04400100800028" ll="SDBG:ἐν:000000">em</a> Jerusalém <a name="04400100800034" ll="SDBG:καί:000005">como</a> <a name="04400100800036" ll="SDBG:ἐν:000000">em</a> toda <a name="04400100800038" ll="SDBG:πᾶς:000000">a</a> <a name="04400100800042" ll="SDBG:Ἰουδαία:000000">Judeia</a> <a name="04400100800044" ll="SDBG:καί:000000">e</a> <a name="04400100800046" ll="SDBG:Σαμάρεια:000000" il="tb070507860">Samaria</a> <a name="04400100800048" ll="SDBG:καί:000000">e</a> <a name="04400100800050" ll="SDBG:ἕως:000002">até</a> aos <a name="04400100800052" ll="SDBG:ἔσχατος:000000">confins</a> da <a name="04400100800056" ll="SDBG:γῆ:000000">terra</a>. 
\v 9 Ditas estas palavras, foi Jesus elevado às <a name="04400100900012" ll="SDBG:ἐπαίρω:000000">alturas</a>, à <a name="04400100900008" ll="SDBG:βλέπω:000000">vista</a> <a name="04400100900010" ll="SDBG:αὐτός:001000">deles</a>, <a name="04400100900002" ll="SDBG:καί:000001">e</a> uma <a name="04400100900016" ll="SDBG:νεφέλη:000000">nuvem</a> <a name="04400100900020" ll="SDBG:αὐτός:001000">o</a> <a name="04400100900018" ll="SDBG:ὑπολαμβάνω:000000">encobriu</a> <a name="04400100900022" ll="SDBG:ἀπό:000000">dos</a> <a name="04400100900028" ll="SDBG:αὐτός:001000">seus</a> <a name="04400100900026" ll="SDBG:ὀφθαλμός:000001">olhos</a>. 
\v 10 <a name="04400101000002" ll="SDBG:καί:000001">E</a>, estando eles com os olhos <a name="04400101000006" ll="SDBG:ἀτενίζω:000000">fitos</a> <a name="04400101000010" ll="SDBG:εἰς:000000">no</a> <a name="04400101000014" ll="SDBG:οὐρανός:000001">céu</a>, enquanto <a name="04400101000018" ll="SDBG:αὐτός:001000">Jesus</a> <a name="04400101000016" ll="SDBG:πορεύομαι:000002">subia</a>, eis que <a name="04400101000026" ll="SDBG:δύο:000000">dois</a> <a name="04400101000024" ll="SDBG:ἀνήρ:000000">varões</a> vestidos <a name="04400101000032;04400101000034" ll="SDBG:ἐν:000007;SDBG:ἐσθής:000000">de</a> <a name="04400101000036" ll="SDBG:λευκός:000000">branco</a> se puseram ao <a name="04400101000028" ll="SDBG:παρίστημι:000006">lado</a> deles 
\v 11 <a name="04400101100002;04400101100004" ll="SDBG:καί:000002;SDBG:ὅς:000000">e</a> lhes disseram: <a name="04400101100008" ll="SDBG:ἀνήρ:000001">Varões</a> <a name="04400101100010" ll="SDBG:Γαλιλαῖος:000000">galileus</a>, por <a name="04400101100012" ll="SDBG:τίς:000001">que</a> <a name="04400101100014" ll="SDBG:ἵστημι:000006">estais</a> <a name="04400101100016" ll="SDBG:ἐμβλέπω:000000">olhando</a> para <a name="04400101100018" ll="SDBG:εἰς:000001">as</a> <a name="04400101100022" ll="SDBG:οὐρανός:000001">alturas</a>? <a name="04400101100024" ll="SDBG:οὗτος:000000">Esse</a> <a name="04400101100028" ll="SDBG:Ἰησοῦς:000000">Jesus</a> <a name="04400101100020" ll="SDBG:ὁ:000000">que</a> <a name="04400101100034" ll="SDBG:ἀπό:000000">dentre</a> vós foi <a name="04400101100032" ll="SDBG:ἀναλαμβάνω:000000">assunto</a> <a name="04400101100038" ll="SDBG:εἰς:000001">ao</a> <a name="04400101100042" ll="SDBG:οὐρανός:000001">céu</a> <a name="04400101100046" ll="SDBG:ἔρχομαι:000001">virá</a> do <a name="04400101100044" ll="SDBG:οὕτω:000000">modo</a> <a name="04400101100050" ll="SDBG:τρόπος:000000">como</a> <a name="04400101100054" ll="SDBG:αὐτός:001000">o</a> <a name="04400101100052" ll="SDBG:θεάομαι:000000">vistes</a> <a name="04400101100056" ll="SDBG:πορεύομαι:000002">subir</a>.
    
\v 12 <a name="04400101200002" ll="SDBG:τότε:000000">Então</a>, <a name="04400101200004" ll="SDBG:ὑποστρέφω:000000">voltaram</a> <a name="04400101200006" ll="SDBG:εἰς:000000">para</a> Jerusalém, <a name="04400101200010" ll="SDBG:ἀπό:000002">do</a> <a name="04400101200012" ll="SDBG:ὄρος:000000" il="20100409_korinthos03;jo-kafrein_f01;tb011017754;tb031315454">monte</a> <a name="04400101200016" ll="SDBG:καλέω:000000">chamado</a> <a name="04400101200018" ll="SDBG:ἐλαιών:000000" il="Aerial-Mount_of_Olives;Fauna:2.28;Gethsemane389;LB00087;NBGbs20090227 1_31;NBGbs20110227-41;NBGbs20110227-42;NBGbs20120710-171;NBGpb20140124a;NBGpb20140204;Olives in press, rp;Olives, black, rp;RS26272_ISR91T-117;il_oil_press_d01;tb052816082">Olival</a>, <a name="04400101200020" ll="SDBG:ὅς:000000">que</a> dista <a name="04400101200024" ll="SDBG:ἐγγύς:000000">daquela</a> cidade tanto como a jornada de um <a name="04400101200028;04400101200030;04400101200032" ll="SDBG:σάββατον:000002;SDBG:ἔχω:000006;SDBG:ὁδός:000006">sábado</a>. 
\v 13 <a name="04400101300004">Quando</a> ali <a name="04400101300006" ll="SDBG:εἰσέρχομαι:000000">entraram</a>, <a name="04400101300014" ll="SDBG:ἀναβαίνω:000000">subiram</a> <a name="04400101300008" ll="SDBG:εἰς:000001">para</a> o <a name="04400101300012" ll="SDBG:ὑπερῷον:000000" il="LB00235;Realia:3.1.6.4">cenáculo</a> <a name="04400101300016" ll="SDBG:οὗ:000000">onde</a> se <a name="04400101300020;04400101300024" ll="SDBG:καταμένω:000000;SDBG:καί:000005;SDBG:τέ:000002">reuniam</a> <a name="04400101300002;04400101300026" ll="SDBG:Πέτρος:000000;SDBG:καί:000001" il="cd090085005;tb061516556">Pedro</a>, <a name="04400101300028;04400101300030" ll="SDBG:καί:000005;SDBG:Ἰωάννης:000001">João</a>, <a name="04400101300032;04400101300034" ll="SDBG:καί:000005;SDBG:Ἰάκωβος:000000">Tiago</a>, <a name="04400101300038" ll="SDBG:Ἀνδρέας:000000">André</a>, <a name="04400101300036;04400101300040" ll="SDBG:Φίλιππος:000000;SDBG:καί:000005">Filipe</a>, <a name="04400101300044" ll="SDBG:Θωμᾶς:000000">Tomé</a>, <a name="04400101300042;04400101300046" ll="SDBG:Βαρθολομαῖος:000000;SDBG:καί:000005">Bartolomeu</a>, Mateus, <a name="04400101300052" ll="SDBG:Ἰάκωβος:000002">Tiago</a>, filho de <a name="04400101300048" ll="SDBG:καί:000005">Alfeu</a>, <a name="04400101300058" ll="SDBG:Σίμων:000001">Simão</a>, o Zelote, <a name="04400101300056" ll="SDBG:καί:000005">e</a> <a name="04400101300066" ll="SDBG:Ἰούδας:000001">Judas</a>, filho de <a name="04400101300068" ll="SDBG:Ἰάκωβος:000004">Tiago</a>. 
\v 14 <a name="04400101400004" ll="SDBG:πᾶς:000000">Todos</a> <a name="04400101400002" ll="SDBG:οὗτος:000000">estes</a> <a name="04400101400008" ll="SDBG:προσκαρτερέω:000000">perseveravam</a> <a name="04400101400010" ll="SDBG:ὁμοθυμαδόν:000000">unânimes</a> em <a name="04400101400014" ll="SDBG:προσευχή:000000" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20121121;NBGmk20140526">oração</a>, <a name="04400101400016" ll="SDBG:σύν:000000">com</a> as <a name="04400101400018" ll="SDBG:γυνή:000000">mulheres</a>, <a name="04400101400020" ll="SDBG:καί:000000">com</a> <a name="04400101400022" ll="SDBG:Μαρία:000000">Maria</a>, <a name="04400101400026" ll="SDBG:μήτηρ:000000">mãe</a> de <a name="04400101400030" ll="SDBG:Ἰησοῦς:000000">Jesus</a>, <a name="04400101400032" ll="SDBG:καί:000000">e</a> com os <a name="04400101400036" ll="SDBG:ἀδελφός:000000">irmãos</a> <a name="04400101400038" ll="SDBG:αὐτός:001000">dele</a>.
    
\v 15 <a name="04400101500002;04400101500004" ll="SDBG:καί:000004;SDBG:ἐν:000019">Naqueles</a> <a name="04400101500008" ll="SDBG:ἡμέρα:000001">dias</a>, <a name="04400101500012" ll="SDBG:ἀνίστημι:000003">levantou</a>-se <a name="04400101500014" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a> <a name="04400101500016" ll="SDBG:ἐν:000001">no</a> <a name="04400101500018" ll="SDBG:μέσος:000000">meio</a> dos <a name="04400101500022" ll="SDBG:ἀδελφός:000001">irmãos</a> (ora, compunha-se a <a name="04400101500034;04400101500038" ll="SDBG:αὐτός:000001">assembleia</a> de <a name="04400101500030" ll="SDBG:ὄχλος:000000">umas</a> <a name="04400101500042" ll="SDBG:ἑκατόν:000000">cento</a> e <a name="04400101500044" ll="SDBG:εἴκοσι:000000">vinte</a> <a name="04400101500032" ll="SDBG:ὄνομα:000001">pessoas</a>) e <a name="04400101500028" ll="SDBG:τέ:000000">disse</a>: 
\v 16 <a name="04400101600002;04400101600004" ll="SDBG:ἀδελφός:000001;SDBG:ἀνήρ:000000">Irmãos</a>, <a name="04400101600006" ll="SDBG:δεῖ:000000">convinha</a> que se <a name="04400101600008" ll="SDBG:πληρόω:000006">cumprisse</a> a <a name="04400101600012" ll="SDBG:γραφή:000001" il="LB00279;NBGpb20140728b;Scribe at work;WritingScroll-ReedPen-01-065d;tb102605482">Escritura</a> <a name="04400101600014" ll="SDBG:ὅς:000000">que</a> o <a name="04400101600020" ll="SDBG:πνεῦμα:000000">Espírito</a> <a name="04400101600024" ll="SDBG:ἅγιος:000000">Santo</a> proferiu anteriormente <a name="04400101600026" ll="SDBG:διά:000002">por</a> <a name="04400101600028" ll="SDBG:στόμα:000001">boca</a> de Davi, acerca <a name="04400101600032" ll="SDBG:περί:000001">de</a> <a name="04400101600034" ll="SDBG:Ἰούδας:000000">Judas</a>, <a name="04400101600010" ll="SDBG:ὁ:000000">que</a> <a name="04400101600038" ll="SDBG:γίνομαι:000002">foi</a> o <a name="04400101600040" ll="SDBG:ὁδηγός:000000">guia</a> daqueles que <a name="04400101600044" ll="SDBG:συλλαμβάνω:000000">prenderam</a> <a name="04400101600046" ll="SDBG:Ἰησοῦς:000000">Jesus</a>, 
\v 17 <a name="04400101700002" ll="SDBG:ὅτι:000001">porque</a> ele era <a name="04400101700004" ll="SDBG:καταριθμέω:000000">contado</a> entre nós <a name="04400101700012" ll="SDBG:καί:000002">e</a> <a name="04400101700014" ll="SDBG:λαγχάνω:000000">teve</a> <a name="04400101700018" ll="SDBG:κλῆρος:000002;SDBG:κλῆρος:000004" il="tb032014463;tb041705921">parte</a> neste <a name="04400101700022" ll="SDBG:διακονία:000001" il="tb051603202">ministério</a>. 
\v 18 (<a name="04400101800004;04400101800006" ll="SDBG:μέν:000000">Ora</a>, este <a name="04400101800002" ll="SDBG:οὗτος:000000">homem</a> <a name="04400101800008" ll="SDBG:κτάομαι:000000">adquiriu</a> um <a name="04400101800010" ll="SDBG:χωρίον:000000">campo</a> <a name="04400101800012" ll="SDBG:ἐκ:000002;SDBG:ἐκ:000012">com</a> o <a name="04400101800014" ll="SDBG:μισθός:000001">preço</a> da <a name="04400101800018" ll="SDBG:ἀδικία:000000">iniquidade</a>; <a name="04400101800020" ll="SDBG:καί:000001">e</a>, <a name="04400101800022" ll="SDBG:πρηνής:000000">precipitando</a>-se, rompeu-se pelo <a name="04400101800028" ll="SDBG:μέσος:000001">meio</a>, <a name="04400101800030" ll="SDBG:καί:000001">e</a> <a name="04400101800034" ll="SDBG:πᾶς:000000">todas</a> as <a name="04400101800040" ll="SDBG:αὐτός:001000">suas</a> <a name="04400101800038" ll="SDBG:σπλάγχνον:000000">entranhas</a> se <a name="04400101800032" ll="SDBG:ἐκχέω:000004">derramaram</a>; 
\v 19 <a name="04400101900002" ll="SDBG:καί:000001">e</a> isto <a name="04400101900006" ll="SDBG:γίνομαι:000002">chegou</a> ao <a name="04400101900004" ll="SDBG:γνωστός:000000">conhecimento</a> de <a name="04400101900008" ll="SDBG:πᾶς:000000">todos</a> os <a name="04400101900012" ll="SDBG:κατοικέω:000000">habitantes</a> de Jerusalém, de <a name="04400101900016" ll="SDBG:ὥστε:000000">maneira</a> <a name="04400101900024" ll="SDBG:ἐκεῖνος:000000">que</a> em <a name="04400101900032" ll="SDBG:αὐτός:001000">sua</a> <a name="04400101900028" ll="SDBG:ἴδιος:000000">própria</a> <a name="04400101900030" ll="SDBG:διάλεκτος:000000">língua</a> esse <a name="04400101900022;04400101900040" ll="SDBG:χωρίον:000000">campo</a> era <a name="04400101900018" ll="SDBG:καλέω:000000">chamado</a> Aceldama, isto é, Campo de <a name="04400101900042" ll="SDBG:αἷμα:000000">Sangue</a>.) 
\v 20 <a name="04400102000004" ll="SDBG:γάρ:000000">Porque</a> está <a name="04400102000002" ll="SDBG:γράφω:000001">escrito</a> <a name="04400102000006" ll="SDBG:ἐν:000000">no</a> <a name="04400102000008" ll="SDBG:βίβλος:000001" il="HK00153">Livro</a> dos <a name="04400102000010" ll="SDBG:ψαλμός:000000">Salmos</a>:<a name="04400102000012" ll="SDBG:γίνομαι:000002">Fique</a> <a name="04400102000020" ll="SDBG:ἔρημος:000000">deserta</a> a <a name="04400102000018" ll="SDBG:αὐτός:001000">sua</a> <a name="04400102000016" ll="SDBG:ἔπαυλις:000000">morada</a>; <a name="04400102000022" ll="SDBG:καί:000000">e</a> não haja <a name="04400102000024" ll="SDBG:μή:000000">quem</a> <a name="04400102000032;04400102000034" ll="SDBG:αὐτός:001000;SDBG:ἐν:000000">nela</a> <a name="04400102000030" ll="SDBG:κατοικέω:000000">habite</a>;<a name="04400102000036" ll="SDBG:καί:000000">e</a>:<a name="04400102000044" ll="SDBG:λαμβάνω:000002">Tome</a> <a name="04400102000046" ll="SDBG:ἕτερος:000001">outro</a> o <a name="04400102000042" ll="SDBG:αὐτός:001000">seu</a> <a name="04400102000040" ll="SDBG:ἐπισκοπή:000001;SDBG:ἐπισκοπή:000002">encargo</a>.
    
\v 21 É necessário, <a name="04400102100004" ll="SDBG:οὖν:000000">pois</a>, que, dos <a name="04400102100012" ll="SDBG:ἀνήρ:000000">homens</a> que nos <a name="04400102100008" ll="SDBG:συνέρχομαι:000001">acompanharam</a> <a name="04400102100016" ll="SDBG:πᾶς:000003">todo</a> o <a name="04400102100018" ll="SDBG:χρόνος:000000">tempo</a> <a name="04400102100014;04400102100020" ll="SDBG:ἐν:000019;SDBG:ὅς:000000">que</a> o <a name="04400102100034" ll="SDBG:κύριος:000000">Senhor</a> <a name="04400102100036" ll="SDBG:Ἰησοῦς:000000">Jesus</a> <a name="04400102100022;04400102100026" ll="SDBG:εἰσέρχομαι:000004;SDBG:ἐξέρχομαι:000003">andou</a> <a name="04400102100028" ll="SDBG:ἐπί:000002">entre</a> nós, 
\v 22 começando <a name="04400102200004" ll="SDBG:ἀπό:000007">no</a> <a name="04400102200008" ll="SDBG:βάπτισμα:000000" il="Bolen00066">batismo</a> de <a name="04400102200010" ll="SDBG:Ἰωάννης:000000">João</a>, <a name="04400102200012" ll="SDBG:ἕως:000000">até</a> ao <a name="04400102200016" ll="SDBG:ἡμέρα:000001">dia</a> em <a name="04400102200018" ll="SDBG:ὅς:000000">que</a> <a name="04400102200022" ll="SDBG:ἀπό:000002">dentre</a> nós foi levado às <a name="04400102200020" ll="SDBG:ἀναλαμβάνω:000000">alturas</a>, <a name="04400102200040" ll="SDBG:εἷς:000000">um</a> destes se <a name="04400102200038" ll="SDBG:γίνομαι:000002">torne</a> <a name="04400102200026" ll="SDBG:μάρτυς:000000">testemunha</a> <a name="04400102200034" ll="SDBG:σύν:000000">conosco</a> da <a name="04400102200032" ll="SDBG:αὐτός:001000">sua</a> <a name="04400102200030" ll="SDBG:ἀνάστασις:000000">ressurreição</a>. 
\v 23 <a name="04400102300002">Então</a>, <a name="04400102300004" ll="SDBG:ἵστημι:000004;SDBG:ἵστημι:000005">propuseram</a> <a name="04400102300006" ll="SDBG:δύο:000000">dois</a>: <a name="04400102300008" ll="SDBG:Ἰωσήφ:000005">José</a>, <a name="04400102300012" ll="SDBG:καλέω:000000">chamado</a> <a name="04400102300016" ll="SDBG:ὅς:000000">Barsabás</a>, cognominado <a name="04400102300020" ll="SDBG:Ἰοῦστος:000000">Justo</a>, <a name="04400102300022" ll="SDBG:καί:000000">e</a> Matias. 
\v 24 <a name="04400102400002" ll="SDBG:καί:000001">E</a>, <a name="04400102400004" ll="SDBG:προσεύχομαι:000000" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20121121;NBGmk20140526">orando</a>, disseram: <a name="04400102400008" ll="SDBG:σύ:000000">Tu</a>, <a name="04400102400010" ll="SDBG:κύριος:000000">Senhor</a>, que conheces o <a name="04400102400012" ll="SDBG:καρδιογνώστης:000000">coração</a> de <a name="04400102400014" ll="SDBG:πᾶς:000000">todos</a>, <a name="04400102400016" ll="SDBG:ἀναδείκνυμι:000000">revela</a>-nos <a name="04400102400030" ll="SDBG:εἷς:000000">qual</a> <a name="04400102400022" ll="SDBG:ἐκ:000008">destes</a> <a name="04400102400028" ll="SDBG:δύο:000000">dois</a> tens escolhido 
\v 25 para <a name="04400102500002" ll="SDBG:λαμβάνω:000002">preencher</a> a vaga neste <a name="04400102500010" ll="SDBG:διακονία:000001" il="tb051603202">ministério</a> <a name="04400102500014" ll="SDBG:καί:000000">e</a> <a name="04400102500016" ll="SDBG:ἀποστολή:000000">apostolado</a>, do <a name="04400102500020" ll="SDBG:ὅς:000000">qual</a> <a name="04400102500024" ll="SDBG:Ἰούδας:000000">Judas</a> se <a name="04400102500022" ll="SDBG:παραβαίνω:000001">transviou</a>, <a name="04400102500026" ll="SDBG:πορεύομαι:000002">indo</a> <a name="04400102500028" ll="SDBG:εἰς:000000">para</a> o seu <a name="04400102500036" ll="SDBG:ἴδιος:000001">próprio</a> <a name="04400102500006" ll="SDBG:τόπος:000002;SDBG:τόπος:000003">lugar</a>. 
\v 26 <a name="04400102600002" ll="SDBG:καί:000001">E</a> os <a name="04400102600004" ll="SDBG:δίδωμι:000000">lançaram</a> <a name="04400102600008" ll="SDBG:αὐτός:001000">em</a> <a name="04400102600006" ll="SDBG:κλῆρος:000000" il="tb032014463;tb041705921">sortes</a>, vindo a <a name="04400102600016" ll="SDBG:κλῆρος:000005" il="tb032014463;tb041705921">sorte</a> <a name="04400102600012" ll="SDBG:πίπτω:000011">recair</a> <a name="04400102600018">sobre</a> Matias, sendo-lhe, então, votado lugar <a name="04400102600026" ll="SDBG:μετά:000000">com</a> os <a name="04400102600030" ll="SDBG:ἕνδεκα:000000">onze</a> <a name="04400102600032" ll="SDBG:ἀπόστολος:000000">apóstolos</a>.
\c 2
\v 1 <a name="04400200100004" ll="SDBG:ἐν:000018">Ao</a> cumprir-se o <a name="04400200100012" ll="SDBG:ἡμέρα:000000">dia</a> de <a name="04400200100016" ll="SDBG:πεντηκοστή:000000">Pentecostes</a>, estavam todos <a name="04400200100008" ll="SDBG:συμπληρόω:000001">reunidos</a> <a name="04400200100024">no</a> mesmo <a name="04400200100028" ll="SDBG:αὐτός:000001">lugar</a>; 
\v 2 de <a name="04400200200006" ll="SDBG:ἄφνω:000000">repente</a>, <a name="04400200200004" ll="SDBG:γίνομαι:000003">veio</a> <a name="04400200200008" ll="SDBG:ἐκ:000000">do</a> <a name="04400200200012" ll="SDBG:οὐρανός:000001">céu</a> um som, <a name="04400200200016" ll="SDBG:ὥσπερ:000000">como</a> de um <a name="04400200200020" ll="SDBG:πνοή:000000">vento</a> <a name="04400200200018;04400200200022" ll="SDBG:βίαιος:000000;SDBG:φέρω:000014">impetuoso</a>, <a name="04400200200002">e</a> <a name="04400200200026" ll="SDBG:πληρόω:000000">encheu</a> <a name="04400200200028" ll="SDBG:ὅλος:000000">toda</a> a <a name="04400200200032" ll="SDBG:οἶκος:000000;SDBG:οἶκος:000006" il="LB00231;NBGbs20100214-204;Realia:3.1;il_capernaum_house_d01">casa</a> <a name="04400200200034" ll="SDBG:οὗ:000000">onde</a> estavam <a name="04400200200038" ll="SDBG:κάθημαι:000000">assentados</a>. 
\v 3 <a name="04400200300002" ll="SDBG:καί:000000">E</a> apareceram, <a name="04400200300008" ll="SDBG:διαμερίζω:000002;SDBG:διαμερίζω:000003">distribuídas</a> entre <a name="04400200300006" ll="SDBG:αὐτός:001000">eles</a>, <a name="04400200300010" ll="SDBG:γλῶσσα:000000">línguas</a>, <a name="04400200300012" ll="SDBG:ὡσεί:000000">como</a> de <a name="04400200300014" ll="SDBG:πῦρ:000000" il="Fire02">fogo</a>, e <a name="04400200300018" ll="SDBG:καθίζω:000002">pousou</a> uma <a name="04400200300020" ll="SDBG:ἐπί:000000">sobre</a> cada <a name="04400200300022;04400200300024" ll="SDBG:εἷς:000000;SDBG:ἕκαστος:000000">um</a> <a name="04400200300026" ll="SDBG:αὐτός:001000">deles</a>. 
\v 4 Todos ficaram cheios do <a name="04400200400008" ll="SDBG:πνεῦμα:000000">Espírito</a> <a name="04400200400010" ll="SDBG:ἅγιος:000000">Santo</a> <a name="04400200400002" ll="SDBG:καί:000000">e</a> passaram a falar <a name="04400200400016" ll="SDBG:λαλέω:000000">em</a> <a name="04400200400018" ll="SDBG:ἕτερος:000001">outras</a> <a name="04400200400020" ll="SDBG:γλῶσσα:000001">línguas</a>, <a name="04400200400022" ll="SDBG:καθώς:000000">segundo</a> o <a name="04400200400026" ll="SDBG:πνεῦμα:000000">Espírito</a> lhes concedia <a name="04400200400028" ll="SDBG:δίδωμι:000001">que</a> <a name="04400200400030;04400200400032" ll="SDBG:αὐτός:001000;SDBG:ἀποφθέγγομαι:000000">falassem</a>.
    
\v 5 <a name="04400200500004">Ora</a>, estavam <a name="04400200500010" ll="SDBG:κατοικέω:000000">habitando</a> em Jerusalém <a name="04400200500012" ll="SDBG:Ἰουδαῖος:001000">judeus</a>, <a name="04400200500014" ll="SDBG:ἀνήρ:000001">homens</a> <a name="04400200500016" ll="SDBG:εὐλαβής:000000">piedosos</a>, vindos <a name="04400200500018" ll="SDBG:ἀπό:000001">de</a> todas <a name="04400200500020" ll="SDBG:πᾶς:000000">as</a> <a name="04400200500022" ll="SDBG:ἔθνος:000000">nações</a> <a name="04400200500026" ll="SDBG:ὑπό:000006">debaixo</a> do <a name="04400200500030" ll="SDBG:οὐρανός:000000;SDBG:οὐρανός:000006">céu</a>. 
\v 6 Quando, <a name="04400200600004" ll="SDBG:δέ:000000">pois</a>, se fez <a name="04400200600002" ll="SDBG:γίνομαι:000003">ouvir</a> aquela <a name="04400200600008" ll="SDBG:φωνή:000000">voz</a>, <a name="04400200600012" ll="SDBG:συνέρχομαι:000000">afluiu</a> a <a name="04400200600016" ll="SDBG:πλῆθος:000001">multidão</a>, <a name="04400200600018" ll="SDBG:καί:000000">que</a> se possuiu de <a name="04400200600020" ll="SDBG:συγχέω:000000">perplexidade</a>, <a name="04400200600022" ll="SDBG:ὅτι:000001">porquanto</a> <a name="04400200600028" ll="SDBG:ἕκαστος:000000">cada</a> <a name="04400200600026" ll="SDBG:εἷς:000000">um</a> <a name="04400200600038" ll="SDBG:αὐτός:001000">os</a> <a name="04400200600024" ll="SDBG:ἀκούω:000000">ouvia</a> <a name="04400200600036" ll="SDBG:λαλέω:000000">falar</a> na sua <a name="04400200600032" ll="SDBG:ἴδιος:000001">própria</a> <a name="04400200600034" ll="SDBG:διάλεκτος:000000">língua</a>. 
\v 7 <a name="04400200700002" ll="SDBG:ἐξίστημι:000000">Estavam</a>, <a name="04400200700004" ll="SDBG:δέ:000000">pois</a>, atônitos <a name="04400200700006" ll="SDBG:καί:000000">e</a> se <a name="04400200700008" ll="SDBG:θαυμάζω:000000">admiravam</a>, <a name="04400200700010" ll="SDBG:λέγω:000000">dizendo</a>: Vede! <a name="04400200700012" ll="SDBG:οὐ:000001">Não</a> são, porventura, <a name="04400200700026" ll="SDBG:Γαλιλαῖος:000000">galileus</a> todos <a name="04400200700018" ll="SDBG:οὗτος:000000">esses</a> <a name="04400200700022" ll="SDBG:ὁ:000000">que</a> aí estão <a name="04400200700024" ll="SDBG:λαλέω:000000">falando</a>? 
\v 8 <a name="04400200800002" ll="SDBG:καί:000003">E</a> <a name="04400200800004" ll="SDBG:πῶς:000000">como</a> os <a name="04400200800008" ll="SDBG:ἀκούω:000000">ouvimos</a> falar, <a name="04400200800010" ll="SDBG:ἕκαστος:000000">cada</a> um em nossa <a name="04400200800014" ll="SDBG:ἴδιος:000001">própria</a> <a name="04400200800016;04400200800020;04400200800022" ll="SDBG:διάλεκτος:000000;SDBG:ἐν:000004;SDBG:ὅς:000000">língua</a> <a name="04400200800024" ll="SDBG:γεννάω:000001">materna</a>? 
\v 9 Somos <a name="04400200900002" ll="SDBG:Πάρθος:000000" il="adr1306212819">partos</a>, <a name="04400200900006" ll="SDBG:Μῆδος:000000" il="adr1305228181">medos</a>, <a name="04400200900010" ll="SDBG:Ἐλαμίτης:000000">elamitas</a> <a name="04400200900004" ll="SDBG:καί:000000">e</a> os <a name="04400200900016" ll="SDBG:κατοικέω:000000">naturais</a> da <a name="04400200900020" ll="SDBG:Μεσοποταμία:000000" il="adr070513161;adr1511187998;adr1511188086">Mesopotâmia</a>, <a name="04400200900022" ll="SDBG:Ἰουδαία:000000">Judeia</a>, <a name="04400200900028" ll="SDBG:Καππαδοκία:000000" il="Göreme_Valley_in_Cappadocia_edit1;tb123100259">Capadócia</a>, <a name="04400200900030" ll="SDBG:Πόντος:000000">Ponto</a> <a name="04400200900008" ll="SDBG:καί:000000">e</a> <a name="04400200900036" ll="SDBG:Ἀσία:000000">Ásia</a>, 
