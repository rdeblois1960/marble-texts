\v 7 Se os <a name="02001900700004" ll="SDBH:אָח:000001">irmãos</a> do <a name="02001900700006" ll="SDBH:רושׁ:000001">pobre</a> <a name="02001900700008" ll="SDBH:שׂנא:000000">o</a> <a name="02001900700008" ll="SDBH:שׂנא:000000">aborrecem</a>,quanto mais <a name="02001900700016" ll="SDBH:רחק:000000" tl="HOTTP2752">se</a> <a name="02001900700016" ll="SDBH:רחק:000000" tl="HOTTP2752">afastarão</a> dele os <a name="02001900700014" ll="SDBH:מֵרֵעַ:000000">seus</a> <a name="02001900700014" ll="SDBH:מֵרֵעַ:000000">amigos</a>!<a name="02001900700020" ll="SDBH:רדף:000004">Corre</a> após eles com <a name="02001900700022" ll="SDBH:אֵמֶר:000000">súplicas</a>, mas não os alcança.
    
\v 8 O que <a name="02001900800002" ll="SDBH:קנה:000002">adquire</a> <a name="02001900800004" ll="SDBH:לֵב:000003">entendimento</a> <a name="02001900800006" ll="SDBH:אהב:000000">ama</a> a <a name="02001900800008" ll="SDBH:נֶפֶשׁ:000001">sua</a> <a name="02001900800008" ll="SDBH:נֶפֶשׁ:000001">alma</a>;o que <a name="02001900800010" ll="SDBH:שׁמר:000002">conserva</a> a <a name="02001900800012" ll="SDBH:תְּבוּנָה:000000">inteligência</a> <a name="02001900800016" ll="SDBH:מצא:000006">acha</a> o <a name="02001900800018" ll="SDBH:טֹוב:000002">bem</a>.
    
\v 9 A <a name="02001900900004" ll="SDBH:שֶׁקֶר:000000">falsa</a> <a name="02001900900002" ll="SDBH:עֵד:000000">testemunha</a> não <a name="02001900900008" ll="SDBH:נקה:000002">fica</a> <a name="02001900900008" ll="SDBH:נקה:000002">impune</a>,e o que <a name="02001900900012" ll="SDBH:פוח:000003">profere</a> <a name="02001900900014" ll="SDBH:כָּזָב:000000">mentiras</a> <a name="02001900900016" ll="SDBH:אבד:000004">perece</a>.
    
\v 10 Ao <a name="02001901000008" ll="SDBH:כְּסִיל:000000">insensato</a> não <a name="02001901000004" ll="SDBH:נָאוֶה:000001">convém</a> a <a name="02001901000010" ll="SDBH:תַּעֲנוּג:000000">vida</a> <a name="02001901000010" ll="SDBH:תַּעֲנוּג:000000">regalada</a>,quanto menos ao <a name="02001901000018" ll="SDBH:עֶבֶד:000000" il="Bolen00049">escravo</a> <a name="02001901000020" ll="SDBH:משׁל:001000">dominar</a> os <a name="02001901000024" ll="SDBH:שַׂר:000000">príncipes</a>!
    
\v 11 A <a name="02001901100002" ll="SDBH:שֶׂכֶל:000000">discrição</a> do <a name="02001901100004" ll="SDBH:אָדָם:000000">homem</a> <a name="02001901100006;02001901100008" ll="SDBH:אַף:000008;SDBH:ארך:000007">o</a> <a name="02001901100006;02001901100008" ll="SDBH:אַף:000008;SDBH:ארך:000007">torna</a> <a name="02001901100006;02001901100008" ll="SDBH:אַף:000008;SDBH:ארך:000007">longânimo</a>,e <a name="02001901100012" ll="SDBH:תִּפְאֶרֶת:000000">sua</a> <a name="02001901100012" ll="SDBH:תִּפְאֶרֶת:000000">glória</a> é <a name="02001901100014">perdoar</a> as <a name="02001901100018" ll="SDBH:פֶּשַׁע:000000">injúrias</a>.
    
\v 12 Como o <a name="02001901200002" ll="SDBH:נַהַם:000000">bramido</a> do <a name="02001901200008" ll="SDBH:כְּפִיר:000000" il="Fauna:2.24a;Fauna:2.24b;Fauna:2.24c">leão</a>, assim é a <a name="02001901200010" ll="SDBH:זַעַף:000000">indignação</a> do <a name="02001901200012" ll="SDBH:מֶלֶךְ:000000">rei</a>;mas <a name="02001901200024" ll="SDBH:רָצֹון:000000">seu</a> <a name="02001901200024" ll="SDBH:רָצֹון:000000">favor</a> é como o <a name="02001901200018" ll="SDBH:טַל:000000" il="Dew">orvalho</a> sobre a <a name="02001901200022" ll="SDBH:עֵשֶׂב:000000">erva</a>.
    
\v 13 O <a name="02001901300008" ll="SDBH:בֵּן:000000">filho</a> <a name="02001901300010" ll="SDBH:כְּסִיל:000000">insensato</a> é a <a name="02001901300002" ll="SDBH:הַוָּה:001000">desgraça</a> <a name="02001901300006" ll="SDBH:אָב:000000">do</a> <a name="02001901300006" ll="SDBH:אָב:000000">pai</a>,e um <a name="02001901300014" ll="SDBH:דֶּלֶף:000000">gotejar</a> <a name="02001901300016" ll="SDBH:טרד:000000">contínuo</a>, as <a name="02001901300018" ll="SDBH:מִדְיָן:000000">contenções</a> da <a name="02001901300020" ll="SDBH:אִשָּׁה:000000">esposa</a>.
    
\v 14 A <a name="02001901400002" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917">casa</a> e os <a name="02001901400006" ll="SDBH:הֹון:000000">bens</a> vêm como <a name="02001901400008" ll="SDBH:נַחֲלָה:000000">herança</a> <a name="02001901400010" ll="SDBH:אָב:000001">dos</a> <a name="02001901400010" ll="SDBH:אָב:000001">pais</a>;mas do <a name="02001901400016" ll="SDBH:יהוה:000000">Senhor</a>, a <a name="02001901400018" ll="SDBH:אִשָּׁה:000000">esposa</a> <a name="02001901400020" ll="SDBH:שׂכל:001000">prudente</a>.
    
\v 15 A <a name="02001901500002" ll="SDBH:עַצְלָה:000000">preguiça</a> <a name="02001901500004" ll="SDBH:נפל:000022">faz</a> <a name="02001901500004" ll="SDBH:נפל:000022">cair</a> em <a name="02001901500006" ll="SDBH:תַּרְדֵּמָה:000000">profundo</a> <a name="02001901500006" ll="SDBH:תַּרְדֵּמָה:000000">sono</a>,e o <a name="02001901500010;02001901500012" ll="SDBH:נֶפֶשׁ:000004;SDBH:רְמִיָּה:000000">ocioso</a> <a name="02001901500014" ll="SDBH:רעב:000000">vem</a> <a name="02001901500014" ll="SDBH:רעב:000000">a</a> <a name="02001901500014" ll="SDBH:רעב:000000">padecer</a> <a name="02001901500014" ll="SDBH:רעב:000000">fome</a>.
    
\v 16 O que <a name="02001901600002" ll="SDBH:שׁמר:000003">guarda</a> o <a name="02001901600004" ll="SDBH:מִצְוָה:000000">mandamento</a> <a name="02001901600006" ll="SDBH:שׁמר:000007">guarda</a> a <a name="02001901600008" ll="SDBH:נֶפֶשׁ:000001">sua</a> <a name="02001901600008" ll="SDBH:נֶפֶשׁ:000001">alma</a>;mas o que <a name="02001901600010" ll="SDBH:בזה:000000">despreza</a> os <a name="02001901600012" ll="SDBH:דֶּרֶךְ:000007" tl="HOTTP2754">seus</a> <a name="02001901600012" ll="SDBH:דֶּרֶךְ:000007" tl="HOTTP2754">caminhos</a>, esse <a name="02001901600016" ll="SDBH:מות:000000">morre</a>.
    
\v 17 Quem <a name="02001901700006" ll="SDBH:חנן:000000">se</a> <a name="02001901700006" ll="SDBH:חנן:000000">compadece</a> do <a name="02001901700008" ll="SDBH:דַּל:001000">pobre</a> ao <a name="02001901700004" ll="SDBH:יהוה:000000">Senhor</a> <a name="02001901700002" ll="SDBH:לוה:001001">empresta</a>,e este lhe <a name="02001901700014" ll="SDBH:שׁלם:000007">paga</a> o seu <a name="02001901700012" ll="SDBH:גְּמוּל:000000">benefício</a>.
    
\v 18 <a name="02001901800002" ll="SDBH:יסר:000000">Castiga</a> a <a name="02001901800004" ll="SDBH:בֵּן:000000">teu</a> <a name="02001901800004" ll="SDBH:בֵּן:000000">filho</a>, enquanto <a name="02001901800008">há</a> <a name="02001901800010" ll="SDBH:תִּקְוָה:001000">esperança</a>,mas não <a name="02001901800022" ll="SDBH:נֶפֶשׁ:000001">te</a> <a name="02001901800020" ll="SDBH:נשׂא:000052">excedas</a> <a name="02001901800016" ll="SDBH:מות:000001">a</a> <a name="02001901800016" ll="SDBH:מות:000001">ponto</a> <a name="02001901800016" ll="SDBH:מות:000001">de</a> <a name="02001901800016" ll="SDBH:מות:000001">matá</a>-<a name="02001901800016" ll="SDBH:מות:000001">lo</a>.
    
\v 19 Homem <a name="02001901900004" ll="SDBH:גָּדֹול:000003">de</a> <a name="02001901900004" ll="SDBH:גָּדֹול:000003">grande</a> <a name="02001901900006" ll="SDBH:חֵמָה:001001">ira</a> tem de <a name="02001901900008" ll="SDBH:נשׂא:000015">sofrer</a> o <a name="02001901900010" ll="SDBH:עֹנֶשׁ:000000">dano</a>;porque, se tu <a name="02001901900016" ll="SDBH:נצל:000005">o</a> <a name="02001901900016" ll="SDBH:נצל:000005">livrares</a>, virás ainda a <a name="02001901900022">fazê</a>-<a name="02001901900022">lo</a> <a name="02001901900022">de</a> <a name="02001901900022">novo</a>.
    
\v 20 <a name="02001902000002">Ouve</a> o <a name="02001902000004" ll="SDBH:עֵצָה:000001">conselho</a> e <a name="02001902000008" ll="SDBH:קבל:000002">recebe</a> a <a name="02001902000010" ll="SDBH:מוּסָר:000000">instrução</a>,para que <a name="02001902000014" ll="SDBH:חכם:000000">sejas</a> <a name="02001902000014" ll="SDBH:חכם:000000">sábio</a> nos <a name="02001902000018" ll="SDBH:אַחֲרִית:000001">teus</a> <a name="02001902000018" ll="SDBH:אַחֲרִית:000001">dias</a> <a name="02001902000018" ll="SDBH:אַחֲרִית:000001">por</a> <a name="02001902000018" ll="SDBH:אַחֲרִית:000001">vir</a>.
    
\v 21 <a name="02001902100002" ll="SDBH:רַב:000001">Muitos</a> <a name="02001902100004" ll="SDBH:מַחֲשָׁבָה:000000">propósitos</a> há no <a name="02001902100008" ll="SDBH:לֵב:000001">coração</a> <a name="02001902100010">do</a> <a name="02001902100010">homem</a>,mas o <a name="02001902100014" ll="SDBH:עֵצָה:000002">desígnio</a> do <a name="02001902100016" ll="SDBH:יהוה:000000">Senhor</a> <a name="02001902100020">permanecerá</a>.
    
\v 22 O que torna <a name="02001902200002" ll="SDBH:תַּאֲוָה:000001">agradável</a> o <a name="02001902200004" ll="SDBH:אָדָם:000000">homem</a> é a <a name="02001902200006" ll="SDBH:חֶסֶד:000000">sua</a> <a name="02001902200006" ll="SDBH:חֶסֶד:000000">misericórdia</a>;o <a name="02001902200012" ll="SDBH:רושׁ:000001">pobre</a> é <a name="02001902200010" ll="SDBH:טֹוב:000002">preferível</a> ao <a name="02001902200016;02001902200018" ll="SDBH:כָּזָב:000001">mentiroso</a>.
    
\v 23 O <a name="02001902300002" ll="SDBH:יִרְאָה:000000">temor</a> do <a name="02001902300004" ll="SDBH:יהוה:000000">Senhor</a> conduz à <a name="02001902300008" ll="SDBH:חַיִּים:000000">vida</a>;<a name="02001902300014" ll="SDBH:לין:000001">aquele</a> <a name="02001902300014" ll="SDBH:לין:000001">que</a> <a name="02001902300014" ll="SDBH:לין:000001">o</a> <a name="02001902300014" ll="SDBH:לין:000001">tem</a> ficará <a name="02001902300012" ll="SDBH:שָׂבֵעַ:000003">satisfeito</a>, e <a name="02001902300020" ll="SDBH:רַע:000004">mal</a> nenhum <a name="02001902300018" ll="SDBH:פקד:000011">o</a> <a name="02001902300018" ll="SDBH:פקד:000011">visitará</a>.
    
\v 24 O <a name="02001902400004" ll="SDBH:עָצֵל:000000">preguiçoso</a> <a name="02001902400002" ll="SDBH:טמן:000001">mete</a> <a name="02001902400006">a</a> <a name="02001902400006">mão</a> no <a name="02001902400012" ll="SDBH:צַלַּחַת:000000">prato</a>e não quer <a name="02001902400022" ll="SDBH:שׁוב:000051">ter</a> o trabalho <a name="02001902400022" ll="SDBH:שׁוב:000051">de</a> a <a name="02001902400022" ll="SDBH:שׁוב:000051">levar</a> <a name="02001902400018" ll="SDBH:פֶּה:000000">à</a> <a name="02001902400018" ll="SDBH:פֶּה:000000">boca</a>.
    
\v 25 Quando <a name="02001902500004">ferires</a> ao <a name="02001902500002" ll="SDBH:ליץ:000000">escarnecedor</a>, o <a name="02001902500008" ll="SDBH:פֶּתִי:000000">simples</a> aprenderá a <a name="02001902500010" ll="SDBH:ערם:001000">prudência</a>;<a name="02001902500014" ll="SDBH:יכח:000004">repreende</a> ao <a name="02001902500018" ll="SDBH:בין:000000">sábio</a>, e <a name="02001902500020" ll="SDBH:בין:000001">crescerá</a> em <a name="02001902500022" ll="SDBH:דַּעַת:000000">conhecimento</a>.
    
\v 26 O que <a name="02001902600002" ll="SDBH:שׁדד:000000">maltrata</a> a seu <a name="02001902600004" ll="SDBH:אָב:000000">pai</a> ou <a name="02001902600006" ll="SDBH:ברח:000001">manda</a> <a name="02001902600006" ll="SDBH:ברח:000001">embora</a> a sua <a name="02001902600008" ll="SDBH:אֵם:000000" il="Motherhood1912">mãe</a><a name="02001902600010" ll="SDBH:בֵּן:000000">filho</a> é que <a name="02001902600012" ll="SDBH:בושׁ:000002">envergonha</a> e <a name="02001902600016" ll="SDBH:חפר:001001">desonra</a>.
    
\v 27 <a name="02001902700004" ll="SDBH:בֵּן:000009" tl="HOTTP2755">Filho</a> <a name="02001902700004" ll="SDBH:בֵּן:000009" tl="HOTTP2755">meu</a>, se <a name="02001902700002" ll="SDBH:חדל:000003" tl="HOTTP2755">deixas</a> de <a name="02001902700008">ouvir</a> a <a name="02001902700010" ll="SDBH:מוּסָר:000000">instrução</a>,<a name="02001902700014" ll="SDBH:שׁגה:000002">desviar</a>-<a name="02001902700014" ll="SDBH:שׁגה:000002">te</a>-<a name="02001902700014" ll="SDBH:שׁגה:000002">ás</a> das <a name="02001902700018" ll="SDBH:אֵמֶר:000000">palavras</a> <a name="02001902700020" ll="SDBH:דַּעַת:000000">do</a> <a name="02001902700020" ll="SDBH:דַּעַת:000000">conhecimento</a>.
    
\v 28 A <a name="02001902800002" ll="SDBH:עֵד:000000">testemunha</a> de <a name="02001902800004" ll="SDBH:בְּלִיַּעַל:000000">Belial</a> <a name="02001902800006" ll="SDBH:ליץ:000003">escarnece</a> da <a name="02001902800008" ll="SDBH:מִשְׁפָּט:000007">justiça</a>,e a <a name="02001902800012" ll="SDBH:פֶּה:000000">boca</a> dos <a name="02001902800014" ll="SDBH:רָשָׁע:000000">perversos</a> <a name="02001902800016" ll="SDBH:בלע:000000">devora</a> <a name="02001902800018" ll="SDBH:אָוֶן:000000">a</a> <a name="02001902800018" ll="SDBH:אָוֶן:000000">iniquidade</a>.
    
\v 29 <a name="02001902900002" ll="SDBH:כון:000012">Preparados</a> estão os <a name="02001902900010" ll="SDBH:שֶׁפֶט:000000" tl="HOTTP2756">juízos</a> para os <a name="02001902900008" ll="SDBH:ליץ:000000">escarnecedores</a>e os <a name="02001902900014" ll="SDBH:מַהֲלֻמֹות:000000">açoites</a>, para as <a name="02001902900018" ll="SDBH:גֵּו:000000">costas</a> dos <a name="02001902900020" ll="SDBH:כְּסִיל:000000">insensatos</a>.
\c 20
\v 1 O <a name="02002000100006" ll="SDBH:יַיִן:000000" il="Flora:2.12a;HK00108;LB00103;LB00107;vineyard1679, rp">vinho</a> é <a name="02002000100002" ll="SDBH:ליץ:000000">escarnecedor</a>, e a <a name="02002000100010" ll="SDBH:שֵׁכָר:000000">bebida</a> <a name="02002000100010" ll="SDBH:שֵׁכָר:000000">forte</a>, <a name="02002000100008" ll="SDBH:המה:000001">alvoroçadora</a>;todo aquele que por eles <a name="02002000100016" ll="SDBH:שׁגה:000002">é</a> <a name="02002000100016" ll="SDBH:שׁגה:000002">vencido</a> não <a name="02002000100022" ll="SDBH:חכם:000000">é</a> <a name="02002000100022" ll="SDBH:חכם:000000">sábio</a>.
    
\v 2 Como o <a name="02002000200002" ll="SDBH:נַהַם:000000">bramido</a> do <a name="02002000200008" ll="SDBH:כְּפִיר:000000" il="Fauna:2.24a;Fauna:2.24b;Fauna:2.24c">leão</a>, é o <a name="02002000200010" ll="SDBH:אֵימָה:000001">terror</a> do <a name="02002000200012" ll="SDBH:מֶלֶךְ:000000">rei</a>;o que <a name="02002000200014">lhe</a> <a name="02002000200014">provoca</a> <a name="02002000200014">a</a> <a name="02002000200014">ira</a> <a name="02002000200016" ll="SDBH:חטא:001000">peca</a> <a name="02002000200016" ll="SDBH:חטא:001000">contra</a> a <a name="02002000200018" ll="SDBH:נֶפֶשׁ:000001">sua</a> própria <a name="02002000200018" ll="SDBH:נֶפֶשׁ:000001">vida</a>.
    
\v 3 <a name="02002000300002" ll="SDBH:כָּבֹוד:000000">Honroso</a> é para o <a name="02002000300008">homem</a> o <a name="02002000300010" ll="SDBH:שֶׁבֶת:000001">desviar</a>-<a name="02002000300010" ll="SDBH:שֶׁבֶת:000001">se</a> de <a name="02002000300014" ll="SDBH:רִיב:000000">contendas</a>,mas todo <a name="02002000300020" ll="SDBH:אֱוִיל:000000">insensato</a> <a name="02002000300022" ll="SDBH:גלע:000000">se</a> <a name="02002000300022" ll="SDBH:גלע:000000">mete</a> <a name="02002000300022" ll="SDBH:גלע:000000">em</a> <a name="02002000300022" ll="SDBH:גלע:000000">rixas</a>.
    
