\id PRO
\c 1
\v 1 <a name="02000100100002" ll="SDBH:מָשָׁל:000000">Provérbios</a> de <a name="02000100100004" ll="SDBH:שְׁלֹמֹה:000000">Salomão</a>, <a name="02000100100006" ll="SDBH:בֵּן:000000">filho</a> <a name="02000100100008" ll="SDBH:דָּוִד:000000">de</a> <a name="02000100100008" ll="SDBH:דָּוִד:000000">Davi</a>, o <a name="02000100100010" ll="SDBH:מֶלֶךְ:000000">rei</a> de <a name="02000100100012" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>.
    
\v 2 Para <a name="02000100200004">aprender</a> a <a name="02000100200006" ll="SDBH:חָכְמָה:000000">sabedoria</a> e o <a name="02000100200010" ll="SDBH:מוּסָר:000000">ensino</a>;para <a name="02000100200014" ll="SDBH:בין:000001">entender</a> as <a name="02000100200016" ll="SDBH:אֵמֶר:000000">palavras</a> de <a name="02000100200018" ll="SDBH:בִּינָה:000000">inteligência</a>;
    
\v 3 para <a name="02000100300004" ll="SDBH:לקח:000000">obter</a> o <a name="02000100300006" ll="SDBH:מוּסָר:000000">ensino</a> do <a name="02000100300008" ll="SDBH:שׂכל:001000">bom</a> <a name="02000100300008" ll="SDBH:שׂכל:001000">proceder</a>,a <a name="02000100300010" ll="SDBH:צֶדֶק:000001">justiça</a>, o <a name="02000100300014" ll="SDBH:מִשְׁפָּט:000007">juízo</a> e a <a name="02000100300018" ll="SDBH:מֵישָׁרִים:000000">equidade</a>;
    
\v 4 para <a name="02000100400004">dar</a> aos <a name="02000100400008" ll="SDBH:פֶּתִי:000000">simples</a> <a name="02000100400010" ll="SDBH:עָרְמָה:000000">prudência</a>e aos <a name="02000100400014" ll="SDBH:נַעַר:000000">jovens</a>, <a name="02000100400016" ll="SDBH:דַּעַת:000000">conhecimento</a> e <a name="02000100400020" ll="SDBH:מְזִמָּה:000001">bom</a> <a name="02000100400020" ll="SDBH:מְזִמָּה:000001">siso</a>.
    
\v 5 <a name="02000100500002">Ouça</a> o <a name="02000100500004" ll="SDBH:חָכָם:000000">sábio</a> e <a name="02000100500008">cresça</a> em <a name="02000100500010" ll="SDBH:לֶקַח:000000">prudência</a>;e o <a name="02000100500014" ll="SDBH:בין:000000">instruído</a> <a name="02000100500018" ll="SDBH:קנה:000002">adquira</a> <a name="02000100500016" ll="SDBH:תַּחְבּוּלֹות:000000">habilidade</a>
    
\v 6 para <a name="02000100600004" ll="SDBH:בין:000001">entender</a> <a name="02000100600006" ll="SDBH:מָשָׁל:000000">provérbios</a> e <a name="02000100600010" ll="SDBH:מְלִיצָה:000001">parábolas</a>,as <a name="02000100600012" ll="SDBH:דָּבָר:000000">palavras</a> e <a name="02000100600018" ll="SDBH:חִידָה:000000">enigmas</a> dos <a name="02000100600014" ll="SDBH:חָכָם:000000">sábios</a>.
    
\v 7 O <a name="02000100700002" ll="SDBH:יִרְאָה:000000">temor</a> do <a name="02000100700004" ll="SDBH:יהוה:000000">Senhor</a> é o <a name="02000100700006" ll="SDBH:רֵאשִׁית:000000">princípio</a> do <a name="02000100700008" ll="SDBH:דַּעַת:000000">saber</a>,mas os <a name="02000100700016" ll="SDBH:אֱוִיל:000000">loucos</a> <a name="02000100700018" ll="SDBH:בוז:000000">desprezam</a> a <a name="02000100700010" ll="SDBH:חָכְמָה:000000">sabedoria</a> e o <a name="02000100700014" ll="SDBH:מוּסָר:000000">ensino</a>.
    
\v 8 <a name="02000100800004" ll="SDBH:בֵּן:000009">Filho</a> <a name="02000100800004" ll="SDBH:בֵּן:000009">meu</a>, <a name="02000100800002">ouve</a> o <a name="02000100800006" ll="SDBH:מוּסָר:000000">ensino</a> <a name="02000100800008" ll="SDBH:אָב:000000">de</a> <a name="02000100800008" ll="SDBH:אָב:000000">teu</a> <a name="02000100800008" ll="SDBH:אָב:000000">pai</a>e não <a name="02000100800014" ll="SDBH:נטשׁ:000006">deixes</a> a <a name="02000100800016" ll="SDBH:תֹּורָה:000000">instrução</a> <a name="02000100800018" ll="SDBH:אֵם:000000" il="Motherhood1912">de</a> <a name="02000100800018" ll="SDBH:אֵם:000000" il="Motherhood1912">tua</a> <a name="02000100800018" ll="SDBH:אֵם:000000" il="Motherhood1912">mãe</a>.
    
\v 9 Porque serão <a name="02000100900004" ll="SDBH:לִוְיָה:000000">diadema</a> de <a name="02000100900006" ll="SDBH:חֵן:000000">graça</a> para a <a name="02000100900012" ll="SDBH:רֹאשׁ:000000">tua</a> <a name="02000100900012" ll="SDBH:רֹאשׁ:000000">cabeça</a>e <a name="02000100900016" ll="SDBH:עֲנָק:000000">colares</a>, para o <a name="02000100900020" ll="SDBH:גַּרְגֶּרֶת:000000">teu</a> <a name="02000100900020" ll="SDBH:גַּרְגֶּרֶת:000000">pescoço</a>.
    
\v 10 <a name="02000101000002" ll="SDBH:בֵּן:000009">Filho</a> <a name="02000101000002" ll="SDBH:בֵּן:000009">meu</a>, se os <a name="02000101000008" ll="SDBH:חַטָּא:000001">pecadores</a> <a name="02000101000006" ll="SDBH:פתה:000001">querem</a> <a name="02000101000006" ll="SDBH:פתה:000001">seduzir</a>-<a name="02000101000006" ll="SDBH:פתה:000001">te</a>,não o <a name="02000101000012" ll="SDBH:אבה:000000" tl="HOTTP2636">consintas</a>.
    
\v 11 Se <a name="02000101100004" ll="SDBH:אמר:000000">disserem</a>: <a name="02000101100006" ll="SDBH:הלך:000001">Vem</a> conosco, <a name="02000101100010" ll="SDBH:ארב:000000">embosquemo</a>-<a name="02000101100010" ll="SDBH:ארב:000000">nos</a> para derramar <a name="02000101100014" ll="SDBH:דָּם:000003">sangue</a>,<a name="02000101100016" ll="SDBH:צפן:000000">espreitemos</a>, ainda que <a name="02000101100022" ll="SDBH:חִנָּם:000001">sem</a> <a name="02000101100022" ll="SDBH:חִנָּם:000001">motivo</a>, os <a name="02000101100020" ll="SDBH:נָקִי:000000">inocentes</a>;
    
\v 12 <a name="02000101200002" ll="SDBH:בלע:000000">traguemo</a>-<a name="02000101200002" ll="SDBH:בלע:000000">los</a> <a name="02000101200008" ll="SDBH:חַי:000000">vivos</a>, como o <a name="02000101200006" ll="SDBH:שְׁאֹול:000000">abismo</a>,e <a name="02000101200012" ll="SDBH:תָּמִים:000000">inteiros</a>, como os <a name="02000101200016" ll="SDBH:ירד:000004">que</a> <a name="02000101200016" ll="SDBH:ירד:000004">descem</a> à <a name="02000101200018" ll="SDBH:בֹּור:000002">cova</a>;
    
\v 13 <a name="02000101300008" ll="SDBH:מצא:000012">acharemos</a> toda sorte de <a name="02000101300004" ll="SDBH:הֹון:000000">bens</a> <a name="02000101300006" ll="SDBH:יָקָר:000000">preciosos</a>;<a name="02000101300010" ll="SDBH:מלא:000002">encheremos</a> de <a name="02000101300014" ll="SDBH:שָׁלָל:000000">despojos</a> a <a name="02000101300012" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917">nossa</a> <a name="02000101300012" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917">casa</a>;
    
\v 14 <a name="02000101400004" ll="SDBH:נפל:000034">lança</a> a <a name="02000101400002" ll="SDBH:גֹּורָל:000001" il="tb032014463">tua</a> <a name="02000101400002" ll="SDBH:גֹּורָל:000001" il="tb032014463">sorte</a> <a name="02000101400008" ll="SDBH:תָּוֶךְ:000000">entre</a> <a name="02000101400008" ll="SDBH:תָּוֶךְ:000000">nós</a>;teremos todos <a name="02000101400012" ll="SDBH:אֶחָד:000000">uma</a> <a name="02000101400012" ll="SDBH:אֶחָד:000000">só</a> <a name="02000101400010" ll="SDBH:כִּיס:000000" il="Realia:10.1">bolsa</a>.
    
\v 15 <a name="02000101500002" ll="SDBH:בֵּן:000009">Filho</a> <a name="02000101500002" ll="SDBH:בֵּן:000009">meu</a>, não te <a name="02000101500006" ll="SDBH:הלך:000001">ponhas</a> a <a name="02000101500010" ll="SDBH:דֶּרֶךְ:000007">caminho</a> com eles;<a name="02000101500014" ll="SDBH:מנע:000001">guarda</a> das <a name="02000101500020" ll="SDBH:נְתִיבָה:000000">suas</a> <a name="02000101500020" ll="SDBH:נְתִיבָה:000000">veredas</a> <a name="02000101500016" ll="SDBH:רֶגֶל:000003">os</a> <a name="02000101500016" ll="SDBH:רֶגֶל:000003">pés</a>;
    
\v 16 porque os <a name="02000101600004" ll="SDBH:רֶגֶל:000003" tl="HOTTP2637">seus</a> <a name="02000101600004" ll="SDBH:רֶגֶל:000003" tl="HOTTP2637">pés</a> <a name="02000101600012" ll="SDBH:רוץ:000000" tl="HOTTP2637">correm</a> para o <a name="02000101600010" ll="SDBH:רַע:000005" tl="HOTTP2637">mal</a>e <a name="02000101600016" ll="SDBH:מהר:000000" tl="HOTTP2637">se</a> <a name="02000101600016" ll="SDBH:מהר:000000" tl="HOTTP2637">apressam</a> a <a name="02000101600020" ll="SDBH:שׁפך:000009" tl="HOTTP2637">derramar</a> <a name="02000101600022" ll="SDBH:דָּם:000003" tl="HOTTP2637">sangue</a>.
    
\v 17 Pois <a name="02000101700004" ll="SDBH:חִנָּם:000002">debalde</a> se <a name="02000101700006" ll="SDBH:זרה:000007">estende</a> a <a name="02000101700010" ll="SDBH:רֶשֶׁת:000000" il="LB00209;LB00210;Realia:1.3.1.1a;Realia:1.3.1.1b;Realia:1.3.1.1c;Realia:1.4.2;mat00691">rede</a>à <a name="02000101700014" ll="SDBH:עַיִן:000000">vista</a> de qualquer <a name="02000101700018;02000101700020" ll="SDBH:בַּעַל:000003;SDBH:כָּנָף:000000">ave</a>.
    
\v 18 Estes <a name="02000101800010" ll="SDBH:ארב:000000">se</a> <a name="02000101800010" ll="SDBH:ארב:000000">emboscam</a> contra o <a name="02000101800008" ll="SDBH:דָּם:000003">seu</a> <a name="02000101800008" ll="SDBH:דָּם:000003">próprio</a> <a name="02000101800008" ll="SDBH:דָּם:000003">sangue</a>e a <a name="02000101800016" ll="SDBH:נֶפֶשׁ:000001">sua</a> <a name="02000101800016" ll="SDBH:נֶפֶשׁ:000001">própria</a> <a name="02000101800016" ll="SDBH:נֶפֶשׁ:000001">vida</a> <a name="02000101800012" ll="SDBH:צפן:000000">espreitam</a>.
    
\v 19 Tal é a <a name="02000101900004" ll="SDBH:אֹרַח:000002" tl="HOTTP2638">sorte</a> de todo <a name="02000101900008" ll="SDBH:בצע:000005">ganancioso</a>;e este espírito de <a name="02000101900010" ll="SDBH:בֶּצַע:000001" il="Lachish_Relief-Booty">ganância</a> <a name="02000101900018" ll="SDBH:לקח:000000">tira</a> a <a name="02000101900014" ll="SDBH:נֶפֶשׁ:000001">vida</a> <a name="02000101900016" ll="SDBH:בַּעַל:000000">de</a> <a name="02000101900016" ll="SDBH:בַּעַל:000000">quem</a> <a name="02000101900016" ll="SDBH:בַּעַל:000000">o</a> <a name="02000101900016" ll="SDBH:בַּעַל:000000">possui</a>.
    
\v 20 <a name="02000102000010" ll="SDBH:רנן:000000">Grita</a> na <a name="02000102000008" ll="SDBH:חוּץ:000004" il="Bethsaida, street;HK00169;NBGbs20130805-708;NBGbs20130806-853;NBGbs20130807-933;Realia:3.13.2;tb052001215">rua</a> a <a name="02000102000002" ll="SDBH:חָכְמָה:000000">Sabedoria</a>,nas <a name="02000102000016" ll="SDBH:רְחֹוב:000000" il="Bethsaida, street;HK00236;NBGbs20130805-708;NBGbs20130806-853;NBGbs20130807-933;tr_ldc_st_to_colossae_f02">praças</a>, <a name="02000102000018">levanta</a> <a name="02000102000020">a</a> <a name="02000102000020">voz</a>;
    
\v 21 do <a name="02000102100004" ll="SDBH:רֹאשׁ:000011" tl="HOTTP2639">alto</a> dos <a name="02000102100006" ll="SDBH:המה:000002" tl="HOTTP2639">muros</a> <a name="02000102100008" ll="SDBH:קרא:000000">clama</a>,à <a name="02000102100012" ll="SDBH:פֶּתַח:000000" il="HK00239;Realia:3.1.2a;Realia:3.1.2b;mat01418">entrada</a> das <a name="02000102100014" ll="SDBH:שַׁעַר:000000" il="CityGate-03-042c;Realia:3.13.3.5b;Tel_Arad">portas</a> e nas <a name="02000102100020" ll="SDBH:עִיר:000000">cidades</a> <a name="02000102100024" ll="SDBH:אמר:000001">profere</a> as <a name="02000102100022" ll="SDBH:אֵמֶר:000000">suas</a> <a name="02000102100022" ll="SDBH:אֵמֶר:000000">palavras</a>:
    
\v 22 Até quando, ó <a name="02000102200006" ll="SDBH:פֶּתִי:000000">néscios</a>, <a name="02000102200008" ll="SDBH:אהב:000000">amareis</a> a <a name="02000102200010" ll="SDBH:פֶּתִי:000000">necedade</a>?E vós, <a name="02000102200014" ll="SDBH:ליץ:000000">escarnecedores</a>, <a name="02000102200018" ll="SDBH:חמד:000000">desejareis</a> o <a name="02000102200016" ll="SDBH:לָצֹון:000000">escárnio</a>?E vós, <a name="02000102200024" ll="SDBH:כְּסִיל:000000">loucos</a>, <a name="02000102200026" ll="SDBH:שׂנא:000000">aborrecereis</a> o <a name="02000102200028" ll="SDBH:דַּעַת:000000">conhecimento</a>?
    
\v 23 <a name="02000102300002" ll="SDBH:שׁוב:000026">Atentai</a> para a <a name="02000102300006" ll="SDBH:תֹּוכַחַת:000000">minha</a> <a name="02000102300006" ll="SDBH:תֹּוכַחַת:000000">repreensão</a>;eis que <a name="02000102300010" ll="SDBH:נבע:000002">derramarei</a> <a name="02000102300010" ll="SDBH:נבע:000002">copiosamente</a> para vós outros o <a name="02000102300014" ll="SDBH:רוּחַ:000002">meu</a> <a name="02000102300014" ll="SDBH:רוּחַ:000002">espírito</a>e vos <a name="02000102300016">farei</a> <a name="02000102300016">saber</a> as <a name="02000102300018" ll="SDBH:דָּבָר:000000">minhas</a> <a name="02000102300018" ll="SDBH:דָּבָר:000000">palavras</a>.
    
\v 24 Mas, porque <a name="02000102400004" ll="SDBH:קרא:000000">clamei</a>, e <a name="02000102400008" ll="SDBH:מאן:000000">vós</a> <a name="02000102400008" ll="SDBH:מאן:000000">recusastes</a>;porque <a name="02000102400010" ll="SDBH:נטה:001006">estendi</a> <a name="02000102400012">a</a> <a name="02000102400012">mão</a>, e não houve <a name="02000102400018" ll="SDBH:קשׁב:000000">quem</a> <a name="02000102400018" ll="SDBH:קשׁב:000000">atendesse</a>;
    
\v 25 antes, <a name="02000102500004" ll="SDBH:פרע:000005">rejeitastes</a> todo o <a name="02000102500008" ll="SDBH:עֵצָה:000001">meu</a> <a name="02000102500008" ll="SDBH:עֵצָה:000001">conselho</a>e não <a name="02000102500016" ll="SDBH:אבה:000002">quisestes</a> a <a name="02000102500012" ll="SDBH:תֹּוכַחַת:000000">minha</a> <a name="02000102500012" ll="SDBH:תֹּוכַחַת:000000">repreensão</a>;
    
\v 26 também eu me <a name="02000102600010" ll="SDBH:שׂחק:000000">rirei</a> na <a name="02000102600008" ll="SDBH:אֵיד:000000">vossa</a> <a name="02000102600008" ll="SDBH:אֵיד:000000">desventura</a>,e, em <a name="02000102600016">vindo</a> o <a name="02000102600018" ll="SDBH:פַּחַד:000000">vosso</a> <a name="02000102600018" ll="SDBH:פַּחַד:000000">terror</a>, eu <a name="02000102600012" ll="SDBH:לעג:000000">zombarei</a>,
    
\v 27 em <a name="02000102700004">vindo</a> o <a name="02000102700014" ll="SDBH:פַּחַד:000000">vosso</a> <a name="02000102700014" ll="SDBH:פַּחַד:000000">terror</a> como a <a name="02000102700012" ll="SDBH:שֹׁואָה:000000">tempestade</a>,em <a name="02000102700028" tl="HOTTP2640">vindo</a> a <a name="02000102700018" ll="SDBH:אֵיד:000000">vossa</a> <a name="02000102700018" ll="SDBH:אֵיד:000000">perdição</a> como o <a name="02000102700022" ll="SDBH:סוּפָה:000000" il="Whirlwind">redemoinho</a>,quando vos <a name="02000102700024" ll="SDBH:אתה:000002">chegar</a> o <a name="02000102700032" ll="SDBH:צָרָה:000000" tl="HOTTP2640">aperto</a> e a <a name="02000102700036" tl="HOTTP2640">angústia</a>.
    
\v 28 Então, <a name="02000102800004" ll="SDBH:קרא:000000">me</a> <a name="02000102800004" ll="SDBH:קרא:000000">invocarão</a>, mas eu não <a name="02000102800010" ll="SDBH:ענה:000000">responderei</a>;<a name="02000102800012" ll="SDBH:שׁחר:001001">procurar</a>-<a name="02000102800012" ll="SDBH:שׁחר:001001">me</a>-<a name="02000102800012" ll="SDBH:שׁחר:001001">ão</a>, porém não <a name="02000102800018" ll="SDBH:מצא:000000">me</a> <a name="02000102800018" ll="SDBH:מצא:000000">hão</a> <a name="02000102800018" ll="SDBH:מצא:000000">de</a> <a name="02000102800018" ll="SDBH:מצא:000000">achar</a>.
    
