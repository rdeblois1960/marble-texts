\v 8 (7) <a name="01900700800004;01900700800006" ll="SDBH:לְאֹם:000000;SDBH:עֵדָה:000000">Reúnam</a>-<a name="01900700800004;01900700800006" ll="SDBH:לְאֹם:000000;SDBH:עֵדָה:000000">se</a> <a name="01900700800008">ao</a> <a name="01900700800008">redor</a> <a name="01900700800008">de</a> <a name="01900700800008">ti</a> <a name="01900700800004;01900700800006" ll="SDBH:לְאֹם:000000;SDBH:עֵדָה:000000">os</a> <a name="01900700800004;01900700800006" ll="SDBH:לְאֹם:000000;SDBH:עֵדָה:000000">povos</a>,e por sobre eles <a name="01900700800020" ll="SDBH:שׁוב:000023" tl="HOTTP2091">remonta</a>-<a name="01900700800020" ll="SDBH:שׁוב:000023" tl="HOTTP2091">te</a> às <a name="01900700800018" ll="SDBH:מָרֹום:000006">alturas</a>.
    
\v 9 (8) O <a name="01900700900002" ll="SDBH:יהוה:000000">Senhor</a> <a name="01900700900004" ll="SDBH:דין:000000">julga</a> os <a name="01900700900006" ll="SDBH:עַם:000000">povos</a>;<a name="01900700900008" ll="SDBH:שׁפט:000003" il="Tel-dan-israelite-gate-im-tor">julga</a>-<a name="01900700900008" ll="SDBH:שׁפט:000003" il="Tel-dan-israelite-gate-im-tor">me</a>, <a name="01900700900010" ll="SDBH:יהוה:000000">Senhor</a>, segundo a <a name="01900700900014" ll="SDBH:צֶדֶק:000001">minha</a> <a name="01900700900014" ll="SDBH:צֶדֶק:000001">retidão</a>e segundo a <a name="01900700900020" ll="SDBH:תֹּם:000001">integridade</a> que há em mim.
    
\v 10 (9) <a name="01900701000002" ll="SDBH:גמר:000001">Cesse</a> a <a name="01900701000006" ll="SDBH:רַע:000005">malícia</a> dos <a name="01900701000008" ll="SDBH:רָשָׁע:000000">ímpios</a>,mas <a name="01900701000012" ll="SDBH:כון:000025">estabelece</a> <a name="01900701000012" ll="SDBH:כון:000025">tu</a> o <a name="01900701000014" ll="SDBH:צַדִּיק:000000">justo</a>;pois <a name="01900701000018" ll="SDBH:בחן:000000">sondas</a> a <a name="01900701000024" ll="SDBH:כִּלְיָה:000001">mente</a> e o <a name="01900701000020" ll="SDBH:לֵב:000001">coração</a>,ó <a name="01900701000028" ll="SDBH:צַדִּיק:000000">justo</a> <a name="01900701000026" ll="SDBH:אֱלֹהִים:000002">Deus</a>.
    
