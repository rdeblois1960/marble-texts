\v 2 <a name="01914600200002" ll="SDBH:הלל:001000">Louvarei</a> ao <a name="01914600200004" ll="SDBH:יהוה:000000">Senhor</a> durante a <a name="01914600200008" ll="SDBH:חַיִּים:000000">minha</a> <a name="01914600200008" ll="SDBH:חַיִּים:000000">vida</a>;<a name="01914600200010" ll="SDBH:זמר:000000" il="HK00188">cantarei</a> <a name="01914600200010" ll="SDBH:זמר:000000" il="HK00188">louvores</a> ao <a name="01914600200014" ll="SDBH:אֱלֹהִים:000001">meu</a> <a name="01914600200014" ll="SDBH:אֱלֹהִים:000001">Deus</a>, enquanto <a name="01914600200018">eu</a> <a name="01914600200018">viver</a>.
    
\v 3 Não <a name="01914600300004" ll="SDBH:בטח:000001">confieis</a> em <a name="01914600300008" ll="SDBH:נָדִיב:000001">príncipes</a>,nem nos <a name="01914600300012" ll="SDBH:בֵּן:000003">filhos</a> <a name="01914600300014" ll="SDBH:אָדָם:000000">dos</a> <a name="01914600300014" ll="SDBH:אָדָם:000000">homens</a>, em quem não há <a name="01914600300022" ll="SDBH:תְּשׁוּעָה:000000">salvação</a>.
    
\v 4 <a name="01914600400002" ll="SDBH:יצא:000000">Sai</a>-<a name="01914600400004" ll="SDBH:רוּחַ:000002">lhes</a> <a name="01914600400004" ll="SDBH:רוּחַ:000002">o</a> <a name="01914600400004" ll="SDBH:רוּחַ:000002">espírito</a>, e eles <a name="01914600400006" ll="SDBH:שׁוב:000037">tornam</a> ao <a name="01914600400010" ll="SDBH:אֲדָמָה:000000" il="Soil">pó</a>;nesse mesmo <a name="01914600400016" ll="SDBH:יֹום:000001">dia</a>, <a name="01914600400022" ll="SDBH:אבד:000006">perecem</a> todos os <a name="01914600400024" ll="SDBH:עֶשְׁתֹּון:000000">seus</a> <a name="01914600400024" ll="SDBH:עֶשְׁתֹּון:000000">desígnios</a>.
    
\v 5 <a name="01914600500002" ll="SDBH:אַשְׁרֵי:000000">Bem</a>-<a name="01914600500002" ll="SDBH:אַשְׁרֵי:000000">aventurado</a> aquele que tem o <a name="01914600500006" ll="SDBH:אֵל:000001">Deus</a> <a name="01914600500008" ll="SDBH:יַעֲקֹב:000000">de</a> <a name="01914600500008" ll="SDBH:יַעֲקֹב:000000">Jacó</a> por <a name="01914600500012" ll="SDBH:עֵזֶר:000000">seu</a> <a name="01914600500012" ll="SDBH:עֵזֶר:000000">auxílio</a>,<a name="01914600500014" ll="SDBH:שֵׂבֶר:000000">cuja</a> <a name="01914600500014" ll="SDBH:שֵׂבֶר:000000">esperança</a> está no <a name="01914600500018" ll="SDBH:יהוה:000000">Senhor</a>, <a name="01914600500020" ll="SDBH:אֱלֹהִים:000001">seu</a> <a name="01914600500020" ll="SDBH:אֱלֹהִים:000001">Deus</a>,
    
\v 6 que <a name="01914600600002">fez</a> os <a name="01914600600004" ll="SDBH:אֶרֶץ:000002;SDBH:שָׁמַיִם:000001">céus</a> e a <a name="01914600600008" ll="SDBH:אֶרֶץ:000000" il="The_Earth">terra</a>,o <a name="01914600600014" ll="SDBH:יָם:000000" il="Rough_seas">mar</a> e tudo o que neles háe <a name="01914600600028" ll="SDBH:שׁמר:000003">mantém</a> para <a name="01914600600034" ll="SDBH:עֹולָם:000000" tl="HOTTP2627">sempre</a> a sua <a name="01914600600030" ll="SDBH:אֱמֶת:000000">fidelidade</a>.
    
\v 7 Que <a name="01914600700002">faz</a> <a name="01914600700004" ll="SDBH:מִשְׁפָּט:000007">justiça</a> aos <a name="01914600700010" ll="SDBH:עשׁק:000001">oprimidos</a>e <a name="01914600700012">dá</a> <a name="01914600700014" ll="SDBH:לֶחֶם:000000" il="Tandır_bread;tb061116157">pão</a> aos <a name="01914600700020" ll="SDBH:רָעֵב:000000">que</a> <a name="01914600700020" ll="SDBH:רָעֵב:000000">têm</a> <a name="01914600700020" ll="SDBH:רָעֵב:000000">fome</a>.O <a name="01914600700022" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914600700024" ll="SDBH:נתר:000003">liberta</a> os <a name="01914600700026" ll="SDBH:אסר:000005">encarcerados</a>.
    
\v 8 O <a name="01914600800002" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914600800004" ll="SDBH:פקח:000000">abre</a> os olhos aos <a name="01914600800006" ll="SDBH:עִוֵּר:000000">cegos</a>,o <a name="01914600800008" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914600800010" ll="SDBH:זקף:000000">levanta</a> os <a name="01914600800012" ll="SDBH:כפף:000000">abatidos</a>,o <a name="01914600800014" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914600800016" ll="SDBH:אהב:000000">ama</a> os <a name="01914600800018" ll="SDBH:צַדִּיק:000000">justos</a>.
    
\v 9 O <a name="01914600900002" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914600900004" ll="SDBH:שׁמר:000007">guarda</a> o <a name="01914600900008" ll="SDBH:גֵּר:000000">peregrino</a>,<a name="01914600900016" ll="SDBH:עוד:001002">ampara</a> o <a name="01914600900010" ll="SDBH:יָתֹום:000000">órfão</a> e a <a name="01914600900014" ll="SDBH:אַלְמָנָה:000000">viúva</a>,porém <a name="01914600900024" ll="SDBH:עות:000005">transtorna</a> o <a name="01914600900020" ll="SDBH:דֶּרֶךְ:000007">caminho</a> dos <a name="01914600900022" ll="SDBH:רָשָׁע:000000">ímpios</a>.
    
\v 10 O <a name="01914601000004" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914601000002" ll="SDBH:מלך:000000">reina</a> para <a name="01914601000008" ll="SDBH:עֹולָם:000000">sempre</a>;o <a name="01914601000010" ll="SDBH:אֱלֹהִים:000001">teu</a> <a name="01914601000010" ll="SDBH:אֱלֹהִים:000001">Deus</a>, ó <a name="01914601000012" ll="SDBH:צִיֹּון:000000" il="tb060716478">Sião</a>, reina de <a name="01914601000016" ll="SDBH:דֹּור:001000">geração</a> em <a name="01914601000020" ll="SDBH:דֹּור:001000">geração</a>.<a name="01914601000022;01914601000024" ll="SDBH:הלל:001000;SDBH:יָהּ:000000" tl="HOTTP2628">Aleluia</a>!
\c 147
\v 1 <a name="01914700100002" ll="SDBH:הלל:001000">Louvai</a> ao <a name="01914700100004" ll="SDBH:יָהּ:000000">Senhor</a>,porque é <a name="01914700100008" ll="SDBH:טֹוב:000002">bom</a> e <a name="01914700100016" ll="SDBH:נָעִים:000001">amável</a> <a name="01914700100010" ll="SDBH:זמר:000000" il="HK00188">cantar</a> <a name="01914700100010" ll="SDBH:זמר:000000" il="HK00188">louvores</a> <a name="01914700100012" ll="SDBH:אֱלֹהִים:000001">ao</a> <a name="01914700100012" ll="SDBH:אֱלֹהִים:000001">nosso</a> <a name="01914700100012" ll="SDBH:אֱלֹהִים:000001">Deus</a>;<a name="01914700100018" ll="SDBH:נָאוֶה:000000" tl="HOTTP2629">fica</a>-lhe <a name="01914700100018" ll="SDBH:נָאוֶה:000000" tl="HOTTP2629">bem</a> o <a name="01914700100020" ll="SDBH:תְּהִלָּה:000004">cântico</a> <a name="01914700100020" ll="SDBH:תְּהִלָּה:000004">de</a> <a name="01914700100020" ll="SDBH:תְּהִלָּה:000004">louvor</a>.
    
\v 2 O <a name="01914700200006" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914700200002" ll="SDBH:בנה:000000">edifica</a> <a name="01914700200004" ll="SDBH:יְרוּשָׁלַ͏ִם:000000" il="NBGbs20090227-1_14;NBGbs20090227-1_34;NBGbs20090425-1_54;NBGbs20110228-1_79;Satellite Bible Atlas00024;jlm_david_city_d04;jlm_hezekiah_city_d03;jlm_topography_d04">Jerusalém</a>e <a name="01914700200012" ll="SDBH:כנס:000000">congrega</a> os <a name="01914700200008" ll="SDBH:נדח:001001" il="Babylon_exile_Fugel;Exile_Lachish;Exile_after_Lachish;Lachish_Relief-exile;Torah;tb112004312">dispersos</a> de <a name="01914700200010" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>;
    
\v 3 <a name="01914700300004" ll="SDBH:רפא:000000">sara</a> os de <a name="01914700300010" ll="SDBH:לֵב:000001">coração</a> <a name="01914700300008" ll="SDBH:שׁבר:000003">quebrantado</a>e <a name="01914700300018" ll="SDBH:עַצֶּבֶת:000000">lhes</a> <a name="01914700300014" ll="SDBH:חבשׁ:000001">pensa</a> <a name="01914700300018" ll="SDBH:עַצֶּבֶת:000000">as</a> <a name="01914700300018" ll="SDBH:עַצֶּבֶת:000000">feridas</a>.
    
\v 4 <a name="01914700400002" ll="SDBH:מנה:000000">Conta</a> o <a name="01914700400004" ll="SDBH:מִסְפָּר:000002">número</a> das <a name="01914700400010" ll="SDBH:כֹּוכָב:000000" il="Stars">estrelas</a>,<a name="01914700400018" ll="SDBH:קרא:000007">chamando</a>-as todas pelo seu <a name="01914700400016" ll="SDBH:שֵׁם:000000">nome</a>.
    
\v 5 <a name="01914700500002" ll="SDBH:גָּדֹול:000002">Grande</a> é o <a name="01914700500004" ll="SDBH:אָדֹון:000000">Senhor</a> <a name="01914700500004" ll="SDBH:אָדֹון:000000">nosso</a> e <a name="01914700500008" ll="SDBH:רַב:000002">mui</a> <a name="01914700500010" ll="SDBH:כֹּחַ:000000">poderoso</a>;o <a name="01914700500014" ll="SDBH:תְּבוּנָה:000000">seu</a> <a name="01914700500014" ll="SDBH:תְּבוּנָה:000000">entendimento</a> não se pode <a name="01914700500018" ll="SDBH:מִסְפָּר:000001">medir</a>.
    
\v 6 O <a name="01914700600006" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914700600002" ll="SDBH:עוד:001002">ampara</a> os <a name="01914700600004" ll="SDBH:עָנָו:000000">humildes</a>e <a name="01914700600008" ll="SDBH:שׁפל:000003">dá</a> com os <a name="01914700600010" ll="SDBH:רָשָׁע:000000">ímpios</a> em <a name="01914700600014" ll="SDBH:אֶרֶץ:000000">terra</a>.
    
\v 7 <a name="01914700700002" ll="SDBH:ענה:000002">Cantai</a> ao <a name="01914700700006" ll="SDBH:יהוה:000000">Senhor</a> com <a name="01914700700010" ll="SDBH:תֹּודָה:000000">ações</a> <a name="01914700700010" ll="SDBH:תֹּודָה:000000">de</a> <a name="01914700700010" ll="SDBH:תֹּודָה:000000">graças</a>;<a name="01914700700012" ll="SDBH:זמר:000000" il="HK00188">entoai</a> <a name="01914700700012" ll="SDBH:זמר:000000" il="HK00188">louvores</a>, ao som da <a name="01914700700020" ll="SDBH:כִּנֹּור:000000" il="BK00175;BK00176;Exile_Lachish;HK00188;NBGmk20131114;adr1306213109;tb072705188">harpa</a>, ao <a name="01914700700016" ll="SDBH:אֱלֹהִים:000001">nosso</a> <a name="01914700700016" ll="SDBH:אֱלֹהִים:000001">Deus</a>,
    
\v 8 que <a name="01914700800004" ll="SDBH:כסה:000000">cobre</a> de <a name="01914700800010" ll="SDBH:עָב:000000" il="Altocumulus_cloud;White_cloud_111">nuvens</a> os <a name="01914700800006" ll="SDBH:שָׁמַיִם:000001">céus</a>,<a name="01914700800014" ll="SDBH:כון:000013">prepara</a> a <a name="01914700800022" ll="SDBH:מָטָר:000000" il="Rain;Rain2">chuva</a> para a <a name="01914700800020" ll="SDBH:אֶרֶץ:000000">terra</a>,<a name="01914700800026" ll="SDBH:צמח:000001">faz</a> <a name="01914700800026" ll="SDBH:צמח:000001">brotar</a> nos <a name="01914700800028" ll="SDBH:הַר:000000">montes</a> a <a name="01914700800030" ll="SDBH:חָצִיר:000001" il="Flora:5.1.2" tl="HOTTP2630">erva</a>
    
\v 9 e <a name="01914700900002">dá</a> <a name="01914700900008" ll="SDBH:לֶחֶם:000000" il="Tandır_bread;tb061116157">o</a> <a name="01914700900008" ll="SDBH:לֶחֶם:000000" il="Tandır_bread;tb061116157">alimento</a> aos <a name="01914700900006" ll="SDBH:בְּהֵמָה:000000">animais</a>e aos <a name="01914700900012" ll="SDBH:בֵּן:000001">filhos</a> dos <a name="01914700900014" ll="SDBH:עֹורֵב:000000" il="Brown-necked_raven;Corvus_cornix">corvos</a>, quando <a name="01914700900018" ll="SDBH:קרא:000000">clamam</a>.
    
\v 10 Não <a name="01914701000012" ll="SDBH:חפץ:000002">faz</a> <a name="01914701000012" ll="SDBH:חפץ:000002">caso</a> da <a name="01914701000006" ll="SDBH:גְּבוּרָה:000000">força</a> do <a name="01914701000010" ll="SDBH:סוּס:000000" il="Fauna:2.18;LB00032">cavalo</a>,nem <a name="01914701000024" ll="SDBH:רצה:000000">se</a> <a name="01914701000024" ll="SDBH:רצה:000000">compraz</a> nos <a name="01914701000018" ll="SDBH:שֹׁוק:000000">músculos</a> do <a name="01914701000022">guerreiro</a>.
    
\v 11 <a name="01914701100002" ll="SDBH:רצה:000000">Agrada</a>-<a name="01914701100002" ll="SDBH:רצה:000000">se</a> o <a name="01914701100004" ll="SDBH:יהוה:000000">Senhor</a> dos <a name="01914701100008" ll="SDBH:יָרֵא:000000">que</a> <a name="01914701100008" ll="SDBH:יָרֵא:000000">o</a> <a name="01914701100008" ll="SDBH:יָרֵא:000000">temem</a>e dos que <a name="01914701100014" ll="SDBH:יחל:000000">esperam</a> na <a name="01914701100018" ll="SDBH:חֶסֶד:000000">sua</a> <a name="01914701100018" ll="SDBH:חֶסֶד:000000">misericórdia</a>.
    
\v 12 <a name="01914701200002" ll="SDBH:שׁבח:000000">Louva</a>, <a name="01914701200004" ll="SDBH:יְרוּשָׁלַ͏ִם:000000" il="NBGbs20090227-1_14;NBGbs20090227-1_34;NBGbs20090425-1_54;NBGbs20110228-1_79;Satellite Bible Atlas00024;jlm_david_city_d04;jlm_hezekiah_city_d03;jlm_topography_d04">Jerusalém</a>, ao <a name="01914701200008" ll="SDBH:יהוה:000000">Senhor</a>;<a name="01914701200010" ll="SDBH:הלל:001000">louva</a>, <a name="01914701200014" ll="SDBH:צִיֹּון:000000" il="tb060716478">Sião</a>, ao <a name="01914701200012" ll="SDBH:אֱלֹהִים:000001">teu</a> <a name="01914701200012" ll="SDBH:אֱלֹהִים:000001">Deus</a>.
    
\v 13 Pois ele <a name="01914701300004" ll="SDBH:חזק:000002">reforçou</a> as <a name="01914701300006" ll="SDBH:בְּרִיחַ:000001">trancas</a> <a name="01914701300008" ll="SDBH:שַׁעַר:000000" il="CityGate-03-042c;Realia:3.13.3.5b;Tel_Arad">das</a> <a name="01914701300008" ll="SDBH:שַׁעַר:000000" il="CityGate-03-042c;Realia:3.13.3.5b;Tel_Arad">tuas</a> <a name="01914701300008" ll="SDBH:שַׁעַר:000000" il="CityGate-03-042c;Realia:3.13.3.5b;Tel_Arad">portas</a>e <a name="01914701300010" ll="SDBH:ברך:001001">abençoou</a> os <a name="01914701300012" ll="SDBH:בֵּן:000000">teus</a> <a name="01914701300012" ll="SDBH:בֵּן:000000">filhos</a>, <a name="01914701300016">dentro</a> <a name="01914701300016">de</a> <a name="01914701300016">ti</a>;
    
\v 14 <a name="01914701400004" ll="SDBH:שׂים:000005">estabeleceu</a> a <a name="01914701400008" ll="SDBH:שָׁלֹום:000000">paz</a> nas <a name="01914701400006" ll="SDBH:גְּבוּל:000000">tuas</a> <a name="01914701400006" ll="SDBH:גְּבוּל:000000">fronteiras</a>e <a name="01914701400014" ll="SDBH:שׂבע:000001">te</a> <a name="01914701400014" ll="SDBH:שׂבע:000001">farta</a> com o <a name="01914701400010" ll="SDBH:חֵלֶב:000001">melhor</a> do <a name="01914701400012" ll="SDBH:חִטָּה:000000" il="Flora:3.1.4a;Flora:3.1.4b;PikiWiki_Israel_7817">trigo</a>.
    
\v 15 Ele <a name="01914701500004" ll="SDBH:שׁלח:000020">envia</a> as <a name="01914701500006" ll="SDBH:אִמְרָה:000000">suas</a> <a name="01914701500006" ll="SDBH:אִמְרָה:000000">ordens</a> à <a name="01914701500008" ll="SDBH:אֶרֶץ:000000">terra</a>,e <a name="01914701500016" ll="SDBH:דָּבָר:000000">sua</a> <a name="01914701500016" ll="SDBH:דָּבָר:000000">palavra</a> <a name="01914701500014" ll="SDBH:רוץ:000006">corre</a> <a name="01914701500012" ll="SDBH:מְהֵרָה:000000">velozmente</a>;
    
\v 16 <a name="01914701600004">dá</a> a <a name="01914701600006" ll="SDBH:שֶׁלֶג:000000" il="NBGbs20130728-115;Snow">neve</a> como <a name="01914701600012" ll="SDBH:צֶמֶר:000000">lã</a>e <a name="01914701600022" ll="SDBH:פזר:000000">espalha</a> a <a name="01914701600014" ll="SDBH:כְּפֹור:000001">geada</a> como <a name="01914701600020" ll="SDBH:אֵפֶר:000000" il="Ash">cinza</a>.
    
