\v 12 <a name="01907901200004" ll="SDBH:שׁוב:000032">Retribui</a>, <a name="01907901200022" ll="SDBH:אֲדֹנָי:000000">Senhor</a>, aos <a name="01907901200008" ll="SDBH:שָׁכֵן:000000">nossos</a> <a name="01907901200008" ll="SDBH:שָׁכֵן:000000">vizinhos</a>, <a name="01907901200010" ll="SDBH:שֶׁבַע:000002">sete</a> <a name="01907901200010" ll="SDBH:שֶׁבַע:000002">vezes</a> tanto,o <a name="01907901200014;01907901200016" ll="SDBH:חֵיק:000000;SDBH:חֶרְפָּה:000002">opróbrio</a> com que <a name="01907901200020" ll="SDBH:חרף:000000">te</a> <a name="01907901200020" ll="SDBH:חרף:000000">vituperaram</a>.
    
\v 13 Quanto a nós, <a name="01907901300006" ll="SDBH:עַם:000000">teu</a> <a name="01907901300006" ll="SDBH:עַם:000000">povo</a> e <a name="01907901300010" ll="SDBH:צֹאן:000000" il="LB00012;LB00015;LB00017;NBGbs20120208-08;NBGbs20120710-139;NBGpb20140401b;Realia:1.2.1;tb111706002">ovelhas</a> <a name="01907901300012" ll="SDBH:מַרְעִית:000001">do</a> <a name="01907901300012" ll="SDBH:מַרְעִית:000001">teu</a> <a name="01907901300012" ll="SDBH:מַרְעִית:000001">pasto</a>,para <a name="01907901300020" ll="SDBH:עֹולָם:000000">sempre</a> te <a name="01907901300014" ll="SDBH:ידה:000002">daremos</a> <a name="01907901300014" ll="SDBH:ידה:000002">graças</a>;de <a name="01907901300024" ll="SDBH:דֹּור:001000">geração</a> em <a name="01907901300028" ll="SDBH:דֹּור:001000">geração</a> <a name="01907901300030" ll="SDBH:ספר:000006">proclamaremos</a> os <a name="01907901300032" ll="SDBH:תְּהִלָּה:000001">teus</a> <a name="01907901300032" ll="SDBH:תְּהִלָּה:000001">louvores</a>.
