\v 15 <a name="01910601500004">Concedeu</a>-lhes <a name="01910601500008" ll="SDBH:שְׁאֵלָה:000000">o</a> <a name="01910601500008" ll="SDBH:שְׁאֵלָה:000000">que</a> <a name="01910601500008" ll="SDBH:שְׁאֵלָה:000000">pediram</a>,mas <a name="01910601500012" ll="SDBH:שׁלח:000020">fez</a> <a name="01910601500014" ll="SDBH:רָזֹון:000000">definhar</a>-<a name="01910601500018" ll="SDBH:נֶפֶשׁ:000001">lhes</a> <a name="01910601500018" ll="SDBH:נֶפֶשׁ:000001">a</a> <a name="01910601500018" ll="SDBH:נֶפֶשׁ:000001">alma</a>.
    
\v 16 <a name="01910601600004" ll="SDBH:קנא:000001">Tiveram</a> <a name="01910601600004" ll="SDBH:קנא:000001">inveja</a> de <a name="01910601600008" ll="SDBH:מֹשֶׁה:000000" il="TEN COMMANDMENTS">Moisés</a>, no <a name="01910601600014" ll="SDBH:מַחֲנֶה:000000" il="HK00220;HK00221;HK00223;il_israel_camp_sinai;il_jordan_crossing_d01">acampamento</a>,e de <a name="01910601600018" ll="SDBH:אַהֲרֹן:000000">Arão</a>, o <a name="01910601600020" ll="SDBH:קָדֹושׁ:000001">santo</a> do <a name="01910601600022" ll="SDBH:יהוה:000000">Senhor</a>.
    
\v 17 <a name="01910601700002" ll="SDBH:פתח:000000">Abriu</a>-<a name="01910601700002" ll="SDBH:פתח:000000">se</a> <a name="01910601700004" ll="SDBH:אֶרֶץ:000000">a</a> <a name="01910601700004" ll="SDBH:אֶרֶץ:000000">terra</a>, e <a name="01910601700008" ll="SDBH:בלע:000000">tragou</a> a <a name="01910601700010" ll="SDBH:דָּתָן:000000">Datã</a>,e <a name="01910601700014" ll="SDBH:כסה:000000">cobriu</a> o <a name="01910601700018" ll="SDBH:עֵדָה:000000">grupo</a> de <a name="01910601700020" ll="SDBH:אֲבִירָם:000000">Abirão</a>.
    
\v 18 <a name="01910601800004" ll="SDBH:בער:000000">Ateou</a>-<a name="01910601800004" ll="SDBH:בער:000000">se</a> um <a name="01910601800006" ll="SDBH:אֵשׁ:000000" il="Fire02">fogo</a> contra o <a name="01910601800010" ll="SDBH:עֵדָה:000000">seu</a> <a name="01910601800010" ll="SDBH:עֵדָה:000000">grupo</a>;a <a name="01910601800012" ll="SDBH:לֶהָבָה:000000" il="Fire02;Flame">chama</a> <a name="01910601800014" ll="SDBH:להט:000002">abrasou</a> os <a name="01910601800016" ll="SDBH:רָשָׁע:000000">ímpios</a>.
    
\v 19 Em <a name="01910601900008" ll="SDBH:חֹרֵב:000000" il="Horeb1;Horeb2">Horebe</a>, <a name="01910601900002">fizeram</a> <a name="01910601900004" ll="SDBH:עֵגֶל:000000" il="Fauna:2.10;LB00022;tb032014107">um</a> <a name="01910601900004" ll="SDBH:עֵגֶל:000000" il="Fauna:2.10;LB00022;tb032014107">bezerro</a>e <a name="01910601900012" ll="SDBH:חוה:001000" il="Prostrate_figures">adoraram</a> o <a name="01910601900016" ll="SDBH:מַסֵּכָה:000000" il="Realia:4.6b;adr1305227007">ídolo</a> <a name="01910601900016" ll="SDBH:מַסֵּכָה:000000" il="Realia:4.6b;adr1305227007">fundido</a>.
    
\v 20 E, assim, <a name="01910602000004" ll="SDBH:מור:000003">trocaram</a> a <a name="01910602000008" ll="SDBH:כָּבֹוד:000000">glória</a> de Deuspelo <a name="01910602000012" ll="SDBH:תַּבְנִית:000000">simulacro</a> de um <a name="01910602000014" ll="SDBH:שֹׁור:000000" il="Fauna:2.10;HK00096;LB00012;LB00018;LB00021;LB00022;NBGpb20140210;Realia:1.1.1a;Realia:8.2b;tb032905283;tb072705204">novilho</a> que <a name="01910602000016" ll="SDBH:אכל:000000">come</a> <a name="01910602000018" ll="SDBH:עֵשֶׂב:000000">erva</a>.
    
\v 21 <a name="01910602100002" ll="SDBH:שׁכח:000000">Esqueceram</a>-<a name="01910602100002" ll="SDBH:שׁכח:000000">se</a> de <a name="01910602100004" ll="SDBH:אֵל:000001">Deus</a>, <a name="01910602100006" ll="SDBH:ישׁע:000001">seu</a> <a name="01910602100006" ll="SDBH:ישׁע:000001">Salvador</a>,que, no <a name="01910602100014" ll="SDBH:מִצְרַיִם:000001" il="Egyptian_temple;Gizah_Pyramids;Goshen;NBGbs20120205-81;NBGbs20120205-82;NBGbs20120207-20;NBGbs20140204-40;tb010705951">Egito</a>, <a name="01910602100008">fizera</a> coisas <a name="01910602100010" ll="SDBH:גָּדֹול:000003">portentosas</a>,
    
\v 22 <a name="01910602200002" ll="SDBH:פלא:000000">maravilhas</a> na <a name="01910602200006" ll="SDBH:אֶרֶץ:000000">terra</a> de <a name="01910602200008" ll="SDBH:חָם:002001">Cam</a>,<a name="01910602200010" ll="SDBH:ירא:000001">tremendos</a> <a name="01910602200010" ll="SDBH:ירא:000001">feitos</a> no <a name="01910602200014" ll="SDBH:יָם:000005;SDBH:יָם־סוּף:000000" il="NBGbs20120204-08;P2060003;Rough_seas;Satellite Bible Atlas00034">mar</a> <a name="01910602200016" ll="SDBH:סוּף:000000" il="Flora:5.1.1;NBGbs20120207-24">Vermelho</a>.
    
\v 23 <a name="01910602300008" ll="SDBH:שׁמד:000000">Tê</a>-<a name="01910602300008" ll="SDBH:שׁמד:000000">los</a>-<a name="01910602300008" ll="SDBH:שׁמד:000000">ia</a> <a name="01910602300008" ll="SDBH:שׁמד:000000">exterminado</a>, como <a name="01910602300004" ll="SDBH:אמר:000000">dissera</a>,<a name="01910602300010">se</a> <a name="01910602300012" ll="SDBH:מֹשֶׁה:000000" il="TEN COMMANDMENTS">Moisés</a>, <a name="01910602300014" ll="SDBH:בָּחִיר:000000">seu</a> <a name="01910602300014" ll="SDBH:בָּחִיר:000000">escolhido</a>,<a name="01910602300010">não</a> <a name="01910602300016;01910602300022;01910602300026" ll="SDBH:פֶּרֶץ:000000">se</a> <a name="01910602300016;01910602300022;01910602300026" ll="SDBH:פֶּרֶץ:000000">houvesse</a> <a name="01910602300016;01910602300022;01910602300026" ll="SDBH:פֶּרֶץ:000000">interposto</a>,<a name="01910602300030" ll="SDBH:שׁוב:000020">impedindo</a> que <a name="01910602300032" ll="SDBH:חֵמָה:001001">sua</a> <a name="01910602300032" ll="SDBH:חֵמָה:001001">cólera</a> os <a name="01910602300036" ll="SDBH:שׁחת:000001">destruísse</a>.
    
\v 24 Também <a name="01910602400004" ll="SDBH:מאס:000000">desprezaram</a> a <a name="01910602400008" ll="SDBH:אֶרֶץ:000000">terra</a> <a name="01910602400010" ll="SDBH:חֶמְדָּה:000001">aprazível</a>e não <a name="01910602400014" ll="SDBH:אמן:000004">deram</a> <a name="01910602400014" ll="SDBH:אמן:000004">crédito</a> à <a name="01910602400018" ll="SDBH:דָּבָר:000000">sua</a> <a name="01910602400018" ll="SDBH:דָּבָר:000000">palavra</a>;
    
\v 25 antes, <a name="01910602500004" ll="SDBH:רגן:000001">murmuraram</a> em <a name="01910602500008" ll="SDBH:אֹהֶל:000000" il="HK00223;NBGbs20090406-1_27;NBGbs20090428_2;NBGbs20120713-532">suas</a> <a name="01910602500008" ll="SDBH:אֹהֶל:000000" il="HK00223;NBGbs20090406-1_27;NBGbs20090428_2;NBGbs20120713-532">tendas</a>e não <a name="01910602500012">acudiram</a> à <a name="01910602500016">voz</a> do <a name="01910602500018" ll="SDBH:יהוה:000000">Senhor</a>.
    
\v 26 Então, lhes jurou, de <a name="01910602600006">mão</a> <a name="01910602600004" ll="SDBH:נשׂא:000049">erguida</a>,que os <a name="01910602600012" ll="SDBH:נפל:000039">havia</a> <a name="01910602600012" ll="SDBH:נפל:000039">de</a> <a name="01910602600012" ll="SDBH:נפל:000039">arrasar</a> no <a name="01910602600020" ll="SDBH:מִדְבָּר:000000" il="RS26447_ISR91T-418;Sinai-191;Sinai-220;Sinai-3;Sinai178;Sinai190;tb113006604">deserto</a>;
    
\v 27 e também <a name="01910602700006" ll="SDBH:נפל:000039" tl="HOTTP2509">derribaria</a> entre as <a name="01910602700014" ll="SDBH:גֹּוי:000000">nações</a> a <a name="01910602700008" ll="SDBH:זֶרַע:000004" il="Children_of_Kabul,_Afghanistan">sua</a> <a name="01910602700008" ll="SDBH:זֶרַע:000004" il="Children_of_Kabul,_Afghanistan">descendência</a>e <a name="01910602700020" ll="SDBH:זרה:000000">os</a> <a name="01910602700020" ll="SDBH:זרה:000000">dispersaria</a> por outras <a name="01910602700026" ll="SDBH:אֶרֶץ:000000">terras</a>.
    
\v 28 Também <a name="01910602800004" ll="SDBH:צמד:000001">se</a> <a name="01910602800004" ll="SDBH:צמד:000001">juntaram</a> a <a name="01910602800008" ll="SDBH:בַּעַל פְּעֹור:000000" il="tb060408235;tb060408297">Baal</a>-<a name="01910602800008" ll="SDBH:בַּעַל פְּעֹור:000000" il="tb060408235;tb060408297">Peor</a>e <a name="01910602800012" ll="SDBH:אכל:000000">comeram</a> os <a name="01910602800014" ll="SDBH:זֶבַח:000000">sacrifícios</a> dos ídolos <a name="01910602800016" ll="SDBH:מות:000000">mortos</a>.
    
\v 29 Assim, <a name="01910602900008" ll="SDBH:מַעֲלָל:000001">com</a> <a name="01910602900008" ll="SDBH:מַעֲלָל:000001">tais</a> <a name="01910602900008" ll="SDBH:מַעֲלָל:000001">ações</a>, o <a name="01910602900004" ll="SDBH:כעס:000001">provocaram</a> <a name="01910602900004" ll="SDBH:כעס:000001">à</a> <a name="01910602900004" ll="SDBH:כעס:000001">ira</a>;e <a name="01910602900012" ll="SDBH:פרץ:000009">grassou</a> <a name="01910602900016" ll="SDBH:מַגֵּפָה:000000">peste</a> entre eles.
    
\v 30 Então, <a name="01910603000004">se</a> <a name="01910603000004">levantou</a> <a name="01910603000006" ll="SDBH:פִּינְחָס:000000">Fineias</a> e <a name="01910603000010" ll="SDBH:פלל:000000">executou</a> o juízo;e <a name="01910603000014" ll="SDBH:עצר:000004">cessou</a> a <a name="01910603000018" ll="SDBH:מַגֵּפָה:000000">peste</a>.
    
\v 31 <a name="01910603100004" ll="SDBH:חשׁב:000007">Isso</a> lhe <a name="01910603100004" ll="SDBH:חשׁב:000007">foi</a> <a name="01910603100004" ll="SDBH:חשׁב:000007">imputado</a> por <a name="01910603100010" ll="SDBH:צְדָקָה:000001">justiça</a>,de <a name="01910603100014" ll="SDBH:דֹּור:001000">geração</a> em <a name="01910603100018" ll="SDBH:דֹּור:001000">geração</a>, <a name="01910603100020;01910603100022" ll="SDBH:עַד:000001;SDBH:עֹולָם:000000">para</a> <a name="01910603100020;01910603100022" ll="SDBH:עַד:000001;SDBH:עֹולָם:000000">sempre</a>.
    
\v 32 Depois, o <a name="01910603200004" ll="SDBH:קצף:000002">indignaram</a> nas <a name="01910603200008" ll="SDBH:מַיִם:000000" il="Water_drop_001">águas</a> de <a name="01910603200010" ll="SDBH:מְרִיבָה:001000">Meribá</a>,e, por causa deles, sucedeu mal a <a name="01910603200018" ll="SDBH:מֹשֶׁה:000000" il="TEN COMMANDMENTS">Moisés</a>,
    
\v 33 pois <a name="01910603300004" ll="SDBH:מרה:000000">foram</a> <a name="01910603300004" ll="SDBH:מרה:000000">rebeldes</a> ao <a name="01910603300008" ll="SDBH:רוּחַ:000002">Espírito</a> de Deus,e Moisés <a name="01910603300012;01910603300016" ll="SDBH:בטה:000000;SDBH:שָׂפָה:000001">falou</a> <a name="01910603300012;01910603300016" ll="SDBH:בטה:000000;SDBH:שָׂפָה:000001">irrefletidamente</a>.
    
\v 34 Não <a name="01910603400004" ll="SDBH:שׁמד:000000">exterminaram</a> os <a name="01910603400010" ll="SDBH:עַם:000000">povos</a>,como o <a name="01910603400016" ll="SDBH:יהוה:000000">Senhor</a> lhes <a name="01910603400014" ll="SDBH:אמר:000000">ordenara</a>.
    
\v 35 Antes, <a name="01910603500004" ll="SDBH:ערב:001001">se</a> <a name="01910603500004" ll="SDBH:ערב:001001">mesclaram</a> com as <a name="01910603500010" ll="SDBH:גֹּוי:000000" il="jlm_soltemp_d01;jlm_soltemp_m06;jlm_soltemp_pillars_m01">nações</a>e <a name="01910603500016" ll="SDBH:מַעֲשֶׂה:000001">lhes</a> <a name="01910603500014" ll="SDBH:למד:000000">aprenderam</a> <a name="01910603500016" ll="SDBH:מַעֲשֶׂה:000001">as</a> <a name="01910603500016" ll="SDBH:מַעֲשֶׂה:000001">obras</a>;
    
\v 36 <a name="01910603600004" ll="SDBH:עבד:000002">deram</a> <a name="01910603600004" ll="SDBH:עבד:000002">culto</a> a <a name="01910603600008" ll="SDBH:עָצָב:000000" il="Realia:4.6b;adr1305227007">seus</a> <a name="01910603600008" ll="SDBH:עָצָב:000000" il="Realia:4.6b;adr1305227007">ídolos</a>,os quais se lhes converteram em <a name="01910603600018" ll="SDBH:מֹוקֵשׁ:000000">laço</a>;
    
\v 37 pois <a name="01910603700004" ll="SDBH:זבח:000001">imolaram</a> <a name="01910603700008" ll="SDBH:בֵּן:000000">seus</a> <a name="01910603700008" ll="SDBH:בֵּן:000000">filhos</a>e <a name="01910603700014" ll="SDBH:בַּת:000000" il="Children_of_Kabul,_Afghanistan">suas</a> <a name="01910603700014" ll="SDBH:בַּת:000000" il="Children_of_Kabul,_Afghanistan">filhas</a> aos <a name="01910603700020" ll="SDBH:שֵׁד:000000" il="adr1305227624">demônios</a>
    
\v 38 e <a name="01910603800004" ll="SDBH:שׁפך:000009">derramaram</a> <a name="01910603800006" ll="SDBH:דָּם:000003">sangue</a> <a name="01910603800008" ll="SDBH:נָקִי:000000">inocente</a>,o <a name="01910603800010" ll="SDBH:דָּם:000003">sangue</a> <a name="01910603800012" ll="SDBH:בֵּן:000000">de</a> <a name="01910603800012" ll="SDBH:בֵּן:000000">seus</a> <a name="01910603800012" ll="SDBH:בֵּן:000000">filhos</a> e <a name="01910603800016" ll="SDBH:בַּת:000000" il="Children_of_Kabul,_Afghanistan">filhas</a>,que <a name="01910603800020" ll="SDBH:זבח:000001">sacrificaram</a> aos <a name="01910603800024" ll="SDBH:עָצָב:000000" il="Realia:4.6b;adr1305227007">ídolos</a> de <a name="01910603800026" ll="SDBH:כְּנַעַן:001000" il="Canaanite-temple;Realia:4.2.1b">Canaã</a>;e a <a name="01910603800034" ll="SDBH:אֶרֶץ:000000">terra</a> <a name="01910603800030" ll="SDBH:חנף:000000">foi</a> <a name="01910603800030" ll="SDBH:חנף:000000">contaminada</a> com <a name="01910603800040" ll="SDBH:דָּם:000000" il="Blood">sangue</a>.
    
\v 39 Assim <a name="01910603900004" ll="SDBH:טמא:000000">se</a> <a name="01910603900004" ll="SDBH:טמא:000000">contaminaram</a> com as <a name="01910603900008" ll="SDBH:מַעֲשֶׂה:000001">suas</a> <a name="01910603900008" ll="SDBH:מַעֲשֶׂה:000001">obras</a>e <a name="01910603900012" ll="SDBH:זנה:000002">se</a> <a name="01910603900012" ll="SDBH:זנה:000002">prostituíram</a> nos <a name="01910603900016" ll="SDBH:מַעֲלָל:000001">seus</a> <a name="01910603900016" ll="SDBH:מַעֲלָל:000001">feitos</a>.
    
\v 40 <a name="01910604000004" ll="SDBH:חרה:000001">Acendeu</a>-<a name="01910604000004" ll="SDBH:חרה:000001">se</a>, por isso, a <a name="01910604000006" ll="SDBH:אַף:000008">ira</a> do <a name="01910604000008" ll="SDBH:יהוה:000000">Senhor</a> contra o <a name="01910604000012" ll="SDBH:עַם:000000">seu</a> <a name="01910604000012" ll="SDBH:עַם:000000">povo</a>,e ele <a name="01910604000016" ll="SDBH:תעב:000000">abominou</a> a <a name="01910604000020" ll="SDBH:נַחֲלָה:000000">sua</a> própria <a name="01910604000020" ll="SDBH:נַחֲלָה:000000">herança</a>
    
\v 41 e <a name="01910604100004">os</a> <a name="01910604100004">entregou</a> ao <a name="01910604100008">poder</a> <a name="01910604100010" ll="SDBH:גֹּוי:000000">das</a> <a name="01910604100010" ll="SDBH:גֹּוי:000000">nações</a>;sobre eles <a name="01910604100014" ll="SDBH:משׁל:001000">dominaram</a> os <a name="01910604100018" ll="SDBH:שׂנא:000001">que</a> <a name="01910604100018" ll="SDBH:שׂנא:000001">os</a> <a name="01910604100018" ll="SDBH:שׂנא:000001">odiavam</a>.
    
\v 42 Também <a name="01910604200004" ll="SDBH:לחץ:000003">os</a> <a name="01910604200004" ll="SDBH:לחץ:000003">oprimiram</a> os <a name="01910604200006" ll="SDBH:איב:000001">seus</a> <a name="01910604200006" ll="SDBH:איב:000001">inimigos</a>,sob <a name="01910604200014">cujo</a> <a name="01910604200014">poder</a> <a name="01910604200010" ll="SDBH:כנע:000003">foram</a> <a name="01910604200010" ll="SDBH:כנע:000003">subjugados</a>.
    
\v 43 <a name="01910604300004" ll="SDBH:רַב:000001">Muitas</a> <a name="01910604300002">vezes</a> <a name="01910604300006" ll="SDBH:נצל:000005">os</a> <a name="01910604300006" ll="SDBH:נצל:000005">libertou</a>,mas eles o <a name="01910604300012" ll="SDBH:מרה:000000">provocaram</a> com os <a name="01910604300016" ll="SDBH:עֵצָה:000002">seus</a> <a name="01910604300016" ll="SDBH:עֵצָה:000002">conselhos</a>e, por <a name="01910604300024" ll="SDBH:עָוֹן:000000" tl="HOTTP2510">sua</a> <a name="01910604300024" ll="SDBH:עָוֹן:000000" tl="HOTTP2510">iniquidade</a>, <a name="01910604300020" ll="SDBH:מכך:000000" tl="HOTTP2510">foram</a> <a name="01910604300020" ll="SDBH:מכך:000000" tl="HOTTP2510">abatidos</a>.
    
\v 44 <a name="01910604400004" ll="SDBH:ראה:000000">Olhou</a>-os, contudo, quando <a name="01910604400010" ll="SDBH:צַר:000002">estavam</a> <a name="01910604400010" ll="SDBH:צַר:000002">angustiados</a>e <a name="01910604400020" ll="SDBH:רִנָּה:000000">lhes</a> <a name="01910604400016">ouviu</a> <a name="01910604400020" ll="SDBH:רִנָּה:000000">o</a> <a name="01910604400020" ll="SDBH:רִנָּה:000000">clamor</a>;
    
