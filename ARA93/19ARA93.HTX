\v 8 (7) O <a name="01904600800002" ll="SDBH:יהוה:000000">Senhor</a> dos <a name="01904600800004" ll="SDBH:צָבָא:000006">Exércitos</a> está conosco;o <a name="01904600800012" ll="SDBH:אֱלֹהִים:000001">Deus</a> de <a name="01904600800014" ll="SDBH:יַעֲקֹב:000000">Jacó</a> é o nosso <a name="01904600800008" ll="SDBH:מִשְׂגָּב:000001">refúgio</a>.
    
\v 9 (8) <a name="01904600900002" ll="SDBH:הלך:000001">Vinde</a>, <a name="01904600900004" ll="SDBH:חזה:000001">contemplai</a> as <a name="01904600900006">obras</a> do <a name="01904600900008" ll="SDBH:יהוה:000000">Senhor</a>,que <a name="01904600900014" ll="SDBH:שַׁמָּה:000000">assolações</a> <a name="01904600900012" ll="SDBH:שׂים:000013">efetuou</a> na <a name="01904600900020" ll="SDBH:אֶרֶץ:000000">terra</a>.
    
\v 10 (9) Ele <a name="01904601000002" ll="SDBH:שׁבת:000003">põe</a> <a name="01904601000002" ll="SDBH:שׁבת:000003">termo</a> à <a name="01904601000004" ll="SDBH:מִלְחָמָה:000000">guerra</a> até aos <a name="01904601000008" ll="SDBH:קָצֶה:000002">confins</a> do <a name="01904601000012" ll="SDBH:אֶרֶץ:000000">mundo</a>,<a name="01904601000016" ll="SDBH:שׁבר:000000">quebra</a> o <a name="01904601000014" ll="SDBH:קֶשֶׁת:000000" il="Realia:2.14.1a;Realia:2.14.1b">arco</a> e <a name="01904601000020" ll="SDBH:קצץ:000003">despedaça</a> a <a name="01904601000022" ll="SDBH:חֲנִית:000000" il="HK00200;Spearheads-02-007a;adr1005306124">lança</a>;<a name="01904601000026" ll="SDBH:שׂרף:000001" il="Fire02">queima</a> os <a name="01904601000024" ll="SDBH:עֲגָלָה:000000;SDBH:עֲגָלָה:000002" il="NBGmk20131008;Realia:8.2b" tl="HOTTP2239">carros</a> no <a name="01904601000032" ll="SDBH:אֵשׁ:000000" il="Fire02">fogo</a>.
    
\v 11 (10) <a name="01904601100002" ll="SDBH:רפה:000007">Aquietai</a>-vos e <a name="01904601100006">sabei</a> que eu sou <a name="01904601100012" ll="SDBH:אֱלֹהִים:000002">Deus</a>;<a name="01904601100014" ll="SDBH:רום:000015">sou</a> <a name="01904601100014" ll="SDBH:רום:000015">exaltado</a> entre as <a name="01904601100020" ll="SDBH:גֹּוי:000000">nações</a>, <a name="01904601100022" ll="SDBH:רום:000015">sou</a> <a name="01904601100022" ll="SDBH:רום:000015">exaltado</a> na <a name="01904601100028" ll="SDBH:אֶרֶץ:000000">terra</a>.
    
\v 12 (11) O <a name="01904601200002" ll="SDBH:יהוה:000000">Senhor</a> dos <a name="01904601200004" ll="SDBH:צָבָא:000006">Exércitos</a> está conosco;o <a name="01904601200012" ll="SDBH:אֱלֹהִים:000001">Deus</a> de <a name="01904601200014" ll="SDBH:יַעֲקֹב:000000">Jacó</a> é o nosso <a name="01904601200008" ll="SDBH:מִשְׂגָּב:000001">refúgio</a>.
\c 47
\v 2 (1) <a name="01904700200008" ll="SDBH:תקע:000001">Batei</a> <a name="01904700200010" ll="SDBH:כַּף:000006">palmas</a>, todos os <a name="01904700200006" ll="SDBH:עַם:000000">povos</a>;<a name="01904700200012" ll="SDBH:רוע:000000">celebrai</a> a <a name="01904700200016" ll="SDBH:אֱלֹהִים:000002">Deus</a> com <a name="01904700200020">vozes</a> de <a name="01904700200022" ll="SDBH:רִנָּה:000000">júbilo</a>.
    
\v 3 (2) Pois o <a name="01904700300004" ll="SDBH:יהוה:000000">Senhor</a> <a name="01904700300006" ll="SDBH:עֶלְיֹון:000000">Altíssimo</a> é <a name="01904700300008" ll="SDBH:ירא:000001">tremendo</a>,é o <a name="01904700300012" ll="SDBH:גָּדֹול:000002">grande</a> <a name="01904700300010" ll="SDBH:מֶלֶךְ:000000">rei</a> de toda a <a name="01904700300020" ll="SDBH:אֶרֶץ:000000">terra</a>.
    
\v 4 (3) Ele nos <a name="01904700400002" ll="SDBH:דבר:000001">submeteu</a> os <a name="01904700400004" ll="SDBH:עַם:000000">povos</a>e pôs sob os <a name="01904700400014" ll="SDBH:רֶגֶל:000000">nossos</a> <a name="01904700400014" ll="SDBH:רֶגֶל:000000">pés</a> as <a name="01904700400010" ll="SDBH:לְאֹם:000000">nações</a>.
    
\v 5 (4) <a name="01904700500002" ll="SDBH:בחר:000000">Escolheu</a>-nos a <a name="01904700500008" ll="SDBH:נַחֲלָה:000000">nossa</a> <a name="01904700500008" ll="SDBH:נַחֲלָה:000000">herança</a>,a <a name="01904700500012" ll="SDBH:גָּאֹון:001000">glória</a> de <a name="01904700500014" ll="SDBH:יַעֲקֹב:000000">Jacó</a>, a quem <a name="01904700500018" ll="SDBH:אהב:000000">ele</a> <a name="01904700500018" ll="SDBH:אהב:000000">ama</a>.
    
\v 6 (5) <a name="01904700600002" ll="SDBH:עלה:000004">Subiu</a> <a name="01904700600004" ll="SDBH:אֱלֹהִים:000002">Deus</a> por entre <a name="01904700600008" ll="SDBH:תְּרוּעָה:000000" il="HK00188">aclamações</a>,o <a name="01904700600010" ll="SDBH:יהוה:000000">Senhor</a>, ao <a name="01904700600014">som</a> de <a name="01904700600016" ll="SDBH:שֹׁופָר:000000" il="BK00179;HK00188;LB00186;NBGmk20140407c;Realia:7.3.1c">trombeta</a>.
    
\v 7 (6) <a name="01904700700002" ll="SDBH:זמר:000000" il="HK00188">Salmodiai</a> a <a name="01904700700004" ll="SDBH:אֱלֹהִים:000002" tl="HOTTP2240">Deus</a>, <a name="01904700700006" ll="SDBH:זמר:000000" il="HK00188">cantai</a> <a name="01904700700006" ll="SDBH:זמר:000000" il="HK00188">louvores</a>;<a name="01904700700008" ll="SDBH:זמר:000000" il="HK00188">salmodiai</a> ao <a name="01904700700012" ll="SDBH:מֶלֶךְ:000000">nosso</a> <a name="01904700700012" ll="SDBH:מֶלֶךְ:000000">Rei</a>, <a name="01904700700014" ll="SDBH:זמר:000000" il="HK00188">cantai</a> <a name="01904700700014" ll="SDBH:זמר:000000" il="HK00188">louvores</a>.
    
\v 8 (7) <a name="01904700800012" ll="SDBH:אֱלֹהִים:000002">Deus</a> é o <a name="01904700800004" ll="SDBH:מֶלֶךְ:000000">Rei</a> de toda a <a name="01904700800010" ll="SDBH:אֶרֶץ:000000">terra</a>;<a name="01904700800014" ll="SDBH:זמר:000000" il="HK00188">salmodiai</a> com harmonioso <a name="01904700800016" ll="SDBH:מַשְׂכִּיל:000000">cântico</a>.
    
\v 9 (8) <a name="01904700900004" ll="SDBH:אֱלֹהִים:000002">Deus</a> <a name="01904700900002" ll="SDBH:מלך:000000">reina</a> sobre as <a name="01904700900008" ll="SDBH:גֹּוי:000000">nações</a>;<a name="01904700900010" ll="SDBH:אֱלֹהִים:000002">Deus</a> <a name="01904700900012">se</a> <a name="01904700900012">assenta</a> no <a name="01904700900018" ll="SDBH:קֹדֶשׁ:000001">seu</a> <a name="01904700900018" ll="SDBH:קֹדֶשׁ:000001">santo</a> <a name="01904700900016" ll="SDBH:כִּסֵּא:000000" il="Ashur-nasir-pal_II_throne;Darius_In_Parse;Roman_throne;Throne_Greek;Throne_Toutankamon;adr1603194845;mb120901102">trono</a>.
    
\v 10 (9) Os <a name="01904701000002" ll="SDBH:נָדִיב:000001">príncipes</a> dos <a name="01904701000004" ll="SDBH:עַם:000000">povos</a> <a name="01904701000006" ll="SDBH:אסף:000000">se</a> <a name="01904701000006" ll="SDBH:אסף:000000">reúnem</a>,o <a name="01904701000008" tl="HOTTP2241">povo</a> do <a name="01904701000010" ll="SDBH:אֱלֹהִים:000001" tl="HOTTP2241">Deus</a> de <a name="01904701000012" ll="SDBH:אַבְרָהָם:000000" il="adr1305165446" tl="HOTTP2241">Abraão</a>,porque a <a name="01904701000018" ll="SDBH:אֱלֹהִים:000002">Deus</a> pertencem os <a name="01904701000020" ll="SDBH:מָגֵן:000000" il="NBGmk20131008">escudos</a> <a name="01904701000022" ll="SDBH:אֶרֶץ:000000">da</a> <a name="01904701000022" ll="SDBH:אֶרֶץ:000000">terra</a>;ele <a name="01904701000026" ll="SDBH:עלה:000016">se</a> <a name="01904701000026" ll="SDBH:עלה:000016">exaltou</a> <a name="01904701000024" ll="SDBH:מְאֹד:000001">gloriosamente</a>.
\c 48
\v 2 (1) <a name="01904800200002" ll="SDBH:גָּדֹול:000002">Grande</a> é o <a name="01904800200004" ll="SDBH:יהוה:000000">Senhor</a> e <a name="01904800200010" ll="SDBH:מְאֹד:000001">mui</a> <a name="01904800200008" ll="SDBH:הלל:001001">digno</a> <a name="01904800200008" ll="SDBH:הלל:001001">de</a> <a name="01904800200008" ll="SDBH:הלל:001001">ser</a> <a name="01904800200008" ll="SDBH:הלל:001001">louvado</a>,na <a name="01904800200014" ll="SDBH:עִיר:000000">cidade</a> <a name="01904800200016" ll="SDBH:אֱלֹהִים:000001">do</a> <a name="01904800200016" ll="SDBH:אֱלֹהִים:000001">nosso</a> <a name="01904800200016" ll="SDBH:אֱלֹהִים:000001">Deus</a>.
    
\v 3 (2) <a name="01904800200018;01904800200020" ll="SDBH:הַר:000000;SDBH:קֹדֶשׁ:000001" il="tb060716478">Seu</a> <a name="01904800200018;01904800200020" ll="SDBH:הַר:000000;SDBH:קֹדֶשׁ:000001" il="tb060716478">santo</a> <a name="01904800200018;01904800200020" ll="SDBH:הַר:000000;SDBH:קֹדֶשׁ:000001" il="tb060716478">monte</a>, <a name="01904800300002" ll="SDBH:יָפֶה:000000">belo</a> e <a name="01904800300004" ll="SDBH:נֹוף:000000">sobranceiro</a>,é a <a name="01904800300006" ll="SDBH:מָשֹׂושׂ:000000">alegria</a> de toda a <a name="01904800300012" ll="SDBH:אֶרֶץ:000000">terra</a>;o monte <a name="01904800300016" ll="SDBH:צִיֹּון:000000" il="tb060716478">Sião</a>, para os <a name="01904800300018" ll="SDBH:יְרֵכָה:000000">lados</a> do <a name="01904800300020" ll="SDBH:צָפֹון:000000;SDBH:צָפֹון:000001">Norte</a>,a <a name="01904800300022" ll="SDBH:קִרְיָה:000000">cidade</a> do <a name="01904800300026" ll="SDBH:רַב:000004">grande</a> <a name="01904800300024" ll="SDBH:מֶלֶךְ:000000">Rei</a>.
    
\v 4 (3) Nos <a name="01904800400006" ll="SDBH:אַרְמֹון:000000">palácios</a> <a name="01904800400006" ll="SDBH:אַרְמֹון:000000">dela</a>,<a name="01904800400002" ll="SDBH:אֱלֹהִים:000002">Deus</a> <a name="01904800400008">se</a> <a name="01904800400008">faz</a> <a name="01904800400008">conhecer</a> como alto <a name="01904800400012" ll="SDBH:מִשְׂגָּב:000001">refúgio</a>.
    
\v 5 (4) Por isso, eis que os <a name="01904800500008" ll="SDBH:מֶלֶךְ:000000">reis</a> <a name="01904800500010" ll="SDBH:יעד:000002">se</a> <a name="01904800500010" ll="SDBH:יעד:000002">coligaram</a>e <a name="01904800500014">juntos</a> <a name="01904800500012" ll="SDBH:עבר:000000">sumiram</a>-se;
    
\v 6 (5) bastou-lhes <a name="01904800600004" ll="SDBH:ראה:000000">vê</a>-lo, e <a name="01904800600008" ll="SDBH:תמהּ:000000">se</a> <a name="01904800600008" ll="SDBH:תמהּ:000000">espantaram</a>,<a name="01904800600010" ll="SDBH:בהל:001000">tomaram</a>-<a name="01904800600010" ll="SDBH:בהל:001000">se</a> <a name="01904800600010" ll="SDBH:בהל:001000">de</a> <a name="01904800600010" ll="SDBH:בהל:001000">assombro</a> e <a name="01904800600012" ll="SDBH:חפז:000001">fugiram</a> <a name="01904800600012" ll="SDBH:חפז:000001">apressados</a>.
    
\v 7 (6) O <a name="01904800700002" ll="SDBH:רְעָדָה:000000">terror</a> ali <a name="01904800700004" ll="SDBH:אחז:000002">os</a> <a name="01904800700004" ll="SDBH:אחז:000002">venceu</a>,e sentiram <a name="01904800700008" ll="SDBH:חִיל:000000">dores</a> como <a name="01904800700014" ll="SDBH:ילד:000000" il="Baby_boy_after_birth;Birth;Children_of_Kabul,_Afghanistan;Motherhood1912">de</a> <a name="01904800700014" ll="SDBH:ילד:000000" il="Baby_boy_after_birth;Birth;Children_of_Kabul,_Afghanistan;Motherhood1912">parturiente</a>.
    
\v 8 (7) Com <a name="01904800800004" ll="SDBH:רוּחַ:000004" il="Gale_Force_Winds" tl="HOTTP2242">vento</a> <a name="01904800800006" ll="SDBH:קָדִים:000002" il="Gale_Force_Winds" tl="HOTTP2242">oriental</a> <a name="01904800800008" ll="SDBH:שׁבר:000000">destruíste</a>as <a name="01904800800010" ll="SDBH:אֳנִיָּה:000000" il="HK00208;LB00213;Realia:8.1b;Realia:8.1c;Sail">naus</a> de <a name="01904800800012" ll="SDBH:תַּרְשִׁישׁ:001000" il="Spain">Társis</a>.
    
\v 9 (8) Como <a name="01904800900006">temos</a> <a name="01904800900006">ouvido</a> dizer, assim o <a name="01904800900010" ll="SDBH:ראה:000000">vimos</a>na <a name="01904800900014" ll="SDBH:עִיר:000000">cidade</a> <a name="01904800900016" ll="SDBH:יהוה:000000">do</a> <a name="01904800900016" ll="SDBH:יהוה:000000">Senhor</a> dos <a name="01904800900018" ll="SDBH:צָבָא:000006">Exércitos</a>,na <a name="01904800900022" ll="SDBH:עִיר:000000">cidade</a> do <a name="01904800900024" ll="SDBH:אֱלֹהִים:000001">nosso</a> <a name="01904800900024" ll="SDBH:אֱלֹהִים:000001">Deus</a>.<a name="01904800900026" ll="SDBH:אֱלֹהִים:000002">Deus</a> <a name="01904800900028" ll="SDBH:כון:000025">a</a> <a name="01904800900028" ll="SDBH:כון:000025">estabelece</a> <a name="01904800900030" ll="SDBH:עַד:000001">para</a> <a name="01904800900032" ll="SDBH:עֹולָם:000000">sempre</a>.
    
\v 10 (9) <a name="01904801000002" ll="SDBH:דמה:001004">Pensamos</a>, ó <a name="01904801000004" ll="SDBH:אֱלֹהִים:000002">Deus</a>, na <a name="01904801000006" ll="SDBH:חֶסֶד:000000">tua</a> <a name="01904801000006" ll="SDBH:חֶסֶד:000000">misericórdia</a>no <a name="01904801000010">meio</a> <a name="01904801000012" ll="SDBH:הֵיכָל:000000">do</a> <a name="01904801000012" ll="SDBH:הֵיכָל:000000">teu</a> <a name="01904801000012" ll="SDBH:הֵיכָל:000000">templo</a>.
    
\v 11 (10) Como o <a name="01904801100004" ll="SDBH:שֵׁם:000002">teu</a> <a name="01904801100004" ll="SDBH:שֵׁם:000002">nome</a>, ó <a name="01904801100006" ll="SDBH:אֱלֹהִים:000002">Deus</a>,assim o <a name="01904801100010" ll="SDBH:תְּהִלָּה:000000">teu</a> <a name="01904801100010" ll="SDBH:תְּהִלָּה:000000">louvor</a> se estende até aos <a name="01904801100014" ll="SDBH:קָצוּ:000000">confins</a> <a name="01904801100016" ll="SDBH:אֶרֶץ:000000">da</a> <a name="01904801100016" ll="SDBH:אֶרֶץ:000000">terra</a>;a <a name="01904801100022" ll="SDBH:יָמִין:000000">tua</a> <a name="01904801100022" ll="SDBH:יָמִין:000000">destra</a> <a name="01904801100020" ll="SDBH:מלא:000008">está</a> <a name="01904801100020" ll="SDBH:מלא:000008">cheia</a> de <a name="01904801100018" ll="SDBH:צֶדֶק:000001">justiça</a>.
    
\v 12 (11) <a name="01904801200002" ll="SDBH:שׂמח:000000">Alegre</a>-<a name="01904801200002" ll="SDBH:שׂמח:000000">se</a> o <a name="01904801200004" ll="SDBH:הַר:000000">monte</a> <a name="01904801200006" ll="SDBH:צִיֹּון:000000" il="tb060716478">Sião</a>,<a name="01904801200008" ll="SDBH:גיל:000000">exultem</a> as <a name="01904801200010" ll="SDBH:בַּת:000001;SDBH:בַּת:000003">filhas</a> de <a name="01904801200012" ll="SDBH:יְהוּדָה:000000">Judá</a>,por causa dos <a name="01904801200016" ll="SDBH:מִשְׁפָּט:000001" il="Tel-dan-israelite-gate-im-tor">teus</a> <a name="01904801200016" ll="SDBH:מִשְׁפָּט:000001" il="Tel-dan-israelite-gate-im-tor">juízos</a>.
    
\v 13 (12) <a name="01904801300002">Percorrei</a> a <a name="01904801300004" ll="SDBH:צִיֹּון:000000" il="tb060716478">Sião</a>, <a name="01904801300008" ll="SDBH:נקף:001002">rodeai</a>-<a name="01904801300008" ll="SDBH:נקף:001002">a</a> toda,<a name="01904801300010" ll="SDBH:ספר:000000">contai</a>-<a name="01904801300012" ll="SDBH:מִגְדָּל:000000" il="NBGpb20140401a">lhe</a> <a name="01904801300012" ll="SDBH:מִגְדָּל:000000" il="NBGpb20140401a">as</a> <a name="01904801300012" ll="SDBH:מִגְדָּל:000000" il="NBGpb20140401a">torres</a>;
    
\v 14 (13) <a name="01904801400002" ll="SDBH:שׁית:000011">notai</a> <a name="01904801400004" ll="SDBH:לֵב:000001">bem</a> os <a name="01904801400008" ll="SDBH:חֵיל:000000">seus</a> <a name="01904801400008" ll="SDBH:חֵיל:000000">baluartes</a>,<a name="01904801400010" ll="SDBH:פסג:000000">observai</a> os <a name="01904801400012" ll="SDBH:אַרְמֹון:000000">seus</a> <a name="01904801400012" ll="SDBH:אַרְמֹון:000000">palácios</a>,para <a name="01904801400016" ll="SDBH:ספר:000006">narrardes</a> às <a name="01904801400020" ll="SDBH:דֹּור:001000">gerações</a> <a name="01904801400022" ll="SDBH:אַחֲרֹון:000001">vindouras</a>
    
\v 15 (14) que este é <a name="01904801500006" ll="SDBH:אֱלֹהִים:000002">Deus</a>,o <a name="01904801500008" ll="SDBH:אֱלֹהִים:000001">nosso</a> <a name="01904801500008" ll="SDBH:אֱלֹהִים:000001">Deus</a> para <a name="01904801500010;01904801500012;01904801500014" ll="SDBH:עַד:001000;SDBH:עֹולָם:000000">todo</a> <a name="01904801500010;01904801500012;01904801500014" ll="SDBH:עַד:001000;SDBH:עֹולָם:000000">o</a> <a name="01904801500010;01904801500012;01904801500014" ll="SDBH:עַד:001000;SDBH:עֹולָם:000000">sempre</a>;ele será <a name="01904801500018">nosso</a> <a name="01904801500018">guia</a> até à <a name="01904801500022" ll="SDBH:מות:000000" tl="HOTTP2243">morte</a>.
\c 49
\v 2 (1) <a name="01904900200010" ll="SDBH:עַם:000000">Povos</a> todos, <a name="01904900200002">escutai</a> isto;<a name="01904900200012" ll="SDBH:אזן:000000">dai</a> <a name="01904900200012" ll="SDBH:אזן:000000">ouvidos</a>, <a name="01904900200016">moradores</a> todos da <a name="01904900200018" ll="SDBH:חֶלֶד:000000">terra</a>,
    
\v 3 (2) tanto <a name="01904900300004;01904900300006" ll="SDBH:אָדָם:000000;SDBH:בֵּן:000003">plebeus</a> como os de <a name="01904900300010;01904900300012" ll="SDBH:בֵּן:000003">fina</a> <a name="01904900300010;01904900300012" ll="SDBH:בֵּן:000003">estirpe</a>,todos <a name="01904900300014">juntamente</a>, <a name="01904900300016" ll="SDBH:עָשִׁיר:000000">ricos</a> e <a name="01904900300020" ll="SDBH:אֶבְיֹון:000000">pobres</a>.
    
\v 4 (3) Os <a name="01904900400002" ll="SDBH:פֶּה:000000">meus</a> <a name="01904900400002" ll="SDBH:פֶּה:000000">lábios</a> <a name="01904900400004" ll="SDBH:דבר:001000">falarão</a> <a name="01904900400006" ll="SDBH:חָכְמָה:000000">sabedoria</a>,e o <a name="01904900400012" ll="SDBH:לֵב:000001">meu</a> <a name="01904900400012" ll="SDBH:לֵב:000001">coração</a> terá <a name="01904900400010" ll="SDBH:הָגוּת:000000">pensamentos</a> <a name="01904900400014" ll="SDBH:תְּבוּנָה:000000">judiciosos</a>.
    
\v 5 (4) <a name="01904900500002" ll="SDBH:נטה:000003">Inclinarei</a> os <a name="01904900500008" ll="SDBH:אֹזֶן:000000">ouvidos</a> a uma <a name="01904900500006" ll="SDBH:מָשָׁל:000000">parábola</a>,<a name="01904900500010" ll="SDBH:פתח:000009">decifrarei</a> o <a name="01904900500016" ll="SDBH:חִידָה:000000">meu</a> <a name="01904900500016" ll="SDBH:חִידָה:000000">enigma</a> ao som da <a name="01904900500014" ll="SDBH:כִּנֹּור:000000" il="BK00175;BK00176;Exile_Lachish;HK00188;NBGmk20131114;adr1306213109;tb072705188">harpa</a>.
    
\v 6 (5) Por que <a name="01904900600004" ll="SDBH:ירא:000000">hei</a> <a name="01904900600004" ll="SDBH:ירא:000000">de</a> <a name="01904900600004" ll="SDBH:ירא:000000">eu</a> <a name="01904900600004" ll="SDBH:ירא:000000">temer</a> nos <a name="01904900600008" ll="SDBH:יֹום:000003">dias</a> da <a name="01904900600010" ll="SDBH:רַע:000004">tribulação</a>,quando <a name="01904900600016">me</a> <a name="01904900600016">salteia</a> a <a name="01904900600012" ll="SDBH:עָוֹן:000000">iniquidade</a> dos que <a name="01904900600014" ll="SDBH:עָקֵב:001000">me</a> <a name="01904900600014" ll="SDBH:עָקֵב:001000">perseguem</a>,
    
\v 7 (6) dos que <a name="01904900700004" ll="SDBH:בטח:000001">confiam</a> nos <a name="01904900700008" ll="SDBH:חַיִל:000001">seus</a> <a name="01904900700008" ll="SDBH:חַיִל:000001">bens</a>e na <a name="01904900700016" ll="SDBH:עֹשֶׁר:000000">sua</a> <a name="01904900700014" ll="SDBH:רֹב:000001">muita</a> <a name="01904900700016" ll="SDBH:עֹשֶׁר:000000">riqueza</a> <a name="01904900700018" ll="SDBH:הלל:001002">se</a> <a name="01904900700018" ll="SDBH:הלל:001002">gloriam</a>?
    
\v 8 (7) Ao <a name="01904900800002" ll="SDBH:אָח:000007" tl="HOTTP2244">irmão</a>, verdadeiramente, <a name="01904900800010" tl="HOTTP2244">ninguém</a> o <a name="01904900800006" ll="SDBH:פדה:000000" tl="HOTTP2244">pode</a> <a name="01904900800008" ll="SDBH:פדה:000000" tl="HOTTP2244">remir</a>,nem <a name="01904900800014">pagar</a> por ele a <a name="01904900800018" ll="SDBH:אֱלֹהִים:000002">Deus</a> o <a name="01904900800020" ll="SDBH:כֹּפֶר:000001">seu</a> <a name="01904900800020" ll="SDBH:כֹּפֶר:000001">resgate</a>
    
\v 9 (8) (Pois a <a name="01904900900006" ll="SDBH:פִּדְיֹון:000000">redenção</a> <a name="01904900900008" ll="SDBH:נֶפֶשׁ:000001">da</a> <a name="01904900900008" ll="SDBH:נֶפֶשׁ:000001">alma</a> <a name="01904900900008" ll="SDBH:נֶפֶשׁ:000001">deles</a> <a name="01904900900004" ll="SDBH:יקר:000000">é</a> <a name="01904900900004" ll="SDBH:יקר:000000">caríssima</a>,e <a name="01904900900012" ll="SDBH:חדל:000000">cessará</a> a tentativa para <a name="01904900900016" ll="SDBH:עֹולָם:000000">sempre</a>.),
    
\v 10 (9) para que continue a <a name="01904901000004" ll="SDBH:חיה:000000">viver</a> <a name="01904901000010" ll="SDBH:נֵצַח:000000">perpetuamente</a>e não <a name="01904901000014" ll="SDBH:ראה:000008">veja</a> a <a name="01904901000018" ll="SDBH:שַׁחַת:000001">cova</a>;
    
\v 11 (10) porquanto <a name="01904901100004" ll="SDBH:ראה:000000" tl="HOTTP2245">vê</a>-<a name="01904901100004" ll="SDBH:ראה:000000" tl="HOTTP2245">se</a> <a name="01904901100008" ll="SDBH:מות:000000">morrerem</a> os <a name="01904901100006" ll="SDBH:חָכָם:000000">sábios</a>e <a name="01904901100018" ll="SDBH:אבד:000004">perecerem</a> <a name="01904901100010">tanto</a> o <a name="01904901100012" ll="SDBH:כְּסִיל:000000">estulto</a> como o <a name="01904901100016" ll="SDBH:בַּעַר:000000">inepto</a>,os quais <a name="01904901100022" ll="SDBH:עזב:000000">deixam</a> a <a name="01904901100026" ll="SDBH:אַחֵר:000000">outros</a> as <a name="01904901100028" ll="SDBH:חַיִל:000001">suas</a> <a name="01904901100028" ll="SDBH:חַיִל:000001">riquezas</a>.
    
\v 12 (11) O <a name="01904901200002" ll="SDBH:קֶרֶב:000000" tl="HOTTP2246">seu</a> pensamento <a name="01904901200002" ll="SDBH:קֶרֶב:000000" tl="HOTTP2246">íntimo</a> é que as <a name="01904901200004" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917">suas</a> <a name="01904901200004" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917">casas</a> serão <a name="01904901200008" ll="SDBH:עֹולָם:000000">perpétuas</a>e, as <a name="01904901200010" ll="SDBH:מִשְׁכָּן:000000">suas</a> <a name="01904901200010" ll="SDBH:מִשְׁכָּן:000000">moradas</a>, para <a name="01904901200014;01904901200016;01904901200018" ll="SDBH:דֹּור:001000">todas</a> <a name="01904901200014;01904901200016;01904901200018" ll="SDBH:דֹּור:001000">as</a> <a name="01904901200014;01904901200016;01904901200018" ll="SDBH:דֹּור:001000">gerações</a>;chegam a <a name="01904901200020" ll="SDBH:קרא:000007">dar</a> <a name="01904901200024" ll="SDBH:שֵׁם:000000">seu</a> <a name="01904901200024" ll="SDBH:שֵׁם:000000">próprio</a> <a name="01904901200024" ll="SDBH:שֵׁם:000000">nome</a> às suas <a name="01904901200028" ll="SDBH:אֲדָמָה:000000" il="Soil" tl="HOTTP2247">terras</a>.
    
\v 13 (12) Todavia, o <a name="01904901300004" ll="SDBH:אָדָם:000000">homem</a> não <a name="01904901300012" ll="SDBH:לין:000001" tl="HOTTP2249">permanece</a> em sua <a name="01904901300008" ll="SDBH:יְקָר:000000" tl="HOTTP2248">ostentação</a>;<a name="01904901300014" ll="SDBH:משׁל:000000">é</a>, antes, <a name="01904901300014" ll="SDBH:משׁל:000000">como</a> os <a name="01904901300020" ll="SDBH:בְּהֵמָה:000000">animais</a>, que <a name="01904901300022" ll="SDBH:דמה:000001">perecem</a>.
    
\v 14 (13) Tal <a name="01904901400004" ll="SDBH:דֶּרֶךְ:000007">proceder</a> é <a name="01904901400006" ll="SDBH:כֶּסֶל:002000">estultícia</a> deles;assim mesmo os <a name="01904901400012" tl="HOTTP2250">seus</a> <a name="01904901400012" tl="HOTTP2250">seguidores</a> <a name="01904901400018" ll="SDBH:רצה:000000">aplaudem</a> o que <a name="01904901400016" ll="SDBH:פֶּה:000001">eles</a> <a name="01904901400016" ll="SDBH:פֶּה:000001">dizem</a>.
    
\v 15 (14) Como <a name="01904901500006" ll="SDBH:צֹאן:000000" il="LB00012;LB00015;LB00017;NBGbs20120208-08;NBGbs20120710-139;NBGpb20140401b;Realia:1.2.1;tb111706002">ovelhas</a> <a name="01904901500012" ll="SDBH:שׁית:000001">são</a> <a name="01904901500012" ll="SDBH:שׁית:000001">postos</a> na <a name="01904901500010" ll="SDBH:שְׁאֹול:000000">sepultura</a>;a <a name="01904901500014" ll="SDBH:מָוֶת:000002">morte</a> <a name="01904901500016" ll="SDBH:רעה:000003" il="BK00005;BK00010;HK00009;LB00001;LB00013;NBGpb20140401b;Realia:1.2.1;Shepherd">é</a> o <a name="01904901500016" ll="SDBH:רעה:000003" il="BK00005;BK00010;HK00009;LB00001;LB00013;NBGpb20140401b;Realia:1.2.1;Shepherd">seu</a> <a name="01904901500016" ll="SDBH:רעה:000003" il="BK00005;BK00010;HK00009;LB00001;LB00013;NBGpb20140401b;Realia:1.2.1;Shepherd">pastor</a>;eles <a name="01904901500020" ll="SDBH:רדה:000000" tl="HOTTP2251">descem</a> <a name="01904901500024" ll="SDBH:יָשָׁר:000004" tl="HOTTP2251">diretamente</a> para a <a name="01904901500030" ll="SDBH:בֹּקֶר:000000" tl="HOTTP2251">cova</a>,onde a <a name="01904901500038" ll="SDBH:צוּר:001000">sua</a> <a name="01904901500038" ll="SDBH:צוּר:001000">formosura</a> se <a name="01904901500042" ll="SDBH:בלה:000002">consome</a>;a <a name="01904901500044" ll="SDBH:שְׁאֹול:000000">sepultura</a> é o lugar em que <a name="01904901500048" ll="SDBH:זְבֻל:000000" tl="HOTTP2252">habitam</a>.
    
\v 16 (15) Mas <a name="01904901600004" ll="SDBH:אֱלֹהִים:000002">Deus</a> <a name="01904901600006" ll="SDBH:פדה:000002">remirá</a> a <a name="01904901600008" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01904901600008" ll="SDBH:נֶפֶשׁ:000001">alma</a> do <a name="01904901600012">poder</a> <a name="01904901600014" ll="SDBH:שְׁאֹול:000000">da</a> <a name="01904901600014" ll="SDBH:שְׁאֹול:000000">morte</a>,pois <a name="01904901600018" ll="SDBH:לקח:000000">ele</a> <a name="01904901600018" ll="SDBH:לקח:000000">me</a> <a name="01904901600018" ll="SDBH:לקח:000000">tomará</a> para si.
    
\v 17 (16) Não <a name="01904901700004" ll="SDBH:ירא:000000">temas</a>, quando <a name="01904901700010">alguém</a> se <a name="01904901700008" ll="SDBH:עשׁר:000000">enriquecer</a>,quando <a name="01904901700014">avultar</a> a <a name="01904901700016" ll="SDBH:כָּבֹוד:000001">glória</a> <a name="01904901700018" ll="SDBH:בַּיִת:000003">de</a> <a name="01904901700018" ll="SDBH:בַּיִת:000003">sua</a> <a name="01904901700018" ll="SDBH:בַּיִת:000003">casa</a>;
    
\v 18 (17) pois, em <a name="01904901800008" ll="SDBH:מָוֶת:000000">morrendo</a>, nada <a name="01904901800010" ll="SDBH:לקח:000000">levará</a> consigo,a <a name="01904901800022" ll="SDBH:כָּבֹוד:000001">sua</a> <a name="01904901800022" ll="SDBH:כָּבֹוד:000001">glória</a> não <a name="01904901800018;01904901800020" ll="SDBH:אַחַר:001000;SDBH:ירד:000004">o</a> <a name="01904901800018;01904901800020" ll="SDBH:אַחַר:001000;SDBH:ירד:000004">acompanhará</a>.
    
\v 19 (18) Ainda que durante a <a name="01904901900008" ll="SDBH:חַיִּים:000000">vida</a> <a name="01904901900004" ll="SDBH:נֶפֶשׁ:000001">ele</a> se tenha <a name="01904901900010" ll="SDBH:ברך:001001">lisonjeado</a>,e ainda que <a name="01904901900014" ll="SDBH:ידה:000002">o</a> <a name="01904901900014" ll="SDBH:ידה:000002">louvem</a> quando <a name="01904901900018" ll="SDBH:יטב:000011">faz</a> <a name="01904901900018" ll="SDBH:יטב:000011">o</a> <a name="01904901900018" ll="SDBH:יטב:000011">bem</a> a si mesmo,
    
\v 20 (19) <a name="01904902000002">irá</a> <a name="01904902000002">ter</a> com a <a name="01904902000006" ll="SDBH:דֹּור:001001">geração</a> <a name="01904902000008" ll="SDBH:אָב:000001">de</a> <a name="01904902000008" ll="SDBH:אָב:000001">seus</a> <a name="01904902000008" ll="SDBH:אָב:000001">pais</a>,os quais <a name="01904902000012;01904902000014" ll="SDBH:לֹא:000000;SDBH:נֵצַח:000000">já</a> <a name="01904902000012;01904902000014" ll="SDBH:לֹא:000000;SDBH:נֵצַח:000000">não</a> <a name="01904902000016" ll="SDBH:ראה:000004">verão</a> <a name="01904902000018" ll="SDBH:אֹור:000000">a</a> <a name="01904902000018" ll="SDBH:אֹור:000000">luz</a>.
    
\v 21 (20) O <a name="01904902100002" ll="SDBH:אָדָם:000000">homem</a>, revestido de <a name="01904902100006" ll="SDBH:יְקָר:000000" tl="HOTTP2253">honrarias</a>, mas sem <a name="01904902100012" ll="SDBH:בין:000001" tl="HOTTP2254">entendimento</a>,é, antes, <a name="01904902100014" ll="SDBH:משׁל:000000">como</a> os <a name="01904902100020" ll="SDBH:בְּהֵמָה:000000">animais</a>, que <a name="01904902100022" ll="SDBH:דמה:000001">perecem</a>.
\c 50
\v 1 <a name="01905000100014" ll="SDBH:דבר:001000">Fala</a> o <a name="01905000100008" ll="SDBH:אֵל:000001">Poderoso</a>, o <a name="01905000100010;01905000100012" ll="SDBH:אֱלֹהִים:000002;SDBH:יהוה:000000">Senhor</a> <a name="01905000100010;01905000100012" ll="SDBH:אֱלֹהִים:000002;SDBH:יהוה:000000">Deus</a>,e <a name="01905000100018" ll="SDBH:קרא:000003">chama</a> <a name="01905000100020" ll="SDBH:אֶרֶץ:000000">a</a> <a name="01905000100020" ll="SDBH:אֶרֶץ:000000">terra</a> desde o <a name="01905000100024;01905000100026" ll="SDBH:מִזְרָח:000001;SDBH:שֶׁמֶשׁ:000000" il="Sun;Sun_pillar;Sunrise;Sunset;Sunset_on_mountain">Levante</a> até ao <a name="01905000100030" ll="SDBH:מָבֹוא:000002">Poente</a>.
    
\v 2 Desde <a name="01905000200004" ll="SDBH:צִיֹּון:000000" il="tb060716478">Sião</a>, <a name="01905000200006" ll="SDBH:מִכְלָל:000000">excelência</a> <a name="01905000200008" ll="SDBH:יֳפִי:000000">de</a> <a name="01905000200008" ll="SDBH:יֳפִי:000000">formosura</a>,<a name="01905000200012" ll="SDBH:יפע:000000;SDBH:יפע:000002">resplandece</a> <a name="01905000200010" ll="SDBH:אֱלֹהִים:000002">Deus</a>.
    
\v 3 <a name="01905000300002">Vem</a> o <a name="01905000300004" ll="SDBH:אֱלֹהִים:000001">nosso</a> <a name="01905000300004" ll="SDBH:אֱלֹהִים:000001">Deus</a> e não <a name="01905000300010" ll="SDBH:חרשׁ:001001">guarda</a> <a name="01905000300010" ll="SDBH:חרשׁ:001001">silêncio</a>;<a name="01905000300016">perante</a> <a name="01905000300016">ele</a> arde um <a name="01905000300012" ll="SDBH:אֵשׁ:000000" il="Fire02">fogo</a> <a name="01905000300018" ll="SDBH:אכל:000002">devorador</a>,<a name="01905000300022" ll="SDBH:סָבִיב:000000">ao</a> <a name="01905000300022" ll="SDBH:סָבִיב:000000">seu</a> <a name="01905000300022" ll="SDBH:סָבִיב:000000">redor</a> <a name="01905000300024" ll="SDBH:שׂער:001000">esbraveja</a> <a name="01905000300026" ll="SDBH:מְאֹד:000001">grande</a> <a name="01905000300024" ll="SDBH:שׂער:001000">tormenta</a>.
    
\v 4 <a name="01905000400002" ll="SDBH:קרא:000000">Intima</a> os <a name="01905000400008" ll="SDBH:שָׁמַיִם:000001">céus</a> lá em <a name="01905000400012">cima</a>e a <a name="01905000400020" ll="SDBH:אֶרֶץ:000000">terra</a>, para <a name="01905000400024" ll="SDBH:דין:000000">julgar</a> o <a name="01905000400026" ll="SDBH:עַם:000000">seu</a> <a name="01905000400026" ll="SDBH:עַם:000000">povo</a>.
    
\v 5 <a name="01905000500002" ll="SDBH:אסף:000001">Congregai</a> os <a name="01905000500006" ll="SDBH:חָסִיד:000000">meus</a> <a name="01905000500006" ll="SDBH:חָסִיד:000000">santos</a>,os que <a name="01905000500010" ll="SDBH:בְּרִית:000000">comigo</a> <a name="01905000500008" ll="SDBH:כרת:000001">fizeram</a> <a name="01905000500010" ll="SDBH:בְּרִית:000000">aliança</a> por meio de <a name="01905000500014" ll="SDBH:זֶבַח:000000">sacrifícios</a>.
    
\v 6 Os <a name="01905000600006" ll="SDBH:שָׁמַיִם:000000">céus</a> <a name="01905000600004" ll="SDBH:נגד:000000">anunciam</a> a <a name="01905000600008" ll="SDBH:צֶדֶק:000001">sua</a> <a name="01905000600008" ll="SDBH:צֶדֶק:000001">justiça</a>,porque é o próprio <a name="01905000600012" ll="SDBH:אֱלֹהִים:000002">Deus</a> que <a name="01905000600014" ll="SDBH:שׁפט:000006" il="Tel-dan-israelite-gate-im-tor">julga</a>.
    
