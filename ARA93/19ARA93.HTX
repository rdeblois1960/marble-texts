\v 21 (20) No <a name="01903102100006" ll="SDBH:סֵתֶר:000000">recôndito</a> <a name="01903102100008" ll="SDBH:פָּנֶה:000000">da</a> <a name="01903102100008" ll="SDBH:פָּנֶה:000000">tua</a> <a name="01903102100008" ll="SDBH:פָּנֶה:000000">presença</a>, <a name="01903102100002" ll="SDBH:סתר:000002">tu</a> <a name="01903102100002" ll="SDBH:סתר:000002">os</a> <a name="01903102100002" ll="SDBH:סתר:000002">esconderás</a>das <a name="01903102100012" ll="SDBH:רֹכֶס:000000">tramas</a> dos <a name="01903102100014">homens</a>,num <a name="01903102100020" ll="SDBH:סֻכָּה:000000">esconderijo</a> <a name="01903102100016" ll="SDBH:צפן:000001">os</a> <a name="01903102100016" ll="SDBH:צפן:000001">ocultarás</a>da <a name="01903102100024" ll="SDBH:רִיב:000000">contenda</a> de <a name="01903102100026" ll="SDBH:לָשֹׁון:000000">línguas</a>.
    
\v 22 (21) <a name="01903102200002" ll="SDBH:ברך:001001">Bendito</a> seja o <a name="01903102200004" ll="SDBH:יהוה:000000">Senhor</a>,que <a name="01903102200008" ll="SDBH:פלא:000002">engrandeceu</a> a <a name="01903102200010" ll="SDBH:חֶסֶד:000000">sua</a> <a name="01903102200010" ll="SDBH:חֶסֶד:000000">misericórdia</a>para comigo, numa <a name="01903102200016" ll="SDBH:עִיר:000000" tl="HOTTP2183">cidade</a> <a name="01903102200018" ll="SDBH:מָצֹור:000000;SDBH:מָצֹור:001000;SDBH:מָצֹור:001002" il="Assyrian_siege;SiegeTower-02-031;Siegeworks-02-029b;adr08072471511" tl="HOTTP2183">sitiada</a>!
    
\v 23 (22) Eu <a name="01903102300006" ll="SDBH:אמר:000000">disse</a> na <a name="01903102300010" ll="SDBH:חפז:000000">minha</a> <a name="01903102300010" ll="SDBH:חפז:000000">pressa</a>:<a name="01903102300012" ll="SDBH:גרז:000000" tl="HOTTP2184">estou</a> <a name="01903102300012" ll="SDBH:גרז:000000" tl="HOTTP2184">excluído</a> da <a name="01903102300016;01903102300018" ll="SDBH:נֶגֶד:000000;SDBH:עַיִן:000000">tua</a> <a name="01903102300016;01903102300018" ll="SDBH:נֶגֶד:000000;SDBH:עַיִן:000000">presença</a>.<a name="01903102300020" ll="SDBH:אָכֵן:000001">Não</a> <a name="01903102300020" ll="SDBH:אָכֵן:000001">obstante</a>, <a name="01903102300022">ouviste</a> a <a name="01903102300026" ll="SDBH:תַּחֲנוּן:000000">minha</a> <a name="01903102300026" ll="SDBH:תַּחֲנוּן:000000">súplice</a> <a name="01903102300024">voz</a>,quando <a name="01903102300030" ll="SDBH:שׁוע:000000">clamei</a> <a name="01903102300030" ll="SDBH:שׁוע:000000">por</a> teu <a name="01903102300030" ll="SDBH:שׁוע:000000">socorro</a>.
    
\v 24 (23) <a name="01903102400002" ll="SDBH:אהב:000000">Amai</a> o <a name="01903102400006" ll="SDBH:יהוה:000000">Senhor</a>, vós todos os <a name="01903102400010" ll="SDBH:חָסִיד:000000">seus</a> <a name="01903102400010" ll="SDBH:חָסִיד:000000">santos</a>.O <a name="01903102400016" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903102400014" ll="SDBH:נצר:000007">preserva</a> os <a name="01903102400012" ll="SDBH:אֵמוּן:001000">fiéis</a>,mas <a name="01903102400020" ll="SDBH:שׁלם:000007">retribui</a> com <a name="01903102400024" ll="SDBH:יֶתֶר:000006">largueza</a> ao <a name="01903102400026;01903102400028" ll="SDBH:גַּאֲוָה:000001">soberbo</a>.
    
\v 25 (24) <a name="01903102500002" ll="SDBH:חזק:000005">Sede</a> <a name="01903102500002" ll="SDBH:חזק:000005">fortes</a>, e <a name="01903102500006" ll="SDBH:אמץ:000005">revigore</a>-<a name="01903102500006" ll="SDBH:אמץ:000005">se</a> o <a name="01903102500008" ll="SDBH:לֵבָב:000001">vosso</a> <a name="01903102500008" ll="SDBH:לֵבָב:000001">coração</a>,vós todos que <a name="01903102500014" ll="SDBH:יחל:000000">esperais</a> no <a name="01903102500018" ll="SDBH:יהוה:000000">Senhor</a>.
\c 32
\v 1 <a name="01903200100008" ll="SDBH:אַשְׁרֵי:000000">Bem</a>-<a name="01903200100008" ll="SDBH:אַשְׁרֵי:000000">aventurado</a> aquele cuja <a name="01903200100012" ll="SDBH:פֶּשַׁע:000000">iniquidade</a> <a name="01903200100010" ll="SDBH:נשׂא:000009">é</a> <a name="01903200100010" ll="SDBH:נשׂא:000009">perdoada</a>,cujo <a name="01903200100016" ll="SDBH:חֲטָאָה:000000">pecado</a> <a name="01903200100014" ll="SDBH:כסה:000009">é</a> <a name="01903200100014" ll="SDBH:כסה:000009">coberto</a>.
    
\v 2 <a name="01903200200002" ll="SDBH:אַשְׁרֵי:000000">Bem</a>-<a name="01903200200002" ll="SDBH:אַשְׁרֵי:000000">aventurado</a> o <a name="01903200200004" ll="SDBH:אָדָם:000000">homem</a> a quem o <a name="01903200200010" ll="SDBH:יהוה:000000">Senhor</a> não <a name="01903200200008" ll="SDBH:חשׁב:000008">atribui</a> <a name="01903200200014" ll="SDBH:עָוֹן:000000">iniquidade</a>e em <a name="01903200200022" ll="SDBH:רוּחַ:000002">cujo</a> <a name="01903200200022" ll="SDBH:רוּחַ:000002">espírito</a> não há <a name="01903200200024" ll="SDBH:רְמִיָּה:001000">dolo</a>.
    
\v 3 Enquanto <a name="01903200300004" ll="SDBH:חרשׁ:001001">calei</a> os meus pecados, <a name="01903200300006" ll="SDBH:בלה:000000">envelheceram</a> os <a name="01903200300008" ll="SDBH:עֶצֶם:000000">meus</a> <a name="01903200300008" ll="SDBH:עֶצֶם:000000">ossos</a>pelos <a name="01903200300012" ll="SDBH:שְׁאָגָה:000000">meus</a> constantes <a name="01903200300012" ll="SDBH:שְׁאָגָה:000000">gemidos</a> todo o <a name="01903200300018" ll="SDBH:יֹום:000007">dia</a>.
    
\v 4 Porque a <a name="01903200400014">tua</a> <a name="01903200400014">mão</a> <a name="01903200400010" ll="SDBH:כבד:000001">pesava</a> <a name="01903200400004" ll="SDBH:יֹומָם:000000">dia</a> e <a name="01903200400008" ll="SDBH:לַיְלָה:000000">noite</a> sobre mim,e o <a name="01903200400018" ll="SDBH:לָשָׁד:001000" tl="HOTTP2186">meu</a> <a name="01903200400018" ll="SDBH:לָשָׁד:001000" tl="HOTTP2186">vigor</a> se <a name="01903200400016" ll="SDBH:הפך:000005" tl="HOTTP2185">tornou</a> em <a name="01903200400022" ll="SDBH:חֲרָבֹון:000000" tl="HOTTP2187">sequidão</a> de <a name="01903200400024" ll="SDBH:קַיִץ:000000">estio</a>.
    
\v 5 <a name="01903200500004">Confessei</a>-<a name="01903200500004">te</a> o <a name="01903200500002" ll="SDBH:חַטָּאת:000000">meu</a> <a name="01903200500002" ll="SDBH:חַטָּאת:000000">pecado</a>e a <a name="01903200500008" ll="SDBH:עָוֹן:000000">minha</a> <a name="01903200500008" ll="SDBH:עָוֹן:000000">iniquidade</a> não mais <a name="01903200500012" ll="SDBH:כסה:000000;SDBH:כסה:000001">ocultei</a>.<a name="01903200500014" ll="SDBH:אמר:000000">Disse</a>: <a name="01903200500016" ll="SDBH:ידה:000001">confessarei</a> ao <a name="01903200500024" ll="SDBH:יהוה:000000">Senhor</a> as <a name="01903200500020" ll="SDBH:פֶּשַׁע:000000">minhas</a> <a name="01903200500020" ll="SDBH:פֶּשַׁע:000000">transgressões</a>;e tu <a name="01903200500030" ll="SDBH:נשׂא:000009">perdoaste</a> a <a name="01903200500032" ll="SDBH:עָוֹן:000000" tl="HOTTP2188">iniquidade</a> <a name="01903200500034" ll="SDBH:חַטָּאת:000000" tl="HOTTP2188">do</a> <a name="01903200500034" ll="SDBH:חַטָּאת:000000" tl="HOTTP2188">meu</a> <a name="01903200500034" ll="SDBH:חַטָּאת:000000" tl="HOTTP2188">pecado</a>.
    
\v 6 Sendo assim, todo homem <a name="01903200600010" ll="SDBH:חָסִיד:000000">piedoso</a> te <a name="01903200600006" ll="SDBH:פלל:000002" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20140526">fará</a> <a name="01903200600006" ll="SDBH:פלל:000002" il="LB00269;LB00270;LB00271;NBGbs20110228-1_61-2;NBGmk20140526">súplicas</a>em <a name="01903200600016" ll="SDBH:עֵת:000000" tl="HOTTP2189">tempo</a> de <a name="01903200600018" ll="SDBH:מצא:000002" tl="HOTTP2189">poder</a> <a name="01903200600018" ll="SDBH:מצא:000002" tl="HOTTP2189">encontrar</a>-te.Com efeito, quando <a name="01903200600024" ll="SDBH:שֶׁטֶף:000000" il="Flooded_land;Flooding">transbordarem</a> <a name="01903200600028" ll="SDBH:רַב:000001">muitas</a> <a name="01903200600026" ll="SDBH:מַיִם:000000" il="Water_drop_001">águas</a>,não o <a name="01903200600034" ll="SDBH:נגע:000002">atingirão</a>.
    
\v 7 Tu és o meu <a name="01903200700004" ll="SDBH:סֵתֶר:000000">esconderijo</a>;<a name="01903200700012" ll="SDBH:נצר:000007">tu</a> <a name="01903200700012" ll="SDBH:נצר:000007">me</a> <a name="01903200700012" ll="SDBH:נצר:000007">preservas</a> da <a name="01903200700010" ll="SDBH:צַר:000002">tribulação</a>e <a name="01903200700018">me</a> <a name="01903200700018">cercas</a> de alegres <a name="01903200700014" ll="SDBH:רֹן:000000" tl="HOTTP2191">cantos</a> de <a name="01903200700016" ll="SDBH:פלט:000001" tl="HOTTP2191">livramento</a>.
    
\v 8 <a name="01903200800002" ll="SDBH:שׂכל:001001">Instruir</a>-<a name="01903200800002" ll="SDBH:שׂכל:001001">te</a>-<a name="01903200800002" ll="SDBH:שׂכל:001001">ei</a> e te <a name="01903200800006" ll="SDBH:ירה:002000">ensinarei</a> o <a name="01903200800010" ll="SDBH:דֶּרֶךְ:000007">caminho</a> <a name="01903200800012" ll="SDBH:זוּ:000000">que</a> deves <a name="01903200800014" ll="SDBH:הלך:000004">seguir</a>;e, sob as <a name="01903200800020" ll="SDBH:עַיִן:000000">minhas</a> <a name="01903200800020" ll="SDBH:עַיִן:000000">vistas</a>, te <a name="01903200800016" ll="SDBH:יעץ:000003">darei</a> <a name="01903200800016" ll="SDBH:יעץ:000003">conselho</a>.
    
\v 9 Não sejais como o <a name="01903200900008" ll="SDBH:סוּס:000000" il="Fauna:2.18;LB00032">cavalo</a> ou a <a name="01903200900012" ll="SDBH:פֶּרֶד:000000" il="Fauna:2.28">mula</a>, sem <a name="01903200900016" ll="SDBH:בין:000001">entendimento</a>,os quais com <a name="01903200900020" ll="SDBH:מֶתֶג:000000" il="HK00029;LB00035;Realia:1.2.6a;Realia:1.2.6b">freios</a> e <a name="01903200900024" ll="SDBH:רֶסֶן:000000" il="HK00029;LB00035;Realia:1.2.6a;Realia:1.2.6b">cabrestos</a> são <a name="01903200900030" ll="SDBH:בלם:000000">dominados</a>;<a name="01903200900032" ll="SDBH:בַּל:000000">de</a> <a name="01903200900032" ll="SDBH:בַּל:000000">outra</a> <a name="01903200900032" ll="SDBH:בַּל:000000">sorte</a> <a name="01903200900032" ll="SDBH:בַּל:000000">não</a> te <a name="01903200900034" ll="SDBH:קרב:000000">obedecem</a>.
    
\v 10 <a name="01903201000002" ll="SDBH:רַב:000001">Muito</a> <a name="01903201000004" ll="SDBH:מַכְאֹוב:000001">sofrimento</a> terá de curtir o <a name="01903201000010" ll="SDBH:רָשָׁע:000000">ímpio</a>,mas o que <a name="01903201000016" ll="SDBH:בטח:000001">confia</a> no <a name="01903201000020" ll="SDBH:יהוה:000000">Senhor</a>, a <a name="01903201000022" ll="SDBH:חֶסֶד:000000">misericórdia</a> <a name="01903201000024">o</a> <a name="01903201000024">assistirá</a>.
    
\v 11 <a name="01903201100002" ll="SDBH:שׂמח:000000">Alegrai</a>-vos no <a name="01903201100006" ll="SDBH:יהוה:000000">Senhor</a> e <a name="01903201100010" ll="SDBH:גיל:000000">regozijai</a>-vos, ó <a name="01903201100012" ll="SDBH:צַדִּיק:000000">justos</a>;<a name="01903201100016" ll="SDBH:רנן:000000">exultai</a>, vós todos que sois <a name="01903201100020" ll="SDBH:יָשָׁר:000004">retos</a> <a name="01903201100022" ll="SDBH:לֵב:000001">de</a> <a name="01903201100022" ll="SDBH:לֵב:000001">coração</a>.
\c 33
\v 1 <a name="01903300100002" ll="SDBH:רנן:000000">Exultai</a>, ó <a name="01903300100004" ll="SDBH:צַדִּיק:000000">justos</a>, no <a name="01903300100008" ll="SDBH:יהוה:000000">Senhor</a>!Aos <a name="01903300100014" ll="SDBH:יָשָׁר:000004">retos</a> <a name="01903300100016" ll="SDBH:נָאוֶה:000001">fica</a> <a name="01903300100016" ll="SDBH:נָאוֶה:000001">bem</a> <a name="01903300100018" ll="SDBH:תְּהִלָּה:000000">louvá</a>-lo.
    
\v 2 <a name="01903300200002" ll="SDBH:ידה:000002">Celebrai</a> o <a name="01903300200006" ll="SDBH:יהוה:000000">Senhor</a> com <a name="01903300200010" ll="SDBH:כִּנֹּור:000000" il="BK00175;BK00176;Exile_Lachish;HK00188;NBGmk20131114;adr1306213109;tb072705188">harpa</a>,<a name="01903300200018" ll="SDBH:זמר:000000" il="HK00188">louvai</a>-o com cânticos no <a name="01903300200014" ll="SDBH:נֵבֶל:001000" il="BK00175;BK00176;Exile_Lachish;NBGmk20131114;adr1306213109;tb072705188">saltério</a> de <a name="01903300200016" ll="SDBH:עָשֹׂור:000002">dez</a> <a name="01903300200016" ll="SDBH:עָשֹׂור:000002">cordas</a>.
    
\v 3 <a name="01903300300002" ll="SDBH:שׁיר:000000" il="HK00188">Entoai</a>-lhe <a name="01903300300008" ll="SDBH:חָדָשׁ:000001">novo</a> <a name="01903300300006" ll="SDBH:שִׁיר:000000" il="HK00188">cântico</a>,<a name="01903300300012" ll="SDBH:נגן:000000" il="BK00176;NBGmk20131114">tangei</a> <a name="01903300300010" ll="SDBH:יטב:000011">com</a> <a name="01903300300010" ll="SDBH:יטב:000011">arte</a> e com <a name="01903300300016" ll="SDBH:תְּרוּעָה:000000" il="HK00188">júbilo</a>.
    
\v 4 Porque a <a name="01903300400006" ll="SDBH:דָּבָר:000000">palavra</a> <a name="01903300400008" ll="SDBH:יהוה:000000">do</a> <a name="01903300400008" ll="SDBH:יהוה:000000">Senhor</a> é <a name="01903300400004" ll="SDBH:יָשָׁר:000003">reta</a>,e todo o <a name="01903300400014" ll="SDBH:מַעֲשֶׂה:000001">seu</a> <a name="01903300400014" ll="SDBH:מַעֲשֶׂה:000001">proceder</a> é <a name="01903300400018" ll="SDBH:אֱמוּנָה:000002">fiel</a>.
    
\v 5 Ele <a name="01903300500002" ll="SDBH:אהב:000000">ama</a> a <a name="01903300500004" ll="SDBH:צְדָקָה:000001">justiça</a> e o <a name="01903300500008" ll="SDBH:מִשְׁפָּט:000007">direito</a>;a <a name="01903300500018" ll="SDBH:אֶרֶץ:000000">terra</a> <a name="01903300500014" ll="SDBH:מלא:000010">está</a> <a name="01903300500014" ll="SDBH:מלא:000010">cheia</a> da <a name="01903300500010" ll="SDBH:חֶסֶד:000000">bondade</a> do <a name="01903300500012" ll="SDBH:יהוה:000000">Senhor</a>.
    
\v 6 Os <a name="01903300600008" ll="SDBH:שָׁמַיִם:000000">céus</a> por sua <a name="01903300600004" ll="SDBH:דָּבָר:000000">palavra</a> se <a name="01903300600010">fizeram</a>,e, pelo <a name="01903300600016" ll="SDBH:רוּחַ:000009">sopro</a> <a name="01903300600018" ll="SDBH:פֶּה:000000">de</a> <a name="01903300600018" ll="SDBH:פֶּה:000000">sua</a> <a name="01903300600018" ll="SDBH:פֶּה:000000">boca</a>, o <a name="01903300600022" ll="SDBH:צָבָא:000004">exército</a> <a name="01903300600022" ll="SDBH:צָבָא:000004">deles</a>.
    
\v 7 Ele <a name="01903300700002" ll="SDBH:כנס:000000">ajunta</a> em <a name="01903300700008" ll="SDBH:נֵד:000000" tl="HOTTP2192">montão</a> as <a name="01903300700010" ll="SDBH:מַיִם:000000" il="Water_drop_001">águas</a> do <a name="01903300700014" ll="SDBH:יָם:000000" il="Rough_seas">mar</a>;e em <a name="01903300700020" ll="SDBH:אֹוצָר:000001">reservatório</a> <a name="01903300700016">encerra</a> as <a name="01903300700022" ll="SDBH:תְּהֹום:000000">grandes</a> <a name="01903300700022" ll="SDBH:תְּהֹום:000000">vagas</a>.
    
\v 8 <a name="01903300800002" ll="SDBH:ירא:000000">Tema</a> ao <a name="01903300800006" ll="SDBH:יהוה:000000">Senhor</a> toda a <a name="01903300800012" ll="SDBH:אֶרֶץ:000000">terra</a>,<a name="01903300800016" ll="SDBH:גור:002000">temam</a>-no todos os <a name="01903300800020">habitantes</a> do <a name="01903300800022" ll="SDBH:תֵּבֵל:000000">mundo</a>.
    
\v 9 Pois ele <a name="01903300900006" ll="SDBH:אמר:000000">falou</a>, e tudo se fez;ele <a name="01903300900014" ll="SDBH:צוה:000000">ordenou</a>, e tudo <a name="01903300900018">passou</a> <a name="01903300900018">a</a> <a name="01903300900018">existir</a>.
    
\v 10 O <a name="01903301000002" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903301000004" ll="SDBH:פור:000000">frustra</a> os <a name="01903301000006" ll="SDBH:עֵצָה:000002">desígnios</a> <a name="01903301000008" ll="SDBH:גֹּוי:000000">das</a> <a name="01903301000008" ll="SDBH:גֹּוי:000000">nações</a>e <a name="01903301000010" ll="SDBH:נוא:000000">anula</a> os <a name="01903301000012" ll="SDBH:מַחֲשָׁבָה:000000">intentos</a> dos <a name="01903301000014" ll="SDBH:עַם:000000">povos</a>.
    
\v 11 O <a name="01903301100002" ll="SDBH:עֵצָה:000002">conselho</a> do <a name="01903301100004" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903301100010">dura</a> para <a name="01903301100008" ll="SDBH:עֹולָם:000000">sempre</a>;os <a name="01903301100012" ll="SDBH:מַחֲשָׁבָה:000000">desígnios</a> <a name="01903301100014" ll="SDBH:לֵב:000001">do</a> <a name="01903301100014" ll="SDBH:לֵב:000001">seu</a> <a name="01903301100014" ll="SDBH:לֵב:000001">coração</a>, por <a name="01903301100018;01903301100020;01903301100022" ll="SDBH:דֹּור:001000">todas</a> <a name="01903301100018;01903301100020;01903301100022" ll="SDBH:דֹּור:001000">as</a> <a name="01903301100018;01903301100020;01903301100022" ll="SDBH:דֹּור:001000">gerações</a>.
    
\v 12 <a name="01903301200002" ll="SDBH:אַשְׁרֵי:000000">Feliz</a> a <a name="01903301200006" ll="SDBH:גֹּוי:000000" il="jlm_soltemp_d01;jlm_soltemp_m06;jlm_soltemp_pillars_m01">nação</a> <a name="01903301200012" ll="SDBH:אֱלֹהִים:000001">cujo</a> <a name="01903301200012" ll="SDBH:אֱלֹהִים:000001">Deus</a> é o <a name="01903301200010" ll="SDBH:יהוה:000000">Senhor</a>,e o <a name="01903301200016" ll="SDBH:עַם:000000">povo</a> que <a name="01903301200018" ll="SDBH:בחר:000000">ele</a> <a name="01903301200018" ll="SDBH:בחר:000000">escolheu</a> para sua <a name="01903301200022" ll="SDBH:נַחֲלָה:000000">herança</a>.
    
\v 13 O <a name="01903301300008" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903301300006" ll="SDBH:נבט:000000">olha</a> dos <a name="01903301300004" ll="SDBH:שָׁמַיִם:000000">céus</a>;<a name="01903301300010" ll="SDBH:ראה:000000">vê</a> todos os <a name="01903301300016" ll="SDBH:בֵּן:000003">filhos</a> dos <a name="01903301300020" ll="SDBH:אָדָם:000000">homens</a>;
    
\v 14 do <a name="01903301400004" ll="SDBH:מָכֹון:000000;SDBH:מָכֹון:000001">lugar</a> <a name="01903301400006">de</a> <a name="01903301400006">sua</a> <a name="01903301400006">morada</a>, <a name="01903301400008" ll="SDBH:שׁגח:000000">observa</a>todos os <a name="01903301400014">moradores</a> da <a name="01903301400018" ll="SDBH:אֶרֶץ:000000">terra</a>,
    
\v 15 ele, que <a name="01903301500004" ll="SDBH:יצר:000000">forma</a> o <a name="01903301500008" ll="SDBH:לֵב:000001">coração</a> <a name="01903301500008" ll="SDBH:לֵב:000001">de</a> todos <a name="01903301500008" ll="SDBH:לֵב:000001">eles</a>,que <a name="01903301500012" ll="SDBH:בין:000001">contempla</a> todas as <a name="01903301500018" ll="SDBH:מַעֲשֶׂה:000001">suas</a> <a name="01903301500018" ll="SDBH:מַעֲשֶׂה:000001">obras</a>.
    
\v 16 Não há <a name="01903301600006" ll="SDBH:מֶלֶךְ:000000">rei</a> que <a name="01903301600008" ll="SDBH:ישׁע:000001">se</a> <a name="01903301600008" ll="SDBH:ישׁע:000001">salve</a> com o <a name="01903301600012" ll="SDBH:רֹב:000000">poder</a> <a name="01903301600014" ll="SDBH:חַיִל:000002" il="HK00197;LB00198;NBGmk20131008a;adr1603195037">dos</a> seus <a name="01903301600014" ll="SDBH:חַיִל:000002" il="HK00197;LB00198;NBGmk20131008a;adr1603195037">exércitos</a>;nem por sua <a name="01903301600024" ll="SDBH:רֹב:000001">muita</a> <a name="01903301600026" ll="SDBH:כֹּחַ:000000">força</a> <a name="01903301600020" ll="SDBH:נצל:000005">se</a> <a name="01903301600020" ll="SDBH:נצל:000005">livra</a> o <a name="01903301600016" ll="SDBH:גִּבֹּור:000000">valente</a>.
    
\v 17 O <a name="01903301700006" ll="SDBH:סוּס:000000" il="Fauna:2.18;LB00032">cavalo</a> <a name="01903301700002" ll="SDBH:שֶׁקֶר:000000">não</a> <a name="01903301700002" ll="SDBH:שֶׁקֶר:000000">garante</a> <a name="01903301700010" ll="SDBH:תְּשׁוּעָה:000000">vitória</a>;a despeito de sua <a name="01903301700016" ll="SDBH:רֹב:000001">grande</a> <a name="01903301700018" ll="SDBH:חַיִל:000000">força</a>, a ninguém <a name="01903301700022" ll="SDBH:מלט:000001">pode</a> <a name="01903301700022" ll="SDBH:מלט:000001">livrar</a>.
    
\v 18 Eis que os <a name="01903301800004" ll="SDBH:עַיִן:000000">olhos</a> do <a name="01903301800006" ll="SDBH:יהוה:000000">Senhor</a> estão sobre os <a name="01903301800010" ll="SDBH:יָרֵא:000000">que</a> <a name="01903301800010" ll="SDBH:יָרֵא:000000">o</a> <a name="01903301800010" ll="SDBH:יָרֵא:000000">temem</a>,sobre os que <a name="01903301800016" ll="SDBH:יחל:000000">esperam</a> na <a name="01903301800020" ll="SDBH:חֶסֶד:000000">sua</a> <a name="01903301800020" ll="SDBH:חֶסֶד:000000">misericórdia</a>,
    
\v 19 para <a name="01903301900004" ll="SDBH:נצל:000005">livrar</a>-<a name="01903301900010" ll="SDBH:נֶפֶשׁ:000001">lhes</a> <a name="01903301900010" ll="SDBH:נֶפֶשׁ:000001">a</a> <a name="01903301900010" ll="SDBH:נֶפֶשׁ:000001">alma</a> da <a name="01903301900008" ll="SDBH:מָוֶת:000000">morte</a>,e, no tempo da <a name="01903301900022" ll="SDBH:רָעָב:000000">fome</a>, <a name="01903301900016" ll="SDBH:חיה:000002">conservar</a>-<a name="01903301900016" ll="SDBH:חיה:000002">lhes</a> <a name="01903301900016" ll="SDBH:חיה:000002">a</a> <a name="01903301900016" ll="SDBH:חיה:000002">vida</a>.
    
\v 20 <a name="01903302000002" ll="SDBH:נֶפֶשׁ:000001">Nossa</a> <a name="01903302000002" ll="SDBH:נֶפֶשׁ:000001">alma</a> <a name="01903302000004" ll="SDBH:חכה:000000">espera</a> no <a name="01903302000008" ll="SDBH:יהוה:000000">Senhor</a>,<a name="01903302000010" ll="SDBH:עֵזֶר:000000">nosso</a> <a name="01903302000010" ll="SDBH:עֵזֶר:000000">auxílio</a> e <a name="01903302000014" ll="SDBH:מָגֵן:000000" il="NBGmk20131008">escudo</a>.
    
\v 21 Nele, o <a name="01903302100008" ll="SDBH:לֵב:000001">nosso</a> <a name="01903302100008" ll="SDBH:לֵב:000001">coração</a> <a name="01903302100006" ll="SDBH:שׂמח:000000">se</a> <a name="01903302100006" ll="SDBH:שׂמח:000000">alegra</a>,pois <a name="01903302100018" ll="SDBH:בטח:000001">confiamos</a> no <a name="01903302100016" ll="SDBH:קֹדֶשׁ:000001">seu</a> <a name="01903302100016" ll="SDBH:קֹדֶשׁ:000001">santo</a> <a name="01903302100014" ll="SDBH:שֵׁם:000003">nome</a>.
    
\v 22 Seja sobre nós, <a name="01903302200006" ll="SDBH:יהוה:000000">Senhor</a>, a <a name="01903302200004" ll="SDBH:חֶסֶד:000000">tua</a> <a name="01903302200004" ll="SDBH:חֶסֶד:000000">misericórdia</a>,como de ti <a name="01903302200014" ll="SDBH:יחל:000000">esperamos</a>.
\c 34
\v 2 (1) <a name="01903400200002" ll="SDBH:ברך:001001">Bendirei</a> o <a name="01903400200006" ll="SDBH:יהוה:000000">Senhor</a> em todo o <a name="01903400200012" ll="SDBH:עֵת:000001">tempo</a>,o <a name="01903400200016" ll="SDBH:תְּהִלָּה:000000">seu</a> <a name="01903400200016" ll="SDBH:תְּהִלָּה:000000">louvor</a> estará <a name="01903400200014" ll="SDBH:תָּמִיד:000000">sempre</a> nos <a name="01903400200020" ll="SDBH:פֶּה:000000">meus</a> <a name="01903400200020" ll="SDBH:פֶּה:000000">lábios</a>.
    
\v 3 (2) <a name="01903400300006" ll="SDBH:הלל:001002">Gloriar</a>-<a name="01903400300006" ll="SDBH:הלל:001002">se</a>-<a name="01903400300006" ll="SDBH:הלל:001002">á</a> no <a name="01903400300004" ll="SDBH:יהוה:000000">Senhor</a> a <a name="01903400300008" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01903400300008" ll="SDBH:נֶפֶשׁ:000001">alma</a>;os <a name="01903400300012" ll="SDBH:עָנָו:000000">humildes</a> o <a name="01903400300010">ouvirão</a> e <a name="01903400300016" ll="SDBH:שׂמח:000000">se</a> <a name="01903400300016" ll="SDBH:שׂמח:000000">alegrarão</a>.
    
\v 4 (3) <a name="01903400400002" ll="SDBH:גדל:000011">Engrandecei</a> o <a name="01903400400006" ll="SDBH:יהוה:000000">Senhor</a> comigo,e todos, <a name="01903400400016">à</a> <a name="01903400400016">uma</a>, <a name="01903400400014" ll="SDBH:שֵׁם:000003">lhe</a> <a name="01903400400012" ll="SDBH:רום:000015">exaltemos</a> <a name="01903400400014" ll="SDBH:שֵׁם:000003">o</a> <a name="01903400400014" ll="SDBH:שֵׁם:000003">nome</a>.
    
\v 5 (4) <a name="01903400500002" ll="SDBH:דרשׁ:000003">Busquei</a> o <a name="01903400500006" ll="SDBH:יהוה:000000">Senhor</a>, e <a name="01903400500010" ll="SDBH:ענה:000000">ele</a> <a name="01903400500010" ll="SDBH:ענה:000000">me</a> <a name="01903400500010" ll="SDBH:ענה:000000">acolheu</a>;<a name="01903400500020" ll="SDBH:נצל:000005">livrou</a>-<a name="01903400500020" ll="SDBH:נצל:000005">me</a> de todos os <a name="01903400500018" ll="SDBH:מְגֹורָה:000000">meus</a> <a name="01903400500018" ll="SDBH:מְגֹורָה:000000">temores</a>.
    
\v 6 (5) <a name="01903400600002" ll="SDBH:נבט:000000" tl="HOTTP2193">Contemplai</a>-o e <a name="01903400600008" ll="SDBH:נהר:001000" tl="HOTTP2193">sereis</a> <a name="01903400600008" ll="SDBH:נהר:001000" tl="HOTTP2193">iluminados</a>,e o <a name="01903400600012" ll="SDBH:פָּנֶה:000000" tl="HOTTP2193">vosso</a> <a name="01903400600012" ll="SDBH:פָּנֶה:000000" tl="HOTTP2193">rosto</a> jamais <a name="01903400600016" ll="SDBH:חפר:001000">sofrerá</a> <a name="01903400600016" ll="SDBH:חפר:001000">vexame</a>.
    
\v 7 (6) <a name="01903400700006" ll="SDBH:קרא:000000">Clamou</a> este <a name="01903400700004" ll="SDBH:עָנִי:000000">aflito</a>, e o <a name="01903400700010" ll="SDBH:יהוה:000000">Senhor</a> o <a name="01903400700012">ouviu</a>e <a name="01903400700022" ll="SDBH:ישׁע:000001">o</a> <a name="01903400700022" ll="SDBH:ישׁע:000001">livrou</a> de todas as <a name="01903400700020" ll="SDBH:צָרָה:000000">suas</a> <a name="01903400700020" ll="SDBH:צָרָה:000000">tribulações</a>.
    
\v 8 (7) O <a name="01903400800004" ll="SDBH:מַלְאָךְ:000000">anjo</a> <a name="01903400800006" ll="SDBH:יהוה:000000">do</a> <a name="01903400800006" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903400800002" ll="SDBH:חנה:000001">acampa</a>-<a name="01903400800002" ll="SDBH:חנה:000001">se</a><a name="01903400800008" ll="SDBH:סָבִיב:000000">ao</a> <a name="01903400800008" ll="SDBH:סָבִיב:000000">redor</a> dos que <a name="01903400800012" ll="SDBH:יָרֵא:000000">o</a> <a name="01903400800012" ll="SDBH:יָרֵא:000000">temem</a> e <a name="01903400800016" ll="SDBH:חלץ:000002">os</a> <a name="01903400800016" ll="SDBH:חלץ:000002">livra</a>.
    
\v 9 (8) Oh! <a name="01903400900002" ll="SDBH:טעם:000003">Provai</a> e <a name="01903400900006" ll="SDBH:ראה:000000">vede</a> que o <a name="01903400900012" ll="SDBH:יהוה:000000">Senhor</a> é <a name="01903400900010" ll="SDBH:טוב:000000">bom</a>;<a name="01903400900014" ll="SDBH:אַשְׁרֵי:000000">bem</a>-<a name="01903400900014" ll="SDBH:אַשְׁרֵי:000000">aventurado</a> o <a name="01903400900018" ll="SDBH:גֶּבֶר:001000">homem</a> que nele <a name="01903400900020" ll="SDBH:חסה:000001">se</a> <a name="01903400900020" ll="SDBH:חסה:000001">refugia</a>.
    
\v 10 (9) <a name="01903401000002" ll="SDBH:ירא:000000">Temei</a> o <a name="01903401000006" ll="SDBH:יהוה:000000">Senhor</a>, vós os <a name="01903401000008" ll="SDBH:קָדֹושׁ:000004">seus</a> <a name="01903401000008" ll="SDBH:קָדֹושׁ:000004">santos</a>,pois nada <a name="01903401000014" ll="SDBH:מַחְסֹור:000000">falta</a> aos que <a name="01903401000018" ll="SDBH:יָרֵא:000000">o</a> <a name="01903401000018" ll="SDBH:יָרֵא:000000">temem</a>.
    
\v 11 (10) Os <a name="01903401100002" ll="SDBH:כְּפִיר:000000" il="Fauna:2.24a;Fauna:2.24b;Fauna:2.24c">leõezinhos</a> <a name="01903401100004" ll="SDBH:רושׁ:000000">sofrem</a> <a name="01903401100004" ll="SDBH:רושׁ:000000">necessidade</a> e <a name="01903401100008" ll="SDBH:רעב:000000">passam</a> <a name="01903401100008" ll="SDBH:רעב:000000">fome</a>,porém aos que <a name="01903401100012" ll="SDBH:דרשׁ:000003">buscam</a> o <a name="01903401100014" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903401100022" ll="SDBH:טֹוב:000002">bem</a> nenhum <a name="01903401100018" ll="SDBH:חסר:000001">lhes</a> <a name="01903401100018" ll="SDBH:חסר:000001">faltará</a>.
    
\v 12 (11) <a name="01903401200002" ll="SDBH:הלך:000001">Vinde</a>, <a name="01903401200004" ll="SDBH:בֵּן:000009">filhos</a>, e <a name="01903401200006">escutai</a>-me;<a name="01903401200014" ll="SDBH:למד:000001">eu</a> <a name="01903401200014" ll="SDBH:למד:000001">vos</a> <a name="01903401200014" ll="SDBH:למד:000001">ensinarei</a> o <a name="01903401200010" ll="SDBH:יִרְאָה:000000">temor</a> do <a name="01903401200012" ll="SDBH:יהוה:000000">Senhor</a>.
    
\v 13 (12) Quem é o <a name="01903401300006">homem</a> que <a name="01903401300010" ll="SDBH:חפץ:000002">ama</a> a <a name="01903401300012" ll="SDBH:חַיִּים:000000">vida</a>e <a name="01903401300014" ll="SDBH:אהב:000000">quer</a> <a name="01903401300016" ll="SDBH:יֹום:000004">longevidade</a> para <a name="01903401300020" ll="SDBH:ראה:000009">ver</a> o <a name="01903401300022" ll="SDBH:טֹוב:000002">bem</a>?
    
\v 14 (13) <a name="01903401400002" ll="SDBH:נצר:000004">Refreia</a> a <a name="01903401400004" ll="SDBH:לָשֹׁון:000000">língua</a> do <a name="01903401400008" ll="SDBH:רַע:000005">mal</a>e os <a name="01903401400012" ll="SDBH:שָׂפָה:000001">lábios</a> de <a name="01903401400016" ll="SDBH:דבר:001000">falarem</a> <a name="01903401400018" ll="SDBH:מִרְמָה:000000">dolosamente</a>.
    
\v 15 (14) <a name="01903401500002" ll="SDBH:סור:000002">Aparta</a>-<a name="01903401500002" ll="SDBH:סור:000002">te</a> do <a name="01903401500006" ll="SDBH:רַע:000005">mal</a> e <a name="01903401500010">pratica</a> <a name="01903401500012" ll="SDBH:טֹוב:000002">o</a> <a name="01903401500012" ll="SDBH:טֹוב:000002">que</a> <a name="01903401500012" ll="SDBH:טֹוב:000002">é</a> <a name="01903401500012" ll="SDBH:טֹוב:000002">bom</a>;<a name="01903401500014" ll="SDBH:בקשׁ:000007">procura</a> a <a name="01903401500016" ll="SDBH:שָׁלֹום:000000">paz</a> e <a name="01903401500020" ll="SDBH:רדף:000001">empenha</a>-<a name="01903401500020" ll="SDBH:רדף:000001">te</a> <a name="01903401500020" ll="SDBH:רדף:000001">por</a> <a name="01903401500020" ll="SDBH:רדף:000001">alcançá</a>-<a name="01903401500020" ll="SDBH:רדף:000001">la</a>.
    
\v 16 (15) Os <a name="01903401600002" ll="SDBH:עַיִן:000000">olhos</a> do <a name="01903401600004" ll="SDBH:יהוה:000000">Senhor</a> repousam sobre os <a name="01903401600008" ll="SDBH:צַדִּיק:000000">justos</a>,e os <a name="01903401600012" ll="SDBH:אֹזֶן:000000">seus</a> <a name="01903401600012" ll="SDBH:אֹזֶן:000000">ouvidos</a> estão abertos ao <a name="01903401600016" ll="SDBH:שַׁוְעָה:000000">seu</a> <a name="01903401600016" ll="SDBH:שַׁוְעָה:000000">clamor</a>.
    
\v 17 (16) O <a name="01903401700002" ll="SDBH:פָּנֶה:000000">rosto</a> do <a name="01903401700004" ll="SDBH:יהוה:000000">Senhor</a> está contra os que <a name="01903401700008">praticam</a> o <a name="01903401700010" ll="SDBH:רַע:000005">mal</a>,para <a name="01903401700020" ll="SDBH:זֵכֶר:000000">lhes</a> <a name="01903401700014" ll="SDBH:כרת:000006">extirpar</a> da <a name="01903401700018" ll="SDBH:אֶרֶץ:000000">terra</a> <a name="01903401700020" ll="SDBH:זֵכֶר:000000">a</a> <a name="01903401700020" ll="SDBH:זֵכֶר:000000">memória</a>.
    
\v 18 (17) <a name="01903401800002" ll="SDBH:צעק:000000">Clamam</a> os justos, e o <a name="01903401800006" ll="SDBH:יהוה:000000">Senhor</a> os <a name="01903401800008">escuta</a>e <a name="01903401800018" ll="SDBH:נצל:000005">os</a> <a name="01903401800018" ll="SDBH:נצל:000005">livra</a> de todas as <a name="01903401800016" ll="SDBH:צָרָה:000000">suas</a> <a name="01903401800016" ll="SDBH:צָרָה:000000">tribulações</a>.
    
\v 19 (18) <a name="01903401900002">Perto</a> está o <a name="01903401900004" ll="SDBH:יהוה:000000">Senhor</a> dos que <a name="01903401900008" ll="SDBH:שׁבר:000003">têm</a> <a name="01903401900010" ll="SDBH:לֵב:000001">o</a> <a name="01903401900010" ll="SDBH:לֵב:000001">coração</a> <a name="01903401900008" ll="SDBH:שׁבר:000003">quebrantado</a>e <a name="01903401900020" ll="SDBH:ישׁע:000001">salva</a> os <a name="01903401900018" ll="SDBH:רוּחַ:000002">de</a> <a name="01903401900018" ll="SDBH:רוּחַ:000002">espírito</a> <a name="01903401900016" ll="SDBH:דַּכָּא:000001">oprimido</a>.
    
\v 20 (19) <a name="01903402000002" ll="SDBH:רַב:000001">Muitas</a> são as <a name="01903402000004" ll="SDBH:רָעָה:000002">aflições</a> do <a name="01903402000006" ll="SDBH:צַדִּיק:000000">justo</a>,mas o <a name="01903402000016" ll="SDBH:יהוה:000000">Senhor</a> de todas <a name="01903402000014" ll="SDBH:נצל:000005">o</a> <a name="01903402000014" ll="SDBH:נצל:000005">livra</a>.
    
\v 21 (20) <a name="01903402100002" ll="SDBH:שׁמר:000007" tl="HOTTP2194">Preserva</a>-<a name="01903402100006" ll="SDBH:עֶצֶם:000000">lhe</a> todos <a name="01903402100006" ll="SDBH:עֶצֶם:000000">os</a> <a name="01903402100006" ll="SDBH:עֶצֶם:000000">ossos</a>,nem <a name="01903402100008" ll="SDBH:אֶחָד:000000">um</a> <a name="01903402100012">deles</a> sequer <a name="01903402100016" ll="SDBH:שׁבר:000000">será</a> <a name="01903402100016" ll="SDBH:שׁבר:000000">quebrado</a>.
    
\v 22 (21) O <a name="01903402200006" ll="SDBH:רָעָה:000002">infortúnio</a> <a name="01903402200002" ll="SDBH:מות:000001">matará</a> o <a name="01903402200004" ll="SDBH:רָשָׁע:000000">ímpio</a>,e os que <a name="01903402200010" ll="SDBH:שׂנא:000000">odeiam</a> o <a name="01903402200012" ll="SDBH:צַדִּיק:000000">justo</a> <a name="01903402200014" ll="SDBH:אשׁם:000002">serão</a> <a name="01903402200014" ll="SDBH:אשׁם:000002">condenados</a>.
    
\v 23 (22) O <a name="01903402300004" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903402300002" ll="SDBH:פדה:000002">resgata</a> a <a name="01903402300006" ll="SDBH:נֶפֶשׁ:000001">alma</a> <a name="01903402300008" ll="SDBH:עֶבֶד:000000" il="Bolen00049">dos</a> <a name="01903402300008" ll="SDBH:עֶבֶד:000000" il="Bolen00049">seus</a> <a name="01903402300008" ll="SDBH:עֶבֶד:000000" il="Bolen00049">servos</a>,e dos que nele <a name="01903402300020" ll="SDBH:חסה:000001">confiam</a> nenhum <a name="01903402300014" ll="SDBH:אשׁם:000002">será</a> <a name="01903402300014" ll="SDBH:אשׁם:000002">condenado</a>.
\c 35
\v 1 <a name="01903500100006" ll="SDBH:ריב:000000">Contende</a>, <a name="01903500100008" ll="SDBH:יהוה:000000">Senhor</a>, com os que <a name="01903500100012" ll="SDBH:יָרִיב:000000">contendem</a> <a name="01903500100012" ll="SDBH:יָרִיב:000000">comigo</a>;<a name="01903500100014" ll="SDBH:לחם:001000">peleja</a> contra os que <a name="01903500100018" ll="SDBH:לחם:001000">contra</a> <a name="01903500100018" ll="SDBH:לחם:001000">mim</a> <a name="01903500100018" ll="SDBH:לחם:001000">pelejam</a>.
    
\v 2 <a name="01903500200002" ll="SDBH:חזק:000017">Embraça</a> o <a name="01903500200004" ll="SDBH:מָגֵן:000000" il="NBGmk20131008">escudo</a> e o <a name="01903500200008" ll="SDBH:צִנָּה:001000" il="NBGmk20131008a">broquel</a>e <a name="01903500200012">ergue</a>-<a name="01903500200012">te</a> em <a name="01903500200016" ll="SDBH:עֶזְרָה:000000">meu</a> <a name="01903500200016" ll="SDBH:עֶזְרָה:000000">auxílio</a>.
    
\v 3 <a name="01903500300004" ll="SDBH:ריק:000001">Empunha</a> a <a name="01903500300006" ll="SDBH:חֲנִית:000000" il="HK00200;Spearheads-02-007a;adr1005306124">lança</a>e <a name="01903500300010" ll="SDBH:סְגֹר:000000" il="Realia:2.8" tl="HOTTP2195">reprime</a> o <a name="01903500300014" ll="SDBH:קרא:001000">passo</a> aos <a name="01903500300016" ll="SDBH:רדף:000000">meus</a> <a name="01903500300016" ll="SDBH:רדף:000000">perseguidores</a>;<a name="01903500300018" ll="SDBH:אמר:000000">dize</a> à <a name="01903500300022" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01903500300022" ll="SDBH:נֶפֶשׁ:000001">alma</a>:Eu sou a <a name="01903500300024" ll="SDBH:יְשׁוּעָה:000000">tua</a> <a name="01903500300024" ll="SDBH:יְשׁוּעָה:000000">salvação</a>.
    
\v 4 <a name="01903500400002" ll="SDBH:בושׁ:000000">Sejam</a> <a name="01903500400002" ll="SDBH:בושׁ:000000">confundidos</a> e <a name="01903500400006" ll="SDBH:כלם:000001">cobertos</a> <a name="01903500400006" ll="SDBH:כלם:000001">de</a> <a name="01903500400006" ll="SDBH:כלם:000001">vexame</a>os que <a name="01903500400008" ll="SDBH:בקשׁ:000000">buscam</a> tirar-<a name="01903500400010" ll="SDBH:נֶפֶשׁ:000001">me</a> <a name="01903500400010" ll="SDBH:נֶפֶשׁ:000001">a</a> <a name="01903500400010" ll="SDBH:נֶפֶשׁ:000001">vida</a>;<a name="01903500400012;01903500400014" ll="SDBH:אָחֹור:000001;SDBH:סוג:000000">retrocedam</a> e <a name="01903500400018" ll="SDBH:חפר:001000">sejam</a> <a name="01903500400018" ll="SDBH:חפר:001000">envergonhados</a>os que <a name="01903500400020" ll="SDBH:חשׁב:000001">tramam</a> <a name="01903500400022" ll="SDBH:רָעָה:000002">contra</a> <a name="01903500400022" ll="SDBH:רָעָה:000002">mim</a>.
    
\v 5 Sejam como a <a name="01903500500006" ll="SDBH:מֹץ:000000" il="HK00097;NBGbs20110227-93;NBGpb20140206;NBGpb20140206b;Realia:1.1.8.4a">palha</a> ao <a name="01903500500010">léu</a> <a name="01903500500012" ll="SDBH:רוּחַ:000004" il="Gale_Force_Winds">do</a> <a name="01903500500012" ll="SDBH:רוּחַ:000004" il="Gale_Force_Winds">vento</a>,<a name="01903500500020" ll="SDBH:דחה:000000">impelindo</a>-os o <a name="01903500500016" ll="SDBH:מַלְאָךְ:000000">anjo</a> do <a name="01903500500018" ll="SDBH:יהוה:000000">Senhor</a>.
    
\v 6 Torne-se-<a name="01903500600004" ll="SDBH:דֶּרֶךְ:000007">lhes</a> <a name="01903500600004" ll="SDBH:דֶּרֶךְ:000007">o</a> <a name="01903500600004" ll="SDBH:דֶּרֶךְ:000007">caminho</a> <a name="01903500600006" ll="SDBH:חֹשֶׁךְ:000000">tenebroso</a> e <a name="01903500600010" ll="SDBH:חֲלַקְלַק:000000">escorregadio</a>,e o <a name="01903500600014" ll="SDBH:מַלְאָךְ:000000">anjo</a> do <a name="01903500600016" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903500600018" ll="SDBH:רדף:000000">os</a> <a name="01903500600018" ll="SDBH:רדף:000000">persiga</a>.
    
\v 7 Pois <a name="01903500700004" ll="SDBH:חִנָּם:000001">sem</a> <a name="01903500700004" ll="SDBH:חִנָּם:000001">causa</a> me <a name="01903500700006;01903500700010" ll="SDBH:טמן:000001;SDBH:שַׁחַת:000000" tl="HOTTP2196">tramaram</a> <a name="01903500700012" ll="SDBH:רֶשֶׁת:000000" il="LB00209;LB00210;Realia:1.3.1.1a;Realia:1.3.1.1b;Realia:1.3.1.1c;Realia:1.4.2;mat00691" tl="HOTTP2196">laços</a>,<a name="01903500700014" ll="SDBH:חִנָּם:000001">sem</a> <a name="01903500700014" ll="SDBH:חִנָּם:000001">causa</a> <a name="01903500700016" ll="SDBH:חפר:000001">abriram</a> <a name="01903500700016" ll="SDBH:חפר:000001">cova</a> para a <a name="01903500700020" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01903500700020" ll="SDBH:נֶפֶשׁ:000001">vida</a>.
    
\v 8 <a name="01903500800002">Venha</a> <a name="01903500800002">sobre</a> <a name="01903500800002">o</a> inimigo a <a name="01903500800004" ll="SDBH:שֹׁואָה:000001">destruição</a>, <a name="01903500800006;01903500800008" ll="SDBH:לֹא:000000">quando</a> ele <a name="01903500800006;01903500800008" ll="SDBH:לֹא:000000">menos</a> <a name="01903500800006;01903500800008" ll="SDBH:לֹא:000000">pensar</a>;e <a name="01903500800018" ll="SDBH:לכד:000000">prendam</a>-<a name="01903500800018" ll="SDBH:לכד:000000">no</a> os <a name="01903500800012" ll="SDBH:רֶשֶׁת:000000" il="LB00209;LB00210;Realia:1.3.1.1a;Realia:1.3.1.1b;Realia:1.3.1.1c;Realia:1.4.2;mat00691">laços</a> que tramou <a name="01903500800016" ll="SDBH:טמן:000001">ocultamente</a>;<a name="01903500800024" ll="SDBH:נפל:000000">caia</a> neles para a sua própria <a name="01903500800022" ll="SDBH:שֹׁואָה:000001" tl="HOTTP2197">ruína</a>.
    
\v 9 E <a name="01903500900004" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01903500900004" ll="SDBH:נֶפֶשׁ:000001">alma</a> se <a name="01903500900006" ll="SDBH:גיל:000000">regozijará</a> no <a name="01903500900010" ll="SDBH:יהוה:000000">Senhor</a>e <a name="01903500900012" ll="SDBH:שׂושׂ:000000">se</a> <a name="01903500900012" ll="SDBH:שׂושׂ:000000">deleitará</a> na <a name="01903500900016" ll="SDBH:יְשׁוּעָה:000000">sua</a> <a name="01903500900016" ll="SDBH:יְשׁוּעָה:000000">salvação</a>.
    
\v 10 Todos os <a name="01903501000004" ll="SDBH:עֶצֶם:000000">meus</a> <a name="01903501000004" ll="SDBH:עֶצֶם:000000">ossos</a> <a name="01903501000006" ll="SDBH:אמר:000000">dirão</a>:
    <a name="01903501000008" ll="SDBH:יהוה:000000">Senhor</a>, quem contigo se assemelha?Pois <a name="01903501000014" ll="SDBH:נצל:000005">livras</a> o <a name="01903501000016" ll="SDBH:עָנִי:000000">aflito</a> daquele que é demais <a name="01903501000020" ll="SDBH:חָזָק:000000">forte</a> para ele,o <a name="01903501000026" ll="SDBH:עָנִי:000000">mísero</a> e o <a name="01903501000030" ll="SDBH:אֶבְיֹון:000000">necessitado</a>, dos <a name="01903501000034" ll="SDBH:גזל:000000">seus</a> <a name="01903501000034" ll="SDBH:גזל:000000">extorsionários</a>.
    
\v 11 <a name="01903501100002">Levantam</a>-<a name="01903501100002">se</a> <a name="01903501100006" ll="SDBH:חָמָס:000000">iníquas</a> <a name="01903501100004" ll="SDBH:עֵד:000003">testemunhas</a>e <a name="01903501100014" ll="SDBH:שׁאל:000000">me</a> <a name="01903501100014" ll="SDBH:שׁאל:000000">arguem</a> <a name="01903501100014" ll="SDBH:שׁאל:000000">de</a> coisas que <a name="01903501100012">eu</a> não <a name="01903501100012">sei</a>.
    
\v 12 <a name="01903501200002" ll="SDBH:שׁלם:000007">Pagam</a>-<a name="01903501200002" ll="SDBH:שׁלם:000007">me</a> o <a name="01903501200004" ll="SDBH:רָעָה:000000">mal</a> pelo <a name="01903501200008" ll="SDBH:טֹובָה:000002">bem</a>,o que é <a name="01903501200010" ll="SDBH:שְׁכֹול:000000" tl="HOTTP2198">desolação</a> para a <a name="01903501200014" ll="SDBH:נֶפֶשׁ:000001" tl="HOTTP2198">minha</a> <a name="01903501200014" ll="SDBH:נֶפֶשׁ:000001" tl="HOTTP2198">alma</a>.
    
\v 13 Quanto a mim, porém, estando <a name="01903501300008" ll="SDBH:חלה:000000">eles</a> <a name="01903501300008" ll="SDBH:חלה:000000">enfermos</a>,as <a name="01903501300010" ll="SDBH:לְבוּשׁ:000000">minhas</a> <a name="01903501300010" ll="SDBH:לְבוּשׁ:000000">vestes</a> eram <a name="01903501300012" ll="SDBH:שַׂק:000001" il="Sackcloth;Sacking;adr1603195064">pano</a> <a name="01903501300012" ll="SDBH:שַׂק:000001" il="Sackcloth;Sacking;adr1603195064">de</a> <a name="01903501300012" ll="SDBH:שַׂק:000001" il="Sackcloth;Sacking;adr1603195064">saco</a>;eu <a name="01903501300014" ll="SDBH:ענה:001001">afligia</a> a <a name="01903501300022" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01903501300022" ll="SDBH:נֶפֶשׁ:000001">alma</a> com <a name="01903501300020" ll="SDBH:צֹום:000000">jejum</a>e em <a name="01903501300026" ll="SDBH:תְּפִלָּה:000000">oração</a> <a name="01903501300032" ll="SDBH:שׁוב:000022">me</a> <a name="01903501300032" ll="SDBH:שׁוב:000022">reclinava</a> sobre o <a name="01903501300030" ll="SDBH:חֵיק:000000">peito</a>,
    
\v 14 <a name="01903501400012" ll="SDBH:הלך:000002">portava</a>-<a name="01903501400012" ll="SDBH:הלך:000002">me</a> como se eles fossem meus <a name="01903501400004" ll="SDBH:רֵעַ:001000">amigos</a> ou meus <a name="01903501400008" ll="SDBH:אָח:000000">irmãos</a>;<a name="01903501400022" ll="SDBH:שׁחח:000005">andava</a> <a name="01903501400022" ll="SDBH:שׁחח:000005">curvado</a>, de <a name="01903501400020" ll="SDBH:קדר:000003">luto</a>, como quem <a name="01903501400016" ll="SDBH:אָבֵל:000000">chora</a> <a name="01903501400018" ll="SDBH:אֵם:000000" il="Motherhood1912">por</a> <a name="01903501400018" ll="SDBH:אֵם:000000" il="Motherhood1912">sua</a> <a name="01903501400018" ll="SDBH:אֵם:000000" il="Motherhood1912">mãe</a>.
    
\v 15 Quando, porém, <a name="01903501500006" ll="SDBH:צֶלַע:000000">tropecei</a>, <a name="01903501500008" ll="SDBH:שׂמח:000000">eles</a> <a name="01903501500008" ll="SDBH:שׂמח:000000">se</a> <a name="01903501500008" ll="SDBH:שׂמח:000000">alegraram</a> e <a name="01903501500012" ll="SDBH:אסף:000000">se</a> <a name="01903501500012" ll="SDBH:אסף:000000">reuniram</a>;<a name="01903501500014" ll="SDBH:אסף:000000">reuniram</a>-<a name="01903501500014" ll="SDBH:אסף:000000">se</a> contra mim;os <a name="01903501500018" ll="SDBH:נָכֶה:000000" tl="HOTTP2199">abjetos</a>, que eu não <a name="01903501500024">conhecia</a>,<a name="01903501500026" ll="SDBH:קרע:000000">dilaceraram</a>-me sem <a name="01903501500032" ll="SDBH:דמם:000001">tréguas</a>;
    
\v 16 como <a name="01903501600004" ll="SDBH:חָנֵף:000000" tl="HOTTP2200">vis</a> <a name="01903501600006" ll="SDBH:לָעֵג:000000" tl="HOTTP2200">bufões</a> em <a name="01903501600008" ll="SDBH:מָעֹוג:000000" tl="HOTTP2200">festins</a>,<a name="01903501600010" ll="SDBH:חרק:000000">rangiam</a> contra mim <a name="01903501600014" ll="SDBH:שֵׁן:000000">os</a> <a name="01903501600014" ll="SDBH:שֵׁן:000000">dentes</a>.
    
\v 17 Até quando, <a name="01903501700002" ll="SDBH:אֲדֹנָי:000000">Senhor</a>, <a name="01903501700008" ll="SDBH:ראה:000000">ficarás</a> <a name="01903501700008" ll="SDBH:ראה:000000">olhando</a>?<a name="01903501700010">Livra</a>-<a name="01903501700012" ll="SDBH:נֶפֶשׁ:000001">me</a> <a name="01903501700012" ll="SDBH:נֶפֶשׁ:000001">a</a> <a name="01903501700012" ll="SDBH:נֶפֶשׁ:000001">alma</a> das <a name="01903501700016" ll="SDBH:שֹׁוא:000000">violências</a> <a name="01903501700016" ll="SDBH:שֹׁוא:000000">deles</a>;dos <a name="01903501700020" ll="SDBH:כְּפִיר:000000" il="Fauna:2.24a;Fauna:2.24b;Fauna:2.24c">leões</a>, a <a name="01903501700022">minha</a> <a name="01903501700022">predileta</a>.
    
\v 18 <a name="01903501800002" ll="SDBH:ידה:000002">Dar</a>-<a name="01903501800002" ll="SDBH:ידה:000002">te</a>-<a name="01903501800002" ll="SDBH:ידה:000002">ei</a> <a name="01903501800002" ll="SDBH:ידה:000002">graças</a> na <a name="01903501800008" ll="SDBH:רַב:000001">grande</a> <a name="01903501800006" ll="SDBH:קָהָל:000000">congregação</a>,<a name="01903501800016" ll="SDBH:הלל:001000">louvar</a>-<a name="01903501800016" ll="SDBH:הלל:001000">te</a>-<a name="01903501800016" ll="SDBH:הלל:001000">ei</a> no meio da <a name="01903501800012" ll="SDBH:עַם:000000">multidão</a> <a name="01903501800014" ll="SDBH:עָצוּם:000000">poderosa</a>.
    
\v 19 Não <a name="01903501900004" ll="SDBH:שׂמח:000000">se</a> <a name="01903501900004" ll="SDBH:שׂמח:000000">alegrem</a> de mim os <a name="01903501900008" ll="SDBH:איב:000001">meus</a> <a name="01903501900008" ll="SDBH:איב:000001">inimigos</a> <a name="01903501900010" ll="SDBH:שֶׁקֶר:000000">gratuitos</a>;não <a name="01903501900016" ll="SDBH:קרץ:000001">pisquem</a> <a name="01903501900018" ll="SDBH:עַיִן:000000">os</a> <a name="01903501900018" ll="SDBH:עַיִן:000000">olhos</a> os que <a name="01903501900014" ll="SDBH:חִנָּם:000001">sem</a> <a name="01903501900014" ll="SDBH:חִנָּם:000001">causa</a> <a name="01903501900012" ll="SDBH:שׂנא:000001">me</a> <a name="01903501900012" ll="SDBH:שׂנא:000001">odeiam</a>.
    
\v 20 Não é de <a name="01903502000006" ll="SDBH:שָׁלֹום:000000">paz</a> que <a name="01903502000008" ll="SDBH:דבר:001000">eles</a> <a name="01903502000008" ll="SDBH:דבר:001000">falam</a>;pelo contrário, <a name="01903502000022" ll="SDBH:חשׁב:000001">tramam</a> <a name="01903502000018;01903502000020" ll="SDBH:דָּבָר:000002;SDBH:מִרְמָה:000000">enganos</a>contra os <a name="01903502000014" ll="SDBH:רָגֵעַ:000000">pacíficos</a> <a name="01903502000016" ll="SDBH:אֶרֶץ:000000">da</a> <a name="01903502000016" ll="SDBH:אֶרֶץ:000000">terra</a>.
    
\v 21 <a name="01903502100004" ll="SDBH:רחב:000009">Escancaram</a> contra mim a <a name="01903502100008" ll="SDBH:פֶּה:000000">boca</a>e <a name="01903502100010" ll="SDBH:אמר:000000">dizem</a>: <a name="01903502100012" ll="SDBH:הֶאָח:000000">Pegamos</a>! <a name="01903502100014" ll="SDBH:הֶאָח:000000">Pegamos</a>!<a name="01903502100016" ll="SDBH:ראה:000000">Vimo</a>-lo com os <a name="01903502100018" ll="SDBH:עַיִן:000000">nossos</a> próprios <a name="01903502100018" ll="SDBH:עַיִן:000000">olhos</a>.
    
\v 22 Tu, <a name="01903502200004" ll="SDBH:יהוה:000000">Senhor</a>, os <a name="01903502200002" ll="SDBH:ראה:000000">viste</a>; não <a name="01903502200008" ll="SDBH:חרשׁ:001001">te</a> <a name="01903502200008" ll="SDBH:חרשׁ:001001">cales</a>;<a name="01903502200010" ll="SDBH:אֲדֹנָי:000000">Senhor</a>, não <a name="01903502200014" ll="SDBH:רחק:000002">te</a> <a name="01903502200014" ll="SDBH:רחק:000002">ausentes</a> de mim.
    
\v 23 <a name="01903502300002" ll="SDBH:עור:000002">Acorda</a> e <a name="01903502300006" ll="SDBH:קיץ:000000">desperta</a> para <a name="01903502300010" ll="SDBH:מִשְׁפָּט:000003">me</a> <a name="01903502300010" ll="SDBH:מִשְׁפָּט:000003">fazeres</a> <a name="01903502300010" ll="SDBH:מִשְׁפָּט:000003">justiça</a>,para a <a name="01903502300020" ll="SDBH:רִיב:000000">minha</a> <a name="01903502300020" ll="SDBH:רִיב:000000">causa</a>, <a name="01903502300012" ll="SDBH:אֱלֹהִים:000001">Deus</a> <a name="01903502300012" ll="SDBH:אֱלֹהִים:000001">meu</a> e <a name="01903502300016" ll="SDBH:אֲדֹנָי:000000">Senhor</a> meu.
    
\v 24 <a name="01903502400002" ll="SDBH:שׁפט:000003" il="Tel-dan-israelite-gate-im-tor">Julga</a>-<a name="01903502400002" ll="SDBH:שׁפט:000003" il="Tel-dan-israelite-gate-im-tor">me</a>, <a name="01903502400008" ll="SDBH:יהוה:000000">Senhor</a>, <a name="01903502400010" ll="SDBH:אֱלֹהִים:000001">Deus</a> <a name="01903502400010" ll="SDBH:אֱלֹהִים:000001">meu</a>, segundo a <a name="01903502400006" ll="SDBH:צֶדֶק:000001">tua</a> <a name="01903502400006" ll="SDBH:צֶדֶק:000001">justiça</a>;não <a name="01903502400016" ll="SDBH:שׂמח:000000">permitas</a> <a name="01903502400016" ll="SDBH:שׂמח:000000">que</a> <a name="01903502400016" ll="SDBH:שׂמח:000000">se</a> <a name="01903502400016" ll="SDBH:שׂמח:000000">regozijem</a> contra mim.
    
\v 25 Não <a name="01903502500004" ll="SDBH:אמר:000000">digam</a> <a name="01903502500004" ll="SDBH:אמר:000000">eles</a> lá no <a name="01903502500008" ll="SDBH:לֵב:000001">seu</a> <a name="01903502500008" ll="SDBH:לֵב:000001">íntimo</a>:<a name="01903502500010" ll="SDBH:הֶאָח:000000">Agora</a>, <a name="01903502500010" ll="SDBH:הֶאָח:000000">sim</a>! Cumpriu-se o <a name="01903502500012" ll="SDBH:נֶפֶשׁ:000001">nosso</a> <a name="01903502500012" ll="SDBH:נֶפֶשׁ:000001">desejo</a>!Não <a name="01903502500016" ll="SDBH:אמר:000000">digam</a>: <a name="01903502500018" ll="SDBH:בלע:000001">Demos</a> <a name="01903502500018" ll="SDBH:בלע:000001">cabo</a> <a name="01903502500018" ll="SDBH:בלע:000001">dele</a>!
    
\v 26 <a name="01903502600002" ll="SDBH:בושׁ:000000">Envergonhem</a>-se e <a name="01903502600008">juntamente</a> <a name="01903502600006" ll="SDBH:חפר:001000">sejam</a> <a name="01903502600006" ll="SDBH:חפר:001000">cobertos</a> <a name="01903502600006" ll="SDBH:חפר:001000">de</a> <a name="01903502600006" ll="SDBH:חפר:001000">vexame</a>os que <a name="01903502600010" ll="SDBH:שָׂמֵחַ:000000">se</a> <a name="01903502600010" ll="SDBH:שָׂמֵחַ:000000">alegram</a> <a name="01903502600012" ll="SDBH:רָעָה:000002">com</a> o <a name="01903502600012" ll="SDBH:רָעָה:000002">meu</a> <a name="01903502600012" ll="SDBH:רָעָה:000002">mal</a>;<a name="01903502600014" ll="SDBH:לבשׁ:000000">cubram</a>-<a name="01903502600014" ll="SDBH:לבשׁ:000000">se</a> <a name="01903502600016" ll="SDBH:בֹּשֶׁת:000000">de</a> <a name="01903502600016" ll="SDBH:בֹּשֶׁת:000000">pejo</a> e <a name="01903502600020" ll="SDBH:כְּלִמָּה:000000">ignomínia</a>os que <a name="01903502600024" ll="SDBH:גדל:000007">se</a> <a name="01903502600024" ll="SDBH:גדל:000007">engrandecem</a> contra mim.
    
\v 27 <a name="01903502700002" ll="SDBH:רנן:000000">Cantem</a> <a name="01903502700002" ll="SDBH:רנן:000000">de</a> <a name="01903502700002" ll="SDBH:רנן:000000">júbilo</a> e <a name="01903502700006" ll="SDBH:שׂמח:000000">se</a> <a name="01903502700006" ll="SDBH:שׂמח:000000">alegrem</a>os que <a name="01903502700008" ll="SDBH:חָפֵץ:000001">têm</a> <a name="01903502700008" ll="SDBH:חָפֵץ:000001">prazer</a> na <a name="01903502700010" ll="SDBH:צֶדֶק:000001">minha</a> <a name="01903502700010" ll="SDBH:צֶדֶק:000001">retidão</a>;e <a name="01903502700014" ll="SDBH:אמר:000000">digam</a> <a name="01903502700016" ll="SDBH:תָּמִיד:000000">sempre</a>:<a name="01903502700018" ll="SDBH:גדל:000006">Glorificado</a> <a name="01903502700018" ll="SDBH:גדל:000006">seja</a> o <a name="01903502700020" ll="SDBH:יהוה:000000">Senhor</a>,que <a name="01903502700024" ll="SDBH:חפץ:000002">se</a> <a name="01903502700024" ll="SDBH:חפץ:000002">compraz</a> na <a name="01903502700026" ll="SDBH:שָׁלֹום:000000">prosperidade</a> <a name="01903502700028" ll="SDBH:עֶבֶד:000000" il="Bolen00049">do</a> <a name="01903502700028" ll="SDBH:עֶבֶד:000000" il="Bolen00049">seu</a> <a name="01903502700028" ll="SDBH:עֶבֶד:000000" il="Bolen00049">servo</a>!
    
\v 28 E a <a name="01903502800004" ll="SDBH:לָשֹׁון:000000">minha</a> <a name="01903502800004" ll="SDBH:לָשֹׁון:000000">língua</a> <a name="01903502800006" ll="SDBH:הגה:000003">celebrará</a> a <a name="01903502800008" ll="SDBH:צֶדֶק:000001">tua</a> <a name="01903502800008" ll="SDBH:צֶדֶק:000001">justiça</a>e o <a name="01903502800016" ll="SDBH:תְּהִלָּה:000001">teu</a> <a name="01903502800016" ll="SDBH:תְּהִלָּה:000001">louvor</a> todo o <a name="01903502800014" ll="SDBH:יֹום:000007">dia</a>.
\c 36
\v 2 (1) Há <a name="01903600200014;01903600200016" ll="SDBH:לֵב:000001" tl="HOTTP2201">no</a> <a name="01903600200014;01903600200016" ll="SDBH:לֵב:000001" tl="HOTTP2201">coração</a> do ímpioa <a name="01903600200002" ll="SDBH:נְאֻם:000000">voz</a> <a name="01903600200004" ll="SDBH:פֶּשַׁע:000000">da</a> <a name="01903600200004" ll="SDBH:פֶּשַׁע:000000">transgressão</a>;não há <a name="01903600200020" ll="SDBH:פַּחַד:000000">temor</a> de <a name="01903600200022" ll="SDBH:אֱלֹהִים:000002">Deus</a>diante de <a name="01903600200028" ll="SDBH:עַיִן:000000">seus</a> <a name="01903600200028" ll="SDBH:עַיִן:000000">olhos</a>.
    
\v 3 (2) Porque a transgressão o <a name="01903600300004" ll="SDBH:חלק:001002">lisonjeia</a> a <a name="01903600300010" ll="SDBH:עַיִן:000000">seus</a> <a name="01903600300010" ll="SDBH:עַיִן:000000">olhos</a>e lhe diz que a <a name="01903600300016" ll="SDBH:עָוֹן:000000">sua</a> <a name="01903600300016" ll="SDBH:עָוֹן:000000">iniquidade</a> não há de ser <a name="01903600300014" ll="SDBH:מצא:000011">descoberta</a>, nem <a name="01903600300020" ll="SDBH:שׂנא:000000" tl="HOTTP2202">detestada</a>.
    
\v 4 (3) As <a name="01903600400002" ll="SDBH:דָּבָר:000000">palavras</a> <a name="01903600400004" ll="SDBH:פֶּה:000000">de</a> <a name="01903600400004" ll="SDBH:פֶּה:000000">sua</a> <a name="01903600400004" ll="SDBH:פֶּה:000000">boca</a> são <a name="01903600400006" ll="SDBH:אָוֶן:000000">malícia</a> e <a name="01903600400010" ll="SDBH:מִרְמָה:000000">dolo</a>;<a name="01903600400012" ll="SDBH:חדל:000003">abjurou</a> o <a name="01903600400016" ll="SDBH:שׂכל:001000">discernimento</a> e a <a name="01903600400020" ll="SDBH:יטב:000007">prática</a> <a name="01903600400020" ll="SDBH:יטב:000007">do</a> <a name="01903600400020" ll="SDBH:יטב:000007">bem</a>.
    
\v 5 (4) No <a name="01903600500008" ll="SDBH:מִשְׁכָּב:000000">seu</a> <a name="01903600500008" ll="SDBH:מִשְׁכָּב:000000">leito</a>, <a name="01903600500004" ll="SDBH:חשׁב:000001">maquina</a> a <a name="01903600500002" ll="SDBH:אָוֶן:000000">perversidade</a>,<a name="01903600500010" ll="SDBH:יצב:000000">detém</a>-<a name="01903600500010" ll="SDBH:יצב:000000">se</a> em <a name="01903600500014" ll="SDBH:דֶּרֶךְ:000007">caminho</a> que não é <a name="01903600500018" ll="SDBH:טֹוב:000002">bom</a>,não <a name="01903600500024" ll="SDBH:מאס:000000">se</a> <a name="01903600500024" ll="SDBH:מאס:000000">despega</a> do <a name="01903600500020" ll="SDBH:רַע:000005">mal</a>.
    
\v 6 (5) A <a name="01903600600010" ll="SDBH:חֶסֶד:000000">tua</a> <a name="01903600600010" ll="SDBH:חֶסֶד:000000">benignidade</a>, <a name="01903600600002" ll="SDBH:יהוה:000000">Senhor</a>, chega até aos <a name="01903600600008" ll="SDBH:שָׁמַיִם:000000">céus</a>,até às <a name="01903600600016" ll="SDBH:שַׁחַק:000002">nuvens</a>, a <a name="01903600600012" ll="SDBH:אֱמוּנָה:000002">tua</a> <a name="01903600600012" ll="SDBH:אֱמוּנָה:000002">fidelidade</a>.
    
\v 7 (6) A <a name="01903600700002" ll="SDBH:צְדָקָה:000001">tua</a> <a name="01903600700002" ll="SDBH:צְדָקָה:000001">justiça</a> é como as <a name="01903600700006" ll="SDBH:הַר:000000">montanhas</a> <a name="01903600700008" ll="SDBH:אֵל:000001;SDBH:אֵל:000008">de</a> <a name="01903600700008" ll="SDBH:אֵל:000001;SDBH:אֵל:000008">Deus</a>;os <a name="01903600700010" ll="SDBH:מִשְׁפָּט:000007">teus</a> <a name="01903600700010" ll="SDBH:מִשְׁפָּט:000007">juízos</a>, como um <a name="01903600700012" ll="SDBH:תְּהֹום:000000">abismo</a> <a name="01903600700014" ll="SDBH:רַב:000000">profundo</a>.<a name="01903600700022" ll="SDBH:ישׁע:000001">Tu</a>, <a name="01903600700024" ll="SDBH:יהוה:000000">Senhor</a>, <a name="01903600700022" ll="SDBH:ישׁע:000001">preservas</a> os <a name="01903600700016" ll="SDBH:אָדָם:000000">homens</a> e os <a name="01903600700020" ll="SDBH:בְּהֵמָה:000001" il="Fauna:2.10">animais</a>.
    
\v 8 (7) Como é <a name="01903600800004" ll="SDBH:יָקָר:000000">preciosa</a>, ó <a name="01903600800008" ll="SDBH:אֱלֹהִים:000002" tl="HOTTP2203">Deus</a>, a <a name="01903600800006" ll="SDBH:חֶסֶד:000000" tl="HOTTP2203">tua</a> <a name="01903600800006" ll="SDBH:חֶסֶד:000000" tl="HOTTP2203">benignidade</a>!Por isso, os <a name="01903600800012" ll="SDBH:בֵּן:000003" tl="HOTTP2203">filhos</a> dos <a name="01903600800014" ll="SDBH:אָדָם:000000">homens</a><a name="01903600800022" ll="SDBH:חסה:000000">se</a> <a name="01903600800022" ll="SDBH:חסה:000000">acolhem</a> à <a name="01903600800018" ll="SDBH:צֵל:000001">sombra</a> <a name="01903600800020" ll="SDBH:כָּנָף:000000">das</a> <a name="01903600800020" ll="SDBH:כָּנָף:000000">tuas</a> <a name="01903600800020" ll="SDBH:כָּנָף:000000">asas</a>.
    
\v 9 (8) <a name="01903600900002" ll="SDBH:רוה:000003">Fartam</a>-<a name="01903600900002" ll="SDBH:רוה:000003">se</a> da <a name="01903600900006" ll="SDBH:דֶּשֶׁן:000000">abundância</a> <a name="01903600900008" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917">da</a> <a name="01903600900008" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917">tua</a> <a name="01903600900008" ll="SDBH:בַּיִת:000000" il="HK00232;LB00235;NBGbs20090430-1_87;NBGbs20110223-127;NBGmk20120826;NBGmk20120826a;NBGmk20140917">casa</a>,e na <a name="01903600900012" ll="SDBH:נַחַל:000000" il="NBGbs20090428-1_39;tb110603119">torrente</a> <a name="01903600900014" ll="SDBH:עֵדֶן:000000">das</a> <a name="01903600900014" ll="SDBH:עֵדֶן:000000">tuas</a> <a name="01903600900014" ll="SDBH:עֵדֶן:000000">delícias</a> <a name="01903600900016" ll="SDBH:שׁקה:000002">lhes</a> <a name="01903600900016" ll="SDBH:שׁקה:000002">dás</a> <a name="01903600900016" ll="SDBH:שׁקה:000002">de</a> <a name="01903600900016" ll="SDBH:שׁקה:000002">beber</a>.
    
\v 10 (9) Pois em ti está o <a name="01903601000006" ll="SDBH:מָקֹור:000000">manancial</a> da <a name="01903601000008" ll="SDBH:חַיִּים:000000">vida</a>;na <a name="01903601000012" ll="SDBH:אֹור:000000">tua</a> <a name="01903601000012" ll="SDBH:אֹור:000000">luz</a>, <a name="01903601000014" ll="SDBH:ראה:000004">vemos</a> <a name="01903601000016" ll="SDBH:אֹור:000000">a</a> <a name="01903601000016" ll="SDBH:אֹור:000000">luz</a>.
    
\v 11 (10) <a name="01903601100002" ll="SDBH:משׁך:000008">Continua</a> a <a name="01903601100004" ll="SDBH:חֶסֶד:000000">tua</a> <a name="01903601100004" ll="SDBH:חֶסֶד:000000">benignidade</a> aos que <a name="01903601100008">te</a> <a name="01903601100008">conhecem</a>,e a <a name="01903601100012" ll="SDBH:צְדָקָה:000001">tua</a> <a name="01903601100012" ll="SDBH:צְדָקָה:000001">justiça</a>, aos <a name="01903601100016" ll="SDBH:יָשָׁר:000004">retos</a> <a name="01903601100018" ll="SDBH:לֵב:000001">de</a> <a name="01903601100018" ll="SDBH:לֵב:000001">coração</a>.
    
\v 12 (11) Não <a name="01903601200004">me</a> <a name="01903601200004">calque</a> o <a name="01903601200006" ll="SDBH:רֶגֶל:000003">pé</a> da <a name="01903601200008" ll="SDBH:גַּאֲוָה:000001">insolência</a>,nem <a name="01903601200018" ll="SDBH:נוד:000005">me</a> <a name="01903601200018" ll="SDBH:נוד:000005">repila</a> a <a name="01903601200012">mão</a> <a name="01903601200014" ll="SDBH:רָשָׁע:000000">dos</a> <a name="01903601200014" ll="SDBH:רָשָׁע:000000">ímpios</a>.
    
\v 13 (12) <a name="01903601300004" ll="SDBH:נפל:000038">Tombaram</a> os <a name="01903601300006" ll="SDBH:פעל:000000">obreiros</a> da <a name="01903601300008" ll="SDBH:אָוֶן:000000">iniquidade</a>;<a name="01903601300010" ll="SDBH:דחה:000001">estão</a> <a name="01903601300010" ll="SDBH:דחה:000001">derruídos</a> e já não <a name="01903601300016" ll="SDBH:יכל:000000">podem</a> <a name="01903601300018">levantar</a>-<a name="01903601300018">se</a>.
\c 37
\v 1 Não <a name="01903700100008" ll="SDBH:חרה:000002">te</a> <a name="01903700100008" ll="SDBH:חרה:000002">indignes</a> por causa dos <a name="01903700100014" ll="SDBH:רעע:000000">malfeitores</a>,nem <a name="01903700100018" ll="SDBH:קנא:000001">tenhas</a> <a name="01903700100018" ll="SDBH:קנא:000001">inveja</a> dos que <a name="01903700100022">praticam</a> a <a name="01903700100024" ll="SDBH:עַוְלָה:000000">iniquidade</a>.
    
\v 2 Pois eles dentro <a name="01903700200010" ll="SDBH:מְהֵרָה:000000">em</a> <a name="01903700200010" ll="SDBH:מְהֵרָה:000000">breve</a> <a name="01903700200012" ll="SDBH:מלל:000000">definharão</a> como a <a name="01903700200008" ll="SDBH:חָצִיר:000001" il="Flora:5.1.2">relva</a>e <a name="01903700200022" ll="SDBH:נבל:000000">murcharão</a> como a <a name="01903700200020" ll="SDBH:דֶּשֶׁא:000000">erva</a> <a name="01903700200018" ll="SDBH:יֶרֶק:000000">verde</a>.
    
\v 3 <a name="01903700300002" ll="SDBH:בטח:000001">Confia</a> no <a name="01903700300006" ll="SDBH:יהוה:000000">Senhor</a> e <a name="01903700300010">faze</a> <a name="01903700300012" ll="SDBH:טֹוב:000002">o</a> <a name="01903700300012" ll="SDBH:טֹוב:000002">bem</a>;<a name="01903700300014" ll="SDBH:שׁכן:000000">habita</a> <a name="01903700300016" ll="SDBH:אֶרֶץ:000000">na</a> <a name="01903700300016" ll="SDBH:אֶרֶץ:000000">terra</a> e <a name="01903700300020" ll="SDBH:רעה:001004">alimenta</a>-<a name="01903700300020" ll="SDBH:רעה:001004">te</a> da <a name="01903700300022" ll="SDBH:אֱמוּנָה:000001">verdade</a>.
    
\v 4 <a name="01903700400004" ll="SDBH:ענג:001000">Agrada</a>-<a name="01903700400004" ll="SDBH:ענג:001000">te</a> do <a name="01903700400008" ll="SDBH:יהוה:000000">Senhor</a>,e <a name="01903700400012">ele</a> <a name="01903700400012">satisfará</a> os <a name="01903700400016" ll="SDBH:מִשְׁאָלָה:000000">desejos</a> <a name="01903700400018" ll="SDBH:לֵב:000001">do</a> <a name="01903700400018" ll="SDBH:לֵב:000001">teu</a> <a name="01903700400018" ll="SDBH:לֵב:000001">coração</a>.
    
\v 5 <a name="01903700500002" ll="SDBH:גלל:000007">Entrega</a> o <a name="01903700500008" ll="SDBH:דֶּרֶךְ:000007">teu</a> <a name="01903700500008" ll="SDBH:דֶּרֶךְ:000007">caminho</a> ao <a name="01903700500006" ll="SDBH:יהוה:000000">Senhor</a>,<a name="01903700500012" ll="SDBH:בטח:000001">confia</a> nele, e o mais <a name="01903700500020">ele</a> <a name="01903700500020">fará</a>.
    
\v 6 <a name="01903700600004" ll="SDBH:יצא:000029">Fará</a> <a name="01903700600004" ll="SDBH:יצא:000029">sobressair</a> a <a name="01903700600012" ll="SDBH:צֶדֶק:000001">tua</a> <a name="01903700600012" ll="SDBH:צֶדֶק:000001">justiça</a> como a <a name="01903700600010" ll="SDBH:אֹור:000000">luz</a>e o <a name="01903700600016" ll="SDBH:מִשְׁפָּט:000001">teu</a> <a name="01903700600016" ll="SDBH:מִשְׁפָּט:000001">direito</a>, como o sol ao <a name="01903700600022" ll="SDBH:צָהֳרַיִם:000000" il="Sun">meio</a>-<a name="01903700600022" ll="SDBH:צָהֳרַיִם:000000" il="Sun">dia</a>.
    
\v 7 <a name="01903700700002" ll="SDBH:דמם:000001">Descansa</a> no <a name="01903700700006" ll="SDBH:יהוה:000000">Senhor</a> e <a name="01903700700010" ll="SDBH:חיל:001001">espera</a> nele,não <a name="01903700700016" ll="SDBH:חרה:000002">te</a> <a name="01903700700016" ll="SDBH:חרה:000002">irrites</a> por causa do <a name="01903700700026">homem</a> que <a name="01903700700020" ll="SDBH:צלח:001003">prospera</a> em <a name="01903700700022" ll="SDBH:דֶּרֶךְ:000007">seu</a> <a name="01903700700022" ll="SDBH:דֶּרֶךְ:000007">caminho</a>,por causa do que <a name="01903700700028">leva</a> <a name="01903700700028">a</a> <a name="01903700700028">cabo</a> os seus <a name="01903700700030" ll="SDBH:מְזִמָּה:000000">maus</a> <a name="01903700700030" ll="SDBH:מְזִמָּה:000000">desígnios</a>.
    
\v 8 <a name="01903700800002" ll="SDBH:רפה:000007">Deixa</a> a <a name="01903700800006" ll="SDBH:אַף:000008">ira</a>, <a name="01903700800010" ll="SDBH:עזב:000006">abandona</a> o <a name="01903700800012" ll="SDBH:חֵמָה:001001">furor</a>;não <a name="01903700800016" ll="SDBH:חרה:000002">te</a> <a name="01903700800016" ll="SDBH:חרה:000002">impacientes</a>; <a name="01903700800018" ll="SDBH:אַךְ:000003">certamente</a>, isso acabará <a name="01903700800022" ll="SDBH:רעע:000006">mal</a>.
    
\v 9 Porque os <a name="01903700900004" ll="SDBH:רעע:000000">malfeitores</a> <a name="01903700900006" ll="SDBH:כרת:000002">serão</a> <a name="01903700900006" ll="SDBH:כרת:000002">exterminados</a>,mas os que <a name="01903700900010" ll="SDBH:קוה:000000">esperam</a> no <a name="01903700900012" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903700900016" ll="SDBH:ירשׁ:000002">possuirão</a> <a name="01903700900018" ll="SDBH:אֶרֶץ:000000">a</a> <a name="01903700900018" ll="SDBH:אֶרֶץ:000000">terra</a>.
    
\v 10 Mais <a name="01903701000006" ll="SDBH:מְעַט:000001">um</a> <a name="01903701000006" ll="SDBH:מְעַט:000001">pouco</a> <a name="01903701000006" ll="SDBH:מְעַט:000001">de</a> <a name="01903701000006" ll="SDBH:מְעַט:000001">tempo</a>, e já não existirá o <a name="01903701000012" ll="SDBH:רָשָׁע:000000">ímpio</a>;<a name="01903701000016" ll="SDBH:בין:000002">procurarás</a> o <a name="01903701000020" ll="SDBH:מָקֹום:000000">seu</a> <a name="01903701000020" ll="SDBH:מָקֹום:000000">lugar</a> e não o acharás.
    
\v 11 Mas os <a name="01903701100004" ll="SDBH:עָנָו:000000">mansos</a> <a name="01903701100006" ll="SDBH:ירשׁ:000002">herdarão</a> <a name="01903701100008" ll="SDBH:אֶרֶץ:000000">a</a> <a name="01903701100008" ll="SDBH:אֶרֶץ:000000">terra</a>e <a name="01903701100012" ll="SDBH:ענג:001000">se</a> <a name="01903701100012" ll="SDBH:ענג:001000">deleitarão</a> na <a name="01903701100016" ll="SDBH:רֹב:000001">abundância</a> de <a name="01903701100018" ll="SDBH:שָׁלֹום:000000">paz</a>.
    
\v 12 <a name="01903701200002" ll="SDBH:זמם:000000">Trama</a> o <a name="01903701200004" ll="SDBH:רָשָׁע:000000">ímpio</a> contra o <a name="01903701200010" ll="SDBH:צַדִּיק:000000">justo</a>e contra ele <a name="01903701200014" ll="SDBH:חרק:000000">ringe</a> os <a name="01903701200018" ll="SDBH:שֵׁן:000000">dentes</a>.
    
\v 13 <a name="01903701300004" ll="SDBH:שׂחק:000000">Rir</a>-<a name="01903701300004" ll="SDBH:שׂחק:000000">se</a>-<a name="01903701300004" ll="SDBH:שׂחק:000000">á</a> dele o <a name="01903701300002" ll="SDBH:אֲדֹנָי:000000">Senhor</a>,pois <a name="01903701300010" ll="SDBH:ראה:000000">vê</a> <a name="01903701300014">estar</a>-<a name="01903701300014">se</a> <a name="01903701300014">aproximando</a> o <a name="01903701300016" ll="SDBH:יֹום:000004">seu</a> <a name="01903701300016" ll="SDBH:יֹום:000004">dia</a>.
    
\v 14 Os <a name="01903701400006" ll="SDBH:רָשָׁע:000000">ímpios</a> <a name="01903701400004" ll="SDBH:פתח:000001">arrancam</a> da <a name="01903701400002" ll="SDBH:חֶרֶב:000001" il="NBGmk20130711a;NBGmk20130711b;NBGmk20130711c;StraightSword-02-003;adr1305240787;tb061616732">espada</a> e <a name="01903701400010" ll="SDBH:דרך:001000" il="HK00192;Realia:2.14.1b;image020a">distendem</a> o <a name="01903701400012" ll="SDBH:קֶשֶׁת:000000" il="Realia:2.14.1a;Realia:2.14.1b">arco</a>para <a name="01903701400016" ll="SDBH:נפל:000039">abater</a> o <a name="01903701400018" ll="SDBH:עָנִי:000000">pobre</a> e <a name="01903701400022" ll="SDBH:אֶבְיֹון:000000">necessitado</a>,para <a name="01903701400026" ll="SDBH:טבח:000001">matar</a> os que trilham o <a name="01903701400028" ll="SDBH:יָשָׁר:000004">reto</a> <a name="01903701400030" ll="SDBH:דֶּרֶךְ:000007">caminho</a>.
    
\v 15 A <a name="01903701500002" ll="SDBH:חֶרֶב:000001" il="NBGmk20130711a;NBGmk20130711b;NBGmk20130711c;StraightSword-02-003;adr1305240787;tb061616732">sua</a> <a name="01903701500002" ll="SDBH:חֶרֶב:000001" il="NBGmk20130711a;NBGmk20130711b;NBGmk20130711c;StraightSword-02-003;adr1305240787;tb061616732">espada</a>, porém, lhes <a name="01903701500004">traspassará</a> o <a name="01903701500008" ll="SDBH:לֵב:000000">próprio</a> <a name="01903701500008" ll="SDBH:לֵב:000000">coração</a>,e os <a name="01903701500012" ll="SDBH:קֶשֶׁת:000000" il="Realia:2.14.1a;Realia:2.14.1b">seus</a> <a name="01903701500012" ll="SDBH:קֶשֶׁת:000000" il="Realia:2.14.1a;Realia:2.14.1b">arcos</a> <a name="01903701500014" ll="SDBH:שׁבר:000000">serão</a> <a name="01903701500014" ll="SDBH:שׁבר:000000">espedaçados</a>.
    
\v 16 <a name="01903701600002" ll="SDBH:טֹוב:000002">Mais</a> <a name="01903701600002" ll="SDBH:טֹוב:000002">vale</a> <a name="01903701600004" ll="SDBH:מְעַט:000000">o</a> <a name="01903701600004" ll="SDBH:מְעַט:000000">pouco</a> do <a name="01903701600010" ll="SDBH:צַדִּיק:000000">justo</a>que a <a name="01903701600014" ll="SDBH:הָמֹון:000001">abundância</a> de <a name="01903701600018" ll="SDBH:רַב:000001" tl="HOTTP2204">muitos</a> <a name="01903701600016" ll="SDBH:רָשָׁע:000000" tl="HOTTP2204">ímpios</a>.
    
\v 17 Pois os <a name="01903701700004" ll="SDBH:זְרֹועַ:000000">braços</a> dos <a name="01903701700006" ll="SDBH:רָשָׁע:000000">ímpios</a> <a name="01903701700008" ll="SDBH:שׁבר:000000">serão</a> <a name="01903701700008" ll="SDBH:שׁבר:000000">quebrados</a>,mas os <a name="01903701700014" ll="SDBH:צַדִּיק:000000">justos</a>, o <a name="01903701700016" ll="SDBH:יהוה:000000">Senhor</a> os <a name="01903701700012" ll="SDBH:סמך:000008">sustém</a>.
    
\v 18 O <a name="01903701800004" ll="SDBH:יהוה:000000">Senhor</a> <a name="01903701800002">conhece</a> os <a name="01903701800006" ll="SDBH:יֹום:000004">dias</a> dos <a name="01903701800008" ll="SDBH:תָּמִים:000004">íntegros</a>;a <a name="01903701800012" ll="SDBH:נַחֲלָה:000000">herança</a> deles permanecerá para <a name="01903701800016" ll="SDBH:עֹולָם:000000">sempre</a>.
    
\v 19 Não <a name="01903701900004" ll="SDBH:בושׁ:000000">serão</a> <a name="01903701900004" ll="SDBH:בושׁ:000000">envergonhados</a> nos <a name="01903701900008" ll="SDBH:עֵת:000000">dias</a> do <a name="01903701900010" ll="SDBH:רָעָה:000002">mal</a>e nos <a name="01903701900016" ll="SDBH:יֹום:000003">dias</a> da <a name="01903701900018" ll="SDBH:רְעָבֹון:000000">fome</a> <a name="01903701900020" ll="SDBH:שׂבע:000000">se</a> <a name="01903701900020" ll="SDBH:שׂבע:000000">fartarão</a>.
    
\v 20 Os <a name="01903702000004" ll="SDBH:רָשָׁע:000000">ímpios</a>, no entanto, <a name="01903702000006" ll="SDBH:אבד:000004">perecerão</a>,e os <a name="01903702000010" ll="SDBH:איב:000001">inimigos</a> do <a name="01903702000012" ll="SDBH:יהוה:000000">Senhor</a> serão como o <a name="01903702000016" ll="SDBH:יָקָר:000000" tl="HOTTP2205">viço</a> das <a name="01903702000018" ll="SDBH:כַּר:000000" tl="HOTTP2205">pastagens</a>;<a name="01903702000020" ll="SDBH:כלה:000011">serão</a> <a name="01903702000020" ll="SDBH:כלה:000011">aniquilados</a> e <a name="01903702000028" ll="SDBH:כלה:000011">se</a> <a name="01903702000028" ll="SDBH:כלה:000011">desfarão</a> em <a name="01903702000026" ll="SDBH:עָשָׁן:000000" il="Smoke" tl="HOTTP2206">fumaça</a>.
    
