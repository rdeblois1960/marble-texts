\v 8 Os <a name="01907100800004" ll="SDBH:פֶּה:000000">meus</a> <a name="01907100800004" ll="SDBH:פֶּה:000000">lábios</a> <a name="01907100800002" ll="SDBH:מלא:000008">estão</a> <a name="01907100800002" ll="SDBH:מלא:000008">cheios</a> do <a name="01907100800006" ll="SDBH:תְּהִלָּה:000001">teu</a> <a name="01907100800006" ll="SDBH:תְּהִלָּה:000001">louvor</a>e da <a name="01907100800014" ll="SDBH:תִּפְאֶרֶת:000000">tua</a> <a name="01907100800014" ll="SDBH:תִּפְאֶרֶת:000000">glória</a> <a name="01907100800008;01907100800010;01907100800012" ll="SDBH:יֹום:000007">continuamente</a>.
    
\v 9 Não <a name="01907100900004" ll="SDBH:שׁלך:000003">me</a> <a name="01907100900004" ll="SDBH:שׁלך:000003">rejeites</a> na minha <a name="01907100900008;01907100900010" ll="SDBH:זִקְנָה:000000;SDBH:עֵת:000000">velhice</a>;quando <a name="01907100900016" ll="SDBH:כֹּחַ:000000">me</a> <a name="01907100900014" ll="SDBH:כלה:000000">faltarem</a> <a name="01907100900016" ll="SDBH:כֹּחַ:000000">as</a> <a name="01907100900016" ll="SDBH:כֹּחַ:000000">forças</a>, não <a name="01907100900020" ll="SDBH:עזב:000003">me</a> <a name="01907100900020" ll="SDBH:עזב:000003">desampares</a>.
    
\v 10 Pois <a name="01907101000004" ll="SDBH:אמר:000000" tl="HOTTP2359">falam</a> contra mim os <a name="01907101000006" ll="SDBH:איב:000001">meus</a> <a name="01907101000006" ll="SDBH:איב:000001">inimigos</a>;e os que <a name="01907101000014" ll="SDBH:נֶפֶשׁ:000001">me</a> <a name="01907101000012" ll="SDBH:שׁמר:000000">espreitam</a> <a name="01907101000014" ll="SDBH:נֶפֶשׁ:000001">a</a> <a name="01907101000014" ll="SDBH:נֶפֶשׁ:000001">alma</a> <a name="01907101000016" ll="SDBH:יעץ:000001">consultam</a> <a name="01907101000018">reunidos</a>,
    
\v 11 <a name="01907101100004" ll="SDBH:אמר:000000">dizendo</a>: <a name="01907101100006" ll="SDBH:אֱלֹהִים:000002">Deus</a> <a name="01907101100008" ll="SDBH:עזב:000003">o</a> <a name="01907101100008" ll="SDBH:עזב:000003">desamparou</a>;<a name="01907101100010" ll="SDBH:רדף:000000">persegui</a>-o e <a name="01907101100014" ll="SDBH:תפשׂ:000001">prendei</a>-<a name="01907101100014" ll="SDBH:תפשׂ:000001">o</a>,pois não há <a name="01907101100020" ll="SDBH:נצל:000005">quem</a> o <a name="01907101100020" ll="SDBH:נצל:000005">livre</a>.
    
\v 12 Não <a name="01907101200006;01907101200010" ll="SDBH:אֱלֹהִים:000001;SDBH:רחק:000002">te</a> <a name="01907101200006;01907101200010" ll="SDBH:אֱלֹהִים:000001;SDBH:רחק:000002">ausentes</a> de mim, ó <a name="01907101200002" ll="SDBH:אֱלֹהִים:000002">Deus</a>;<a name="01907101200006;01907101200010" ll="SDBH:אֱלֹהִים:000001;SDBH:רחק:000002">Deus</a> <a name="01907101200006;01907101200010" ll="SDBH:אֱלֹהִים:000001;SDBH:רחק:000002">meu</a>, <a name="01907101200018" ll="SDBH:חושׁ:000001">apressa</a>-<a name="01907101200018" ll="SDBH:חושׁ:000001">te</a> em <a name="01907101200014" ll="SDBH:עֶזְרָה:000000">socorrer</a>-<a name="01907101200014" ll="SDBH:עֶזְרָה:000000">me</a>.
    
\v 13 <a name="01907101300002" ll="SDBH:בושׁ:000000">Sejam</a> <a name="01907101300002" ll="SDBH:בושׁ:000000">envergonhados</a> e <a name="01907101300004" ll="SDBH:כלה:000011" tl="HOTTP2360">consumidos</a><a name="01907101300006" ll="SDBH:שׂטן:000000">os</a> que são <a name="01907101300006" ll="SDBH:שׂטן:000000">adversários</a> <a name="01907101300008" ll="SDBH:נֶפֶשׁ:000001">de</a> <a name="01907101300008" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01907101300008" ll="SDBH:נֶפֶשׁ:000001">alma</a>;<a name="01907101300010" ll="SDBH:עטה:000001">cubram</a>-<a name="01907101300010" ll="SDBH:עטה:000001">se</a> de <a name="01907101300012" ll="SDBH:חֶרְפָּה:000000">opróbrio</a> e de <a name="01907101300016" ll="SDBH:כְּלִמָּה:000000" tl="HOTTP2361">vexame</a>os que <a name="01907101300018" ll="SDBH:בקשׁ:000007">procuram</a> o <a name="01907101300020" ll="SDBH:רָעָה:000002">mal</a> <a name="01907101300020" ll="SDBH:רָעָה:000002">contra</a> <a name="01907101300020" ll="SDBH:רָעָה:000002">mim</a>.
    
\v 14 Quanto a mim, <a name="01907101400008" ll="SDBH:יחל:000000">esperarei</a> <a name="01907101400006" ll="SDBH:תָּמִיד:000000">sempre</a>e <a name="01907101400018" ll="SDBH:תְּהִלָּה:000000">te</a> <a name="01907101400018" ll="SDBH:תְּהִלָּה:000000">louvarei</a> <a name="01907101400012">mais</a> e mais.
    
\v 15 A <a name="01907101500002" ll="SDBH:פֶּה:000000">minha</a> <a name="01907101500002" ll="SDBH:פֶּה:000000">boca</a> <a name="01907101500004" ll="SDBH:ספר:000006">relatará</a> a <a name="01907101500006" ll="SDBH:צְדָקָה:000001">tua</a> <a name="01907101500006" ll="SDBH:צְדָקָה:000001">justiça</a>e <a name="01907101500008;01907101500012" ll="SDBH:יֹום:000007">de</a> <a name="01907101500008;01907101500012" ll="SDBH:יֹום:000007">contínuo</a> os feitos da <a name="01907101500014" ll="SDBH:תְּשׁוּעָה:000000">tua</a> <a name="01907101500014" ll="SDBH:תְּשׁוּעָה:000000">salvação</a>,ainda que eu não <a name="01907101500020" tl="HOTTP2362">saiba</a> o seu <a name="01907101500022" ll="SDBH:סְפֹרֹות:000000" tl="HOTTP2362">número</a>.
    
\v 16 <a name="01907101600002">Sinto</a>-<a name="01907101600002">me</a> na <a name="01907101600006" ll="SDBH:גְּבוּרָה:000002">força</a> do <a name="01907101600008;01907101600010" ll="SDBH:אֲדֹנָי:000000;SDBH:יהוה:000000">Senhor</a> <a name="01907101600008;01907101600010" ll="SDBH:אֲדֹנָי:000000;SDBH:יהוה:000000">Deus</a>;e <a name="01907101600012" ll="SDBH:זכר:000002">rememoro</a> a <a name="01907101600014" ll="SDBH:צְדָקָה:000001">tua</a> <a name="01907101600014" ll="SDBH:צְדָקָה:000001">justiça</a>, a tua somente.
    
\v 17 <a name="01907101700004" ll="SDBH:למד:000001">Tu</a> <a name="01907101700004" ll="SDBH:למד:000001">me</a> <a name="01907101700004" ll="SDBH:למד:000001">tens</a> <a name="01907101700004" ll="SDBH:למד:000001">ensinado</a>, ó <a name="01907101700002" ll="SDBH:אֱלֹהִים:000002">Deus</a>, desde a <a name="01907101700008" ll="SDBH:נְעוּרִים:000000">minha</a> <a name="01907101700008" ll="SDBH:נְעוּרִים:000000">mocidade</a>;e até <a name="01907101700014">agora</a> <a name="01907101700016" ll="SDBH:נגד:000000">tenho</a> <a name="01907101700016" ll="SDBH:נגד:000000">anunciado</a> as <a name="01907101700018" ll="SDBH:פלא:000000">tuas</a> <a name="01907101700018" ll="SDBH:פלא:000000">maravilhas</a>.
    
\v 18 Não <a name="01907101800018" ll="SDBH:עזב:000003">me</a> <a name="01907101800018" ll="SDBH:עזב:000003">desampares</a>, pois, ó <a name="01907101800014" ll="SDBH:אֱלֹהִים:000002">Deus</a>,<a name="01907101800006" ll="SDBH:עַד:000002">até</a> à minha <a name="01907101800008" ll="SDBH:זִקְנָה:000000">velhice</a> e às <a name="01907101800012" ll="SDBH:שֵׂיבָה:000000">cãs</a>;até que <a name="01907101800022" ll="SDBH:נגד:000000">eu</a> <a name="01907101800022" ll="SDBH:נגד:000000">tenha</a> <a name="01907101800022" ll="SDBH:נגד:000000">declarado</a> à presente <a name="01907101800028" ll="SDBH:דֹּור:001000" tl="HOTTP2363">geração</a> a <a name="01907101800024" ll="SDBH:זְרֹועַ:000001">tua</a> <a name="01907101800024" ll="SDBH:זְרֹועַ:000001">força</a>e às <a name="01907101800034" tl="HOTTP2363">vindouras</a> o <a name="01907101800036" ll="SDBH:גְּבוּרָה:000000">teu</a> <a name="01907101800036" ll="SDBH:גְּבוּרָה:000000">poder</a>.
    
\v 19 Ora, a <a name="01907101900004" ll="SDBH:צְדָקָה:000001">tua</a> <a name="01907101900004" ll="SDBH:צְדָקָה:000001">justiça</a>, ó <a name="01907101900006" ll="SDBH:אֱלֹהִים:000002">Deus</a>, se eleva até aos <a name="01907101900010" ll="SDBH:מָרֹום:000001">céus</a>.<a name="01907101900016" ll="SDBH:גָּדֹול:000003">Grandes</a> coisas <a name="01907101900014">tens</a> <a name="01907101900014">feito</a>, ó <a name="01907101900018" ll="SDBH:אֱלֹהִים:000002">Deus</a>;quem é semelhante a ti?
    
\v 20 Tu, que <a name="01907102000006" ll="SDBH:ראה:000003">me</a> <a name="01907102000006" ll="SDBH:ראה:000003">tens</a> <a name="01907102000006" ll="SDBH:ראה:000003">feito</a> <a name="01907102000006" ll="SDBH:ראה:000003">ver</a> <a name="01907102000010" ll="SDBH:רַב:000001">muitas</a> <a name="01907102000008" ll="SDBH:צָרָה:000000">angústias</a> e <a name="01907102000014" ll="SDBH:רָעָה:000002">males</a>,<a name="01907102000020" ll="SDBH:חיה:000002">me</a> <a name="01907102000020" ll="SDBH:חיה:000002">restaurarás</a> <a name="01907102000016" ll="SDBH:שׁוב:000041">ainda</a> <a name="01907102000020" ll="SDBH:חיה:000002">a</a> <a name="01907102000020" ll="SDBH:חיה:000002">vida</a>e <a name="01907102000032" ll="SDBH:שׁוב:000041">de</a> <a name="01907102000032" ll="SDBH:שׁוב:000041">novo</a> <a name="01907102000034" ll="SDBH:עלה:000005">me</a> <a name="01907102000034" ll="SDBH:עלה:000005">tirarás</a> dos <a name="01907102000026" ll="SDBH:תְּהֹום:000000">abismos</a> da <a name="01907102000030" ll="SDBH:אֶרֶץ:000000">terra</a>.
    
\v 21 <a name="01907102100002">Aumenta</a> a <a name="01907102100004" ll="SDBH:גְּדוּלָּה:000000">minha</a> <a name="01907102100004" ll="SDBH:גְּדוּלָּה:000000">grandeza</a>,<a name="01907102100010" ll="SDBH:נחם:000000">conforta</a>-<a name="01907102100010" ll="SDBH:נחם:000000">me</a> <a name="01907102100008">novamente</a>.
    
\v 22 Eu também <a name="01907102200006" ll="SDBH:ידה:000002">te</a> <a name="01907102200006" ll="SDBH:ידה:000002">louvo</a> com <a name="01907102200010;01907102200012" ll="SDBH:כְּלִי:000000;SDBH:נֵבֶל:001000" il="BK00175;BK00176;Exile_Lachish;NBGmk20131114;adr1306213109;tb072705188">a</a> <a name="01907102200010;01907102200012" ll="SDBH:כְּלִי:000000;SDBH:נֵבֶל:001000" il="BK00175;BK00176;Exile_Lachish;NBGmk20131114;adr1306213109;tb072705188">lira</a>,celebro a <a name="01907102200014" ll="SDBH:אֱמֶת:000000">tua</a> <a name="01907102200014" ll="SDBH:אֱמֶת:000000">verdade</a>, ó <a name="01907102200016" ll="SDBH:אֱלֹהִים:000001">meu</a> <a name="01907102200016" ll="SDBH:אֱלֹהִים:000001">Deus</a>;<a name="01907102200018" ll="SDBH:זמר:000000" il="HK00188">cantar</a>-te-<a name="01907102200018" ll="SDBH:זמר:000000" il="HK00188">ei</a> salmos na <a name="01907102200024" ll="SDBH:כִּנֹּור:000000" il="BK00175;BK00176;Exile_Lachish;HK00188;NBGmk20131114;adr1306213109;tb072705188">harpa</a>,ó <a name="01907102200026" ll="SDBH:קָדֹושׁ:000000">Santo</a> de <a name="01907102200028" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>.
    
\v 23 Os <a name="01907102300004" ll="SDBH:שָׂפָה:000001">meus</a> <a name="01907102300004" ll="SDBH:שָׂפָה:000001">lábios</a> <a name="01907102300002" ll="SDBH:רנן:000000">exultarão</a>quando <a name="01907102300008" ll="SDBH:זמר:000000" il="HK00188">eu</a> te <a name="01907102300008" ll="SDBH:זמר:000000" il="HK00188">salmodiar</a>;também exultará a <a name="01907102300014" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01907102300014" ll="SDBH:נֶפֶשׁ:000001">alma</a>, que <a name="01907102300018" ll="SDBH:פדה:000002">remiste</a>.
    
\v 24 Igualmente a <a name="01907102400004" ll="SDBH:לָשֹׁון:000000">minha</a> <a name="01907102400004" ll="SDBH:לָשֹׁון:000000">língua</a> <a name="01907102400012" ll="SDBH:הגה:000003">celebrará</a> a <a name="01907102400014" ll="SDBH:צְדָקָה:000001">tua</a> <a name="01907102400014" ll="SDBH:צְדָקָה:000001">justiça</a> <a name="01907102400006;01907102400010" ll="SDBH:יֹום:000007">todo</a> o <a name="01907102400006;01907102400010" ll="SDBH:יֹום:000007">dia</a>;pois <a name="01907102400018" ll="SDBH:בושׁ:000000">estão</a> <a name="01907102400018" ll="SDBH:בושׁ:000000">envergonhados</a> e <a name="01907102400022" ll="SDBH:חפר:001000">confundidos</a>os que <a name="01907102400024" ll="SDBH:בקשׁ:000007">procuram</a> o <a name="01907102400026" ll="SDBH:רָעָה:000002">mal</a> <a name="01907102400026" ll="SDBH:רָעָה:000002">contra</a> <a name="01907102400026" ll="SDBH:רָעָה:000002">mim</a>.
