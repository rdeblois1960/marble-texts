\v 20 Eles <a name="01913902000004" ll="SDBH:אמר:000000" tl="HOTTP2605">se</a> <a name="01913902000004" ll="SDBH:אמר:000000" tl="HOTTP2605">rebelam</a> <a name="01913902000008" ll="SDBH:מְזִמָּה:000000">insidiosamente</a> <a name="01913902000004" ll="SDBH:אמר:000000" tl="HOTTP2605">contra</a> <a name="01913902000004" ll="SDBH:אמר:000000" tl="HOTTP2605">ti</a>e como <a name="01913902000018" ll="SDBH:עָר:000000" tl="HOTTP2606">teus</a> <a name="01913902000018" ll="SDBH:עָר:000000" tl="HOTTP2606">inimigos</a> <a name="01913902000010" ll="SDBH:נשׂא:000048" tl="HOTTP2606">falam</a> <a name="01913902000016" ll="SDBH:שָׁוְא:000000" tl="HOTTP2606">malícia</a>.
    
\v 21 Não <a name="01913902100010" ll="SDBH:שׂנא:000000">aborreço</a> <a name="01913902100010" ll="SDBH:שׂנא:000000">eu</a>, <a name="01913902100008" ll="SDBH:יהוה:000000">Senhor</a>, os <a name="01913902100006" ll="SDBH:שׂנא:000000">que</a> <a name="01913902100006" ll="SDBH:שׂנא:000000">te</a> <a name="01913902100006" ll="SDBH:שׂנא:000000">aborrecem</a>?E não <a name="01913902100018" ll="SDBH:קוט:000000">abomino</a> os que <a name="01913902100016" ll="SDBH:תְּקֹומֵם:000000">contra</a> <a name="01913902100016" ll="SDBH:תְּקֹומֵם:000000">ti</a> <a name="01913902100016" ll="SDBH:תְּקֹומֵם:000000">se</a> <a name="01913902100016" ll="SDBH:תְּקֹומֵם:000000">levantam</a>?
    
\v 22 <a name="01913902200006" ll="SDBH:שׂנא:000000">Aborreço</a>-<a name="01913902200006" ll="SDBH:שׂנא:000000">os</a> com <a name="01913902200004" ll="SDBH:שִׂנְאָה:000000">ódio</a> <a name="01913902200002" ll="SDBH:תַּכְלִית:000002">consumado</a>;para mim são <a name="01913902200010" ll="SDBH:איב:000001">inimigos</a> de fato.
    
\v 23 <a name="01913902300002" ll="SDBH:חקר:000000">Sonda</a>-<a name="01913902300002" ll="SDBH:חקר:000000">me</a>, ó <a name="01913902300004" ll="SDBH:אֵל:000001">Deus</a>, e <a name="01913902300008">conhece</a> o <a name="01913902300010" ll="SDBH:לֵבָב:000001">meu</a> <a name="01913902300010" ll="SDBH:לֵבָב:000001">coração</a>,<a name="01913902300012" ll="SDBH:בחן:000000">prova</a>-<a name="01913902300012" ll="SDBH:בחן:000000">me</a> e <a name="01913902300016">conhece</a> os <a name="01913902300018" ll="SDBH:שַׂרְעַפִּים:000000">meus</a> <a name="01913902300018" ll="SDBH:שַׂרְעַפִּים:000000">pensamentos</a>;
    
\v 24 <a name="01913902400004" ll="SDBH:ראה:000000">vê</a> se há em mim algum <a name="01913902400008" ll="SDBH:דֶּרֶךְ:000007">caminho</a> <a name="01913902400010" ll="SDBH:עֹצֶב:000001">mau</a>e <a name="01913902400016" ll="SDBH:נחה:000000">guia</a>-me pelo <a name="01913902400020" ll="SDBH:דֶּרֶךְ:000007">caminho</a> <a name="01913902400022" ll="SDBH:עֹולָם:000000">eterno</a>.
\c 140
\v 2 (1) <a name="01914000200002" ll="SDBH:חלץ:000002">Livra</a>-<a name="01914000200002" ll="SDBH:חלץ:000002">me</a>, <a name="01914000200004" ll="SDBH:יהוה:000000">Senhor</a>, do <a name="01914000200008" ll="SDBH:אָדָם:000000">homem</a> <a name="01914000200010" ll="SDBH:רַע:000001">perverso</a>,<a name="01914000200018" ll="SDBH:נצר:000007">guarda</a>-<a name="01914000200018" ll="SDBH:נצר:000007">me</a> do <a name="01914000200014">homem</a> <a name="01914000200016" ll="SDBH:חָמָס:000000">violento</a>,
    
\v 3 (2) cujo <a name="01914000300010" ll="SDBH:לֵב:000001">coração</a> <a name="01914000300004" ll="SDBH:חשׁב:000001">maquina</a> <a name="01914000200010;01914000300006" ll="SDBH:רַע:000001;SDBH:רָעָה:000000">iniquidades</a>e <a name="01914000300012;01914000300014" ll="SDBH:יֹום:000007">vive</a> <a name="01914000300016" ll="SDBH:גור:001000">forjando</a> <a name="01914000300018" ll="SDBH:מִלְחָמָה:000000">contendas</a>.
    
\v 4 (3) <a name="01914000400002" ll="SDBH:שׁנן:000000">Aguçam</a> a <a name="01914000400004" ll="SDBH:לָשֹׁון:000000">língua</a> como a <a name="01914000400008" ll="SDBH:נָחָשׁ:000000" il="ArbeLevy-Vipera_palaestinae_4;Fauna:4.9a;Fauna:4.9b">serpente</a>;sob <a name="01914000400016" ll="SDBH:שָׂפָה:000001">os</a> <a name="01914000400016" ll="SDBH:שָׂפָה:000001">lábios</a> têm <a name="01914000400010" ll="SDBH:חֵמָה:000000">veneno</a> de <a name="01914000400012" ll="SDBH:עַכְשׁוּב:000000">áspide</a>.
    
\v 5 (4) <a name="01914000500002" ll="SDBH:שׁמר:000007">Guarda</a>-<a name="01914000500002" ll="SDBH:שׁמר:000007">me</a>, <a name="01914000500004" ll="SDBH:יהוה:000000">Senhor</a>, da <a name="01914000500008">mão</a> dos <a name="01914000500010" ll="SDBH:רָשָׁע:000000">ímpios</a>,<a name="01914000500018" ll="SDBH:נצר:000007">preserva</a>-<a name="01914000500018" ll="SDBH:נצר:000007">me</a> do <a name="01914000500014">homem</a> <a name="01914000500016" ll="SDBH:חָמָס:000000">violento</a>,os quais <a name="01914000500022" ll="SDBH:חשׁב:000001">se</a> <a name="01914000500022" ll="SDBH:חשׁב:000001">empenham</a> por <a name="01914000500028">me</a> <a name="01914000500026" ll="SDBH:דחה:000000">desviar</a> <a name="01914000500028">os</a> <a name="01914000500028">passos</a>.
    
\v 6 (5) Os <a name="01914000600004" ll="SDBH:גֵּאֶה:000000">soberbos</a> <a name="01914000600002" ll="SDBH:טמן:000001">ocultaram</a> <a name="01914000600006" ll="SDBH:פַּח:000000">armadilhas</a> e <a name="01914000600012" ll="SDBH:חֶבֶל:000001" tl="HOTTP2607">cordas</a> contra mim,<a name="01914000600014" ll="SDBH:פרשׂ:000000">estenderam</a>-me uma <a name="01914000600016" ll="SDBH:רֶשֶׁת:000000" il="LB00209;LB00210;Realia:1.3.1.1a;Realia:1.3.1.1b;Realia:1.3.1.1c;Realia:1.4.2;mat00691" tl="HOTTP2608">rede</a> à <a name="01914000600020" ll="SDBH:יָד:000026">beira</a> <a name="01914000600022" ll="SDBH:מַעְגָּל:000001">do</a> <a name="01914000600022" ll="SDBH:מַעְגָּל:000001">caminho</a>,<a name="01914000600026" ll="SDBH:שׁית:000001">armaram</a> <a name="01914000600024" ll="SDBH:מֹוקֵשׁ:000000">ciladas</a> contra mim.
    
\v 7 (6) <a name="01914000700002" ll="SDBH:אמר:000000">Digo</a> ao <a name="01914000700006" ll="SDBH:יהוה:000000">Senhor</a>: tu és o <a name="01914000700008" ll="SDBH:אֵל:000001">meu</a> <a name="01914000700008" ll="SDBH:אֵל:000001">Deus</a>;<a name="01914000700012" ll="SDBH:אזן:000000">acode</a>, <a name="01914000700014" ll="SDBH:יהוה:000000">Senhor</a>, à <a name="01914000700016">voz</a> <a name="01914000700018" ll="SDBH:תַּחֲנוּן:000000">das</a> <a name="01914000700018" ll="SDBH:תַּחֲנוּן:000000">minhas</a> <a name="01914000700018" ll="SDBH:תַּחֲנוּן:000000">súplicas</a>.
    
\v 8 (7) Ó <a name="01914000800002;01914000800004" ll="SDBH:אֲדֹנָי:000000;SDBH:יהוה:000000">Senhor</a>, <a name="01914000800006" ll="SDBH:עֹז:000002">força</a> <a name="01914000800008" ll="SDBH:יְשׁוּעָה:000000">da</a> <a name="01914000800008" ll="SDBH:יְשׁוּעָה:000000">minha</a> <a name="01914000800008" ll="SDBH:יְשׁוּעָה:000000">salvação</a>,<a name="01914000800010" ll="SDBH:סכך:000001">tu</a> <a name="01914000800014" ll="SDBH:רֹאשׁ:000000">me</a> <a name="01914000800010" ll="SDBH:סכך:000001">protegeste</a> <a name="01914000800014" ll="SDBH:רֹאשׁ:000000">a</a> <a name="01914000800014" ll="SDBH:רֹאשׁ:000000">cabeça</a> no <a name="01914000800018" ll="SDBH:יֹום:000003">dia</a> da <a name="01914000800020" ll="SDBH:נֶשֶׁק:000002">batalha</a>.
    
\v 9 (8) Não <a name="01914000900004">concedas</a>, <a name="01914000900006" ll="SDBH:יהוה:000000">Senhor</a>, ao <a name="01914000900010" ll="SDBH:רָשָׁע:000000">ímpio</a> os seus <a name="01914000900008" ll="SDBH:מַאֲוַי:000000">desejos</a>;não <a name="01914000900016" ll="SDBH:פוק:001002" tl="HOTTP2609">permitas</a> que vingue o <a name="01914000900012" ll="SDBH:זְמָם:000000" tl="HOTTP2609">seu</a> mau <a name="01914000900012" ll="SDBH:זְמָם:000000" tl="HOTTP2609">propósito</a>.
    
\v 10 (9) Se exaltam a <a name="01914001000002" ll="SDBH:רֹאשׁ:000000" tl="HOTTP2609">cabeça</a> os que <a name="01914001000004" tl="HOTTP2609">me</a> <a name="01914001000004" tl="HOTTP2609">cercam</a>,<a name="01914001000012" ll="SDBH:כסה:000009">cubra</a>-<a name="01914001000012" ll="SDBH:כסה:000009">os</a> a <a name="01914001000006" ll="SDBH:עָמָל:000001">maldade</a> <a name="01914001000008" ll="SDBH:שָׂפָה:000001">dos</a> <a name="01914001000008" ll="SDBH:שָׂפָה:000001">seus</a> <a name="01914001000008" ll="SDBH:שָׂפָה:000001">lábios</a>.
    
\v 11 (10) <a name="01914001100002;01914001100004" ll="SDBH:מוט:000004">Caiam</a> sobre eles <a name="01914001100008" ll="SDBH:גֶּחָל:000000" il="Burningcoal">brasas</a> <a name="01914001100008" ll="SDBH:גֶּחָל:000000" il="Burningcoal">vivas</a>, <a name="01914001100016" ll="SDBH:נפל:000001">sejam</a> <a name="01914001100016" ll="SDBH:נפל:000001">atirados</a> ao <a name="01914001100014" ll="SDBH:אֵשׁ:000000" il="Fire02">fogo</a>,lançados em <a name="01914001100020" ll="SDBH:מַהֲמֹר:000000">abismos</a> para que não mais <a name="01914001100024">se</a> <a name="01914001100024">levantem</a>.
    
\v 12 (11) O <a name="01914001200002;01914001200004" ll="SDBH:לָשֹׁון:000006">caluniador</a> não <a name="01914001200008" ll="SDBH:כון:000024">se</a> <a name="01914001200008" ll="SDBH:כון:000024">estabelecerá</a> na <a name="01914001200014" ll="SDBH:אֶרֶץ:000000">terra</a>;ao <a name="01914001200016">homem</a> <a name="01914001200018" ll="SDBH:חָמָס:000000">violento</a>, o <a name="01914001200020" ll="SDBH:רַע:000005">mal</a> <a name="01914001200022" ll="SDBH:צוד:000000">o</a> <a name="01914001200022" ll="SDBH:צוד:000000">perseguirá</a> com <a name="01914001200026" ll="SDBH:מַדְחֵפָה:000000">golpe</a> <a name="01914001200026" ll="SDBH:מַדְחֵפָה:000000">sobre</a> <a name="01914001200026" ll="SDBH:מַדְחֵפָה:000000">golpe</a>.
    
\v 13 (12) <a name="01914001300004">Sei</a> que o <a name="01914001300010" ll="SDBH:יהוה:000000">Senhor</a> <a name="01914001300008">manterá</a> a <a name="01914001300012" ll="SDBH:דִּין:000001">causa</a> do <a name="01914001300014" ll="SDBH:עָנִי:000000">oprimido</a>e o <a name="01914001300016" ll="SDBH:מִשְׁפָּט:000003">direito</a> do <a name="01914001300018" ll="SDBH:אֶבְיֹון:000000">necessitado</a>.
    
