\v 6 <a name="01912800600004" ll="SDBH:ראה:000000">vejas</a> <a name="01912800600006" ll="SDBH:בֵּן:000000">os</a> <a name="01912800600006" ll="SDBH:בֵּן:000000">filhos</a> de <a name="01912800600010" ll="SDBH:בֵּן:000000">teus</a> <a name="01912800600010" ll="SDBH:בֵּן:000000">filhos</a>.<a name="01912800600012" ll="SDBH:שָׁלֹום:000000">Paz</a> sobre <a name="01912800600016" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a>!
\c 129
\v 1 <a name="01912900100008" ll="SDBH:רַב:000006">Muitas</a> <a name="01912900100008" ll="SDBH:רַב:000006">vezes</a> <a name="01912900100010" ll="SDBH:צרר:000008">me</a> <a name="01912900100010" ll="SDBH:צרר:000008">angustiaram</a> desde a <a name="01912900100014" ll="SDBH:נְעוּרִים:000000">minha</a> <a name="01912900100014" ll="SDBH:נְעוּרִים:000000">mocidade</a>,<a name="01912900100020" ll="SDBH:יִשְׂרָאֵל:000000">Israel</a> que o <a name="01912900100016" ll="SDBH:אמר:000000">diga</a>;
    
\v 2 desde a <a name="01912900200008" ll="SDBH:נְעוּרִים:000000">minha</a> <a name="01912900200008" ll="SDBH:נְעוּרִים:000000">mocidade</a>, <a name="01912900200004" ll="SDBH:צרר:000008">me</a> <a name="01912900200004" ll="SDBH:צרר:000008">angustiaram</a>,todavia, não <a name="01912900200014" ll="SDBH:יכל:000002">prevaleceram</a> contra mim.
    
\v 3 Sobre o <a name="01912900300004" ll="SDBH:גַּב:000001">meu</a> <a name="01912900300004" ll="SDBH:גַּב:000001">dorso</a> <a name="01912900300006" ll="SDBH:חרשׁ:000001" il="NBGpb20140210">lavraram</a> os <a name="01912900300008" ll="SDBH:חרשׁ:000001" il="NBGpb20140210">aradores</a>;nele <a name="01912900300010" ll="SDBH:ארך:000003">abriram</a> <a name="01912900300010" ll="SDBH:ארך:000003">longos</a> <a name="01912900300018">sulcos</a>.
    
\v 4 Mas o <a name="01912900400002" ll="SDBH:יהוה:000000">Senhor</a> é <a name="01912900400004" ll="SDBH:צַדִּיק:000000">justo</a>;<a name="01912900400006" ll="SDBH:קצץ:000003">cortou</a> as <a name="01912900400008" ll="SDBH:עֲבֹת:000000" il="Realia:1.14">cordas</a> <a name="01912900400010" ll="SDBH:רָשָׁע:000000">dos</a> <a name="01912900400010" ll="SDBH:רָשָׁע:000000">ímpios</a>.
    
\v 5 <a name="01912900500002" ll="SDBH:בושׁ:000000">Sejam</a> <a name="01912900500002" ll="SDBH:בושׁ:000000">envergonhados</a> e <a name="01912900500006;01912900500008" ll="SDBH:אָחֹור:000001;SDBH:סוג:000000">repelidos</a>todos os que <a name="01912900500012" ll="SDBH:שׂנא:000001">aborrecem</a> a <a name="01912900500014" ll="SDBH:צִיֹּון:000000" il="tb060716478">Sião</a>!
    
\v 6 Sejam como a <a name="01912900600006" ll="SDBH:חָצִיר:000001" il="Flora:5.1.2">erva</a> dos <a name="01912900600008" ll="SDBH:גָּג:000000">telhados</a>,que <a name="01912900600016" ll="SDBH:יבשׁ:000000">seca</a> <a name="01912900600012" ll="SDBH:קַדְמָה:000002">antes</a> de <a name="01912900600014" ll="SDBH:שׁלף:000000">florescer</a>,
    
\v 7 com a qual não <a name="01912900700006" ll="SDBH:מלא:000002">enche</a> a <a name="01912900700008" ll="SDBH:כַּף:000000">mão</a> o <a name="01912900700010" ll="SDBH:קצר:000006" il="Bolen00107;Flora:3.1.1a;LB00102;Stooks_of_barley_in_West_Somerset">ceifeiro</a>,nem os <a name="01912900700014" ll="SDBH:חֹצֶן:000000">braços</a>, o <a name="01912900700016" ll="SDBH:עמר:000000">que</a> <a name="01912900700016" ll="SDBH:עמר:000000">ata</a> os <a name="01912900700016" ll="SDBH:עמר:000000">feixes</a>!
    
\v 8 E também os <a name="01912900800010" ll="SDBH:עבר:000000">que</a> <a name="01912900800010" ll="SDBH:עבר:000000">passam</a> não <a name="01912900800006" ll="SDBH:אמר:000000">dizem</a>:A <a name="01912900800012" ll="SDBH:בְּרָכָה:000000">bênção</a> <a name="01912900800014" ll="SDBH:יהוה:000000">do</a> <a name="01912900800014" ll="SDBH:יהוה:000000">Senhor</a> seja convosco!<a name="01912900800018" ll="SDBH:ברך:001001">Nós</a> vos <a name="01912900800018" ll="SDBH:ברך:001001">abençoamos</a> em <a name="01912900800024" ll="SDBH:שֵׁם:000001">nome</a> do <a name="01912900800026" ll="SDBH:יהוה:000000">Senhor</a>!
\c 130
