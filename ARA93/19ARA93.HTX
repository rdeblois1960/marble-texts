\v 43 (42) <a name="01908904300002" ll="SDBH:רום:000032">Exaltaste</a> a <a name="01908904300004" ll="SDBH:יָמִין:000000">destra</a> <a name="01908904300006" ll="SDBH:צַר:000003">dos</a> <a name="01908904300006" ll="SDBH:צַר:000003">seus</a> <a name="01908904300006" ll="SDBH:צַר:000003">adversários</a>e <a name="01908904300008" ll="SDBH:שׂמח:000002">deste</a> <a name="01908904300008" ll="SDBH:שׂמח:000002">regozijo</a> a todos os <a name="01908904300012" ll="SDBH:איב:000001">seus</a> <a name="01908904300012" ll="SDBH:איב:000001">inimigos</a>.
    
\v 44 (43) Também <a name="01908904400004" ll="SDBH:שׁוב:000051" tl="HOTTP2454">viraste</a> o <a name="01908904400006" ll="SDBH:צֹר:000002;SDBH:צוּר:002000" tl="HOTTP2454">fio</a> <a name="01908904400008" ll="SDBH:חֶרֶב:000001" il="NBGmk20130711a;NBGmk20130711b;NBGmk20130711c;StraightSword-02-003;adr1305240787;tb061616732" tl="HOTTP2454">da</a> <a name="01908904400008" ll="SDBH:חֶרֶב:000001" il="NBGmk20130711a;NBGmk20130711b;NBGmk20130711c;StraightSword-02-003;adr1305240787;tb061616732" tl="HOTTP2454">sua</a> <a name="01908904400008" ll="SDBH:חֶרֶב:000001" il="NBGmk20130711a;NBGmk20130711b;NBGmk20130711c;StraightSword-02-003;adr1305240787;tb061616732" tl="HOTTP2454">espada</a>e não <a name="01908904400014">o</a> <a name="01908904400014">sustentaste</a> na <a name="01908904400020" ll="SDBH:מִלְחָמָה:000000">batalha</a>.
    
\v 45 (44) <a name="01908904500002" ll="SDBH:שׁבת:000003">Fizeste</a> <a name="01908904500002" ll="SDBH:שׁבת:000003">cessar</a> o <a name="01908904500006" ll="SDBH:טָהֹור:000004" tl="HOTTP2455">seu</a> <a name="01908904500006" ll="SDBH:טָהֹור:000004" tl="HOTTP2455">esplendor</a>e <a name="01908904500018" ll="SDBH:מגר:000000">deitaste</a> por <a name="01908904500016" ll="SDBH:אֶרֶץ:000000">terra</a> o <a name="01908904500010" ll="SDBH:כִּסֵּא:000002" il="Darius_In_Parse;Roman_throne;Throne_Greek;Throne_Toutankamon;mb120901102">seu</a> <a name="01908904500010" ll="SDBH:כִּסֵּא:000002" il="Darius_In_Parse;Roman_throne;Throne_Greek;Throne_Toutankamon;mb120901102">trono</a>.
    
\v 46 (45) <a name="01908904600002" ll="SDBH:קצר:000002">Abreviaste</a> os <a name="01908904600004" ll="SDBH:יֹום:000004">dias</a> <a name="01908904600006" ll="SDBH:עֲלוּמִים:000000">da</a> <a name="01908904600006" ll="SDBH:עֲלוּמִים:000000">sua</a> <a name="01908904600006" ll="SDBH:עֲלוּמִים:000000">mocidade</a>e o <a name="01908904600008" ll="SDBH:עטה:000003">cobriste</a> de <a name="01908904600012" ll="SDBH:בּוּשָׁה:000000">ignomínia</a>.
    
