\v 15 Mas o <a name="01909401500010" ll="SDBH:מִשְׁפָּט:000007" tl="HOTTP2474">juízo</a> <a name="01909401500008" ll="SDBH:שׁוב:000041" tl="HOTTP2474">se</a> <a name="01909401500008" ll="SDBH:שׁוב:000041" tl="HOTTP2474">converterá</a> em <a name="01909401500006" ll="SDBH:צֶדֶק:000001" tl="HOTTP2474">justiça</a>,e <a name="01909401500014" ll="SDBH:אַחַר:001001">segui</a>-<a name="01909401500014" ll="SDBH:אַחַר:001001">la</a>-<a name="01909401500014" ll="SDBH:אַחַר:001001">ão</a> todos os <a name="01909401500020" ll="SDBH:לֵב:000001">de</a> <a name="01909401500020" ll="SDBH:לֵב:000001">coração</a> <a name="01909401500018" ll="SDBH:יָשָׁר:000004">reto</a>.
    
\v 16 Quem <a name="01909401600004">se</a> <a name="01909401600004">levantará</a> a meu favor, contra os <a name="01909401600010" ll="SDBH:רעע:000000">perversos</a>?Quem <a name="01909401600014" ll="SDBH:יצב:000000">estará</a> comigo contra os que <a name="01909401600020" ll="SDBH:פעל:000000">praticam</a> a <a name="01909401600022" ll="SDBH:אָוֶן:000000">iniquidade</a>?
    
\v 17 <a name="01909401700002">Se</a> <a name="01909401700002">não</a> <a name="01909401700002">fora</a> o <a name="01909401700006" ll="SDBH:עֶזְרָה:000000">auxílio</a> do <a name="01909401700004" ll="SDBH:יהוה:000000">Senhor</a>,<a name="01909401700012" ll="SDBH:מְעַט:000001">já</a> a <a name="01909401700018" ll="SDBH:נֶפֶשׁ:000001">minha</a> <a name="01909401700018" ll="SDBH:נֶפֶשׁ:000001">alma</a> <a name="01909401700014" ll="SDBH:שׁכן:000000">estaria</a> na região do <a name="01909401700016" ll="SDBH:דּוּמָה:000000">silêncio</a>.
    
\v 18 Quando eu <a name="01909401800004" ll="SDBH:אמר:000000">digo</a>: <a name="01909401800006" ll="SDBH:מוט:000000">resvala</a>-<a name="01909401800008" ll="SDBH:רֶגֶל:000003">me</a> <a name="01909401800008" ll="SDBH:רֶגֶל:000003">o</a> <a name="01909401800008" ll="SDBH:רֶגֶל:000003">pé</a>,a <a name="01909401800010" ll="SDBH:חֶסֶד:000000">tua</a> <a name="01909401800010" ll="SDBH:חֶסֶד:000000">benignidade</a>, <a name="01909401800012" ll="SDBH:יהוה:000000">Senhor</a>, <a name="01909401800014" ll="SDBH:סעד:000001">me</a> <a name="01909401800014" ll="SDBH:סעד:000001">sustém</a>.
    
\v 19 Nos <a name="01909401900004" ll="SDBH:רֹב:000000">muitos</a> <a name="01909401900006" ll="SDBH:שַׂרְעַפִּים:000000">cuidados</a> que <a name="01909401900010">dentro</a> <a name="01909401900010">de</a> <a name="01909401900010">mim</a> se multiplicam,as <a name="01909401900012" ll="SDBH:תַּנְחוּם:000000">tuas</a> <a name="01909401900012" ll="SDBH:תַּנְחוּם:000000">consolações</a> <a name="01909401900016" ll="SDBH:נֶפֶשׁ:000001">me</a> <a name="01909401900014" ll="SDBH:שׁעע:001001">alegram</a> <a name="01909401900016" ll="SDBH:נֶפֶשׁ:000001">a</a> <a name="01909401900016" ll="SDBH:נֶפֶשׁ:000001">alma</a>.
    
\v 20 <a name="01909402000004" ll="SDBH:חבר:000004">Pode</a>, acaso, <a name="01909402000004" ll="SDBH:חבר:000004">associar</a>-<a name="01909402000004" ll="SDBH:חבר:000004">se</a> <a name="01909402000004" ll="SDBH:חבר:000004">contigo</a> o <a name="01909402000006" ll="SDBH:כִּסֵּא:000002" il="Darius_In_Parse;Roman_throne;Throne_Greek;Throne_Toutankamon;mb120901102">trono</a> <a name="01909402000008" ll="SDBH:הַוָּה:001000">da</a> <a name="01909402000008" ll="SDBH:הַוָּה:001000">iniquidade</a>,o qual <a name="01909402000010" ll="SDBH:יצר:000001">forja</a> o <a name="01909402000012" ll="SDBH:עָמָל:000001">mal</a>, tendo uma <a name="01909402000016" ll="SDBH:חֹק:000002">lei</a> por pretexto?
    
\v 21 <a name="01909402100002" ll="SDBH:גדד:001000" tl="HOTTP2475">Ajuntam</a>-<a name="01909402100002" ll="SDBH:גדד:001000" tl="HOTTP2475">se</a> contra a <a name="01909402100006" ll="SDBH:נֶפֶשׁ:000001">vida</a> do <a name="01909402100008" ll="SDBH:צַדִּיק:000000">justo</a>e <a name="01909402100016" ll="SDBH:רשׁע:000001">condenam</a> o <a name="01909402100012" ll="SDBH:דָּם:000002">sangue</a> <a name="01909402100014" ll="SDBH:נָקִי:000000">inocente</a>.
    
\v 22 Mas o <a name="01909402200006" ll="SDBH:יהוה:000000">Senhor</a> é o meu <a name="01909402200012" ll="SDBH:מִשְׂגָּב:000001">baluarte</a> e o <a name="01909402200016" ll="SDBH:אֱלֹהִים:000001">meu</a> <a name="01909402200016" ll="SDBH:אֱלֹהִים:000001">Deus</a>,o <a name="01909402200020" ll="SDBH:צוּר:000002">rochedo</a> em <a name="01909402200022" ll="SDBH:מַחְסֶה:000001">que</a> <a name="01909402200022" ll="SDBH:מַחְסֶה:000001">me</a> <a name="01909402200022" ll="SDBH:מַחְסֶה:000001">abrigo</a>.
    
\v 23 Sobre eles <a name="01909402300004" ll="SDBH:שׁוב:000032">faz</a> <a name="01909402300004" ll="SDBH:שׁוב:000032">recair</a> a <a name="01909402300010" ll="SDBH:אָוֶן:000000">sua</a> <a name="01909402300010" ll="SDBH:אָוֶן:000000">iniquidade</a>e pela <a name="01909402300016" ll="SDBH:רָעָה:000000">malícia</a> <a name="01909402300016" ll="SDBH:רָעָה:000000">deles</a> próprios <a name="01909402300018" ll="SDBH:צמת:000002" tl="HOTTP2476">os</a> <a name="01909402300018" ll="SDBH:צמת:000002" tl="HOTTP2476">destruirá</a>;o <a name="01909402300022" ll="SDBH:יהוה:000000">Senhor</a>, <a name="01909402300024" ll="SDBH:אֱלֹהִים:000001">nosso</a> <a name="01909402300024" ll="SDBH:אֱלֹהִים:000001">Deus</a>, <a name="01909402300020" ll="SDBH:צמת:000002" tl="HOTTP2476">os</a> <a name="01909402300020" ll="SDBH:צמת:000002" tl="HOTTP2476">exterminará</a>.
\c 95
\v 1 <a name="01909500100002" ll="SDBH:הלך:000001">Vinde</a>, <a name="01909500100004" ll="SDBH:רנן:000000">cantemos</a> ao <a name="01909500100008" ll="SDBH:יהוה:000000">Senhor</a>, <a name="01909500100010" ll="SDBH:רוע:000000">com</a> <a name="01909500100010" ll="SDBH:רוע:000000">júbilo</a>,<a name="01909500100010" ll="SDBH:רוע:000000">celebremos</a> o <a name="01909500100014" ll="SDBH:צוּר:000002">Rochedo</a> <a name="01909500100016" ll="SDBH:יֵשַׁע:000000">da</a> <a name="01909500100016" ll="SDBH:יֵשַׁע:000000">nossa</a> <a name="01909500100016" ll="SDBH:יֵשַׁע:000000">salvação</a>.
    
\v 2 <a name="01909500200002" ll="SDBH:קדם:000000">Saiamos</a> ao <a name="01909500200004" ll="SDBH:פָּנֶה:000000">seu</a> <a name="01909500200004" ll="SDBH:פָּנֶה:000000">encontro</a>, com <a name="01909500200008" ll="SDBH:תֹּודָה:000000">ações</a> <a name="01909500200008" ll="SDBH:תֹּודָה:000000">de</a> <a name="01909500200008" ll="SDBH:תֹּודָה:000000">graças</a>,<a name="01909500200014" ll="SDBH:רוע:000000">vitoriemo</a>-lo com <a name="01909500200012" ll="SDBH:זָמִיר:000000" il="HK00188">salmos</a>.
    
\v 3 Porque o <a name="01909500300008" ll="SDBH:יהוה:000000">Senhor</a> é o <a name="01909500300004" ll="SDBH:אֵל:000001">Deus</a> <a name="01909500300006" ll="SDBH:גָּדֹול:000002">supremo</a>e o <a name="01909500300014" ll="SDBH:גָּדֹול:000002">grande</a> <a name="01909500300012" ll="SDBH:מֶלֶךְ:000000">Rei</a> acima de todos os <a name="01909500300020" ll="SDBH:אֱלֹהִים:000000">deuses</a>.
    
\v 4 Nas <a name="01909500400006">suas</a> <a name="01909500400006">mãos</a> estão as <a name="01909500400008" ll="SDBH:מֶחְקָר:000000">profundezas</a> <a name="01909500400010" ll="SDBH:אֶרֶץ:000000">da</a> <a name="01909500400010" ll="SDBH:אֶרֶץ:000000">terra</a>,e as <a name="01909500400014" ll="SDBH:תֹּועָפֹות:000000">alturas</a> dos <a name="01909500400016" ll="SDBH:הַר:000000">montes</a> lhe pertencem.
    
\v 5 Dele é o <a name="01909500500008" ll="SDBH:יָם:000000" il="Rough_seas">mar</a>, pois ele <a name="01909500500014">o</a> <a name="01909500500014">fez</a>;<a name="01909500500022" ll="SDBH:יצר:000000">obra</a> <a name="01909500500022" ll="SDBH:יצר:000000">de</a> <a name="01909500500020">suas</a> <a name="01909500500020">mãos</a>, os <a name="01909500500018" ll="SDBH:יַבֶּשֶׁת:000000">continentes</a>.
    
\v 6 <a name="01909500600002">Vinde</a>, <a name="01909500600004" ll="SDBH:חוה:001000" il="Prostrate_figures">adoremos</a> e <a name="01909500600008" ll="SDBH:כרע:000000" il="Kneeling_Saint">prostremo</a>-<a name="01909500600008" ll="SDBH:כרע:000000" il="Kneeling_Saint">nos</a>;<a name="01909500600010" ll="SDBH:ברך:000000" il="Kneeling_Saint">ajoelhemos</a> <a name="01909500600014">diante</a> <a name="01909500600016" ll="SDBH:יהוה:000000">do</a> <a name="01909500600016" ll="SDBH:יהוה:000000">Senhor</a>, <a name="01909500600018">que</a> <a name="01909500600018">nos</a> <a name="01909500600018">criou</a>.
    
\v 7 Ele é o <a name="01909500700006" ll="SDBH:אֱלֹהִים:000001">nosso</a> <a name="01909500700006" ll="SDBH:אֱלֹהִים:000001">Deus</a>, e nós, <a name="01909500700012" ll="SDBH:עַם:000000" tl="HOTTP2477">povo</a> <a name="01909500700014" ll="SDBH:מַרְעִית:000001" tl="HOTTP2477">do</a> <a name="01909500700014" ll="SDBH:מַרְעִית:000001" tl="HOTTP2477">seu</a> <a name="01909500700014" ll="SDBH:מַרְעִית:000001" tl="HOTTP2477">pasto</a>e <a name="01909500700018" ll="SDBH:צֹאן:000000" il="LB00012;LB00015;LB00017;NBGbs20120208-08;NBGbs20120710-139;NBGpb20140401b;Realia:1.2.1;tb111706002" tl="HOTTP2477">ovelhas</a> <a name="01909500700020" tl="HOTTP2477">de</a> <a name="01909500700020" tl="HOTTP2477">sua</a> <a name="01909500700020" tl="HOTTP2477">mão</a>.<a name="01909500700024" ll="SDBH:יֹום:000005">Hoje</a>, se <a name="01909500700032">ouvirdes</a> a <a name="01909500700030">sua</a> <a name="01909500700030">voz</a>,
    
\v 8 não <a name="01909500800004" ll="SDBH:קשׁה:000004">endureçais</a> <a name="01909500800006" ll="SDBH:לֵבָב:000001">o</a> <a name="01909500800006" ll="SDBH:לֵבָב:000001">coração</a>, como em <a name="01909500800010" ll="SDBH:מְרִיבָה:001000">Meribá</a>,como no <a name="01909500800014" ll="SDBH:יֹום:000003">dia</a> de <a name="01909500800016" ll="SDBH:מַסָּה:001000">Massá</a>, no <a name="01909500800022" ll="SDBH:מִדְבָּר:000000" il="RS26447_ISR91T-418;Sinai-191;Sinai-220;Sinai-3;Sinai178;Sinai190;tb113006604">deserto</a>,
    
\v 9 quando <a name="01909500900006" ll="SDBH:אָב:000001">vossos</a> <a name="01909500900006" ll="SDBH:אָב:000001">pais</a> <a name="01909500900004" ll="SDBH:נסה:000000">me</a> <a name="01909500900004" ll="SDBH:נסה:000000">tentaram</a>, <a name="01909500900008" ll="SDBH:בחן:000000">pondo</a>-<a name="01909500900008" ll="SDBH:בחן:000000">me</a> <a name="01909500900008" ll="SDBH:בחן:000000">à</a> <a name="01909500900008" ll="SDBH:בחן:000000">prova</a>,não obstante <a name="01909500900012" ll="SDBH:ראה:000000">terem</a> <a name="01909500900012" ll="SDBH:ראה:000000">visto</a> as <a name="01909500900014" ll="SDBH:פֹּעַל:000000">minhas</a> <a name="01909500900014" ll="SDBH:פֹּעַל:000000">obras</a>.
    
\v 10 Durante <a name="01909501000002" ll="SDBH:אַרְבַּע:000001">quarenta</a> <a name="01909501000004" ll="SDBH:שָׁנָה:000000">anos</a>, <a name="01909501000006" ll="SDBH:קוט:000000">estive</a> <a name="01909501000006" ll="SDBH:קוט:000000">desgostado</a> com essa <a name="01909501000010" ll="SDBH:דֹּור:001000">geração</a>e <a name="01909501000014" ll="SDBH:אמר:000000">disse</a>: é <a name="01909501000016" ll="SDBH:עַם:000000" tl="HOTTP2478">povo</a> <a name="01909501000020" ll="SDBH:לֵבָב:000001">de</a> <a name="01909501000020" ll="SDBH:לֵבָב:000001">coração</a> <a name="01909501000018" ll="SDBH:תעה:000002">transviado</a>,não <a name="01909501000030">conhece</a> os <a name="01909501000032" ll="SDBH:דֶּרֶךְ:000007">meus</a> <a name="01909501000032" ll="SDBH:דֶּרֶךְ:000007">caminhos</a>.
    
\v 11 Por isso, <a name="01909501100004" ll="SDBH:שׁבע:000000" il="Oath_Jacob_Esau">jurei</a> na <a name="01909501100008" ll="SDBH:אַף:000008">minha</a> <a name="01909501100008" ll="SDBH:אַף:000008">ira</a>:não <a name="01909501100012">entrarão</a> no <a name="01909501100016" ll="SDBH:מְנוּחָה:000001">meu</a> <a name="01909501100016" ll="SDBH:מְנוּחָה:000001">descanso</a>.
\c 96
\v 1 <a name="01909600100002" ll="SDBH:שׁיר:000000" il="HK00188">Cantai</a> ao <a name="01909600100006" ll="SDBH:יהוה:000000">Senhor</a> um <a name="01909600100008" ll="SDBH:שִׁיר:000000" il="HK00188">cântico</a> <a name="01909600100010" ll="SDBH:חָדָשׁ:000001">novo</a>,<a name="01909600100012" ll="SDBH:שׁיר:000000" il="HK00188">cantai</a> ao <a name="01909600100016" ll="SDBH:יהוה:000000">Senhor</a>, todas as <a name="01909600100022" ll="SDBH:אֶרֶץ:000000">terras</a>.
    
\v 2 <a name="01909600200002" ll="SDBH:שׁיר:000000" il="HK00188">Cantai</a> ao <a name="01909600200006" ll="SDBH:יהוה:000000">Senhor</a>, <a name="01909600200008" ll="SDBH:ברך:001001">bendizei</a> o <a name="01909600200010" ll="SDBH:שֵׁם:000003">seu</a> <a name="01909600200010" ll="SDBH:שֵׁם:000003">nome</a>;<a name="01909600200012" ll="SDBH:בשׂר:000000">proclamai</a> a <a name="01909600200022" ll="SDBH:יְשׁוּעָה:000000">sua</a> <a name="01909600200022" ll="SDBH:יְשׁוּעָה:000000">salvação</a>, <a name="01909600200016" ll="SDBH:יֹום:000001">dia</a> após <a name="01909600200020" ll="SDBH:יֹום:000001">dia</a>.
    
\v 3 <a name="01909600300002" ll="SDBH:ספר:000006">Anunciai</a> entre as <a name="01909600300008" ll="SDBH:גֹּוי:000000">nações</a> a <a name="01909600300010" ll="SDBH:כָּבֹוד:000000">sua</a> <a name="01909600300010" ll="SDBH:כָּבֹוד:000000">glória</a>,entre todos os <a name="01909600300018" ll="SDBH:עַם:000000">povos</a>, as <a name="01909600300020" ll="SDBH:פלא:000000">suas</a> <a name="01909600300020" ll="SDBH:פלא:000000">maravilhas</a>.
    
\v 4 Porque <a name="01909600400004" ll="SDBH:גָּדֹול:000002">grande</a> é o <a name="01909600400006" ll="SDBH:יהוה:000000">Senhor</a> e <a name="01909600400012" ll="SDBH:מְאֹד:000001">mui</a> <a name="01909600400010" ll="SDBH:הלל:001001">digno</a> <a name="01909600400010" ll="SDBH:הלל:001001">de</a> <a name="01909600400010" ll="SDBH:הלל:001001">ser</a> <a name="01909600400010" ll="SDBH:הלל:001001">louvado</a>,<a name="01909600400014" ll="SDBH:ירא:000001">temível</a> mais que todos os <a name="01909600400022" ll="SDBH:אֱלֹהִים:000000">deuses</a>.
    
\v 5 Porque todos os <a name="01909600500006" ll="SDBH:אֱלֹהִים:000000">deuses</a> dos <a name="01909600500010" ll="SDBH:עַם:000000">povos</a> não passam de <a name="01909600500012" ll="SDBH:אֱלִיל:000000" il="Realia:4.6b;adr1305227007">ídolos</a>;o <a name="01909600500016" ll="SDBH:יהוה:000000">Senhor</a>, porém, <a name="01909600500020">fez</a> os <a name="01909600500018" ll="SDBH:שָׁמַיִם:000000">céus</a>.
    
\v 6 <a name="01909600600002" ll="SDBH:הֹוד:000000">Glória</a> e <a name="01909600600006" ll="SDBH:הָדָר:000000">majestade</a> estão <a name="01909600600010">diante</a> <a name="01909600600010">dele</a>,<a name="01909600600012" ll="SDBH:עֹז:000002">força</a> e <a name="01909600600016" ll="SDBH:תִּפְאֶרֶת:000000">formosura</a>, no <a name="01909600600020" ll="SDBH:מִקְדָּשׁ:000001" il="HK00245;NBGlo20140606;NBGlo20140606a;NBGlo20140606b;NBGlo20140708c;NBGlo20140825;jlm_soltemp_d01;jlm_soltemp_m06;jlm_soltemp_pillars_m01">seu</a> <a name="01909600600020" ll="SDBH:מִקְדָּשׁ:000001" il="HK00245;NBGlo20140606;NBGlo20140606a;NBGlo20140606b;NBGlo20140708c;NBGlo20140825;jlm_soltemp_d01;jlm_soltemp_m06;jlm_soltemp_pillars_m01">santuário</a>.
    
\v 7 <a name="01909600700002">Tributai</a> ao <a name="01909600700006" ll="SDBH:יהוה:000000">Senhor</a>, ó <a name="01909600700008" ll="SDBH:מִשְׁפָּחָה:000000">famílias</a> <a name="01909600700010" ll="SDBH:עַם:000000">dos</a> <a name="01909600700010" ll="SDBH:עַם:000000">povos</a>,<a name="01909600700012">tributai</a> ao <a name="01909600700016" ll="SDBH:יהוה:000000">Senhor</a> <a name="01909600700018" ll="SDBH:כָּבֹוד:000000">glória</a> e <a name="01909600700022" ll="SDBH:עֹז:000002">força</a>.
    
