\v 1 Já <a name="02200500100002">entrei</a> no <a name="02200500100006" ll="SDBH:גַּן:000000">meu</a> <a name="02200500100006" ll="SDBH:גַּן:000000">jardim</a>, <a name="02200500100008" ll="SDBH:אָחֹות:000002">minha</a> <a name="02200500100008" ll="SDBH:אָחֹות:000002">irmã</a>, <a name="02200500100010" ll="SDBH:כַּלָּה:000001" il="ef0078">noiva</a> minha;<a name="02200500100012" ll="SDBH:ארה:000000">colhi</a> a <a name="02200500100014" ll="SDBH:מֹר:000000" il="Flora:4.1.8a;Flora:4.1.8b;Flora:4.1.8c">minha</a> <a name="02200500100014" ll="SDBH:מֹר:000000" il="Flora:4.1.8a;Flora:4.1.8b;Flora:4.1.8c">mirra</a> com a <a name="02200500100018" ll="SDBH:בֹּשֶׂם:000001" il="Bolen00098;Flora:4.2.4;tb091403025">especiaria</a>,<a name="02200500100020" ll="SDBH:אכל:000000">comi</a> o <a name="02200500100022" ll="SDBH:יַעַר:000000" il="Honeycomb1">meu</a> <a name="02200500100022" ll="SDBH:יַעַר:000000" il="Honeycomb1">favo</a> com o <a name="02200500100026" ll="SDBH:דְּבַשׁ:000000" il="Honeycomb1">mel</a>,<a name="02200500100028" ll="SDBH:שׁתה:000000">bebi</a> o <a name="02200500100030" ll="SDBH:יַיִן:000000" il="Flora:2.12a;HK00108;LB00103;LB00107;vineyard1679, rp">meu</a> <a name="02200500100030" ll="SDBH:יַיִן:000000" il="Flora:2.12a;HK00108;LB00103;LB00107;vineyard1679, rp">vinho</a> com o <a name="02200500100034" ll="SDBH:חָלָב:000000" il="Milk">leite</a>.<a name="02200500100036" ll="SDBH:אכל:000000">Comei</a> e <a name="02200500100040" ll="SDBH:שׁתה:000000">bebei</a>, <a name="02200500100038" ll="SDBH:רֵעַ:001000">amigos</a>;<a name="02200500100044" ll="SDBH:שׁכר:000000">bebei</a> <a name="02200500100044" ll="SDBH:שׁכר:000000">fartamente</a>, ó <a name="02200500100046" ll="SDBH:דֹּוד:000000;SDBH:דֹּוד:000001">amados</a>.
    
\v 2 Eu dormia, mas o <a name="02200500200008" ll="SDBH:לֵב:000001">meu</a> <a name="02200500200008" ll="SDBH:לֵב:000001">coração</a> <a name="02200500200010" ll="SDBH:עור:000000">velava</a>;eis a <a name="02200500200012">voz</a> <a name="02200500200014" ll="SDBH:דֹּוד:000000">do</a> <a name="02200500200014" ll="SDBH:דֹּוד:000000">meu</a> <a name="02200500200014" ll="SDBH:דֹּוד:000000">amado</a>, que está <a name="02200500200016" ll="SDBH:דפק:000000">batendo</a>:<a name="02200500200018" ll="SDBH:פתח:000002">Abre</a>-me, <a name="02200500200022" ll="SDBH:אָחֹות:000002">minha</a> <a name="02200500200022" ll="SDBH:אָחֹות:000002">irmã</a>, <a name="02200500200024" ll="SDBH:רַעְיָה:000000">querida</a> <a name="02200500200024" ll="SDBH:רַעְיָה:000000">minha</a>,<a name="02200500200026" ll="SDBH:יֹונָה:000000" il="BK00066;BK00067;Columba_livia;Fauna:3.7a;Fauna:3.7b;HK00064;LB00068;Rock_pigeons">pomba</a> <a name="02200500200026" ll="SDBH:יֹונָה:000000" il="BK00066;BK00067;Columba_livia;Fauna:3.7a;Fauna:3.7b;HK00064;LB00068;Rock_pigeons">minha</a>, <a name="02200500200028" ll="SDBH:תָּם:000001">imaculada</a> <a name="02200500200028" ll="SDBH:תָּם:000001">minha</a>,porque a <a name="02200500200032" ll="SDBH:רֹאשׁ:000000">minha</a> <a name="02200500200032" ll="SDBH:רֹאשׁ:000000">cabeça</a> está <a name="02200500200034" ll="SDBH:מלא:000000">cheia</a> <a name="02200500200036" ll="SDBH:טַל:000000" il="Dew">de</a> <a name="02200500200036" ll="SDBH:טַל:000000" il="Dew">orvalho</a>,os <a name="02200500200038" ll="SDBH:קְוֻצֹּות:000000">meus</a> <a name="02200500200038" ll="SDBH:קְוֻצֹּות:000000">cabelos</a>, das <a name="02200500200040" ll="SDBH:רָסִיס:000000">gotas</a> <a name="02200500200042" ll="SDBH:לַיְלָה:000000">da</a> <a name="02200500200042" ll="SDBH:לַיְלָה:000000">noite</a>.
    
