\v 1 <a name="04202400100004" ll="SDBG:δέ:000004">Mas</a>, no primeiro dia da <a name="04202400100010" ll="SDBG:σάββατον:000001">semana</a>, alta <a name="04202400100012;04202400100014" ll="SDBG:βαθύς:000001;SDBG:ὄρθρος:000001">madrugada</a>, <a name="04202400100022" ll="SDBG:ἔρχομαι:000001">foram</a> elas <a name="04202400100016" ll="SDBG:ἐπί:000004">ao</a> <a name="04202400100020" ll="SDBG:μνῆμα:000000" il="NBGbs20090425-1_50;NBGbs20110228-1_34-2;NBGpb20140610;NBGpb20140610a;Sidonian_burial_caves;Tombs-rock-cut;il_tomb_rolling_stone_f02;jlm_kokhim_tomb_d01;jlm_tomb_new_d01;tb051906433;tb052816039;tb053116857;tb060116282;tb110800207;tb110800210">túmulo</a>, <a name="04202400100024" ll="SDBG:φέρω:000000">levando</a> os <a name="04202400100030" ll="SDBG:ἄρωμα:000000" il="Bolen00048;Bolen00098">aromas</a> <a name="04202400100026" ll="SDBG:ὅς:000000">que</a> haviam <a name="04202400100028" ll="SDBG:ἑτοιμάζω:000000">preparado</a>. 
\v 2 <a name="04202400200004" ll="SDBG:δέ:000001">E</a> <a name="04202400200002" ll="SDBG:εὑρίσκω:000001">encontraram</a> a <a name="04202400200008" ll="SDBG:λίθος:000003" il="NBGbs20110228-1_32;NBGpb20140610a;Realia:6.3;Tombs-rock-cut;il_tomb_rolling_stone_f02;jlm_kokhim_tomb_d01;jlm_tomb_new_d01">pedra</a> <a name="04202400200010" ll="SDBG:ἀποκυλίω:000000">removida</a> <a name="04202400200012" ll="SDBG:ἀπό:000002">do</a> <a name="04202400200016" ll="SDBG:μνημεῖον:000000" il="Garden Tomb entrance, DSC00469;NBGpb20140610;NBGpb20140610a;Tombs-rock-cut;il_tomb_rolling_stone_f02;jlm_kokhim_tomb_d01;jlm_tomb_new_d01;tb051906433;tb051908224;tb052816039;tb053116857;tb060116282;tb110800207;tb110800210">sepulcro</a>; 
\v 3 mas, ao <a name="04202400300002" ll="SDBG:εἰσέρχομαι:000000">entrarem</a>, <a name="04202400300006" ll="SDBG:οὐ:000000">não</a> <a name="04202400300008" ll="SDBG:εὑρίσκω:000000">acharam</a> o <a name="04202400300012" ll="SDBG:σῶμα:000000">corpo</a> do <a name="04202400300016" ll="SDBG:κύριος:000000">Senhor</a> <a name="04202400300018" ll="SDBG:Ἰησοῦς:000000">Jesus</a>. 
\v 4 <a name="04202400400004" ll="SDBG:γίνομαι:000003">Aconteceu</a> que, perplexas a esse <a name="04202400400014" ll="SDBG:περί:000003">respeito</a>, <a name="04202400400012" ll="SDBG:αὐτός:001000">apareceram</a>-lhes <a name="04202400400024" ll="SDBG:δύο:000000">dois</a> <a name="04202400400022" ll="SDBG:ἀνήρ:000000">varões</a> <a name="04202400400006" ll="SDBG:ἐν:000019">com</a> vestes <a name="04202400400034" ll="SDBG:ἀστράπτω:000000">resplandecentes</a>. 
\v 5 <a name="04202400500006" ll="SDBG:γίνομαι:000002">Estando</a> <a name="04202400500008" ll="SDBG:αὐτός:001000">elas</a> possuídas de <a name="04202400500002" ll="SDBG:ἔμφοβος:000000">temor</a>, <a name="04202400500012" ll="SDBG:κλίνω:000004">baixando</a> os <a name="04202400500016" ll="SDBG:πρόσωπον:000000" il="tb011017836">olhos</a> <a name="04202400500018">para</a> o <a name="04202400500022" ll="SDBG:γῆ:000002">chão</a>, eles <a name="04202400500028" ll="SDBG:αὐτός:001000">lhes</a> falaram: Por <a name="04202400500030" ll="SDBG:τίς:000001">que</a> <a name="04202400500032" ll="SDBG:ζητέω:000000">buscais</a> <a name="04202400500038" ll="SDBG:μετά:000007">entre</a> os <a name="04202400500042" ll="SDBG:νεκρός:000000">mortos</a> ao que <a name="04202400500036" ll="SDBG:ζάω:000000">vive</a>? 
\v 6 Ele <a name="04202400600002" ll="SDBG:οὐ:000000">não</a> está <a name="04202400600006" ll="SDBG:ὧδε:000000">aqui</a>, <a name="04202400600008" ll="SDBG:ἀλλά:000000">mas</a> <a name="04202400600010" ll="SDBG:ἐγείρω:000004">ressuscitou</a>. Lembrai-vos de <a name="04202400600014" ll="SDBG:ὡς:000001">como</a> vos <a name="04202400600016" ll="SDBG:λαλέω:000000">preveniu</a>, estando <a name="04202400600020" ll="SDBG:ἔτι:000000">ainda</a> <a name="04202400600024" ll="SDBG:ἐν:000000">na</a> <a name="04202400600028" ll="SDBG:Γαλιλαία:000000" il="tb052200102;tb110306599">Galileia</a>, 
\v 7 quando <a name="04202400700002" ll="SDBG:λέγω:000000">disse</a>: Importa <a name="04202400700014" ll="SDBG:δεῖ:000000">que</a> o <a name="04202400700006" ll="SDBG:υἱός:000014">Filho</a> do <a name="04202400700010" ll="SDBG:ἄνθρωπος:000007">Homem</a> seja <a name="04202400700016" ll="SDBG:παραδίδωμι:000004">entregue</a> <a name="04202400700018">nas</a> <a name="04202400700020" ll="SDBG:χείρ:000004">mãos</a> de <a name="04202400700024" ll="SDBG:ἁμαρτωλός:001000">pecadores</a>, <a name="04202400700026" ll="SDBG:καί:000001">e</a> seja <a name="04202400700028" ll="SDBG:σταυρόω:000000">crucificado</a>, <a name="04202400700030" ll="SDBG:καί:000001">e</a> <a name="04202400700038" ll="SDBG:ἀνίστημι:000006">ressuscite</a> no <a name="04202400700034" ll="SDBG:τρίτος:000000">terceiro</a> <a name="04202400700036" ll="SDBG:ἡμέρα:000001">dia</a>. 
\v 8 <a name="04202400800002" ll="SDBG:καί:000001">Então</a>, se lembraram das <a name="04202400800010" ll="SDBG:αὐτός:001000">suas</a> <a name="04202400800008" ll="SDBG:ῥῆμα:000001">palavras</a>. 
\v 9 <a name="04202400900002;04202400900022" ll="SDBG:καί:000000;SDBG:καί:000001">E</a>, <a name="04202400900004" ll="SDBG:ὑποστρέφω:000000">voltando</a> <a name="04202400900006" ll="SDBG:ἀπό:000002">do</a> <a name="04202400900010" ll="SDBG:μνημεῖον:000000" il="Garden Tomb entrance, DSC00469;NBGpb20140610;NBGpb20140610a;Tombs-rock-cut;il_tomb_rolling_stone_f02;jlm_kokhim_tomb_d01;jlm_tomb_new_d01;tb051906433;tb051908224;tb052816039;tb053116857;tb060116282;tb110800207;tb110800210">túmulo</a>, <a name="04202400900012" ll="SDBG:ἀπαγγέλλω:000000">anunciaram</a> <a name="04202400900016" ll="SDBG:πᾶς:000000">todas</a> estas coisas aos <a name="04202400900020" ll="SDBG:ἕνδεκα:000000">onze</a> e a <a name="04202400900024" ll="SDBG:πᾶς:000000">todos</a> os mais que com eles estavam. 
\v 10 Eram <a name="04202401000010" ll="SDBG:Μαρία:000002">Maria</a> <a name="04202401000008" ll="SDBG:Μαγδαληνή:000000">Madalena</a>, <a name="04202401000014" ll="SDBG:Ἰωάννα:000000">Joana</a> <a name="04202401000012" ll="SDBG:καί:000000">e</a> <a name="04202401000018" ll="SDBG:Μαρία:000003">Maria</a>, mãe de <a name="04202401000022" ll="SDBG:Ἰάκωβος:000000">Tiago</a>; <a name="04202401000016" ll="SDBG:καί:000000">também</a> as demais que estavam <a name="04202401000030" ll="SDBG:σύν:000000">com</a> <a name="04202401000032" ll="SDBG:αὐτός:001000">elas</a> <a name="04202401000034" ll="SDBG:λέγω:000000">confirmaram</a> estas coisas <a name="04202401000036" ll="SDBG:πρός:000004">aos</a> <a name="04202401000040" ll="SDBG:ἀπόστολος:000000">apóstolos</a>. 
\v 11 Tais <a name="04202401100016" ll="SDBG:ῥῆμα:000001">palavras</a> <a name="04202401100008" ll="SDBG:αὐτός:001000">lhes</a> <a name="04202401100004" ll="SDBG:φαίνω:000001">pareciam</a> um <a name="04202401100010" ll="SDBG:ὡσεί:000000">como</a> <a name="04202401100012" ll="SDBG:λῆρος:000000">delírio</a>, <a name="04202401100002">e</a> não <a name="04202401100022" ll="SDBG:ἀπιστέω:000000">acreditaram</a> <a name="04202401100024" ll="SDBG:αὐτός:001000">nelas</a>. 
\v 12 <a name="04202401200006" ll="SDBG:Πέτρος:000000" il="cd090085005;tb061516556">Pedro</a>, <a name="04202401200004">porém</a>, <a name="04202401200008" ll="SDBG:ἀνίστημι:000004">levantando</a>-se, <a name="04202401200010" ll="SDBG:τρέχω:000000">correu</a> <a name="04202401200012" ll="SDBG:ἐπί:000004">ao</a> <a name="04202401200016" ll="SDBG:μνημεῖον:000000" il="Garden Tomb entrance, DSC00469;NBGpb20140610;NBGpb20140610a;Tombs-rock-cut;il_tomb_rolling_stone_f02;jlm_kokhim_tomb_d01;jlm_tomb_new_d01;tb051906433;tb051908224;tb052816039;tb053116857;tb060116282;tb110800207;tb110800210">sepulcro</a>. <a name="04202401200018;04202401200030" ll="SDBG:καί:000001">E</a>, <a name="04202401200020" ll="SDBG:παρακύπτω:000000;SDBG:παρακύπτω:000001">abaixando</a>-se, nada mais <a name="04202401200022" ll="SDBG:βλέπω:000000">viu</a>, senão os lençóis de <a name="04202401200026" ll="SDBG:ὀθόνιον:000000" il="Bolen00103">linho</a>; e <a name="04202401200032" ll="SDBG:ἀπέρχομαι:000004">retirou</a>-se para casa, <a name="04202401200038" ll="SDBG:θαυμάζω:000000">maravilhado</a> do que havia <a name="04202401200042" ll="SDBG:γίνομαι:000003">acontecido</a>.
    
\v 13 <a name="04202401300012" ll="SDBG:ἐν:000018">Naquele</a> <a name="04202401300010" ll="SDBG:αὐτός:001000">mesmo</a> <a name="04202401300018" ll="SDBG:ἡμέρα:000001">dia</a>, <a name="04202401300006" ll="SDBG:δύο:000000">dois</a> <a name="04202401300008;04202401300014" ll="SDBG:αὐτός:001000;SDBG:ἐκ:000008">deles</a> estavam de <a name="04202401300022" ll="SDBG:πορεύομαι:000001">caminho</a> <a name="04202401300024" ll="SDBG:εἰς:000000">para</a> uma <a name="04202401300026" ll="SDBG:κώμη:000000" il="il_sepphoris_village_d01">aldeia</a> <a name="04202401300038;04202401300040" ll="SDBG:ὄνομα:000000;SDBG:ὅς:000000">chamada</a> Emaús, <a name="04202401300028" ll="SDBG:ἀπέχω:000001">distante</a> <a name="04202401300034" ll="SDBG:ἀπό:000002">de</a> Jerusalém <a name="04202401300032" ll="SDBG:ἑξήκοντα:000000">sessenta</a> <a name="04202401300030" ll="SDBG:στάδιος:000000">estádios</a>. 
\v 14 <a name="04202401400002" ll="SDBG:καί:000000">E</a> iam <a name="04202401400006" ll="SDBG:ὁμιλέω:000000">conversando</a> a <a name="04202401400008;04202401400010" ll="SDBG:πρός:000004;SDBG:ἀλλήλων:000000">respeito</a> <a name="04202401400012" ll="SDBG:περί:000001">de</a> <a name="04202401400014" ll="SDBG:πᾶς:000000">todas</a> as coisas <a name="04202401400018" ll="SDBG:συμβαίνω:000000">sucedidas</a>. 
\v 15 <a name="04202401500004" ll="SDBG:γίνομαι:000003">Aconteceu</a> que, <a name="04202401500006" ll="SDBG:ἐν:000019">enquanto</a> <a name="04202401500010" ll="SDBG:ὁμιλέω:000000">conversavam</a> <a name="04202401500002" ll="SDBG:καί:000001">e</a> <a name="04202401500016" ll="SDBG:συζητέω:000001">discutiam</a>, o <a name="04202401500012;04202401500014" ll="SDBG:αὐτός:001000;SDBG:καί:000000">próprio</a> <a name="04202401500022" ll="SDBG:Ἰησοῦς:000000">Jesus</a> se <a name="04202401500024" ll="SDBG:ἐγγίζω:000000">aproximou</a> e ia <a name="04202401500026" ll="SDBG:συμπορεύομαι:000000">com</a> <a name="04202401500020" ll="SDBG:αὐτός:001001">eles</a>. 
\v 16 Os <a name="04202401600008" ll="SDBG:αὐτός:001000">seus</a> <a name="04202401600006" ll="SDBG:ὀφθαλμός:000000">olhos</a>, <a name="04202401600004" ll="SDBG:δέ:000002">porém</a>, <a name="04202401600010" ll="SDBG:κρατέω:000001">estavam</a> como que impedidos de <a name="04202401600018" ll="SDBG:αὐτός:001000">o</a> <a name="04202401600016" ll="SDBG:ἐπιγινώσκω:000003">reconhecer</a>. 
\v 17 <a name="04202401700004" ll="SDBG:δέ:000001">Então</a>, <a name="04202401700008" ll="SDBG:αὐτός:001000">lhes</a> perguntou Jesus: <a name="04202401700010" ll="SDBG:τίς:000000">Que</a> é <a name="04202401700016" ll="SDBG:οὗτος:000000">isso</a> que vos <a name="04202401700020" ll="SDBG:ἀντιβάλλω:000000">preocupa</a> <a name="04202401700028">e</a> de que ides tratando à medida que <a name="04202401700026" ll="SDBG:περιπατέω:000000">caminhais</a>? E eles pararam <a name="04202401700032" ll="SDBG:σκυθρωπός:000000">entristecidos</a>. 
\v 18 <a name="04202401800006" ll="SDBG:εἷς:000001">Um</a>, <a name="04202401800004" ll="SDBG:δέ:000001">porém</a>, <a name="04202401800008" ll="SDBG:ὄνομα:000000">chamado</a> Cleopas, <a name="04202401800002" ll="SDBG:ἀποκρίνομαι:000000">respondeu</a>, dizendo: És o <a name="04202401800020" ll="SDBG:μόνος:000000">único</a>, porventura, que, tendo <a name="04202401800022" ll="SDBG:παροικέω:000000">estado</a> <a name="04202401800036" ll="SDBG:ἐν:000000">em</a> Jerusalém, ignoras as <a name="04202401800034" ll="SDBG:γίνομαι:000003">ocorrências</a> <a name="04202401800040" ll="SDBG:ἐν:000019">destes</a> últimos <a name="04202401800044" ll="SDBG:ἡμέρα:000002">dias</a>? 
\v 19 Ele <a name="04202401900006" ll="SDBG:αὐτός:001000">lhes</a> perguntou: <a name="04202401900008" ll="SDBG:ποῖος:000000">Quais</a>? <a name="04202401900002;04202401900012" ll="SDBG:δέ:000001;SDBG:καί:000001">E</a> explicaram: O que <a name="04202401900020" ll="SDBG:περί:000006">aconteceu</a> a <a name="04202401900022" ll="SDBG:Ἰησοῦς:000000">Jesus</a>, o Nazareno, <a name="04202401900028" ll="SDBG:ὅς:000000">que</a> <a name="04202401900030" ll="SDBG:γίνομαι:000001">era</a> varão <a name="04202401900034" ll="SDBG:προφήτης:000000">profeta</a>, <a name="04202401900036" ll="SDBG:δυνατός:000002">poderoso</a> <a name="04202401900032;04202401900038" ll="SDBG:ἀνήρ:000000;SDBG:ἐν:000015">em</a> <a name="04202401900040" ll="SDBG:ἔργον:000000">obras</a> <a name="04202401900042" ll="SDBG:καί:000000">e</a> <a name="04202401900044" ll="SDBG:λόγος:000000">palavras</a>, <a name="04202401900046" ll="SDBG:ἐναντίον:000001">diante</a> de <a name="04202401900050" ll="SDBG:θεός:000000">Deus</a> e de <a name="04202401900054" ll="SDBG:πᾶς:000000">todo</a> o <a name="04202401900058" ll="SDBG:λαός:000002">povo</a>, 
\v 20 e <a name="04202402000002" ll="SDBG:ὅπως:000000;SDBG:ὅπως:000002">como</a> os principais <a name="04202402000012" ll="SDBG:ἀρχιερεύς:000000" il="LB00319">sacerdotes</a> <a name="04202402000014" ll="SDBG:καί:000000">e</a> as nossas <a name="04202402000018" ll="SDBG:ἄρχων:000000" il="tb011506651;tb041705529;tbs91069704">autoridades</a> o <a name="04202402000006" ll="SDBG:παραδίδωμι:000001">entregaram</a> <a name="04202402000022" ll="SDBG:εἰς:000010">para</a> ser <a name="04202402000024" ll="SDBG:κρίμα:000003">condenado</a> à <a name="04202402000026" ll="SDBG:θάνατος:000000">morte</a> <a name="04202402000028" ll="SDBG:καί:000001">e</a> <a name="04202402000008" ll="SDBG:αὐτός:001000">o</a> <a name="04202402000030" ll="SDBG:σταυρόω:000000">crucificaram</a>. 
\v 21 <a name="04202402100004" ll="SDBG:δέ:000002">Ora</a>, nós <a name="04202402100006" ll="SDBG:ἐλπίζω:000000">esperávamos</a> <a name="04202402100008" ll="SDBG:ὅτι:000000">que</a> fosse <a name="04202402100010" ll="SDBG:αὐτός:001000">ele</a> quem <a name="04202402100016" ll="SDBG:μέλλω:000000">havia</a> de <a name="04202402100018" ll="SDBG:λυτρόω:000000">redimir</a> a <a name="04202402100022" ll="SDBG:Ἰσραήλ:000001">Israel</a>; <a name="04202402100024" ll="SDBG:ἀλλά:000000">mas</a>, <a name="04202402100026;04202402100030" ll="SDBG:γέ:000000;SDBG:σύν:000001">depois</a> de <a name="04202402100032" ll="SDBG:πᾶς:000000">tudo</a> isto, <a name="04202402100042" ll="SDBG:ἄγω:000005;SDBG:ἄγω:000006">é</a> já este o <a name="04202402100036" ll="SDBG:τρίτος:000000">terceiro</a> <a name="04202402100040" ll="SDBG:ἡμέρα:000001">dia</a> <a name="04202402100044;04202402100046" ll="SDBG:ἀπό:000007;SDBG:ὅς:000000">desde</a> que tais coisas <a name="04202402100050" ll="SDBG:γίνομαι:000003">sucederam</a>. 
\v 22 É verdade <a name="04202402200004" ll="SDBG:καί:000002">também</a> que <a name="04202402200008" ll="SDBG:τὶς:000000">algumas</a> <a name="04202402200006" ll="SDBG:γυνή:000000">mulheres</a>, <a name="04202402200010" ll="SDBG:ἐκ:000008">das</a> que conosco estavam, nos <a name="04202402200014" ll="SDBG:ἐξίστημι:000000">surpreenderam</a>, tendo <a name="04202402200018" ll="SDBG:γίνομαι:000007">ido</a> de madrugada <a name="04202402200022" ll="SDBG:ἐπί:000004">ao</a> <a name="04202402200026" ll="SDBG:μνημεῖον:000000" il="Garden Tomb entrance, DSC00469;NBGpb20140610;NBGpb20140610a;Tombs-rock-cut;il_tomb_rolling_stone_f02;jlm_kokhim_tomb_d01;jlm_tomb_new_d01;tb051906433;tb051908224;tb052816039;tb053116857;tb060116282;tb110800207;tb110800210">túmulo</a>; 
\v 23 <a name="04202402300002">e</a>, <a name="04202402300004" ll="SDBG:μή:000000">não</a> <a name="04202402300006" ll="SDBG:εὑρίσκω:000000">achando</a> o <a name="04202402300010" ll="SDBG:σῶμα:000000">corpo</a> de Jesus, <a name="04202402300014" ll="SDBG:ἔρχομαι:000001">voltaram</a> <a name="04202402300016" ll="SDBG:λέγω:000000">dizendo</a> terem <a name="04202402300024" ll="SDBG:ὁράω:000000">tido</a> uma <a name="04202402300020" ll="SDBG:ὀπτασία:000000">visão</a> de <a name="04202402300022" ll="SDBG:ἄγγελος:000001" il="Kneeling angel;tb031115118">anjos</a>, os <a name="04202402300026" ll="SDBG:ὅς:000000">quais</a> <a name="04202402300028" ll="SDBG:λέγω:000000">afirmam</a> que <a name="04202402300012" ll="SDBG:αὐτός:001000">ele</a> <a name="04202402300032" ll="SDBG:ζάω:000001">vive</a>. 
\v 24 De fato, alguns <a name="04202402400010" ll="SDBG:σύν:000000">dos</a> nossos <a name="04202402400004" ll="SDBG:ἀπέρχομαι:000000">foram</a> <a name="04202402400014" ll="SDBG:ἐπί:000004">ao</a> <a name="04202402400018" ll="SDBG:μνημεῖον:000000" il="Garden Tomb entrance, DSC00469;NBGpb20140610;NBGpb20140610a;Tombs-rock-cut;il_tomb_rolling_stone_f02;jlm_kokhim_tomb_d01;jlm_tomb_new_d01;tb051906433;tb051908224;tb052816039;tb053116857;tb060116282;tb110800207;tb110800210">sepulcro</a> <a name="04202402400002" ll="SDBG:καί:000001">e</a> <a name="04202402400022" ll="SDBG:εὑρίσκω:000001">verificaram</a> a <a name="04202402400020" ll="SDBG:καί:000001">exatidão</a> <a name="04202402400024" ll="SDBG:οὕτω:000000">do</a> <a name="04202402400026" ll="SDBG:καθώς:000002">que</a> disseram as <a name="04202402400032" ll="SDBG:γυνή:000000">mulheres</a>; <a name="04202402400038" ll="SDBG:δέ:000002">mas</a> <a name="04202402400040" ll="SDBG:οὐ:000000">não</a> <a name="04202402400036" ll="SDBG:αὐτός:001000">o</a> viram. 
\v 25 <a name="04202402500002" ll="SDBG:καί:000001">Então</a>, <a name="04202402500004" ll="SDBG:αὐτός:001000">lhes</a> disse Jesus: <a name="04202402500012" ll="SDBG:ὦ:000000">Ó</a> <a name="04202402500014" ll="SDBG:ἀνόητος:000000">néscios</a> <a name="04202402500016" ll="SDBG:καί:000000">e</a> <a name="04202402500018" ll="SDBG:βραδύς:000000">tardos</a> de <a name="04202402500022" ll="SDBG:καρδία:000000">coração</a> para <a name="04202402500026;04202402500028" ll="SDBG:πιστεύω:000000">crer</a> <a name="04202402500030" ll="SDBG:πᾶς:000000">tudo</a> o <a name="04202402500032" ll="SDBG:ὅς:000000">que</a> os <a name="04202402500038" ll="SDBG:προφήτης:000000">profetas</a> <a name="04202402500034" ll="SDBG:λαλέω:000000">disseram</a>! 
\v 26 Porventura, <a name="04202402600002" ll="SDBG:οὐχί:000001">não</a> <a name="04202402600006" ll="SDBG:δεῖ:000001">convinha</a> que o <a name="04202402600012" ll="SDBG:Χριστός:000000">Cristo</a> <a name="04202402600008" ll="SDBG:πάσχω:000000">padecesse</a> <a name="04202402600014" ll="SDBG:καί:000001">e</a> <a name="04202402600016" ll="SDBG:εἰσέρχομαι:000003">entrasse</a> <a name="04202402600018" ll="SDBG:εἰς:000010">na</a> sua <a name="04202402600022" ll="SDBG:δόξα:000001">glória</a>? 
\v 27 <a name="04202402700002" ll="SDBG:καί:000001">E</a>, começando <a name="04202402700006" ll="SDBG:ἀπό:000001">por</a> Moisés, <a name="04202402700010;04202402700012" ll="SDBG:καί:000000;SDBG:ἀπό:000001">discorrendo</a> por <a name="04202402700014" ll="SDBG:πᾶς:000000">todos</a> os <a name="04202402700018" ll="SDBG:προφήτης:000000">Profetas</a>, <a name="04202402700020" ll="SDBG:διερμηνεύω:000001">expunha</a>-lhes o que a <a name="04202402700036" ll="SDBG:ἑαυτοῦ:000000">seu</a> <a name="04202402700034" ll="SDBG:περί:000006">respeito</a> constava <a name="04202402700024" ll="SDBG:ἐν:000000">em</a> <a name="04202402700026" ll="SDBG:πᾶς:000000">todas</a> as <a name="04202402700030" ll="SDBG:γραφή:000002" il="LB00279;NBGpb20140728b;Scribe at work;WritingScroll-ReedPen-01-065d;tb102605482">Escrituras</a>.
    
\v 28 Quando se <a name="04202402800004" ll="SDBG:ἐγγίζω:000000">aproximavam</a> <a name="04202402800006" ll="SDBG:εἰς:000000">da</a> <a name="04202402800010" ll="SDBG:κώμη:000000" il="il_sepphoris_village_d01">aldeia</a> para <a name="04202402800012" ll="SDBG:οὗ:000000">onde</a> <a name="04202402800014" ll="SDBG:πορεύομαι:000001">iam</a>, fez ele menção de <a name="04202402800024" ll="SDBG:πορεύομαι:000002">passar</a> adiante. 
\v 29 <a name="04202402900002">Mas</a> eles <a name="04202402900006" ll="SDBG:αὐτός:001000">o</a> <a name="04202402900004" ll="SDBG:παραβιάζομαι:000000">constrangeram</a>, <a name="04202402900008" ll="SDBG:λέγω:000000">dizendo</a>: <a name="04202402900010" ll="SDBG:μένω:000000">Fica</a> <a name="04202402900012" ll="SDBG:μετά:000000">conosco</a>, <a name="04202402900016" ll="SDBG:ὅτι:000001">porque</a> é <a name="04202402900020" ll="SDBG:ἑσπέρα:000000">tarde</a>, <a name="04202402900024;04202402900034" ll="SDBG:καί:000001">e</a> o <a name="04202402900032" ll="SDBG:ἡμέρα:000001">dia</a> já <a name="04202402900026" ll="SDBG:κλίνω:000002;SDBG:κλίνω:000003">declina</a>. E <a name="04202402900036" ll="SDBG:εἰσέρχομαι:000000">entrou</a> para <a name="04202402900040" ll="SDBG:μένω:000000">ficar</a> <a name="04202402900042" ll="SDBG:σύν:000000">com</a> <a name="04202402900044" ll="SDBG:αὐτός:001000">eles</a>. 
\v 30 <a name="04202403000002;04202403000026" ll="SDBG:καί:000001;SDBG:καί:000004">E</a> <a name="04202403000004" ll="SDBG:γίνομαι:000003">aconteceu</a> que, <a name="04202403000006" ll="SDBG:ἐν:000019">quando</a> estavam à <a name="04202403000010" ll="SDBG:κατακλίνω:000001" il="Realia:5.7b">mesa</a>, <a name="04202403000018" ll="SDBG:λαμβάνω:000000">tomando</a> ele o <a name="04202403000022" ll="SDBG:ἄρτος:000000" il="CN01861;LB00136;Tandır_bread;tb061116157">pão</a>, <a name="04202403000024" ll="SDBG:εὐλογέω:000000">abençoou</a>-o e, tendo-o <a name="04202403000028" ll="SDBG:κλάω:000000">partido</a>, <a name="04202403000012" ll="SDBG:αὐτός:001000">lhes</a> <a name="04202403000030" ll="SDBG:ἐπιδίδωμι:000000">deu</a>; 
\v 31 <a name="04202403100004" ll="SDBG:δέ:000001">então</a>, se <a name="04202403100002" ll="SDBG:αὐτός:001000">lhes</a> <a name="04202403100006" ll="SDBG:διανοίγω:000000">abriram</a> os <a name="04202403100010" ll="SDBG:ὀφθαλμός:000000">olhos</a>, <a name="04202403100012">e</a> <a name="04202403100016" ll="SDBG:αὐτός:001000">o</a> <a name="04202403100014" ll="SDBG:ἐπιγινώσκω:000003">reconheceram</a>; <a name="04202403100018" ll="SDBG:καί:000001">mas</a> <a name="04202403100020" ll="SDBG:αὐτός:001000">ele</a> <a name="04202403100022;04202403100024" ll="SDBG:γίνομαι:000002;SDBG:ἄφαντος:000000">desapareceu</a> <a name="04202403100026" ll="SDBG:ἀπό:000002">da</a> presença <a name="04202403100028" ll="SDBG:αὐτός:001000">deles</a>. 
\v 32 <a name="04202403200002" ll="SDBG:καί:000001">E</a> disseram um ao <a name="04202403200006;04202403200008" ll="SDBG:πρός:000004;SDBG:ἀλλήλων:000000">outro</a>: Porventura, <a name="04202403200010" ll="SDBG:οὐχί:000001">não</a> nos <a name="04202403200018" ll="SDBG:καίω:000000">ardia</a> o <a name="04202403200014" ll="SDBG:καρδία:000000">coração</a>, <a name="04202403200026" ll="SDBG:ὡς:000004">quando</a> ele, <a name="04202403200022" ll="SDBG:ἐν:000000">pelo</a> <a name="04202403200036" ll="SDBG:ὁδός:000001">caminho</a>, nos <a name="04202403200028" ll="SDBG:λαλέω:000000">falava</a>, <a name="04202403200038" ll="SDBG:ὡς:000004">quando</a> nos <a name="04202403200040" ll="SDBG:διανοίγω:000001">expunha</a> as <a name="04202403200046" ll="SDBG:γραφή:000002" il="LB00279;NBGpb20140728b;Scribe at work;WritingScroll-ReedPen-01-065d;tb102605482">Escrituras</a>? 
\v 33 <a name="04202403300002;04202403300018" ll="SDBG:καί:000001">E</a>, na <a name="04202403300006" ll="SDBG:αὐτός:001000">mesma</a> <a name="04202403300010" ll="SDBG:ὥρα:000000">hora</a>, <a name="04202403300004" ll="SDBG:ἀνίστημι:000004">levantando</a>-se, <a name="04202403300012" ll="SDBG:ὑποστρέφω:000000">voltaram</a> <a name="04202403300014" ll="SDBG:εἰς:000000">para</a> Jerusalém, onde <a name="04202403300020" ll="SDBG:εὑρίσκω:000000">acharam</a> reunidos os <a name="04202403300026" ll="SDBG:ἕνδεκα:000000">onze</a> e outros <a name="04202403300032" ll="SDBG:σύν:000000">com</a> <a name="04202403300034" ll="SDBG:αὐτός:001000">eles</a>, 
\v 34 os quais <a name="04202403400002" ll="SDBG:λέγω:000000">diziam</a>: O <a name="04202403400012" ll="SDBG:κύριος:000000">Senhor</a> <a name="04202403400008" ll="SDBG:ἐγείρω:000004">ressuscitou</a> <a name="04202403400014" ll="SDBG:καί:000001">e</a> já apareceu a <a name="04202403400018" ll="SDBG:Σίμων:000000">Simão</a>! 
\v 35 <a name="04202403500002" ll="SDBG:καί:000001">Então</a>, os dois <a name="04202403500006" ll="SDBG:ἐξηγέομαι:000000">contaram</a> o que lhes acontecera <a name="04202403500010" ll="SDBG:ἐν:000002">no</a> <a name="04202403500014" ll="SDBG:ὁδός:000001">caminho</a> <a name="04202403500016" ll="SDBG:καί:000001">e</a> <a name="04202403500018" ll="SDBG:ὡς:000002">como</a> fora por <a name="04202403500004" ll="SDBG:αὐτός:001000">eles</a> <a name="04202403500020" ll="SDBG:γινώσκω:000004">reconhecido</a> <a name="04202403500024" ll="SDBG:ἐν:000015">no</a> <a name="04202403500028" ll="SDBG:κλάσις:000000">partir</a> do <a name="04202403500032" ll="SDBG:ἄρτος:000000" il="CN01861;LB00136;Tandır_bread;tb061116157">pão</a>.
    
\v 36 <a name="04202403600008" ll="SDBG:λαλέω:000000">Falavam</a> ainda estas coisas quando Jesus <a name="04202403600012" ll="SDBG:ἵστημι:000006">apareceu</a> <a name="04202403600014">no</a> <a name="04202403600016" ll="SDBG:μέσος:000000">meio</a> <a name="04202403600006" ll="SDBG:αὐτός:001000">deles</a> <a name="04202403600020" ll="SDBG:καί:000001">e</a> <a name="04202403600010" ll="SDBG:αὐτός:001001">lhes</a> <a name="04202403600022" ll="SDBG:λέγω:000000">disse</a>: <a name="04202403600026" ll="SDBG:εἰρήνη:000000">Paz</a> seja convosco! 
\v 37 Eles, <a name="04202403700004" ll="SDBG:δέ:000002">porém</a>, <a name="04202403700002" ll="SDBG:πτοέω:000000">surpresos</a> <a name="04202403700006" ll="SDBG:καί:000000">e</a> <a name="04202403700008;04202403700010" ll="SDBG:γίνομαι:000002;SDBG:ἔμφοβος:000000">atemorizados</a>, <a name="04202403700012" ll="SDBG:δοκέω:000000">acreditavam</a> estarem <a name="04202403700016" ll="SDBG:θεωρέω:000000">vendo</a> um <a name="04202403700014" ll="SDBG:πνεῦμα:000003">espírito</a>. 
\v 38 <a name="04202403800002" ll="SDBG:καί:000001">Mas</a> ele <a name="04202403800006" ll="SDBG:αὐτός:001000">lhes</a> disse: Por <a name="04202403800008" ll="SDBG:τίς:000001">que</a> estais <a name="04202403800010" ll="SDBG:ταράσσω:000001">perturbados</a>? <a name="04202403800014" ll="SDBG:καί:000000">E</a> por que <a name="04202403800022" ll="SDBG:ἀναβαίνω:000004">sobem</a> <a name="04202403800020" ll="SDBG:διαλογισμός:000003">dúvidas</a> <a name="04202403800024">ao</a> vosso <a name="04202403800028" ll="SDBG:καρδία:000000">coração</a>? 
\v 39 Vede as minhas <a name="04202403900006" ll="SDBG:χείρ:000000">mãos</a> <a name="04202403900010" ll="SDBG:καί:000000">e</a> os meus <a name="04202403900014" ll="SDBG:πούς:000000">pés</a>, <a name="04202403900018" ll="SDBG:ὅτι:000000">que</a> <a name="04202403900022">sou</a> <a name="04202403900008" ll="SDBG:ἐγώ:000000">eu</a> <a name="04202403900024" ll="SDBG:αὐτός:001001">mesmo</a>; <a name="04202403900026" ll="SDBG:ψηλαφάω:000000">apalpai</a>-me <a name="04202403900030" ll="SDBG:καί:000000">e</a> verificai, <a name="04202403900034" ll="SDBG:ὅτι:000000">porque</a> um <a name="04202403900036" ll="SDBG:πνεῦμα:000003">espírito</a> <a name="04202403900044" ll="SDBG:οὐ:000000">não</a> <a name="04202403900046" ll="SDBG:ἔχω:000000">tem</a> <a name="04202403900038" ll="SDBG:σάρξ:000000">carne</a> <a name="04202403900040" ll="SDBG:καί:000000">nem</a> <a name="04202403900042" ll="SDBG:ὀστέον:000000">ossos</a>, <a name="04202403900048" ll="SDBG:καθώς:000002">como</a> <a name="04202403900052" ll="SDBG:θεωρέω:000000">vedes</a> que eu <a name="04202403900054" ll="SDBG:ἔχω:000000">tenho</a>. 
\v 40 Dizendo isto, <a name="04202404000010" ll="SDBG:αὐτός:001000">mostrou</a>-lhes as <a name="04202404000014" ll="SDBG:χείρ:000000">mãos</a> <a name="04202404000002" ll="SDBG:καί:000001">e</a> os <a name="04202404000020" ll="SDBG:πούς:000000">pés</a>. 
\v 41 <a name="04202404100004;04202404100016" ll="SDBG:δέ:000001;SDBG:καί:000000">E</a>, por não <a name="04202404100006" ll="SDBG:ἀπιστέω:000000">acreditarem</a> eles <a name="04202404100002" ll="SDBG:ἔτι:000000">ainda</a>, por <a name="04202404100010" ll="SDBG:ἀπό:000008">causa</a> da <a name="04202404100014" ll="SDBG:χαρά:000000">alegria</a>, e estando <a name="04202404100018" ll="SDBG:θαυμάζω:000000">admirados</a>, Jesus <a name="04202404100008" ll="SDBG:αὐτός:001000">lhes</a> disse: <a name="04202404100024" ll="SDBG:ἔχω:000000">Tendes</a> <a name="04202404100030" ll="SDBG:ἐνθάδε:000000">aqui</a> <a name="04202404100026">alguma</a> coisa que <a name="04202404100028" ll="SDBG:βρώσιμος:000000">comer</a>? 
\v 42 <a name="04202404200004" ll="SDBG:δέ:000001">Então</a>, <a name="04202404200008" ll="SDBG:αὐτός:001000">lhe</a> <a name="04202404200006" ll="SDBG:ἐπιδίδωμι:000000">apresentaram</a> um <a name="04202404200014" ll="SDBG:κηρίον:000000;SDBG:μελίσσιος:000000;SDBG:μέρος:000000">pedaço</a> de <a name="04202404200010" ll="SDBG:ἰχθύς:000000" il="Fauna:5.1b;HK00207;LB00210;LB00212">peixe</a> <a name="04202404200012" ll="SDBG:ὀπτός:000000">assado</a> [e um favo de mel]. 
\v 43 E ele comeu na presença deles.
    
\v 44 A seguir, Jesus <a name="04202404400008" ll="SDBG:αὐτός:001000">lhes</a> disse: São <a name="04202404400010" ll="SDBG:οὗτος:000000">estas</a> as <a name="04202404400014" ll="SDBG:λόγος:000002">palavras</a> <a name="04202404400018" ll="SDBG:ὅς:000000">que</a> eu vos <a name="04202404400020" ll="SDBG:λαλέω:000000">falei</a>, estando <a name="04202404400026" ll="SDBG:ἔτι:000000">ainda</a> <a name="04202404400030" ll="SDBG:σύν:000000">convosco</a>: <a name="04202404400036" ll="SDBG:δεῖ:000000">importava</a> se <a name="04202404400038" ll="SDBG:πληρόω:000006">cumprisse</a> <a name="04202404400040" ll="SDBG:πᾶς:000003">tudo</a> o <a name="04202404400064" ll="SDBG:περί:000006">que</a> de mim está <a name="04202404400044" ll="SDBG:γράφω:000001">escrito</a> <a name="04202404400046" ll="SDBG:ἐν:000000">na</a> <a name="04202404400050" ll="SDBG:νόμος:000001">Lei</a> de Moisés, nos <a name="04202404400058" ll="SDBG:προφήτης:000000">Profetas</a> <a name="04202404400054" ll="SDBG:καί:000000">e</a> nos <a name="04202404400062" ll="SDBG:ψαλμός:000000">Salmos</a>. 
\v 45 <a name="04202404500002" ll="SDBG:τότε:000000">Então</a>, <a name="04202404500006" ll="SDBG:αὐτός:001000">lhes</a> <a name="04202404500004" ll="SDBG:διανοίγω:000002">abriu</a> o <a name="04202404500010" ll="SDBG:νοῦς:000000">entendimento</a> para <a name="04202404500014" ll="SDBG:συνίημι:000000">compreenderem</a> as <a name="04202404500018" ll="SDBG:γραφή:000002" il="LB00279;NBGpb20140728b;Scribe at work;WritingScroll-ReedPen-01-065d;tb102605482">Escrituras</a>; 
\v 46 <a name="04202404600002" ll="SDBG:καί:000001">e</a> <a name="04202404600006" ll="SDBG:αὐτός:001000">lhes</a> disse: <a name="04202404600008;04202404600010" ll="SDBG:οὕτω:000000;SDBG:ὅτι:000000">Assim</a> está <a name="04202404600012" ll="SDBG:γράφω:000001">escrito</a> que o <a name="04202404600018" ll="SDBG:Χριστός:000000">Cristo</a> havia de <a name="04202404600014" ll="SDBG:πάσχω:000000">padecer</a> <a name="04202404600020" ll="SDBG:καί:000001">e</a> <a name="04202404600022" ll="SDBG:ἀνίστημι:000006">ressuscitar</a> <a name="04202404600024" ll="SDBG:ἐκ:000006">dentre</a> os <a name="04202404600026" ll="SDBG:νεκρός:000000">mortos</a> no <a name="04202404600030" ll="SDBG:τρίτος:000000">terceiro</a> <a name="04202404600032" ll="SDBG:ἡμέρα:000001">dia</a> 
\v 47 <a name="04202404700002" ll="SDBG:καί:000001">e</a> que <a name="04202404700006" ll="SDBG:ἐπί:000015">em</a> <a name="04202404700012" ll="SDBG:αὐτός:001000">seu</a> <a name="04202404700010" ll="SDBG:ὄνομα:000000">nome</a> se <a name="04202404700004" ll="SDBG:κηρύσσω:000002">pregasse</a> <a name="04202404700014" ll="SDBG:μετάνοια:000000">arrependimento</a> para <a name="04202404700018" ll="SDBG:ἄφεσις:000000">remissão</a> de <a name="04202404700020" ll="SDBG:ἁμαρτία:000002">pecados</a> <a name="04202404700016" ll="SDBG:εἰς:000006">a</a> <a name="04202404700024" ll="SDBG:πᾶς:000003">todas</a> as <a name="04202404700028" ll="SDBG:ἔθνος:000000">nações</a>, começando <a name="04202404700032" ll="SDBG:ἀπό:000002">de</a> Jerusalém. 
\v 48 Vós sois <a name="04202404800004" ll="SDBG:μάρτυς:000000">testemunhas</a> destas coisas. 
\v 49 Eis que <a name="04202404900008" ll="SDBG:ἀποστέλλω:000000">envio</a> <a name="04202404900020" ll="SDBG:ἐπί:000007">sobre</a> vós a <a name="04202404900012" ll="SDBG:ἐπαγγελία:000000">promessa</a> de meu <a name="04202404900016" ll="SDBG:πατήρ:000003">Pai</a>; <a name="04202404900028" ll="SDBG:καθίζω:000002">permanecei</a>, pois, <a name="04202404900030" ll="SDBG:ἐν:000000">na</a> <a name="04202404900034" ll="SDBG:πόλις:000001">cidade</a>, <a name="04202404900036" ll="SDBG:ἕως:000000">até</a> que <a name="04202404900042" ll="SDBG:ἐκ:000005">do</a> <a name="04202404900044" ll="SDBG:ὕψος:000001">alto</a> sejais <a name="04202404900040" ll="SDBG:ἐνδύω:000000">revestidos</a> de <a name="04202404900046" ll="SDBG:δύναμις:000001">poder</a>.
    
\v 50 <a name="04202405000004">Então</a>, <a name="04202405000006" ll="SDBG:αὐτός:001000">os</a> <a name="04202405000002" ll="SDBG:ἐξάγω:000000">levou</a> para <a name="04202405000014" ll="SDBG:Βηθανία:000000" il="HK00367">Betânia</a> <a name="04202405000016" ll="SDBG:καί:000001">e</a>, <a name="04202405000018" ll="SDBG:ἐπαίρω:000000">erguendo</a> as <a name="04202405000022" ll="SDBG:χείρ:000000">mãos</a>, <a name="04202405000024" ll="SDBG:αὐτός:001000">os</a> <a name="04202405000026" ll="SDBG:εὐλογέω:000001">abençoou</a>. 
\v 51 <a name="04202405100004" ll="SDBG:γίνομαι:000003">Aconteceu</a> que, <a name="04202405100006" ll="SDBG:ἐν:000019">enquanto</a> <a name="04202405100012" ll="SDBG:αὐτός:001000">os</a> <a name="04202405100010" ll="SDBG:εὐλογέω:000001">abençoava</a>, ia-se <a name="04202405100016" ll="SDBG:διΐστημι:000001">retirando</a> <a name="04202405100014;04202405100018" ll="SDBG:αὐτός:001000;SDBG:ἀπό:000002">deles</a>, sendo <a name="04202405100024" ll="SDBG:ἀναφέρω:000001">elevado</a> <a name="04202405100026" ll="SDBG:εἰς:000001">para</a> o <a name="04202405100030" ll="SDBG:οὐρανός:000001">céu</a>. 
\v 52 <a name="04202405200002" ll="SDBG:καί:000001">Então</a>, <a name="04202405200004" ll="SDBG:αὐτός:001000">eles</a>, <a name="04202405200006" ll="SDBG:προσκυνέω:000000">adorando</a>-o, <a name="04202405200010" ll="SDBG:ὑποστρέφω:000000">voltaram</a> <a name="04202405200012" ll="SDBG:εἰς:000000">para</a> Jerusalém, tomados <a name="04202405200016" ll="SDBG:μετά:000003">de</a> <a name="04202405200020" ll="SDBG:μέγας:000001">grande</a> <a name="04202405200018" ll="SDBG:χαρά:000000">júbilo</a>; 
\v 53 <a name="04202405300002" ll="SDBG:καί:000001">e</a> estavam sempre <a name="04202405300010" ll="SDBG:ἐν:000000">no</a> templo, louvando a <a name="04202405300020" ll="SDBG:θεός:000000">Deus</a>.
