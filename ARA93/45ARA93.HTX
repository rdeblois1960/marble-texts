\v 8 <a name="04501400800006" ll="SDBG:γάρ:000000">Porque</a>, <a name="04501400800002" ll="SDBG:ἐάν:000000">se</a> <a name="04501400800008" ll="SDBG:ζάω:000000">vivemos</a>, para o <a name="04501400800012" ll="SDBG:κύριος:000000">Senhor</a> <a name="04501400800014" ll="SDBG:ζάω:000000">vivemos</a>; <a name="04501400800016" ll="SDBG:ἐάν:000000">se</a> morremos, para o <a name="04501400800024" ll="SDBG:κύριος:000000">Senhor</a> morremos. <a name="04501400800028" ll="SDBG:ἐάν:000000">Quer</a>, <a name="04501400800032" ll="SDBG:οὖν:000000">pois</a>, <a name="04501400800034" ll="SDBG:ζάω:000000">vivamos</a> <a name="04501400800036" ll="SDBG:ἐάν:000000">ou</a> morramos, somos do <a name="04501400800044" ll="SDBG:κύριος:000000">Senhor</a>. 
\v 9 <a name="04501400900006" ll="SDBG:γάρ:000000">Foi</a> precisamente <a name="04501400900002" ll="SDBG:εἰς:000005">para</a> esse fim que <a name="04501400900008" ll="SDBG:Χριστός:000001">Cristo</a> morreu <a name="04501400900012" ll="SDBG:καί:000001">e</a> ressurgiu: <a name="04501400900016" ll="SDBG:ἵνα:000000">para</a> ser <a name="04501400900026" ll="SDBG:κυριεύω:000000">Senhor</a> <a name="04501400900018" ll="SDBG:καί:000005">tanto</a> de <a name="04501400900020" ll="SDBG:νεκρός:000000">mortos</a> <a name="04501400900022" ll="SDBG:καί:000005">como</a> de <a name="04501400900014" ll="SDBG:ζάω:000001;SDBG:ἀναζάω:000000">vivos</a>. 
\v 10 <a name="04501401000002;04501401000014" ll="SDBG:σύ:000000">Tu</a>, <a name="04501401000004" ll="SDBG:δέ:000000">porém</a>, por <a name="04501401000006" ll="SDBG:τίς:000001">que</a> <a name="04501401000008" ll="SDBG:κρίνω:000005">julgas</a> teu <a name="04501401000012" ll="SDBG:ἀδελφός:000001">irmão</a>? <a name="04501401000018" ll="SDBG:καί:000003">E</a> tu, por <a name="04501401000022" ll="SDBG:τίς:000001">que</a> <a name="04501401000024" ll="SDBG:ἐξουθενέω:000000">desprezas</a> o teu? <a name="04501401000034" ll="SDBG:γάρ:000000">Pois</a> <a name="04501401000032" ll="SDBG:πᾶς:000000">todos</a> <a name="04501401000036" ll="SDBG:παρίστημι:000008">compareceremos</a> perante o <a name="04501401000040" ll="SDBG:βῆμα:000000" il="Bema-Corinth;tr_srd_synagogue_bema_f01">tribunal</a> de <a name="04501401000044" ll="SDBG:θεός:000000">Deus</a>. 
\v 11 <a name="04501401100004" ll="SDBG:γάρ:000000">Como</a> está <a name="04501401100002" ll="SDBG:γράφω:000001">escrito</a>:Por <a name="04501401100008" ll="SDBG:ἐγώ:000000">minha</a> <a name="04501401100006" ll="SDBG:ζάω:000000">vida</a>, <a name="04501401100010" ll="SDBG:λέγω:000000">diz</a> o <a name="04501401100012" ll="SDBG:κύριος:000000">Senhor</a>, diante de mim se <a name="04501401100018" ll="SDBG:κάμπτω:000000">dobrará</a> <a name="04501401100020" ll="SDBG:πᾶς:000000">todo</a> <a name="04501401100022" ll="SDBG:γόνυ:000001;SDBG:γόνυ:000002">joelho</a>, <a name="04501401100024" ll="SDBG:καί:000000">e</a> <a name="04501401100026" ll="SDBG:πᾶς:000000">toda</a> <a name="04501401100028" ll="SDBG:γλῶσσα:000006">língua</a> dará <a name="04501401100030" ll="SDBG:ἐξομολογέω:000001;SDBG:ἐξομολογέω:000004">louvores</a> a <a name="04501401100034" ll="SDBG:θεός:000000">Deus</a>.
    
\v 12 <a name="04501401200004" ll="SDBG:οὖν:000000">Assim</a>, <a name="04501401200002" ll="SDBG:ἄρα:000000">pois</a>, cada <a name="04501401200006" ll="SDBG:ἕκαστος:000000">um</a> de nós <a name="04501401200016" ll="SDBG:δίδωμι:000007">dará</a> <a name="04501401200014" ll="SDBG:λόγος:000005">contas</a> <a name="04501401200010" ll="SDBG:περί:000001;SDBG:περί:000006">de</a> si <a name="04501401200012" ll="SDBG:ἑαυτοῦ:000000">mesmo</a> a <a name="04501401200020" ll="SDBG:θεός:000000">Deus</a>.
    
