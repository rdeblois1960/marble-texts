\v 7 
    
    <a name="06600100700004">Il</a> <a name="06600100700004">vient</a> <a name="06600100700006">avec</a> les <a name="06600100700010" ll="SDBG:νεφέλη:000000">nuées</a> :<a name="06600100700018;06600100700020" ll="SDBG:πᾶς:000000;SDBG:ὀφθαλμός:000001">tous</a> <a name="06600100700016">le</a> <a name="06600100700014" ll="SDBG:ὁράω:000000">verront</a>,même ceux <a name="06600100700028" ll="SDBG:ἐκκεντέω:000000">qui</a> <a name="06600100700026">l</a>’<a name="06600100700028" ll="SDBG:ἐκκεντέω:000000">ont</a> <a name="06600100700028" ll="SDBG:ἐκκεντέω:000000">transpercé</a>,
    et <a name="06600100700038" ll="SDBG:πᾶς:000000">toutes</a> les <a name="06600100700042" ll="SDBG:φυλή:000001">tribus</a> de la <a name="06600100700046" ll="SDBG:γῆ:000000">terre</a> <a name="06600100700032" ll="SDBG:κόπτω:000001" il="HK00290">se</a> <a name="06600100700032" ll="SDBG:κόπτω:000001" il="HK00290">lamenteront</a> <a name="06600100700034;06600100700036">à</a> <a name="06600100700034;06600100700036">son</a> <a name="06600100700034;06600100700036">sujet</a>.<a name="06600100700048" ll="SDBG:ναί:000000">Oui</a>, <a name="06600100700050" ll="SDBG:ἀμήν:000000">amen</a> !
    
\v 8 
